        <Static id="Static05" taborder="12" left="29.23%" top="80" right="26.92%" height="84" onclick="Static05_onclick"/>
        <Static id="Static06" taborder="13" cssclass="sta_WF_TopLine" left="0" top="70" right="0" height="2"/>
        <Grid id="grd_list" taborder="21" binddataset="ds_main" scrollbars="autovert" scrollpixel="all" useinputpanel="false" autofittype="col" nodataimage="URL('theme://Image\grd_WF_NoDataImg.png')" onvscroll="grd_list_onvscroll" left="10" top="81" right="10" bottom="11" onslide="grd_list_onslide">
