        <Edit id="edt_count" taborder="0" displaynulltext="수량을 입력하세요" left="9" top="85" width="401" height="47" inputtype="number,dot" autoselect="false" onchar="edt_count_onchar" style="align:right middle;" onkeydown="edt_count_onkeydown"/>
        <Static id="Static04" taborder="74" text="수량등록" cssclass="sta_POP_TitleBg" left="0" top="0" width="420" height="70"/>
