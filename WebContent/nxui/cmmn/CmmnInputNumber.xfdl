        <Static id="Static13" taborder="48" text="w10" cssclass="sta_GUIDE_Area" visible="false" left="410" top="80" width="10" height="135" style="align:center middle;"/>
        <Static id="Static15" taborder="50" text="w10" cssclass="sta_GUIDE_Area" visible="false" left="0" top="80" width="10" height="136" style="align:center middle;"/>
        <Static id="Static03" taborder="54" text="h20" cssclass="sta_GUIDE_Area" visible="false" left="0" top="196" style="align:center middle;" width="420" height="20"/>
        <Static id="Static00" taborder="68" text="h10" cssclass="sta_GUIDE_Area" visible="false" left="0" top="139" width="420" height="10" style="align:center middle;"/>
        <Button id="btn_insert" taborder="1" text="입력" cssclass="btn_WF_Function" left="345" top="149" width="65" height="47" onclick="btn_insert_onclick"/>
