        <Static id="sta_itemName" taborder="76" text="Static01" left="137" top="149" width="215" height="47" style="color:red;" wordwrap="char"/>
        <MaskEdit id="msk_count" taborder="77" left="464" top="94" width="400" height="45" onchar="msk_count_onchar" type="number" mask="9,999.99" maskchar="_" limitbymask="decimal"/>
      </Layout>
