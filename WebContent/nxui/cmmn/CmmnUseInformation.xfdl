<?xml version="1.0" encoding="utf-8"?>
<FDL version="1.5">
  <TypeDefinition url="..\default_typedef.xml"/>
  <Form id="CmmnUseInformation" classname="GUIDE_class" left="0" top="0" width="540" height="760" titletext="Sub_MenuList">
    <Layouts>
      <Layout>
        <Grid id="grd_list" taborder="0" useinputpanel="false" left="0" top="0" height="756" right="0" cssclass="grd_WF_SubMenuList" binddataset="ds_grd" scrollbars="none" autofittype="col" oncellclick="grd_list_oncellclick">
          <Formats>
            <Format id="default">
              <Columns>
