              </Band>
              <Band id="body">
                <Cell text="bind:menuName"/>
              </Band>
            </Format>
          </Formats>
        </Grid>
      </Layout>
    </Layouts>
    <Bind/>
    <Objects>
      <Dataset id="ds_grd" firefirstcount="0" firenextcount="0" useclientlayout="false" updatecontrol="true" enableevent="true" loadkeymode="keep" loadfiltermode="keep" reversesubsum="false">
        <ColumnInfo>
          <Column id="menuName" type="STRING" size="256"/>
          <Column id="menuId" type="STRING" size="256"/>
          <Column id="url" type="STRING" size="256"/>
        </ColumnInfo>
        <Rows>
          <Row>
            <Col id="menuName">이용안내</Col>
            <Col id="menuId">01</Col>
            <Col id="url">M_MMB::UseInformation.xfdl</Col>
          </Row>
          <Row>
