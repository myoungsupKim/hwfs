<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT V.TRANS_TYPE,
       V.TRANS_GUBUN,
       V.TYPE_NAME,
       V.GUBUN_NAME,
       V.SET1,
       V.SET2,
       V.SET3,
       V.SET4,
       V.SET5
  FROM MAS_TICKET_CODE V
  WHERE (TRANS_GUBUN = #TRANS_GUBUN#)
</statement>
	<input default-name="ds_cond">
<col name="TRANS_GUBUN" size="255" type="VARCHAR" description="" />	
	</input>
	<output default-name="ds_list">
<col name="TRANS_TYPE" size="255" type="VARCHAR" description="" />	
<col name="TRANS_GUBUN" size="255" type="VARCHAR" description="" />	
<col name="TYPE_NAME" size="255" type="VARCHAR" description="" />	
<col name="GUBUN_NAME" size="255" type="VARCHAR" description="" />	
<col name="SET1" size="255" type="VARCHAR" description="" />	
<col name="SET2" size="255" type="VARCHAR" description="" />	
<col name="SET3" size="255" type="VARCHAR" description="" />	
<col name="SET4" size="255" type="VARCHAR" description="" />	
<col name="SET5" size="255" type="VARCHAR" description="" />	
	</output>
</query>