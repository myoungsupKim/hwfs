<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT 
	   UPJANG_CD
     , ITEM_CD
FROM SLA_IF_MEAL_TICKET_SAL_MST
WHERE UPJANG_CD = #UPJANG#
AND ITEM_CD = #ITEM_CODE#
</statement>
	<input default-name="ds_cond">
<col name="ITEM_CODE" size="" type="VARCHAR" description="" />	
<col name="UPJANG" size="" type="VARCHAR" description="" />	
	</input>
	<output default-name="ds_cnt">
<col name="CNT" size="" type="NUMERIC" description="" />
	</output>
</query>