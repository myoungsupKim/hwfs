<?xml version="1.0" encoding="euc-kr"?><query dynamic="true">	<type>select</type>	<description><![CDATA[]]></description>	<reload>true</reload>	<monitoring>true</monitoring>	<result-count>1</result-count>	<statement>
  SELECT DECODE(COUNT(*), 0, 'false', 'true') ABLE_YN        FROM SCC_COMMON_CODE
   WHERE GROUP_CODE = '전체매출'
     AND CODE       = #UPJANG# 

	</statement>
	<input default-name="ds_Exp">		<col name="UPJANG" size="" type="NUMERIC" description="" />	</input>	<output default-name="ds_list">		<col name="ABLE_YN" size="255" type="VARCHAR" description="" />	</output>
</query>