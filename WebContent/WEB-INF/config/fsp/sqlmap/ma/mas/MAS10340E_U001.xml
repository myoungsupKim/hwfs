<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>update</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
    UPDATE MAS_ADVANCE
    SET  GAMU      = #GAMU#,
         GADATE    = #GADATE#,
         GANO      = #GANO#
    WHERE (ADV_ID  = #ADV_ID#) 
</statement>
	<input default-name="ds_listSource">
	<col name="GAMU" size="2" type="VARCHAR" description="" /> 
	<col name="GADATE" size="8" type="VARCHAR" description="" /> 
	<col name="GANO" size="22" type="NUMERIC" description="" /> 		
	<col name="ADV_ID" size="22" type="NUMERIC" description="" /> 			
	</input>
	<output default-name="ds_list">
	</output>
</query>