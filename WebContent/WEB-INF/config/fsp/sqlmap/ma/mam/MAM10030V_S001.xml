	, DTL_CD_NAME AS DTL_CD_NAME
	, ORDER_BY AS ORDER_BY
  FROM MAC_CD_DTL
 WHERE  MST_CD = 'C05'
      AND  USE_YN = 'N'
   ORDER BY ORDER_BY	]]></statement>
