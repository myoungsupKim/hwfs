<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>update</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
UPDATE 
 	MAP_DEPRN_STD
SET  DEPRN_NAME = #DEPRN_NAME#
	, DEPRN_TYPE = #DEPRN_TYPE#
	, USE_YY = #USE_YY#
	, RATE = #RATE#
	, STD_REMARK = #STD_REMARK#
	, ATTR01 = #ATTR01#
	, ATTR02 = #ATTR02#
	, ATTR03 = #ATTR03#
	, ATTR04 = #ATTR04#
	, ATTR05 = #ATTR05#
	, ATTR06 = #ATTR06#
	, ATTR07 = #ATTR07#
	, ATTR08 = #ATTR08#
	, ATTR09 = #ATTR09#
	, ATTR10 = #ATTR10#
	, CUSER = #CUSER#
	, CDATE = #CDATE#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 DEPRN_CLASS = #DEPRN_CLASS#
	]]></statement>
