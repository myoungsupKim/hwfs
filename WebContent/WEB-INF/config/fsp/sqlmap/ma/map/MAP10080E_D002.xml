<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM MAP_MAKEUP_STD2
WHERE 
	 YY = #YY#
	 AND UPJANG = #UPJANG#
	 AND CLASS_CD = #CLASS_CD#
	]]></statement>
	<input default-name="ds_din2">
		<col name="YY" size="4" type="VARCHAR" description="" /> 
		<col name="UPJANG" size="6" type="VARCHAR" description="" /> 
		<col name="CLASS_CD" size="10" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_dout2">
	</output>
</query>
