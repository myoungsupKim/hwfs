<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[확정 여부 조회 SQL]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
		SELECT COUNT(1) AS IMPROV_YN   												
		  FROM MAP_BIZ_LOG_MGMT												
		 WHERE YY = #INPUT_YY# 	
		   AND UPJANG = #UPJANG#  
		   AND BIZ_CLASS = #BIZ_CLASS#
	</statement>
	<input default-name="ds_Cond">
	</input>
	<output default-name="ds_last_confirm_YN">
		<col name="IMPROV_YN" size="256" type="VARCHAR" description="" />  
	</output>
</query>