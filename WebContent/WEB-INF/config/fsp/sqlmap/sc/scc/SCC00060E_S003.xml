<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT SYS_ID, SYS_NAME
FROM SCC_SYSTEM 
WHERE SYS_TYPE = #inputVal#
ORDER BY SORT_SEQ	]]></statement>
	<input default-name="ds_SCC_SYSTEM_INPUT">
		<col name="inputVal" size="255" type="VARCHAR" description="" /> 
	</input>
	<output default-name="DS_SCC_SYSTEM">
		<col name="SYS_ID" size="22" type="NUMERIC" description="" /> 
		<col name="SYS_NAME" size="100" type="VARCHAR" description="" /> 
	</output>
</query>
