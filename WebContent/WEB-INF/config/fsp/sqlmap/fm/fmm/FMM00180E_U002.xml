<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>update</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
UPDATE 
 	FMM_STD_MENU_RECIPE
SET 
	 ORI_TYPE = #ORI_TYPE#
	, CUSER = #g_EmpNo#
	, CDATE = SYSDATE
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 UPJANG = #UPJANG#
	 AND MENU_CD = #MENU_CD#
	 AND RECIPE_CD = #RECIPE_CD#	]]></statement>
