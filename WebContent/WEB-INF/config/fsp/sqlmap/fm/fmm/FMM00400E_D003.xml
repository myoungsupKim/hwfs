<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[--레시피 삭제 시 추천식단 레시피 아이템정보 삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM FMM_TERM_MENU_RECIPE_ITEM
WHERE 
	 TERM_MENU_CD = #TERM_MENU_CD#
	 AND RECIPE_CD = #RECIPE_CD#
	]]></statement>
	<input default-name="ds_Menu">
		<col name="TERM_MENU_CD" size="20" type="VARCHAR" description="" /> 
		<col name="RECIPE_CD" size="20" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_Menu">
	</output>
</query>
