<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>update</type>
	<description><![CDATA[표준레시피 자재업데이트]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
UPDATE 
 	FMM_STD_RECIPE_ITEM
SET 
	 ITEM_SEQ = #ITEM_SEQ#
	, NEED_QTY = #NEED_QTY#
	, EDIBLE_QTY = #EDIBLE_QTY#
	, CUT_SPEC = #CUT_SPEC#
	, REMARK = #REMARK#
	, USE_YN = #USE_YN#
	, ATTR01 = #ATTR01#
	, ATTR02 = #ATTR02#
	, ATTR03 = #ATTR03#
	, ATTR04 = #ATTR04#
	, ATTR05 = #ATTR05#
	, ATTR06 = #ATTR06#
	, ATTR07 = #ATTR07#
	, ATTR08 = #ATTR08#
	, ATTR09 = #ATTR09#
	, ATTR10 = #ATTR10#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE	UPJANG = #UPJANG#
	 AND RECIPE_CD = #RECIPE_CD#
	 AND ITEM_CODE = #ITEM_CODE#
	]]></statement>
