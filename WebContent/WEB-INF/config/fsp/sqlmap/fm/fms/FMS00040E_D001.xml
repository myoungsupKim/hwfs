	<statement>
<![CDATA[
DELETE FROM SCC_ROLE_MENU
WHERE 
	 SYS_TYPE = #SYS_TYPE#
	 AND ROLE = #ROLE#
	 AND MENU_ID = #MENU_ID#	]]></statement>
