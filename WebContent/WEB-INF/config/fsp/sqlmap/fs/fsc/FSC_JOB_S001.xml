<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<isEqual col="BLANK" value="Y">
SELECT '' AS JOB_CD, '' AS JOB_NAME FROM DUAL
UNION ALL
</isEqual>
SELECT JOB_CD, JOB_NAME
FROM
        (SELECT V.JOB_CD,
                   V.JOB_NAME
              FROM MAS_JOB V
         WHERE	(1 = 1)
         AND    (V.SAUP_TYPE&lt;&gt;'004')
         <isNotNull col="JOB_CD">
             AND        (V.JOB_CD = #JOB_CD#)
         </isNotNull>
         <isNotNull col="JOB_NAME">
            AND        (V.JOB_NAME = #JOB_NAME#)
         </isNotNull>
         ORDER BY V.JOB_CD)	</statement>
	<input default-name="ds_cond">
		<col name="JOB_CD" size="255" type="VARCHAR" description="직군코드" /> 
		<col name="JOB_NAME" size="255" type="VARCHAR" description="직군명" /> 
		<col name="SAUP_TYPE" size="255" type="VARCHAR" description="사업형태" /> 
	</input>
	<output default-name="ds_list">
		<col name="JOB_CD" size="255" type="VARCHAR" description="직군코드" /> 
		<col name="JOB_NAME" size="255" type="VARCHAR" description="직군명" /> 
	</output>
</query>
