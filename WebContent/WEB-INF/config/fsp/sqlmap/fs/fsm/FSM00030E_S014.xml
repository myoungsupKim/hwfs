<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT  TERM_MENU_CD	
 FROM FSM_TERM_MENU_MST
 WHERE 	 MENU_DATE=#fa_MENU_DATE#
AND MEAL=#fa_MEAL#
AND CORNER=#fa_CORNER#
AND MENU_TYPE=#fa_MENU_TYPE#	]]></statement>
	<input default-name="ds_in">
		<col name="fa_MENU_DATE" size="255" type="VARCHAR" description="" /> 
		<col name="fa_MEAL" size="255" type="VARCHAR" description="" /> 
		<col name="fa_CORNER" size="255" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_cnt">
		<col name="TERM_MENU_CD" size="20" type="VARCHAR" description="" /> 
	</output>
</query>
