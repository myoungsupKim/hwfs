<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[사업장식단 레시피리스트 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT  UPJANG
        , MENU_CD
        , RECIPE_CD
        , RECIPE_SEQ
        , RECIPE_NAME
FROM     FSM_REAL_RECIPE_MST
WHERE    UPJANG   = #MST_UPJANG#
AND      MENU_CD  = #MST_MENU_CD#	]]></statement>
	<input default-name="ds_input">
	</input>
	<output default-name="ds_Detail1">
	</output>
</query>
