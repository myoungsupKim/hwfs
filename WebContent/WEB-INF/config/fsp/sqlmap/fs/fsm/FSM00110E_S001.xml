  FROM FSA_HALL_MST
 WHERE UPJANG = #UPJANG#
   AND HALL_CD = #HALL_CD#	]]></statement>
	<input default-name="ds_Input">
	</input>
	<output default-name="ds_HallClass">
	</output>
</query>
