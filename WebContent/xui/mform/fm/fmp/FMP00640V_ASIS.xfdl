function fn_edBtn_OnClick(obj:Button, e:ClickEventInfo)
{
	var sAddWhere = "";
	
	switch(obj.name)
	{
		case "btn_MainUpjang":
			sAddWhere = " AND A.MAIN_UPJANG = A.UPJANG ";

			break;

		case "btn_Upjang":
			if (ffn_ChkNull(ed_MainUpjang.userdata))
			{
				g_Message("EE", fn_AlertMsg("004"));
				if (ed_MainUpjang.enable) ed_MainUpjang.setFocus();
				return;
			}
			sAddWhere = " AND A.MAIN_UPJANG = " + ed_MainUpjang.userdata;
			//사업장인 경우 관리업장만 조회
			if (application.g_UserKind == '사업장')
				sAddWhere += " AND ( A.UPJANG = " + application.g_Upjang + " OR EXISTS (SELECT 1 FROM FMS_USER_UPJANG@FCUS_TO_FSUS B WHERE B.USE_YN = 'Y' AND B.UPJANG = A.UPJANG AND B.SABUN = '" + application.g_EmpNo + "') )";

			break;

		default:
			break;
	}
	//fm공통 팝업클릭 호출
	if (ffn_btn_OnClick(obj, sAddWhere)) fn_Trigger_edBtn(eval("ed_" + gfn_subStr(obj.name, 4)));
}

function fn_edBtn_OnKeyDown(obj:Edit, e:KeyEventInfo)
{
	ffn_ed_OnKeyDown(obj,e.keycode);
	//Del의 경우 뒷처리
	if (e.keycode == 46) fn_Trigger_edBtn(obj);
}

function fn_Trigger_edBtn(obj)
{
	switch(obj)
	{
		case ed_MainUpjang:
			//사업장
			ed_Upjang.userdata = ""; ed_Upjang.value = "";
			//공급업체
			fn_SetOtcust();

			break;
			
		default:

			break;
	}
}

function fn_SetOtcust()
{
	/*-------------*/
	http.Sync = true;
	/*-------------*/
	// 초기화
	ds_Otcust.clearData();
	// 조회
	var actionName, cmdName, inData, outData, otherArg, callBackFnc;
	// 파라미터셋팅
    outData		= "ds_Otcust=ds_List";
    otherArg	= "MAIN_UPJANG=" + wrapQuote(gfn_trim(ed_MainUpjang.userdata));
	//action 정보 초기화 
	fsp_clear(this); 
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "fm/fmz:FMZ_PO_OTCUST_S003");
	// 서버 호출 
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);
	/*-------------*/
	http.Sync = false;
	/*-------------*/
	//전체추가 및 기본창고지정
	ds_Otcust.insertRow(0);
	ds_Otcust.setColumn(0, "OTCUSTCD", "");
	ds_Otcust.setColumn(0, "OTCUSTNM", "전체");
	cbo_Otcust.index = 0;
	if (ds_Otcust.findRowExpr("OTCUSTCD=='0000'") >= 0) cbo_Otcust.value = "0000";
}

