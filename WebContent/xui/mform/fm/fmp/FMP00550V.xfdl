/*------------------------------------------------------------
	1. Function ID : fn_Print()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_print()
{

	var sKind;
	var rKind;
	if (tab_List.tabindex == 0) sKind = "대분류";
	else if (tab_List.tabindex == 1) sKind = "중분류";
	else if (tab_List.tabindex == 2) sKind = "소분류";
	else sKind = "상품";
	
	var arrParams = SCReport.newParams ();
	
	SCReport.setParams (arrParams, SCReport.RPT_TITLE, "수불내역");
	/*
	if ( chk_Day.value == true ) {
		if ( sKind == "상품" )  {
			rKind = "/FICS/FMP00550V_수불내역_상품일자별.ozr";
		} else {
			rKind = "/FICS/FMP00550V_수불내역_상품.ozr";
		}
	} else {
		if ( sKind == "상품" ) {
			rKind = "/FICS/FMP00550V_수불내역_분류일자별.ozr";
		} else {
			rKind = "/FICS/FMP00550V_수불내역_분류.ozr";	
		}
	}
	*/
	rKind = "/FICS/FMP00550V_수불내역_상품일자별.ozr";
	SCReport.setParams (arrParams, SCReport.RPT_NAME, rKind);
	
	SCReport.setParams (arrParams, "cnt", 9);
	SCReport.setParams (arrParams, "PARAM1", ed_Upjang.userdata);	//업장코드
	SCReport.setParams (arrParams, "PARAM2", ed_Upjang.value);		//업장명
	SCReport.setParams (arrParams, "PARAM3", me_Sdate.value);		//기간
	SCReport.setParams (arrParams, "PARAM4", me_Edate.value);		//기간
	SCReport.setParams (arrParams, "PARAM5", cbo_Subinv.value);		//창고코드
	SCReport.setParams (arrParams, "PARAM6", cbo_Subinv.text);		//창고명
	SCReport.setParams (arrParams, "PARAM7", sKind);				//구분
	
	/*
	if ( chk_Day.value == true ) {
		SCReport.setParams (arrParams, "PARAM8", "1");	//일자별
	} else {
		SCReport.setParams (arrParams, "PARAM8", "0");	//일자별
	}
	*/
	SCReport.setParams (arrParams, "PARAM8", "1");	//일자별
	if ( chk_ZERO.value == true ) {
		SCReport.setParams (arrParams, "PARAM9", "1");
	} else { 
		SCReport.setParams (arrParams, "PARAM9", "0");
	}
	
	SCReport.setParams (arrParams, "zoom", 150);
	SCReport.viewReport (this, arrParams, false);	
}

