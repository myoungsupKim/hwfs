function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(obj, false);

	//초기화
   	fsp_init(this);
   	v_ds_check = "ds_list";
   	//ta_Sql.Visible = (global.g_EmpNo == gfmsAdmin);

	ed_Upjang.userdata = application.g_Upjang;
	ed_Upjang.value     = application.g_UpjangNm;
	me_Sdate.value     = fn_CurrDateWeek(); 
	me_Edate.value     = me_Sdate.value;
	
	//창고세팅
	//fn_SetSubinv();
	
	//창고 정보 받아 오기(콤보 박스)	
	fn_sql_common("C",strQuery_FMS_SUBINV," AND B.UPJANG = " + gfn_toString(ed_Upjang.userdata)+ "" ,"","",cbo_Subinv,-1,ds_SUBINV);
	cbo_Subinv.index = 0;

	if (ed_Upjang.enable) ed_Upjang.setFocus();	
}

