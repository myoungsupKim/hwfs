function fn_Trigger_edBtn(obj)
{
	switch(obj)
	{
		case ed_Upjang:
			if(ed_Upjang.value != "")
			{
				fn_sql_common("C",strQuery_FMS_SUBINV," AND B.UPJANG = " + gfn_toString(ed_Upjang.userdata)+ "" ,"","",cbo_Subinv,-1,ds_SUBINV);
				cbo_Subinv.index = 0;
			}
			
			break;
			
		default:

			break;
	}
}

function fn_SetSubinv()
{
	/*-------------*/
	http.Sync = true;
	/*-------------*/
	// 초기화
	ds_SUBINV.clearData();
	// 조회
	var actionName, cmdName, inData, outData, otherArg, callBackFnc;
	// 파라미터셋팅
    outData		= "ds_SUBINV=ds_List";
    otherArg	= "UPJANG=" + wrapQuote(ed_Upjang.userdata);
	//action 정보 초기화 
	fsp_clear(this); 
    // 서버에서 조회할 정보 추가 
	//tit_addSearchActionInfo(this, "fm/fmp:FMP00550V_S002");
	fsp_addSearch(this, "fm/fmz:FMZ_PO_SUBINV_S001");
	// 서버 호출 
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);
	/*-------------*/
	http.Sync = false;
	/*-------------*/
	//기본창고지정
	if (ds_SUBINV.rowcount > 0) cbo_Subinv.index = 0;
}

//=========================================================================================
// [ PART 7]
// 	1. Function ID : fn_Search()
//	2. 개       요 : 왼쪽의 상품 코드 조회 부분 조회 조건은 업장/대분류/중분류/소분류/아이템 별.
//	3. 기       능 : 조회.
//=========================================================================================
function fn_search()
{
	var irtn = G_fn_CalcMonth(me_Sdate.value,me_Edate.value);
	var tabIndex = tab_List.tabindex;
	
	ds_cond.clearData();
	ds_list.clearData();
	ds_cond.addRow();
	
	ds_cond.setColumn(0, "UPJANG", ed_Upjang.userdata);
	//ds_cond.setColumn(0, "SUBINV_CODE", cbo_Subinv.value);
	
	// 사업장 필수체크
	if(ffn_ChkNull(ed_Upjang.value))
	{
		g_Message("EE", "사업장을 선택하세요");
		ed_Upjang.setFocus();
		return;
	}
	
	// 만약에 창고 코드가 없으면 전체로 보고 데이터셋에 NULL
	if (cbo_Subinv.value == ''){
		ds_cond.setColumn(0, "SUBINV_CODE", "");
	}
	else
	{
		ds_cond.setColumn(0, "SUBINV_CODE",   cbo_Subinv.value);
	}
	ds_cond.setColumn(0, "S_DATE", me_Sdate.value);
	ds_cond.setColumn(0, "E_DATE", me_Edate.value);
	ds_cond.setColumn(0, "CHK_DAY", chk_Day.value);	
	
//=========================================================================================
// HISTORY : 2008/07/22 
// 수정자 : 노규완
// 수정 내용 : 재고량 0 제외 체크 박스 기능 추가 
//	           재고량 0 제외 체크 박스 체크 시 그리드의 재고량 0인 것들은 화면에 보이지 않음.
//             FMP00550V_T001.jsp 수정. (데이터셋에 ZERO 추가)
//=========================================================================================	
	if (chk_ZERO.value == true){										//재고량 0 제외 체크 박스.
		ds_cond.setColumn(0,"ZERO","1");
	}
	else{
		ds_cond.setColumn(0,"ZERO","");		
	}
	
//=========================================================================================	
// 2008/07/22 수정 완료
//=========================================================================================	
    switch(tabIndex)
	{
			case 0 :					//대분류 텝
			  // TAB1 --------------
			  ds_cond.setColumn(0,"ITEM_CLASS","ITEM_CLASS2");
			  ds_cond.setColumn(0,"ITEM_DETAIL","0");			  
			  break;
		    case 1 :					//중분류 텝
		    
			  // TAB2 --------------    
			  ds_cond.setColumn(0,"ITEM_CLASS","ITEM_CLASS3");
			  ds_cond.setColumn(0,"ITEM_DETAIL","0");
			  
			  break;
			case 2 :					//소분류 텝
			  // TAB3 --------------
			  ds_cond.setColumn(0,"ITEM_CLASS","ITEM_CLASS4");
			  ds_cond.setColumn(0,"ITEM_DETAIL","0");			  
			  
			  break;
			case 3 :					//상품별 내역 텝
			  // TAB4 -------------- 			  
			  ds_cond.setColumn(0,"ITEM_DETAIL","1");			
			  break;			  			  
			default :
				break;
	}	
	
	if (Math.floor((irtn % 12),0) > 2 ) {
	 gfn_alert("2개월 이상은 조회 불가능 입니다. 확인 하시기 바랍니다.");
		return;
	}
	
	if (me_Sdate.value > me_Edate.value){
		gfn_alert("시작일이 종료일 보다 이후 날짜입니다. 확인 하시기 바랍니다.");
		return;
	}	
		
	if (me_Edate.value == '') {
		gfn_alert("조회 기간이 없습니다. 반드시 기간을 입력 해 주십시오.") ;
		return;
	}
	if ( gfn_subStr(me_Sdate.value,0,6)<> gfn_subStr(me_Edate.value,0,6))
	{
		g_Message("EE", "동일월에서만 조회가 가능합니다.");
		me_Edate.setFocus();
		return;
	}
			
    //일자별 조회 또는 그냥 조회에 따른 화면 그리드 필드 사이즈 조절.
    if (chk_Day.value == '1') {
		tab_List.tab_List1.grd_list1.setFormatColProperty(1,"size",80);
		//tab_List.tab_List1.grd_list1.SetColProp(2,"Width",220);
		tab_List.tab_List2.grd_list2.setFormatColProperty(1,"size",80);
		//tab_List.tab_List2.grd_list2.SetColProp(2,"Width",220);
		tab_List.tab_List3.grd_list3.setFormatColProperty(1,"size",80);
		//tab_List.tab_List3.grd_list3.SetColProp(2,"Width",220);
		tab_List.tab_List4.grd_list4.setFormatColProperty(1,"size",80);
		tab_List.tab_List4.grd_list4.setFormatColProperty(3,"size",100);
		//tab_List.tab_List4.grd_list4.SetColProp(3,"Width",110);		
    }
    else {
		tab_List.tab_List1.grd_list1.setFormatColProperty(1,"size",0);
		//tab_List.tab_List1.grd_list1.SetColProp(2,"Width",330);	
		tab_List.tab_List2.grd_list2.setFormatColProperty(1,"size",0);
		//tab_List.tab_List2.grd_list2.SetColProp(2,"Width",330);
		tab_List.tab_List3.grd_list3.setFormatColProperty(1,"size",0);
		//tab_List.tab_List3.grd_list3.SetColProp(2,"Width",330);
		tab_List.tab_List4.grd_list4.setFormatColProperty(1,"size",0);
		tab_List.tab_List4.grd_list4.setFormatColProperty(3,"size",180);
		//tab_List.tab_List4.grd_list4.SetColProp(3,"Width",165);		
    }
		
   	// 조회
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "FMP00550V_T001";
	strURL   = "U_svc::" + "fm/fmp/FMP00550V_T001.jsp";
	// 데이터셋 설정
	strInDatasets  = "ds_cond=ds_cond";
	strOutDatasets = "ds_list=ds_list";
	// 콜백함수 지정
	//	strCallbackFunc = "fn_CallBack_Search";
	// 서버 호출 
	Transaction(strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc);	
}

