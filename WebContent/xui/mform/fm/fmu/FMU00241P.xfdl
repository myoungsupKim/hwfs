{
	gfn_gridSort(obj, e); //(eval(obj.binddataset), obj, e.cell);
}

function btn_List1_OnClick(obj:Button, e:ClickEventInfo)
{
	if (obj.name == "btn_List1")
		fn_SaveRate(opener.ds_List, ds_List3, "ITEM_CLASS3");
	else
		fn_SaveRate(opener.ds_List, ds_List4, "ITEM_CLASS4");
}

function fn_SaveRate(parObj, chiObj, ColName)
{
	if (parObj.rowcount < 1) return;
	if (chiObj.getCountNF() < 1) return;
	if (SearchRowNF(chiObj,"gfn_length(gfn_trim(OP_RATE))>0")< 0)
	{
		g_Message("EE","일괄적용할 운영율을 입력하세요.");
		return;
	}
	
	//dv_Prg.visible = true;
	if (! g_Confirm("01", '운영율을 일괄적용하시겠습니까?!'))
	{
		//dv_Prg.visible = false;
		return;
	}
	SetWaitCursor(true);
	setCapture();
	//
	Sort(parObj,ColName);
	var jStart = 0;
	// '0'도 아닌 Null인 경우 아무작업두 안한다.
	chiObj.filter("");
	for (i=0; i<chiObj.rowcount; i++)
	{
		for (j=jStart; j<parObj.rowcount; j++)
		{
			//jStart = j;
			if (parObj.getColumn(j,ColName) == chiObj.getColumn(i,"CLASS_CODE"))
			{
				parObj.setColumn(j,"OP_RATE",chiObj.getColumn(i,"OP_RATE"));
			}
		}
	}
	chiObj.filter("gfn_subStr(CLASS_CODE,0,5)==gfn_subStr('" + ds_List3.getColumn(0,"CLASS_CODE") + "',0,5)");
	Sort(parObj,"ITEM_CLASS4");
	parObj.rowposition = 0;
	//분류별 운영율 저장
	fn_SaveData();
	//
	releaseCapture();
	SetWaitCursor(false);
	//dv_Prg.visible = false;
	
	g_Message("01", '운영율이 적용되었습니다');
}

function fn_SaveData()
{
	// 파라미터셋팅
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "FMU00241P_T002";
	strURL   = "U_svc::" + "fm/fmu/FMU00241P_T002.jsp";
	// 데이터셋 설정U
	strInDatasets  = "ds_List2=ds_List2 ds_List3=ds_List3 ds_List4=ds_List4";
	strOutDatasets = "";
	// 파라메터 설정
	strArgument  = "g_Upjang=" + wrapQuote(g_Upjang);
	strArgument += " g_EmpNo=" + wrapQuote(g_EmpNo);
	strArgument += " sUpjang=" + fa_Upjang;
	// 콜백함수 지정
	strCallbackFunc = "fn_CallBack_Save";
	// 서버 호출 
	//Transaction(strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc);
	jsp_transaction(this, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, false, false, true);
}

function fn_CallBack_Save(errCode, errMsg)
{
	ta_Sql.value = fa_Sql;
	if (errCode <> 0) g_Message("EE","분류별 운영율 적용중 오류가 발생하였습니다.");
}

function ds_List2_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var i, j;
	if (e.columnid == "OP_RATE")
	{
		var objTar, iColidx, nCnt;
	
		if (obj.name == "ds_List2")
		{
			objTar = ds_List3; iColidx = 4;
		}
		else if (obj.name == "ds_List3")
		{
			objTar = ds_List4; iColidx = 6;
		}
// 		for (i=0; i<objTar.getCountNF(); i++)
// 		{
// 			if (gfn_subStr(obj.getColumn(e.row,"CLASS_CODE"),0,iColidx)== gfn_subStr(GetColumnNF(objTar,i,"CLASS_CODE"),0,iColidx))
// 				SetColumnNF(objTar,i,e.columnid,obj.getColumn(e.row,e.columnid));
// 		}
		
		for(i = 0; i < ds_List3.rowcount; i++)
		{
			ds_List3.setColumn(i, e.columnid, obj.getColumn(e.row, e.columnid));
			
			ds_List4.filter("gfn_subStr(CLASS_CODE,0,5)==gfn_subStr('" + ds_List3.getColumn(i,"CLASS_CODE") + "',0,5)");
			
			for(j = 0; j < ds_List4.rowcount; j++)
			{
				ds_List4.setColumn( j, e.columnid, obj.getColumn(e.row, e.columnid) );
			}
		}
		ds_List4.filter("gfn_subStr(CLASS_CODE,0,5)==gfn_subStr('" + ds_List3.getColumn(0,"CLASS_CODE") + "',0,5)");
		


	}
}

function ds_List2_OnRowPosChanged(obj:Dataset, e)
{
	ds_List3.filter("gfn_subStr(CLASS_CODE,0,3)==gfn_subStr('" + obj.getColumn(e.newrow,"CLASS_CODE") + "',0,3)");
	if (ds_List3.rowcount > 0)
	{
		ds_List3.rowposition = 0;
		var e1 = new EventInfo();
		e1.newrow = 0;
		ds_List3_OnRowPosChanged(ds_List3,e1);
	}
}

function ds_List3_OnRowPosChanged(obj:Dataset, e1)
{
	ds_List4.filter("gfn_subStr(CLASS_CODE,0,5)==gfn_subStr('" + obj.getColumn(e1.newrow,"CLASS_CODE") + "',0,5)");
}

function ds_List3_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	for(var j = 0; j < ds_List4.rowcount; j++)
	{
		ds_List4.setColumn( j, e.columnid, obj.getColumn(e.row, e.columnid) );
	}
}
