function ds_List_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if ( e.columnid == "OP_PRICE" )
	{
		//운영단가 입력 시 운영율 자동계산
		if (numFormat(obj.getColumn(e.row, "SALE_PRICE"))== "0")
		{
			obj.setColumn(e.row, "OP_RATE", 100 * e.newvalue);
		}
		else
		{
			//운영단가가 0이면 운영율도 0으로 설정...
			if (numFormat(e.newvalue)== "0")
				obj.setColumn(e.row, "OP_RATE", 0);
			else
				obj.setColumn(e.row, "OP_RATE", 100 * ((toNumber(e.newvalue) / toNumber(obj.getColumn(e.row, "SALE_PRICE"))) - 1));
		}
	}
	//운영금액 자동설정
	else if ( (e.columnid == "OP_RATE") || (e.columnid == "CEIL_VAL") )
	{
		if ( numFormat(obj.getColumn(e.row, "OP_PRICE"))== "0" )
			obj.setColumn(e.row, "OP_AMT", Math.ceil((toNumber(obj.getColumn(e.row,"SALE_PRICE")) + Math.round(toNumber(obj.getColumn(e.row,"SALE_PRICE")) * toNumber(obj.getColumn(e.row,"OP_RATE")) / 100)) / Math.pow(10,toNumber(obj.getColumn(e.row,"CEIL_VAL")))) * Math.pow(10,toNumber(obj.getColumn(e.row,"CEIL_VAL"))));
		else
			obj.setColumn(e.row, "OP_AMT", obj.getColumn(e.row,"OP_PRICE"));
	}
}

