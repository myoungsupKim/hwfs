	strURL   = "U_svc::" + "fm/fmu/FMU00240E_T004.jsp";
	// 데이터셋 설정U
	strInDatasets  = "ds_Cond=ds_Cond ds_List=ds_List ds_UpjLst=ds_UpjLst";
	strOutDatasets = "";
