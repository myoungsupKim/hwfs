function fn_search()
{
	var sRtn = fn_ChkSearch("SEARCH");
	if (sRtn != null)
	{
		g_Message("EE", sRtn);
		return;
	}

	// 초기화
	ds_Cond.clearData();
	ds_List.clearData(); ds_MultiUpj.clearData();
	ds_Cond.addRow();
