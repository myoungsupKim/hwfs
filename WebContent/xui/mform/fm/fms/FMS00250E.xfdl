/*------------------------------------------------------------(60)
	1. Function ID : fn_Search()
	2. 개       요 : 업무프로세스에 따른 조회작업
	3. 기       능 : 검색조건에 해당하는 정보를 조회
	4. 인 수 설 명 : 
*/
function fn_search()
{
	//본사사업장 선택여부
	if (gfn_length(gfn_trim(ed_MainUpjang.value))== 0)
	{
		g_Message("EE", fn_AlertMsg("004"));
		return;
	}
	// 초기화
	ds_CustI.clearData();
	ds_CustO.clearData();
	ds_CustI.addRow();
	ds_CustI.setColumn(0, "UPJANG", ed_MainUpjang.userdata);
	ds_CustI.setColumn(0, "OTCUSTCD", ed_Otcust.userdata);
	ds_CustI.setColumn(0, "CUSER", application.g_EmpNo);
	
	// 조회
	var actionName, cmdName, inData, outData, otherArg, callBackFnc;
	
	// 파라미터셋팅
    inData		= "ds_CustI=ds_CustI";
    outData		= "ds_CustO=ds_CustO";
    callBackFnc	= "fn_CallBack_Search";
	
	//action 정보 초기화 
	fsp_clear(this); 
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "fm/fms:FMS00250E_S001");
	// 서버 호출 
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출 
}

