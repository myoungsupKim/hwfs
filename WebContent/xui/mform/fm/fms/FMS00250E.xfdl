// 일괄삭제
function btn_DelteAll_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_CustO.rowcount < 1)
	{
		g_Message("EE","식당정보 조회 후 작업하세요.");
		return;
	}
	
	if (gfn_dsIsUpdated(ds_CustO)==true)
	{
		g_Message("EE", "변경된 데이터가 있습니다.\n 저장 후 삭제해십시오.");
		return;
	}
	
	// 삭제확인
	if(!g_Confirm("","일괄 삭제 하시겠습니까?"))
	{
		return false;
	}
	
	ds_CustO.deleteAll();
	
	fsp_clear(this);
    fsp_addSave(this, "", "", "fm/fms:FMS00250E_D001");
    fsp_callService(this, "", "", "ds_IUD=ds_CustO:U", "", "", "fn_CallBack_Save");		// 서비스 호출 
}

function fn_FormKeyDown(obj:Form, e:KeyEventInfo)
{
	gfn_formKeyDown(obj, e);
	
	//엔터시 조회
	if ( (e.keycode == 13) && (e.fromreferenceobject == ed_Otcust) ) fn_search();
}
