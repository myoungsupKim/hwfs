function fn_Excel_Import(obj:Button, e:ClickEventInfo)
{
	if (ds_CustO.rowcount < 1)
	{
		g_Message("EE", "식당정보 조회 후 작업하세요.");
		return;
	}
	
	//엑셀 임포트
	fd_Excel.Type = "OPEN";	
	//fd_Excel.Filter = "All(*.*)|*.*|Excel File(*.xls)|*.xls|";
	//fd_Excel.Filter = "Excel File(*.xls)|*.xls|Excel File(*.xlsx)|*.xlsx|";
	fd_Excel.Filter = "All(*.*)|*.*|Excel File(*.xls)|*.xls|Excel File(*.xlsx)|*.xlsx|";
	
	if( fd_Excel.Open() )
	{
		var sFullFileName = fd_Excel.FilePath + '\\' + fd_Excel.FileName;
		gd_list.enableredraw = false;
		ds_Excel.clearData();
		//ext_ExcelImportByIndex(파일풀패스, 시트순번, 깡통데이타셑);
		ext_ExcelImportByIndex(sFullFileName, 0, "ds_Excel");
		fn_SetImpData();
		gd_list.enableredraw = true;
	}
}

function fn_SetImpData()
{
	var nRow, sErrMsg, sErrCode;

	ds_ErrLst.clearData();
	for (i = 1; i < ds_Excel.rowcount; i++)
	{
		if (gfn_length(gfn_trim(ds_Excel.getColumn(i,"Col02")))== 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", i + "행)공급업체코드 미등록");
			continue;
		}
		if (gfn_length(gfn_trim(ds_Excel.getColumn(i,"Col04")))== 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", i + "행)사업장코드 미등록");
			continue;
		}
		if (gfn_length(gfn_trim(ds_Excel.getColumn(i,"Col06")))== 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", i + "행)식당코드 미등록");
			continue;
		}		
		// if (length(trim(ds_Excel.GetColumn(i,"Col08"))) == 0)
		// {
			// ds_ErrLst.AddRow();
			// ds_ErrLst.SetColumn(ds_ErrLst.row, "errmsg", i + "행)기준사업장코드 미등록");
			// continue;
		// }
		// if (length(trim(ds_Excel.GetColumn(i,"Col09"))) == 0)
		// {
			// ds_ErrLst.AddRow();
			// ds_ErrLst.SetColumn(ds_ErrLst.row, "errmsg", i + "행)기준사업장명 미등록");
			// continue;
		// }
		if (gfn_length(gfn_trim(ds_Excel.getColumn(i,"Col10")))== 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", i + "행)기준식당코드 미등록");
			continue;
		}		
		//식당코드 + 사업장코드로 매핑조회
		nRow = ds_CustO.findRowExpr("OTCUSTCD=='" + ds_Excel.getColumn(i,"Col02") + "' && SUBINV_CODE=='" + ds_Excel.getColumn(i,"Col06") +"'");
		
		if (nRow < 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", i + "행)해당하는 공급업체(" + ds_Excel.getColumn(i,"Col02") + ")/식당(" + ds_Excel.getColumn(i,"Col06") + ")을 찾을 수가 없습니다");
			continue;
		}
		else
		{
		//공급업체 기준 코드 설정
			ds_CustO.setColumn(nRow, "OTCUST_SUBINV_CODE", ds_Excel.getColumn(i,"Col10"));
			ds_CustO.setColumn(nRow, "OTCUST_SUBINV_NAME", ds_Excel.getColumn(i,"Col11"));
		}
	}
}

