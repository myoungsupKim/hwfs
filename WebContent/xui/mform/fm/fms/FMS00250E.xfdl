/*------------------------------------------------------------
	1. Function ID : fn_Save()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_save()
{
	// 데이타체크
	var sRtn = fn_ChkData();
	if (sRtn != null)
	{
		g_Message("EE",sRtn);
		return;
	}
	
	if (! g_Confirm("02")) return;
	
   	// action 정보 초기화 
    fsp_clear(this);
	// 초기화
	// 파라미터셋팅
    // 서비스셋팅
    var actionName, cmdName, inData, outData, otherArg, callBackFnc;
    var singleSql, insertSql, updateSql, deleteSql, saveFlagColumn, keySqlName, keyIncrement, callbackSql;
    inData		= "ds_IUD=ds_CustO:U ds_CustI=ds_CustI";
    outData		= "ds_CustO=ds_CustO";
    callBackFnc	= "fn_CallBack_Save";    
	
	// 파라미터셋팅(사용자정보 및 관리업장정보)
    insertSql	= "fm/fms:FMS00250E_I001";
    updateSql	= "fm/fms:FMS00250E_U001";
    deleteSql	= "";
    callbackSql	= "fm/fms:FMS00250E_S001";
    
    http.Sync = true;
    // 서버에서 작업할 정보 추가 
    fsp_addSave(this, insertSql, updateSql, deleteSql, saveFlagColumn, keySqlName, keyIncrement, callbackSql);
	// 서버 호출 
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출 
    http.Sync = false;
}

