	if(email == ""){
		ds_main.setColumn(ds_main.rowposition, "EMAIL", ""); 	// 이메일
	
	}else if(!gfn_isEmail(email)) {
		gfn_alert("msg.err.validator.email");  //e-mail이 잘못된 형태로 입력 되었습니다.		
		ds_main.setColumn(ds_main.rowposition, "EMAIL","");
		return;
	}
}
