function fn_setTransText(strText, option) {
	if ( gfn_isNull(strText) ) {
		stc_trans.text = "배   송   지";
		stc_trans.style.cursor = "default";
	} else {
		stc_trans.text = strText;
		stc_trans.style.cursor = "hand";
	}
	
	if ( !gfn_isNull(option) ) {
		stc_trans.trans_upjang = option.upjang;
		stc_trans.trans_cnt = option.cnt;
	}
}

function Combo_OnChanged(obj:Combo, e:ItemChangeEventInfo)
{
	
	var v_cond; 
	switch(obj.name)
	{
		case "Cbo_MasterClass0":
			
			//g_Message("EE", gfn_length(gfn_trim(Cbo_MasterClass0.value)) );
			if( gfn_length(gfn_trim(Cbo_MasterClass0.value))== 0)
			{
				ds_class1_out.filter("0");
				ds_class2_out.filter("0");
				ds_class3_out.filter("0");			
				ds_class4_out.filter("0");				
			}
			else
			{
				v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,2)=='" + gfn_trim(Cbo_MasterClass0.value)+"'";
			//g_Message("EE", v_cond );
				ds_class1_out.filter(v_cond);
				ds_class2_out.filter("0");
				ds_class3_out.filter("0");		
				ds_class4_out.filter("0");	
			}
			Cbo_MasterClass1.index = 0;
			Cbo_MasterClass2.index = 0;
			Cbo_MasterClass3.index = 0;
			Cbo_MasterClass4.index = 0;	
			break;
		case "Cbo_MasterClass1":
			
			//g_Message("EE", Cbo_MasterClass1.value);
			if( gfn_length(gfn_trim(Cbo_MasterClass1.value))== 0)
			{
				ds_class2_out.filter("0");
				ds_class3_out.filter("0");				
				ds_class4_out.filter("0");		
			}
			else
			{
				v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,4)=='" + gfn_trim(Cbo_MasterClass1.value)+"'";
				ds_class2_out.filter(v_cond);
				ds_class3_out.filter("0");			
				ds_class4_out.filter("0");
			}
			Cbo_MasterClass2.index = 0;
			Cbo_MasterClass3.index = 0;
			Cbo_MasterClass4.index = 0;	
			break;
		case "Cbo_MasterClass2":
		
			//g_Message("EE", Cbo_MasterClass3.value);
			if( gfn_length(gfn_trim(Cbo_MasterClass2.value))== 0)
			{
				v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,4)=='" + gfn_trim(Cbo_MasterClass1.value)+"'";				
				ds_class3_out.filter("0");			
				ds_class4_out.filter("0");

			}
			else
			{
				v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,6)=='" + gfn_trim(Cbo_MasterClass2.value)+"'";				
				ds_class3_out.filter(v_cond);			
				ds_class4_out.filter("0");
			}
			Cbo_MasterClass3.index = 0;		
			Cbo_MasterClass4.index = 0;		
			break;
		case "Cbo_MasterClass3":
		
			//g_Message("EE", Cbo_MasterClass3.value);
			if( gfn_length(gfn_trim(Cbo_MasterClass3.value))== 0)
			{
				v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,6)=='" + gfn_trim(Cbo_MasterClass2.value)+"'";				
				ds_class4_out.filter("0");

			}
			else
			{
				v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,8)=='" + gfn_trim(Cbo_MasterClass3.value)+"'";				
				ds_class4_out.filter(v_cond);
			}
			Cbo_MasterClass4.index = 0;		
		    break;
		case "Cbo_MasterClass4":
		    break;
		default:
			ds_class1_out.filter("");
			ds_class2_out.filter("0");
			ds_class3_out.filter("0");
			ds_class4_out.filter("0");
		    break;
	}
}

function Combo_Init()
{
	if( gfn_length(gfn_trim(Cbo_MasterClass0.value))> 0)
	{
		//g_Message("EE", Cbo_MasterClass0.value);
		v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,2)=='" + gfn_trim(Cbo_MasterClass0.value)+"'";
		ds_class1_out.filter(v_cond);		
	}
	if( gfn_length(gfn_trim(Cbo_MasterClass1.value))> 0)
	{
		//g_Message("EE", Cbo_MasterClass1.value);
		v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,4)=='" + gfn_trim(Cbo_MasterClass1.value)+"'";
		ds_class2_out.filter(v_cond);
	}
	if( gfn_length(gfn_trim(Cbo_MasterClass2.value))> 0)
	{
		//g_Message("EE", Cbo_MasterClass2.value);
		v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,6)=='" + gfn_trim(Cbo_MasterClass2.value)+"'";
		ds_class3_out.filter(v_cond);
	}
	if( gfn_length(gfn_trim(Cbo_MasterClass3.value))> 0)
	{
		//g_Message("EE", Cbo_MasterClass3.value);
		v_cond = "gfn_length(gfn_trim(CODE))==0 || gfn_subStr(CODE,0,8)=='" + gfn_trim(Cbo_MasterClass3.value)+"'";
		ds_class4_out.filter(v_cond);
	}
	if( gfn_length(gfn_trim(Cbo_MasterClass1.value))== 0)
	{
		ds_class1_out.filter("");
		ds_class2_out.filter("0");
		ds_class3_out.filter("0");
		ds_class4_out.filter("0");
	}
	
}


function edt_email_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
