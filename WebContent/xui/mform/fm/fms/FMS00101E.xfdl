/*
 ******************************************************************************************
 * 함  수  명   : ed_pop_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
 * 기      능   : 업장엔터시팝업
 * 작  성  자   : 박지영
 * 작성  일자   : 2007-12-18  
 
 ******************************************************************************************
 */
function ed_pop_OnKeyDown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13)
	{
		var objme = eval(obj.userdata);
		objme.click();
	}
	else if (e.keycode == 46)
	{
		var nRow = ds_main.rowposition;
		var strCodeCol,strNameCol;
		
		switch(obj.userdata)
		{
			case "btn_pop1" : 		// 본사그룹
				strCodeCol 	= "MAIN_UPJANG"; 
				strNameCol 	= "MAIN_UPJANG_NM";
				break;
			case "btn_pop2" : 		// 영업부서
				strCodeCol 	= "DEPT_ID"; 
				strNameCol 	= "DEPT_ID_NM";
				break;
			case "btn_pop3" : 		// 계약사원
				strCodeCol 	= "CONT_SALES_SABUN"; 
				strNameCol 	= "CONT_SALES_SABUN_NM";
				break;
			case "btn_pop4" : 		// 영업사원
				strCodeCol 	= "PART_SALES_SABUN"; 
				strNameCol 	= "PART_SALES_SABUN_NM";
				break;
			case "btn_pop5" : 		//C/S 사원
				strCodeCol 	= "CS_SABUN"; 
				strNameCol 	= "CS_SABUN_NM";
				break;
		} 
		
		ds_main.setColumn(nRow, strCodeCol, "");			// 코드
		ds_main.setColumn(nRow, strNameCol, "");			// 명
	}
}

