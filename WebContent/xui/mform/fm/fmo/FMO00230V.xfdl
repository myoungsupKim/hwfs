function btn_MainUpjang_OnClick(obj:Button, e:ClickEventInfo)
{
	var v_Query = " SELECT MAIN_UPJANG, UPJANGNM_DISP AS MAIN_UPJANGNM FROM FMS_UPJANG WHERE MAIN_UPJANG = UPJANG ";
	
	fn_sql_common_setup(v_Query,
						"MAIN_UPJANG",
						"UPJANGNM_DISP",
						"본사코드",
						"본사명",
						"",
