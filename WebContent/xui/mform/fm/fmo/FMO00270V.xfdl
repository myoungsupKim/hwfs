/*
 ******************************************************************************************
 * 함  수  명   : fn_Search()
 * 기      능   : [조회버튼] 검색조건에 해당하는 정보를 조회
 ******************************************************************************************
 */
function fn_search()
{
	// 필수 검색조건 체크
	if( gfn_length(gfn_trim(ds_Input.getColumn(0, "MAIN_UPJANG")))== 0 ) {
		g_Message("EE", "본사를 선택해 주세요.");
		ed_MainUpjangNm.setFocus();
		return;
	}
	if( gfn_length(gfn_trim(ds_Input.getColumn(0, "UPJANG")))== 0 ) {
		g_Message("EE", "사업장을 선택해 주세요.");
		ed_UpjangNm.setFocus();
		return;
	}
	if( gfn_length(gfn_trim(ds_Input.getColumn(0, "FROM_DATE")))== 0 ) {
		g_Message("EE", "조회년월 시작일을 선택/입력해 주세요.");
		me_FromDate.setFocus();
		return;
	} else {
		if( gfn_length(gfn_trim(ds_Input.getColumn(0, "FROM_DATE")))!= 6 ) {
			g_Message("EE", "올바른 조회년월 시작일을 선택/입력해 주세요.");
			me_FromDate.setFocus();
			return;
		}
	}
	if( gfn_length(gfn_trim(ds_Input.getColumn(0, "TO_DATE")))== 0 ) {
		g_Message("EE", "조회년월 종료일을 선택/입력해 주세요.");
		me_ToDate.setFocus();
		return;
	} else {
		if( gfn_length(gfn_trim(ds_Input.getColumn(0, "TO_DATE")))!= 6 ) {
			g_Message("EE", "올바른 조회년월 종료일을 선택/입력해 주세요.");
			me_ToDate.setFocus();
			return;
		}
	}
	//action 정보 초기화 
	fsp_clear(this); 
	fn_SetGridHeader();
	
	ds_List.clearData();
	fsp_addSearch(this,"fm/fmo:FMO00270V_S002");
	fsp_callService(this, "", "", "ds_Input=ds_Input", "ds_List=ds_List", "", "fn_CallBackSearch");
}
/*------------------------------------------------------------
	1. Function    : fn_CallBackSearch(errCode, errMsg)
	2. Description : fn_Search() CallBack Function
	3. Arguments   : 
	4. History     : 2008. 02. 26 - 최초작성
*------------------------------------------------------------*/
function fn_CallBackSearch(errCode, errMsg)
{	
	if( errCode != 0 ) {
		return;
	} else {
	}
	
	if(ds_List.rowcount < 1){
		gfn_alert("조회한 내용이 없습니다.");
	}else{
		//Dataset생성
		var objPivotHeadDs = fn_makeHeadDs(ds_List);
 		var objPivotBodyDs = fn_makeBodyDs(ds_List,objPivotHeadDs);
		fn_crossTabGrid(Grid2,objPivotHeadDs,objPivotBodyDs);				
	}
}

//Pivot Head Dataset생성
function fn_makeHeadDs(objDs)
{
	var sPivotHeadDs = objDs.name + "_PivotHead";
	var objPivotHeadDs;
	if ( this.isValidObject(sPivotHeadDs) ) {
		objPivotHeadDs = eval(sPivotHeadDs);
	} else {
		objPivotHeadDs = new Dataset();
		objPivotHeadDs.name = sPivotHeadDs;
		this.addChild(objPivotHeadDs.name,objPivotHeadDs);
	}

	objPivotHeadDs.clear();
	
	objDs.keystring = "S:YYMM";
	var sPreKey 	  = "";
	var sCurrCode 	  = "";
	var sCurrText 	  = "";
	var nRow;
	for ( var i = 0; i < objDs.rowcount; i++ )
	{
		sPrevDate = objDs.getColumn(i, "YYMM");
		sNextDate = objDs.getColumn(i-1, "YYMM");
		if ( sPrevDate != sNextDate)	
		{			
			nRow = objPivotHeadDs.addRow();
			objPivotHeadDs.addColumn("YYMM","STRING",255);						
			objPivotHeadDs.addColumn("DISP_HEAD1","STRING",255);
			objPivotHeadDs.addColumn("DISP_HEAD2","STRING",255);
			objPivotHeadDs.setColumn(nRow,"YYMM",sPrevDate);
			objPivotHeadDs.setColumn(nRow,"DISP_HEAD1","예상");
			objPivotHeadDs.setColumn(nRow,"DISP_HEAD2","실적");
		}
	}
	return objPivotHeadDs;

}

//Pivot Dataset생성
function fn_makeBodyDs(objDs,objPivotHeadDs)
{
	var sPivotDs = objDs.name + "_Pivot";
	var objPivotDs;
	if ( this.isValidObject(sPivotDs) ) {
		objPivotDs = eval(sPivotDs);
	} else {
		objPivotDs = new Dataset();
		objPivotDs.name = sPivotDs;
		this.addChild(objPivotDs.name,objPivotDs);
	}
	objPivotDs.clear();
	
	//고정컬럼 추가
	objPivotDs.addColumn("LINE_ID","BIGDECIMAL",39);
	objPivotDs.addColumn("LINE_NAME","STRING",100);
	objPivotDs.addColumn("SORT_SEQ","BIGDECIMAL",39);
	objPivotDs.addColumn("YYMM","STRING",6);
	objPivotDs.addColumn("ACT_AMT","BIGDECIMAL",39);
	objPivotDs.addColumn("EXP_AMT","BIGDECIMAL",39);

	//Pivot컬럼 추가
	for (var i=0; i<objPivotHeadDs.rowcount; i++) {
		objPivotDs.addColumn("P_ACT_AMT"+objPivotHeadDs.getColumn(i,0),"BIGDECIMAL",39);
		objPivotDs.addColumn("P_EXP_AMT"+objPivotHeadDs.getColumn(i,0),"BIGDECIMAL",39);
	}

	//objPivotHeadDs	헤더데이터셋	
	var sPrevSEQ = "";
	var sNextSEQ = "";
	var nAddRow;
	var sYYDD;
	var sPrevLINE_ID = "";
	var sNextLINE_ID = "";
	objDs.keystring = "S:SORT_SEQ+YYMM";
	
	objDs.enableevent = false;
	for(var j=0; j<objDs.rowcount;j++){
		
		sPrevLINE_ID = objDs.getColumn(j,"SORT_SEQ");
		sNextLINE_ID = objDs.getColumn(j-1, "SORT_SEQ");
		
		if(sPrevLINE_ID != sNextLINE_ID){
			nAddRow = objPivotDs.addRow();	
			objPivotDs.setColumn(nAddRow,"LINE_ID",objDs.getColumn(j,"LINE_ID"));
			objPivotDs.setColumn(nAddRow,"LINE_NAME",objDs.getColumn(j,"LINE_NAME"));
			objPivotDs.setColumn(nAddRow,"SORT_SEQ",objDs.getColumn(j,"SORT_SEQ"));
		}
	}
	
	for(var k=0; k<objDs.rowcount;k++){
		var nRow = objPivotDs.findRow("SORT_SEQ",objDs.getColumn(k,"SORT_SEQ"));
		objPivotDs.setColumn(nRow,"P_ACT_AMT" + objDs.getColumn(k,"YYMM"),objDs.getColumn(k,"ACT_AMT"));		
		objPivotDs.setColumn(nRow,"P_EXP_AMT" + objDs.getColumn(k,"YYMM"),objDs.getColumn(k,"EXP_AMT"));		
	}
	objDs.enableevent = true;

	var curColid = "";
	var sumActAmt = 0;
	var sumExpAmt = 0;
	
	objPivotDs.enableevent = false;
	for(var m=0; m<objPivotDs.rowcount;m++){
		sumActAmt = 0;
		sumExpAmt = 0;
 		for(var l=0;l<objPivotDs.colcount;l++){
			curColid = objPivotDs.getColID(l+6);
			curColid = String(curColid).substr(0, 9);					
			if(curColid == "P_ACT_AMT") {
				sumActAmt += gfn_nvl(objPivotDs.getColumn(m, objPivotDs.getColID(l+6)), 0);
			}else if(curColid == "P_EXP_AMT"){
				sumExpAmt += gfn_nvl(objPivotDs.getColumn(m, objPivotDs.getColID(l+6)), 0);
			}
 		}
 		objPivotDs.setColumn(m, "ACT_AMT", sumActAmt);
 		objPivotDs.setColumn(m, "EXP_AMT", sumExpAmt);
	}
	objPivotDs.enableevent = true;
	return objPivotDs;
}

//Pivot 그리드포맷생성
function fn_crossTabGrid(objGrid,objPivotHeadDs,objPivotBodyDs)
{
		var v_Formats = "";
		var v_Columns = "";
		var v_Rows = "";
		var v_BandHead = "";
		var v_BandBody = "";

		v_Columns += '<Column size="29" band="left" />\n';
		v_Columns += '<Column size="148" band="left" />\n';
		v_Columns += '<Column size="100" band="left" />\n';
		v_Columns += '<Column size="100" band="left" />\n';
		
		v_Rows += '<Row size="24" band="head" />\n';
		v_Rows += '<Row size="24" band="head" />\n';
		v_Rows += '<Row size="24" />\n';
		
		v_BandHead += '<Cell rowspan="2" displaytype="text" class="head_Excel" taborder="undefined"/>\n';
		v_BandHead += '<Cell col="1" rowspan="2" displaytype="text" text="구분" taborder="undefined"/>\n';
		v_BandHead += '<Cell col="2" colspan="2" displaytype="text" text="합계" taborder="undefined"/>\n';
		v_BandHead += '<Cell row="1" col="2" displaytype="text" text="예상"/>\n';
		v_BandHead += '<Cell row="1" col="3" displaytype="text" text="실적"/>\n';
		
		    
    
		
		v_BandBody += '<Cell celltype="head" displaytype="normal" style="align:center;" text="bind:SORT_SEQ"/>\n';
		v_BandBody += '<Cell col="1" celltype="head" displaytype="text" style="align:left;" text="bind:LINE_NAME"/>\n';
		v_BandBody += '<Cell col="2" celltype="head" displaytype="number" style="align:right;" expr="bind:&apos;EXP_AMT&apos;" mask="#,##0"/>\n';
		v_BandBody += '<Cell col="3" celltype="head" displaytype="number" style="align:right;" expr="bind:&apos;ACT_AMT&apos;" mask="#,##0"/>\n';
		
		// 포맷 생성
		var v_HeadText = '';
		var v_HeadColIdx = 4;
		var v_BodyColIdx = 4;
		
		for (var i=0; i<objPivotHeadDs.getRowCount(); i++) {
			
			// Column 생성
			v_Columns += '<Column size="100" />\n';
			v_Columns += '<Column size="100" />\n';

			// Head 생성
			if (v_HeadText != objPivotHeadDs.getColumn(i, "YYMM")) {
				v_HeadText = objPivotHeadDs.getColumn(i, "YYMM");
								
				// Row 1
				v_BandHead += '<Cell col="'+v_HeadColIdx+'" colspan="'+2+'" text="'+gfn_getFormat(v_HeadText,"@@@@년 @@월")+'"/>\n';
				
				// Row 2
				for (var j=i; j<objPivotHeadDs.getRowCount(); j++) {
					if (v_HeadText == objPivotHeadDs.getColumn(j, "YYMM")) {
						v_BandHead += '<Cell row="1" col="'+v_HeadColIdx+'" text="예상"/>\n';
						v_BandHead += '<Cell row="1" col="'+(v_HeadColIdx+1)+'" text="실적"/>\n';
						v_HeadColIdx += 2;
					}
				}
			}

			// Body 생성
			v_BandBody += '<Cell style="align:right;" col="'+v_BodyColIdx+'" displaytype="number" text="expr:gfn_nvl(P_EXP_AMT'+objPivotHeadDs.getColumn(i, "YYMM")+',0)"/>\n';
 			v_BandBody += '<Cell style="align:right;" col="'+(v_BodyColIdx+1)+'" displaytype="number" text="expr:gfn_nvl(P_ACT_AMT'+objPivotHeadDs.getColumn(i, "YYMM")+',0)"/>\n';
			v_BodyColIdx+=2;
		}

		v_Formats = '<Formats>\n';
		v_Formats += '<Format id="default">\n';
		v_Formats += '<Columns>\n';
		v_Formats += v_Columns;
		v_Formats += '</Columns>\n';
		v_Formats += '<Rows>\n';
		v_Formats += v_Rows;
		v_Formats += '</Rows>\n';
		v_Formats += '<Band id="head">\n';
		v_Formats += v_BandHead;
		v_Formats += '</Band>\n';	
		v_Formats += '<Band id="body">\n';
		v_Formats += v_BandBody;
		v_Formats += '</Band>\n';		
		v_Formats += '</Format>\n';	
		v_Formats += '</Formats>\n';
		
		//trace(v_Formats);
		objGrid.enableredraw = false;
		objGrid.formats = v_Formats;
		objGrid.binddataset = objPivotBodyDs.name;
		objGrid.enableredraw = true;	
}


