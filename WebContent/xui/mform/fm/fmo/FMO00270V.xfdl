	if(me_ToDate.getLength() == 6){
		var add1year = gfn_addMonth(me_FromDate.value + '01',12);
		
		//시작일에서 1년을 더한값이 종료일보다 크면 1년이상 조회할수 없다는 메세지 
		if(parseInt(add1year) <= parseInt(me_ToDate.value + '01'))
		{
			gfn_alert("1년이상 조회할수 없습니다.");
			//1년기간을 셋팅
			me_FromDate.value = gfn_subStr(gfn_addMonth(me_ToDate.value+'01',-11),0,6);
		}		
		
		if(me_ToDate.value <= me_FromDate.value)
			gfn_alert("일자가 부정확합니다.");	
	}
}


function Grid2_OnHeadClick(obj:Grid, e:GridClickEventInfo)
{
	if( e.cell == 0 ) {	// Excel Export
		gfn_exportExcel(obj, "Sheet1");
	}	
}

function fn_FormKeyDown(obj:Form, e:KeyEventInfo)
{
	gfn_formKeyDown(obj, e); //(obj,e.fromreferenceobject,e.keycode,e.shiftKey,e.ctrlKey,e.altKey,-1,-1);
	if ((e.keycode == 13) && (e.fromreferenceobject == me_ToDate)) fn_search();
	
}	
