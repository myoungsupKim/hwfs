        (g_AuthLevel != "14")
       )
    {
    
		if (real_UPJANG != g_Upjang){
		
			g_Message("EE", "해당 사용자는 소속업장에 대해서만\n저장/삭제 가능합니다." + "\n" +
			                "확인하여 주십시요.");
			return false;
		}
    }
    
	return true;

}			 
