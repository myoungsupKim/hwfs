function frm_InitSearchDS_This(objInDs, objOutDs, F_ArrCol, F_ArrColType, str_WhereCol, str_WhereColValue, str_SearchActionInfo)
{


    var str_callbackFunction = "";
	//Combo DATASET Composition#########################
	if (objOutDs.name == "ds_INVEST_AMT"){
	
			str_callbackFunction = "fn_callback_SetINVEST_AMT";
	}
	
 	objInDs.clear();
	
	for (i=0;i<gfn_length(F_ArrCol);i++){
	    //alert(F_ArrCol[i]);
		objInDs.addColumn(F_ArrCol[i],F_ArrColType[i],256);			
	}

	
	objInDs.addRow();
	objInDs.setColumn(0, str_WhereCol, str_WhereColValue);
	
	// action 정보 초기화 
    fsp_clear(this);  
	
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, str_SearchActionInfo);
	
	// 서버 호출 
	//tit_callService(this, "", "", "ds_cond=" + objInDs.id, objOutDs.id + "=ds_list","","fn_callback_SetINVEST_AMT");
	fsp_callService(this, "", "", "ds_cond=" + objInDs.name, objOutDs.name + "=ds_list","",str_callbackFunction);
	//Combo DATASET Composition#########################	  	
}

function frm_InitComboSearchDS(objCombo, objInDs, objOutDs, F_ArrCol, F_ArrColType, str_WhereCol, str_WhereColValue, str_SearchActionInfo)
{
    var str_callbackComboFuntion = "";
    
    if (objCombo.name == "cbo_GUBUN_CD2"){
		str_callbackComboFuntion = "callbackCombo";
    }
    
	//Combo DATASET Composition#########################
	if (objInDs.getColCount() > 0)
		objInDs.clear();
	
	if (objOutDs.getColCount() > 0)		
		objOutDs.clear();
 	
 	
	
	for (i=0;i<gfn_length(F_ArrCol);i++){
	    //alert(F_ArrCol[i]);
		objInDs.addColumn(F_ArrCol[i],F_ArrColType[i],256);					
	}
	
	for (i=0;i<2;i++){
	    //alert(F_ArrCol[i]);
		objOutDs.addColumn(F_ArrCol[i],F_ArrColType[i],256);			
	}	
	
	
	objInDs.addRow();
	objInDs.setColumn(0, str_WhereCol, str_WhereColValue); 
	
	objCombo.innerdataset = objOutDs.name;
	objCombo.codecolumn   = F_ArrCol[0];
	objCombo.datacolumn   = F_ArrCol[1];
	
	// action 정보 초기화 
    fsp_clear(this);  
	
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, str_SearchActionInfo);
	
	http.Sync = true;
	// 서버 호출 
	fsp_callService(this, "", "", "ds_cond=" + objInDs.name, objOutDs.name + "=ds_list","",str_callbackComboFuntion);		
	http.Sync = false;
	//Combo DATASET Composition#########################	  
		
	
}

