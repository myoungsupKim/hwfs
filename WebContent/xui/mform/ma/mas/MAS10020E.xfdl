function fn_MakeUpjangSQLLocal()
{
	var strQuery_AuthUpjang = "";
	
	switch(g_AuthLevel)
	{
		case "00":  //일반관리자
			strQuery_AuthUpjang = strQueryLocal_Upjang + " AND A.UPJANG = '" + g_Upjang + "' ";
			break;	
		case "10":  //영양사
			strQuery_AuthUpjang = strQueryLocal_Upjang + " AND A.UPJANG = '" + g_Upjang + "' ";
			break;
		case "12":  //리더영양사
			strQuery_AuthUpjang = strQueryLocal_Upjang + " AND A.LEADER_SABUN = '" + g_EmpNo + "' ";
			break;
		case "14": //소팀장
			strQuery_AuthUpjang = strQueryLocal_Upjang + " AND A.TM_CD = '" + g_Tm_Cd + "' ";
			break;
		case "16": //팀장
			strQuery_AuthUpjang = strQueryLocal_Upjang + " AND A.TM_CD = '" + g_Tm_Cd + "' "; 
			break;
		case "18": //경영자
			strQuery_AuthUpjang = strQueryLocal_Upjang;
			break;	
		case "99": //전산관리자(현업)
			strQuery_AuthUpjang = strQueryLocal_Upjang;
			break;	
		case "999": //전산관리자(전산)
			strQuery_AuthUpjang = strQueryLocal_Upjang;
			break;						
		default:
			strQuery_AuthUpjang = strQueryLocal_Upjang + " AND A.UPJANG = '" + g_Upjang + "' ";
			break;
	}	
	
	return strQuery_AuthUpjang;
}








