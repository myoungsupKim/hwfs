			g_Message("EE", "-금액을 입력할 수 없습니다.\n" +
							"확인하여 주십시요.");
								
			eval("me_" + e.columnid).value = "";
			eval("me_" + e.columnid).setFocus();		
						
			return false;  									
		}		
	}	
}




