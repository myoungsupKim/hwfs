function edtBtn_TRANS_GUBUN_OnClick(obj:Button, e:ClickEventInfo)
{   


    if (
        (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"))== "0") 
       ) 
    return;
    
    var str_Where = "";
    

    //구분코드####################################################################################
    if (obj.name == "edtBtn_TRANS_GUBUN"){	
		
		str_Where += " AND  (TRANS_TYPE = 'X')";
	    str_Where += " AND  (TRANS_GUBUN NOT IN ('XL','XC','XR'))";
	    
	    if (ds_CloseMaster.findRowExpr("CODE=='" + g_EmpNo + "'") == -1){
	    
	    str_Where += " AND  (TRANS_GUBUN NOT IN ('XL','XC','XR'))";
	    }
    }
       
    //식권종류####################################################################################
    if (obj.name == "edtBtn_ITEM_TYPE"){	
		
		str_Where += " AND  (SET1 = 'Y')";	  
    }
                       
    //자재코드####################################################################################      
    if (obj.name == "edtBtn_ITEM_CODE"){	
		
		if (
			(gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "ITEM_TYPE"))== 0) 
		   ) 
		return;		
		
		str_Where += " AND  (EXISTS (SELECT 'X' FROM MAS_TICKET_STD WHERE UPJANG = " + ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG") + " AND ITEM_TYPE = " + ds_listInput.getColumn(ds_listInput.rowposition, "ITEM_TYPE") + " AND ITEM_CODE = HLDC_PO_ITEM_MST.ITEM_CODE AND USE_YN = 'Y')) ";			
    }     
                    
	fn_edtBtn_OnClick(obj, str_Where);

    if (gfn_length(eval("edt_" + gfn_subStr(obj.name, 7)).userdata)!= 0)
	ds_listInput.setColumn(ds_listInput.rowposition, gfn_subStr(obj.name, 7), eval("edt_" + gfn_subStr(obj.name, 7)).userdata);	
			    		
