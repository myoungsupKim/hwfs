				return false;                
			}							
				
			//alert(2);
			if (!fn_GetSystemOpenDateCheck(ds_listInput.getColumn(ds_listInput.rowposition, "TRANS_DD"))) return false;
					        
       
			//식권관리유형일 경우 식권비교 팝업창을 띄운다.#########################
				
			if(!fn_OpenTicketLineWindow("INSERT", ds_listInput.getColumn(ds_listInput.rowposition, "QTY"), ds_MAS_TICKET_CODE.getColumn(0, "SET2"))) return false;
				
			//식권관리유형일 경우 식권비교 팝업창을 띄운다.#########################       
		}			

        
		//저장#############################################################
	    if (str_Gubun == "Master"){

			
			//각 구분별 대체 마감체크##############################저장전 중복확인
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(f_Saved_ID)!= "0")
               )
            {			
				if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))) return false;
			}
			//각 구분별 대체 마감체크##############################저장전 중복확인
			
			
							   
			//TICKET_SEQ지정#################################################################
            if(!fn_SaveTrxData(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))) return false;
            //TICKET_SEQ지정#################################################################    						           			
						
							           		//trace("ds_MAS_TICKET_MST.rowcount : " + ds_MAS_TICKET_MST.rowcount);	
			//식권잔액체크##########################################저장전 중복확인
			if (ds_MAS_TICKET_MST.rowcount > 0){
			    
			    var real_MAS_TICKET_MST_GROUP_S;
			    			
				real_MAS_TICKET_MST_GROUP_S = fn_GetMAS_TICKET_MST_GROUP_S(ds_cond);
							//trace("real_MAS_TICKET_MST_GROUP_S : " + real_MAS_TICKET_MST_GROUP_S);	
				for (i=0;i<ds_MAS_TICKET_MST.rowcount;i++){
					
					ds_MAS_TICKET_MST.setColumn(i, "TICKET_GROUP_SEQ", real_MAS_TICKET_MST_GROUP_S);								
					ds_MAS_TICKET_MST.setColumn(i, "CURR_TICKET_SEQ", ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"));	
							
					ds_MAS_TICKET_MST.setColumn(i, "UPJANG", ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"));			
				}
				//trace("식권" + ds_listInput.getColumn(ds_listInput.rowposition, "TRANS_GUBUNNM") + "잔액체크");
				//trace(ds_MAS_TICKET_MST.saveXML());
				if (!fn_GetBalanceLine(ds_cond, ds_MAS_TICKET_MST, "식권" + ds_listInput.getColumn(ds_listInput.rowposition, "TRANS_GUBUNNM") + "잔액체크"))
						return false;   		
						
				//trace(2);
				//순차적이지 않은 시작번호체크######################################################################
				if (ds_MAS_TICKET_CODE.getColumn(0, "SET2") == "FR_TO"){
				
					if (!fn_GetMaxFR_TO_YN(ds_cond, ds_MAS_TICKET_MST))
							return false;   	
                }
                //순차적이지 않은 시작번호체크######################################################################
                //trace(3);
                										
				ds_listInput.setColumn(ds_listInput.rowposition, "TICKET_GROUP_SEQ", real_MAS_TICKET_MST_GROUP_S);	
				ds_listInput.setColumn(ds_listInput.rowposition, "TICKET_STARTNUM",  ds_MAS_TICKET_MST.getColumn(ds_MAS_TICKET_MST.rowposition, "TICKET_STARTNUM"));	
				ds_listInput.setColumn(ds_listInput.rowposition, "TICKET_ENDNUM",    ds_MAS_TICKET_MST.getColumn(ds_MAS_TICKET_MST.rowposition, "TICKET_ENDNUM"));	
				ds_listInput.setColumn(ds_listInput.rowposition, "SOURCE_ID",        ds_MAS_TICKET_MST.getColumn(ds_MAS_TICKET_MST.rowposition, "TICKET_SEQ"));	
				
	        }    
			//식권잔액체크##########################################저장전 중복확인
						trace(4);				           			
			// action 정보 초기화
			fsp_clear(this);	
			//trace("numFormat(f_Saved_ID) : " + numFormat(f_Saved_ID));
            if (numFormat(f_Saved_ID)== "0"){
            
				//식권정보 저장            
				fsp_addMulti(this, "ma/mas:MAS10290E_I001");
			}
			else {
			
				//식권정보 저장            
				fsp_addSingle(this, "ma/mas:MAS10290E_U001");			
			}
                              						
			
			f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ");
			fsp_callService(this, "", "", "ds_cond=ds_listInput:A", "",  "", "fn_AfterSaveSearch()");
	    }
	   
	    
		return true;
}


function fn_AfterSaveSearch(errCode, errMsg)
{
	if (errCode != 0){
		
		g_Message("EE");		
		return;
	}
		
    
	fn_search();	
	fn_insert();
		
	http.Sync = true;
	ds_list.rowposition = -1;
	ds_list.rowposition = ds_list.findRowExpr("TICKET_SEQ==" + f_Saved_ID);
	http.Sync = false;
	
}

function fn_AfterSaveSearch2(errCode, errMsg)
{
	if (errCode != 0){		
		g_Message("EE");		
		return;
	} else {
		g_Message("EE","팀장이 변경되었습니다.");				
	}
    
	fn_search();
	
}

