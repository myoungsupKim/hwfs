//각 구분별 대체마감체크############################################################
function fn_GetALT_CLOSE(objOutDs, real_TICKET_SEQ)
{

    //대체마감체크#####################################################################
    var str_CloseErr = "";
    
    objOutDs.clear();
    objOutDs.addColumn("SUBTEAM_MST_CONGUBN","STRING",256);	       
    objOutDs.addColumn("APPROV_CONGUBN","STRING",256);	       
    objOutDs.addColumn("TEAM_MST_CONGUBN","STRING",256);	       

    
	// action 정보 초기화 
    fsp_clear(this);  
	
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "ma/mas:MAS10290E_S003");
	
	//alert(str_ACCTCD);
	//alert(str_DATE_STD);
	// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", "", objOutDs.name + "=ds_list","TICKET_SEQ=" + real_TICKET_SEQ
	                                                           ,"");	
	http.Sync = false;
	if (objOutDs.getColumn(objOutDs.rowposition, "SUBTEAM_MST_CONGUBN") != "N")     str_CloseErr = "해당항목에 대해\n소팀장승인 되었으므로\n수정/삭제할 수 없습니다.";

	if (objOutDs.getColumn(objOutDs.rowposition, "APPROV_CONGUBN") != "N")     str_CloseErr = "해당항목에 대해\n마스터승인 되었으므로\n수정/삭제할 수 없습니다.";

	if (objOutDs.getColumn(objOutDs.rowposition, "TEAM_MST_CONGUBN") != "N")   str_CloseErr = "해당항목에 대해\n팀장승인 되었으므로\n수정/삭제할 수 없습니다.";

	
	if (gfn_length(str_CloseErr )!= 0){
	
		g_Message("EE", str_CloseErr + "\n" +
						"확인하여 주십시요.");			 
		return false;        
	}

	return true;
	//대체마감체크#####################################################################
 
}		
