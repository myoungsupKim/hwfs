				//ds_listInput.setColumn(ds_listInput.row, "TICKET_ENDNUM", toNumber(ds_listInput.getColumn(ds_listInput.row, "TICKET_STARTNUM")) + toNumber(ds_listInput.getColumn(ds_listInput.row, "QTY")));
