			}
		}
	}	
	
	if (e.columnid == "QTY"){
	    
	    
	    ds_listInput.setColumn(ds_listInput.rowposition, "AMT", toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "UPRICE")) * toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")));
	    
	    if (ds_MAS_TICKET_CODE.getColumn(0, "SET2") == "FR_TO"){
