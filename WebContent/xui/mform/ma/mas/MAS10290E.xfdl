function fn_GetTicketFromTo(objOutDs, real_UPJANG, str_ITEM_CODE)
{

    
    //잔액체크#####################################################################
    objOutDs.clear();
    objOutDs.addColumn("TICKET_MAXNUM","INT",256);	       
    
	// action 정보 초기화 
    fsp_clear(this);  
	
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "ma/mas:MAS10290E_S011");
	
	//alert(str_ACCTCD);
	//alert(str_DATE_STD);
	// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", "", objOutDs.name + "=ds_list","UPJANG=" + wrapQuote(real_UPJANG) + 
	                                                           " ITEM_CODE=" + wrapQuote(str_ITEM_CODE)
	                                                           ,"");	
	http.Sync = false;
	
                          	
	
	//alert(objOutDs.GetColumn(objOutDs.row, "ACCTCD_DATE") );
	return objOutDs.getColumn(objOutDs.rowposition, "TICKET_MAXNUM");
	//잔액체크#####################################################################
	
}

function fn_GetTicketUPRICE(objOutDs, real_UPJANG, str_ITEM_CODE)
{

    
    //잔액체크#####################################################################
    objOutDs.clear();
    objOutDs.addColumn("UPRICE","INT",256);	       
    
