function before_SaveCheck(str_Gubun)
{

        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
        
        f_Saved_ID = 0;
        

		
        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
		


	    if (str_Gubun == "Master"){

			
	        
	        //입력일때 Confirm체크##################################
	        if (!g_Confirm("02", "전도금신청자동전표를 생성 하시겠습니까?")) return false;	             
	        //입력일때 Confirm체크##################################
	             
	             
	        if (gfn_length(me_GADATETO.value)== 0){
	       
				g_Message("EE", "전표일자가 없습니다.\n" +
				                "확인하여 주십시요."); 
				me_GADATETO.setFocus();
				return false;  	        
	        }		             
	             
	             
	        if (gfn_length(edt_RTNDEPT_ID.userdata)== 0){
	       
				g_Message("EE", "결재귀속부서가 없습니다.\n" +
				                "확인하여 주십시요."); 
				edt_RTNDEPT_ID.setFocus();
				return false;  	        
	        }	             
	             
	             
	             
	        if (gfn_length(gfn_trim(edt_DESC.value))== 0){
	       
				g_Message("EE", "적요가 없습니다.\n" +
				                "확인하여 주십시요.");    
                edt_DESC.setFocus();
				return false;  	        
	        }
	        

			//전표분개##############################################
			fn_SetSlip();	
				
			if (ds_list.rowcount == 0){
	
				return false;
			}	  
			      
            //차대 합계 Verify체크##################################
			if (toNumber(ds_list.getSum('DRAMT')) != toNumber(ds_list.getSum('CRAMT'))){
				
				g_Message("EE", "차대변 합계금액이 일치하지 않습니다.\n" +
				                "확인하여 주십시요.");                
				return false;                
			}
			//차대 합계 Verify체크##################################
						
						
			//GANO 세팅#######################################################################
			f_LAST_DATE = me_GADATETO.value;
			f_GAMU = "YA";
			f_GANO = fn_GetHLDC_GA_GETMAXNO("YA", f_LAST_DATE);
            
		}			

        
		//저장#############################################################
	    if (str_Gubun == "Master"){


		
		
			
			//전표데이터세팅##########################################
			if (ds_list.rowcount > 0){
			   
				for (i=0;i<ds_list.rowcount;i++){
					
					ds_list.setColumn(i, "GAMU",   "YA");			
					ds_list.setColumn(i, "GADATE", f_LAST_DATE);
					ds_list.setColumn(i, "GANO",   f_GANO);					
					ds_list.setColumn(i, "GALINE", i + 1);		
								
					
					
					ds_list.setColumn(i, "GADEPT_ID",        g_Deptid);	
					ds_list.setColumn(i, "GAGUBUN",          "G02");	
					ds_list.setColumn(i, "GAGUBUN_DETAIL",   "003");		
					
					if (ds_list.getColumn(i, "DRAMT") != 0){
						ds_list.setColumn(i, "DCGUBUN",      "1");		
					}
					else {
						ds_list.setColumn(i, "DCGUBUN",      "2");		
					}
						

				}
				
	        }    
			//전표데이터세팅##########################################
			
			
			
			// action 정보 초기화
			fsp_clear(this);	  		
										    		        
	    
            //전표데이터 저장.            
   		    fsp_addMulti(this, "ma/mas:MAS10309E_I001");
   		    
   		    //원본데이터 UPDATE.     
   		    ds_UpdatecondFilter.clear();
			
			Copy(ds_UpdatecondFilter,ds_list);
			
			
			ds_UpdatecondFilter.filter("toNumber(MST_ID)!=0");
			
			CopyF(ds_Updatecond,ds_UpdatecondFilter);
   		    
   		           
   		    fsp_addMulti(this, "ma/mas:MAS10330E_U001");   		    
   		    
            //원본데이터 UPDATE.     

			//TextArea0.Text = ds_list.SaveXML();
			
			fsp_callService(this, "", "", "ds_cond=ds_list:A ds_Updatecond=ds_Updatecond:A", "",  "", "fn_AfterSaveSearch()");
			
	    }
	   
	    
		return true;
}

