function ds_listInput_OnColumnChanged(obj:Dataset, e)
{
	var F_ArrCol          = new Array(-1);
	var F_ArrColType      = new Array(-1);
	
	if (e.columnid == "ITEM_TYPE"){
	

						
	    if (-1 == -1){
	        
	        
			for (i=0;i<ds_listInput.colcount;i++){

				if (
				    (ds_listInput.getColID(i) != "UPJANG") &&
				    (ds_listInput.getColID(i) != "UPJANGNM") &&
				    (ds_listInput.getColID(i) != "TRANS_DD") &&
				    (ds_listInput.getColID(i) != "TRANS_GUBUN") &&
				    (ds_listInput.getColID(i) != "TRANS_GUBUNNM") &&
				    (ds_listInput.getColID(i) != "ITEM_TYPE") &&
				    (ds_listInput.getColID(i) != "ITEM_TYPENM") 				    
				   )
				{

					ds_listInput.setColumn(ds_listInput.rowposition, ds_listInput.getColID(i), "");
				}
			}	
																
		}
					
	}

	if (e.columnid == "UPRICE"){
	    
	    
	    ds_listInput.setColumn(ds_listInput.rowposition, "AMT", toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "UPRICE")) * toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")));
	    
	    if (toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")) == 0){
	    
			ds_listInput.setColumn(ds_listInput.rowposition, "TICKET_ENDNUM", toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_STARTNUM")) + toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")));
		}
		else {

			ds_listInput.setColumn(ds_listInput.rowposition, "TICKET_ENDNUM", toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_STARTNUM")) + toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")) - 1);		
		}
	}	
	
	if (e.columnid == "QTY"){
	    
	    
	    ds_listInput.setColumn(ds_listInput.rowposition, "AMT", toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "UPRICE")) * toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")));
	    
	    if (toNumber(e.newvalue) == 0){
	    
			ds_listInput.setColumn(ds_listInput.rowposition, "TICKET_ENDNUM", toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_STARTNUM")) + toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")));
		}
		else {

			ds_listInput.setColumn(ds_listInput.rowposition, "TICKET_ENDNUM", toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_STARTNUM")) + toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")) - 1);		
		}
	}	
		
	if (e.columnid == "ITEM_CODE"){
		
		var real_TICKET_STARTNUM = 0;
        if (
            (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"))!= "0") &&
            (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "ITEM_CODE"))!= 0)
           )
        {
		    
			real_TICKET_STARTNUM = fn_GetTicketFromTo(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"), ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"), ds_listInput.getColumn(ds_listInput.rowposition, "ITEM_CODE"));
			ds_listInput.setColumn(ds_listInput.rowposition, "TICKET_STARTNUM", real_TICKET_STARTNUM);
		}
	}
			
	if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))!= "0"){
	
	
		
		//Disabled항목 Enabled처리####################################
		me_TRANS_DD.enable        = false;
		
		edt_TRANS_GUBUN.enable    = false;
		edtBtn_TRANS_GUBUN.enable = false;
		
		edt_ITEM_TYPE.enable      = false;
		edtBtn_ITEM_TYPE.enable   = false;
		
		edt_ITEM_CODE.enable      = false;
		edtBtn_ITEM_CODE.enable   = false;
			
		me_TICKET_STARTNUM.enable = false;
		me_TICKET_ENDNUM.enable   = false;	
		//Disabled항목 Enabled처리####################################	
	}	

}




