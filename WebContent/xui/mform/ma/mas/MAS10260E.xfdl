	if (objOutDs.getColumn(objOutDs.rowposition, "WR_YN") == "Y")   str_CloseErr = "입고항목에 대해\n환불이 발생했으므로\n수정/삭제할 수 없습니다.";
			                          	
	if (objOutDs.getColumn(objOutDs.rowposition, "XO_YN") == "Y")   str_CloseErr = "입고항목에 대해\n불용폐기가 발생했으므로\n수정/삭제할 수 없습니다.";
	
	if (objOutDs.getColumn(objOutDs.rowposition, "XW_YN") == "Y")   str_CloseErr = "입고항목에 대해\n회수폐기가 발생했으므로\n수정/삭제할 수 없습니다.";
	
	if (objOutDs.getColumn(objOutDs.rowposition, "XR_YN") == "Y")   str_CloseErr = "입고항목에 대해\n환불폐기가 발생했으므로\n수정/삭제할 수 없습니다.";
	
	if (objOutDs.getColumn(objOutDs.rowposition, "AUTO_YN") == "Y") str_CloseErr = "자동입고된 항목이므로\n수정/삭제할 수 없습니다.";
	
	if (gfn_length(str_CloseErr )!= 0){
	
		g_Message("EE", str_CloseErr + "\n" +
						"확인하여 주십시요.");			 
		return false;        
	}

	return true;
	//대체마감체크#####################################################################
 
}		
