function before_SaveCheck(str_Gubun)
{
		
		//권한에 따른 저장/삭제 가능체크#########################################################################
		if (!fn_GetSaveAuthBy_AuthLevel(ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"))) return false;
		//권한에 따른 저장/삭제 가능체크#########################################################################
			
       
       /*
        업무구분별로 addAction 지정세팅##########################################        
        */
        
        f_Saved_ID = 0;
        
	
        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
		
		
 		if (ds_listInput.rowcount == 0){

			return;
		}

	    if (str_Gubun == "Master"){

			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"))== "0")
			{
			
				g_Message("EE", "업장을 지정하여 주십시요.");
				edt_upjang.setFocus();
				return false;
			}
		
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "TRANS_DD"))== 0)
			{
			
				g_Message("EE", "등록일자가 누락되었습니다.");
				me_TRANS_DD.setFocus();
				return false;
			}
	        
	        //입력일때 Confirm체크##################################
	        if ( 
	             (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))== "0")
	           )
	        {
				if (!g_Confirm("02")) return false;	             
	        }     
	        //입력일때 Confirm체크##################################
	             
	             
	        //수정일때 Confirm체크##################################
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))!= "0")
               )
            {	        
				if (!g_Confirm("02", "데이터를 수정하시겠습니까?")) return false;
			}
	        //수정일때 Confirm체크##################################
	        
			//각 구분별 대체 마감체크##############################
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))!= "0")
               )
            {			
				if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))) return false;
			}
			//각 구분별 대체 마감체크##############################	        
					        


			
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "TRANS_GUBUN"))== 0){
				
				g_Message("EE", "구분이 누락되었습니다.\n" +
				                "확인하여 주십시요.");
                edt_TRANS_GUBUN.setFocus();
				return false;                
			}
			
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "ITEM_TYPE"))== 0){
				
				g_Message("EE", "식권종류가 누락되었습니다.\n" +
				                "확인하여 주십시요.");
                edt_ITEM_TYPE.setFocus();
				return false;                
			}			
			
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "ITEM_CODE"))== 0){
				
				g_Message("EE", "자재코드가 누락되었습니다.\n" +
				                "확인하여 주십시요.");
                edt_ITEM_CODE.setFocus();
				return false;                
			}				
			
			
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "UPRICE"))== "0")
			{
			
				g_Message("EE", "수불금액이 누락되었습니다.\n" +
				                "확인하여 주십시요.");
				me_UPRICE.setFocus();
				return false;
			}			
			
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "QTY"))== "0")
			{
			
				g_Message("EE", "수량이 누락되었습니다.\n" +
				                "확인하여 주십시요.");
				me_QTY.setFocus();
				return false;
			}
			
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "AMT"))== "0")
			{
			
				g_Message("EE", "금액이 누락되었습니다.\n" +
				                "확인하여 주십시요.");
				me_AMT.setFocus();
				return false;
			}	
								
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "REMARK_DESC"))== 0){
				
				g_Message("EE", "적요가 누락되었습니다.\n" +
				                "확인하여 주십시요.");
                edt_REMARK_DESC.setFocus();
				return false;                
			}								
					
						
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_STARTNUM"))== "0")
			{
			
				g_Message("EE", "시작번호가 누락되었습니다.\n" +
				                "확인하여 주십시요.");
				me_TICKET_STARTNUM.setFocus();
				return false;
			}							
						
			
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_ENDNUM"))== "0")
			{
			
				g_Message("EE", "종료번호가 누락되었습니다.\n" +
				                "확인하여 주십시요.");
				me_TICKET_ENDNUM.setFocus();
				return false;
			}		
	
			if (toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_ENDNUM")) - toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_STARTNUM")) + 1 != toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "QTY")))
			{
			
				g_Message("EE", "시작번호와 종료번호가 수량과 상이합니다.\n" +
				                "확인하여 주십시요.");
				me_TICKET_ENDNUM.setFocus();
				return false;
			}	
        
			//TICKET_SEQ지정#################################################################
            if(!fn_SaveTrxData(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))) return false;
            //TICKET_SEQ지정#################################################################           
		}			

        
		//저장#############################################################
	    if (str_Gubun == "Master"){

			
			//각 구분별 대체 마감체크##############################저장전 중복확인
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(f_Saved_ID)!= "0")
               )
            {			
				if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))) return false;
			}
			//각 구분별 대체 마감체크##############################저장전 중복확인
			
			
		    // 번호 비교 작업 시작 ###################################################			    
			if (!fn_GetTicketSaveCheck(ds_cond, 
				 			           ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"), 
				    		           ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"), 
							           ds_listInput.getColumn(ds_listInput.rowposition, "ITEM_CODE"), 											
							           ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_STARTNUM"), 				
							           ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_ENDNUM")))
            {
            
				g_Message("EE", "식권번호가 중복됩니다.\n" +
				                "확인하여 주십시요.");            
				return false;
			}
							   
						           			
							           			
			// action 정보 초기화
			fsp_clear(this);	
			
            if (numFormat(f_Saved_ID)== "0"){
            
				//식권정보 저장            
				fsp_addSingle(this, "ma/mas:MAS10250E_I001");
			}
			else {
			
				//식권정보 저장            
				fsp_addSingle(this, "ma/mas:MAS10250E_U001");			
			}
                              						
			
			f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ");
			fsp_callService(this, "", "", "ds_cond=ds_listInput:A", "",  "", "fn_AfterSaveSearch()");
	    }
	   
	    
		return true;
}


function fn_AfterSaveSearch(errCode, errMsg)
{
	if (errCode != 0){
		
		g_Message("EE");		
		return;
	}
		
    
	fn_search();	
	fn_insert();
		
	http.Sync = true;
	ds_list.rowposition = -1;
	ds_list.rowposition = ds_list.findRowExpr("TICKET_SEQ==" + f_Saved_ID);
	http.Sync = false;
	
}

