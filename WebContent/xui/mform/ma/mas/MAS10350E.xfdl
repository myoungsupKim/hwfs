	ds_cond.clear();
	
	ds_cond.addColumn("TRX_DATE","STRING",256);
				
	ds_cond.addRow();
	
	ds_cond.setColumn(ds_cond.rowposition, "TRX_DATE",    me_Date.value);
       					
	fn_DataSetAllClear();

    http.Sync = true;
    
	//Timeout 설정처리###########################################
	var t_timeout;
