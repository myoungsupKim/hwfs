				ds_HLDC_GA_INTERFACE.setColumn(0, "lngRtn",      "");			
	        }    
			//전표데이터세팅##########################################
			
			
			
			// action 정보 초기화
			fsp_clear(this);	  		
										    		        
	    
            //전표데이터 저장.            
   		    fsp_addMulti(this, "ma/mas:MAS10309E_I001");
   		    
   		         
   		    
   		    
   		    //원본데이터 UPDATE.     
   		    
   		    ds_Updatecond.clear();
			      
			ds_Updatecond.addColumn("TRX_DATE","STRING",256);       
			ds_Updatecond.addColumn("GAMU","STRING",256);       
			ds_Updatecond.addColumn("GADATE","STRING",256);       
			ds_Updatecond.addColumn("GANO","INT",256);       
			
			ds_Updatecond.addRow();
			
			ds_Updatecond.setColumn(0, "TRX_DATE", me_Date.value);
			ds_Updatecond.setColumn(0, "GAMU",     f_GAMU);
			ds_Updatecond.setColumn(0, "GADATE",   f_LAST_DATE);
			ds_Updatecond.setColumn(0, "GANO",     f_GANO);
   		          
   		    fsp_addSingle(this, "ma/mas:MAS10350E_U001");
   		    fsp_addSingle(this, "ma/mas:MAS10350E_U002");
   		    //tit_addSingleActionInfo(this, "ma/mas:MAS10350E_U003");
   		    fsp_addSingle(this, "ma/mas:MAS10309E_I002");//MAS_GA_INTERFACE_PRO     
   		    
            //원본데이터 UPDATE.     

			//TextArea0.Text = ds_list.SaveXML();
			
			http.Sync = true;
			
			//Timeout 설정처리###########################################
			var t_timeout;
			t_timeout = application.httptimeout;
			application.httptimeout=2000; //1800초
			
			//http.Compress = true;
			//http.CompressMethod = "ZLIB";
			//http.XmlFormat = false;					
			//Timeout 설정처리###########################################
			
			fsp_callService(this, "", "", "ds_cond=ds_list:A ds_Updatecond=ds_Updatecond:A  ds_HLDC_GA_INTERFACE=ds_HLDC_GA_INTERFACE:A", "",  "", "fn_AfterSaveSearch()");
			
			//Timeout 설정처리###########################################
			application.httptimeout=t_timeout; //60초
			
			//http.Compress = false;
			//http.CompressMethod = "";
			//http.XmlFormat = true;					
			//Timeout 설정처리###########################################
						
			http.Sync = false;
			//tit_callService(this, "", "", "ds_cond=ds_list:A", "",  "", "fn_AfterSaveSearch()");
			

	    }
	   
	    
		return true;
}

function fn_AfterSaveSearch(errCode, errMsg)
{
	if (errCode != 0){
		
		g_Message("EE");		
		return;
	}
		
    g_Message("02", "전표번호:" + f_GAMU + "-" + f_LAST_DATE + "-" + f_GANO + "\n" + 
                    "저장되었습니다.");
    
	fn_search();	
	
}

