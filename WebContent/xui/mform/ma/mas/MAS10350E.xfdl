function fn_GetMU_Default()
{
	
	
	ds_cond.clear();
	
	ds_cond.addColumn("MU_CD","STRING",256);
	ds_cond.addColumn("DEPT_ID","INT",256);
	ds_cond.addColumn("DEPT_IDNM","STRING",256);
		
	
	// action 정보 초기화 
    fsp_clear(this);  
	
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "ma/mas:MAS10350E_S003");
		
	
	// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", "", ds_cond.name + "=ds_list",""
	                                                           ,"");	
	http.Sync = false;
    
	for (i=0;i<ds_cond.rowcount;i++){
				
		if (ds_cond.getColumn(i, "MU_CD") == "YA"){
		
			f_FS_DefaultDeptId     = ds_cond.getColumn(i, "DEPT_ID");
			f_FS_DefaultDeptIdNm   = ds_cond.getColumn(i, "DEPT_IDNM");
		}
		else {
		
			f_LS_DefaultDeptId     = ds_cond.getColumn(i, "DEPT_ID");
			f_LS_DefaultDeptIdNm   = ds_cond.getColumn(i, "DEPT_IDNM");
		}
    
    }
}

