function btn_InitSlip_OnClick(obj:Button, e:ClickEventInfo)
{
    
    if (gfn_length(me_GADATE.value)== 0){    

		g_Message("EE", "전표일자가 선택되어 있지 않습니다.\n" +
						"확인하여 주십시요."); 
        me_GADATE.setFocus();
        return false;
    }    
	
    if (numFormat(me_GANO.value)== "0"){    

