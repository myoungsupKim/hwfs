function ds_List_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{	
	
	var sumAmt = 0;
	for(var i=0; i<ds_List.getRowCount(); i++)
	{	
		sumAmt = 0;
		sumAmt = Math.round(parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_01"), '0')) + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_02"), '0')) + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_03"), '0')) 
			   + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_04"), '0')) + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_05"), '0')) + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_06"), '0'))
			   + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_07"), '0')) + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_08"), '0')) + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_09"), '0'))
			   + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_10"), '0')) + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_11"), '0')) + parseInt(gfn_nvl(ds_List.getColumn(i, "MM_AMT_12"), '0')));
		ds_List.setColumn(i,"SUM_AMT",sumAmt);
	}
}

