function fn_sum(arg){
	var temp;
		temp = Math.round(ds_List.getCaseSum("ACCT_CLASS== '매출액'",arg) 
					    - ds_List.getCaseSum("ACCT_CLASS== '재료비'",arg) 
						- ds_List.getCaseSum("ACCT_CLASS== '경비'",arg) 
						- ds_List.getCaseSum("ACCT_CLASS== '인건비'",arg));
	return temp;
}

function fn_searchPre(obj:Button,  e:ClickEventInfo)
{
		
	if(gfn_isNull(txtYy.value)){
		g_Message("EE", Static02.text + "을 입력하세요.");
		txtYy.setFocus();
		return false;
	}
	
	if(gfn_isNull(Cbo_gbn.value)){
		g_Message("EE", Static4.text + "을 선택하세요.");
		Cbo_gbn.setFocus();
		return false;
	}
	//필수항목 
	if (!fn_ChkManNull()) return;
	
	// 초기화
	fn_ClearData();
	ds_List.clearData();
	//ds_Cond.clearData();
	ds_Cond.addRow();
	
	ds_Cond.setColumn(0, "INPUT_YY",  yy.value);
	ds_Cond.setColumn(0, "UPJANG", edt_upjang.getUpjangCode()); 
	ds_Cond.setColumn(0, "BIZ_CLASS", Cbo_gbn.value);
	ds_Cond.setColumn(0, "BIZ_CLASS2", Cbo_gbn2.value);
	ds_Cond.setColumn(0, "CLOSE_TYPE", rd_CostType.value);
	//ds_Cond.setColumn(0, "CONFIRM_YN", ds_List.getColumn(0, "CONFIRM_YN"));
	
	// 월추정이 등록되어있는지 확인.
	//fn_ConfirmYn();
	
	// 조회
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "MAP30020E_T004";
	strURL   = "U_svc::" + "ma/map/MAP30020E_T004.jsp";
	
	// 데이터셋 설정
	strInDatasets  = "ds_Cond=ds_Cond";
	strOutDatasets = "ds_List=ds_List";
	
	// 파라메터 설정
	strArgument  = "";
	strArgument = " g_Yy=" + wrapQuote(txtYy.value);
	
	// 콜백함수 지정
	strCallbackFunc = "fn_CallBack";
	//strCallbackFunc = "";
	
	// 서버 호출 
	jsp_transaction(this, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, false, false, true);	
	
	//엑셀등록활성화
	if (ds_List.rowcount > 0)
	{
		btn_excelUpload.visible = true;
	} else
	{
		btn_excelUpload.visible = false;
	}

}

