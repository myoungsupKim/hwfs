function fn_save()
{
	if(rd_CostType.value == "U"){
		g_Message("EE", "재무금액은 저장할수 없습니다.");
		return;
	}
	
	if(ds_List.rowcount < 1){
		g_Message("EE", "저장 할 대상이 없습니다.");
		return;
	}
	
	if (! g_Confirm("02", '월추정을 등록 하시겠습니까?')) return;
	// 초기화
	//ds_Cond.clearData();
	ds_Cond.addRow();
	
	var intYy;
	// 입력년도의 1~12월
	if(!gfn_isNull(yy.value)){
		intYy = yy.value; //yy.value+'01,'+yy.value+'02,'+yy.value+'03,'+yy.value+'04,'+yy.value+'05,'+yy.value+'06,'+yy.value+'07,'+yy.value+'08,'+yy.value+'09,'+yy.value+'10,'+yy.value+'11,'+yy.value+'12' ;
	}
	
	ds_Cond.setColumn(0, "INPUT_YY",  intYy);
	ds_Cond.setColumn(0,"UPJANG", edt_upjang.getUpjangCode());
	//ds_Cond.setColumn(0,"BIZ_CLASS", Cbo_gbn.value);
	ds_Cond.setColumn(0, "BIZ_CLASS2", Cbo_gbn2.value);
	
	if(!gfn_isNull(ds_List.getColumn(1, "CONFIRM_YN"))){
		ds_Cond.setColumn(0, "CONFIRM_YN", ds_List.getColumn(1, "CONFIRM_YN"));
	}else{
		ds_Cond.setColumn(0, "CONFIRM_YN", "N");
	}
	
	//
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "MAP30020E_T002";
	strURL   = "U_svc::" + "ma/map/MAP30020E_T002.jsp";

	// 데이터셋 설정
	//strInDatasets  = "ds_Cond=ds_Cond " + "ds_List=ds_List:U " + "ds_List_ALL=ds_List:A ";
	strInDatasets  = "ds_Cond=ds_Cond ds_List=ds_List:A";
	strOutDatasets = "";
	// 파라메터 설정
	strArgument  = "";
	// 콜백함수 지정
	strCallbackFunc = "fn_CallBack_Save";
	// 서버 호출 
	jsp_transaction(this, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, false, false, true);
}

