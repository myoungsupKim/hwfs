function btn_excelUpload_onclick(obj:Button,  e:ClickEventInfo)
{

    if (ds_List.rowposition == -1)
    {
        gfn_alert("조회 후 진행해 주십시오.");
        return;
    }

    if (!gfn_confirm("엑셀 3번째 라인부터 등록됩니다.\n엑셀정보를 등록 하시겠습니까?")) return false;

	this.setWaitCursor(true, true);
	
	var strFileName = gfn_excelFileDialog("LOAD", "EXCEL");

	if (strFileName.length == 0)
	{
		this.setWaitCursor(false, true);
		return;
	}

	ds_excelDtlList.clearData();
	
 	var rtn = gfn_importExcel(strFileName, "ds_excelDtlList", 3, 1);	//3번째 라인부터 데이터 처리
 	
	this.setWaitCursor(false, true);

	var strAcctCd;
	var amt01, amt02, amt03, amt04, amt05, amt06, amt07, amt08, amt09, amt10, amt11, amt12;
	
	for(var i = 0; i < ds_excelDtlList.getRowCount(); i++)
	{
		strAcctCd = ds_excelDtlList.getColumn(i,"E");

		if(gfn_length(strAcctCd) == 8)
		{
			amt01 = ds_excelDtlList.getColumn(i,"H");
			amt02 = ds_excelDtlList.getColumn(i,"I");
			amt03 = ds_excelDtlList.getColumn(i,"J");
			amt04 = ds_excelDtlList.getColumn(i,"K");
			amt05 = ds_excelDtlList.getColumn(i,"L");
			amt06 = ds_excelDtlList.getColumn(i,"M");
			amt07 = ds_excelDtlList.getColumn(i,"N");
			amt08 = ds_excelDtlList.getColumn(i,"O");
			amt09 = ds_excelDtlList.getColumn(i,"P");
			amt10 = ds_excelDtlList.getColumn(i,"Q");
			amt11 = ds_excelDtlList.getColumn(i,"R");
			amt12 = ds_excelDtlList.getColumn(i,"S");
			
			for(var j = 0; j < ds_List.getRowCount(); j++)
			{
				if(ds_List.getColumn(j,"ACCT_CD") == strAcctCd)
				{
					if(fn_checkCell( j, "01" ) == "masknumber" && gfn_length(amt01) > 0) ds_List.setColumn(j, "MM_AMT_01", amt01);
					if(fn_checkCell( j, "02" ) == "masknumber" && gfn_length(amt02) > 0) ds_List.setColumn(j, "MM_AMT_02", amt02);
					if(fn_checkCell( j, "03" ) == "masknumber" && gfn_length(amt03) > 0) ds_List.setColumn(j, "MM_AMT_03", amt03);
					if(fn_checkCell( j, "04" ) == "masknumber" && gfn_length(amt04) > 0) ds_List.setColumn(j, "MM_AMT_04", amt04);
					if(fn_checkCell( j, "05" ) == "masknumber" && gfn_length(amt05) > 0) ds_List.setColumn(j, "MM_AMT_05", amt05);
					if(fn_checkCell( j, "06" ) == "masknumber" && gfn_length(amt06) > 0) ds_List.setColumn(j, "MM_AMT_06", amt06);
					if(fn_checkCell( j, "07" ) == "masknumber" && gfn_length(amt07) > 0) ds_List.setColumn(j, "MM_AMT_07", amt07);
					if(fn_checkCell( j, "08" ) == "masknumber" && gfn_length(amt08) > 0) ds_List.setColumn(j, "MM_AMT_08", amt08);
					if(fn_checkCell( j, "09" ) == "masknumber" && gfn_length(amt09) > 0) ds_List.setColumn(j, "MM_AMT_09", amt09);
					if(fn_checkCell( j, "10" ) == "masknumber" && gfn_length(amt10) > 0) ds_List.setColumn(j, "MM_AMT_10", amt10);
					if(fn_checkCell( j, "11" ) == "masknumber" && gfn_length(amt11) > 0) ds_List.setColumn(j, "MM_AMT_11", amt11);
					if(fn_checkCell( j, "12" ) == "masknumber" && gfn_length(amt12) > 0) ds_List.setColumn(j, "MM_AMT_12", amt12);
					break;
				}
			}
		}
	}
	gfn_alert("등록되었습니다.");
}
