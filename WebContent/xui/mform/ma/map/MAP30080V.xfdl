		return;
	}
*/
    // 초기화
	ds_in.clearData();
    gfn_clearGrid(grd_Main);
    grd_Main.binddataset = "";
   
	fsp_clear(this);
	
	//선택한 것이 75개가 넘는지 확인
	for(var i = ds_upjang_out.rowcount; i >= 0; i--){
		if(ds_upjang_out.getColumn(i, "SELECT") == "1"){
			cnt++;	
		}
	}
  
	if(cnt > 75){
		gfn_alert("사업장은 최대 75개까지 선택가능합니다.");
		return;
	}	
	// 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "ma/mam:MAM20020V_S001");
	// 서버 호출 
	http.Sync = true;
	if ((g_AuthLevel == "10") || (g_AuthLevel == "12"))  { // 영양사, 리더영양사 
