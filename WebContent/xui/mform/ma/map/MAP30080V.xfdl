function fn_search(){
	var cnt = 0;
	var i  = 0;
/*    
    if (length(edt_Tm.Text) == 0)  {
		g_Message("EE", gfn_msg(SCCW00005, "팀"));
		edt_Tm.SetFocus();
		return;
