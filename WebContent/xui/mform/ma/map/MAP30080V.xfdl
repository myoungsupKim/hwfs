	}else{
		fsp_callService(this, "", "", "", "ds_out=ds_out","YY=" + wrapQuote(cbo_Yy.value) + " CLOSE_TYPE=" + wrapQuote(rdo_Type.value)  + " TM_CD=" + wrapQuote(edt_Tm.userdata) + " UPJANG=" + wrapQuote(edt_Upjang.userdata) + " SUB_JOB_CD=" + wrapQuote(cbo_Subjob.value) + " LOC_CD=" + wrapQuote(cbo_Loc.value) + " SAUP_TYPE=" + wrapQuote(cbo_Saup.value) + " NEW_GUBUN=" + wrapQuote(cbo_New.value),"fn_afterSearch");
          
     
     
     for(i = 0; i < n_dsCNT.length; i++)
     {
		this.removeChild(n_dsCNT[i]);
     }
     n_printCNT = 0;
     n_dsCNT = new Array;
     
		for(i = 0; i <= ds_upjang_out.rowcount; i++){
			// 선택 된 사업장만 조회한다.
			if(ds_upjang_out.getColumn(i, "SELECT") == "1"){
				var dsObj;
				
				// 선택 된 업장만큼의 DATASET 동적생성
				fn_gridCreate("ds_out1_" + i);
				
				dsObj = eval("ds_out1_" + i);
				dsObj.addColumn("ACCT_CD",    "STRING",  20);
				dsObj.addColumn("ACCT_LEVEL", "STRING",  20);
				dsObj.addColumn("ACCT_NAME",  "STRING",  100);
				dsObj.addColumn("AMT01",      "DECIMAL", 22);
				dsObj.addColumn("AMT02",      "DECIMAL", 22);
				dsObj.addColumn("AMT03",      "DECIMAL", 22);
				dsObj.addColumn("AMT04",      "DECIMAL", 22);
				dsObj.addColumn("AMT05",      "DECIMAL", 22);
				dsObj.addColumn("AMT06",      "DECIMAL", 22);
				dsObj.addColumn("AMT07",      "DECIMAL", 22);
				dsObj.addColumn("AMT08",      "DECIMAL", 22);
				dsObj.addColumn("AMT09",      "DECIMAL", 22);
				dsObj.addColumn("AMT10",      "DECIMAL", 22);
				dsObj.addColumn("AMT11",      "DECIMAL", 22);
				dsObj.addColumn("AMT12",      "DECIMAL", 22);

				n_CNT = i;
				n_printCNT++;

				fsp_callService(this, "", "", "", "ds_out1_" + i + "=ds_out","YY=" + wrapQuote(cbo_Yy.value) + " CLOSE_TYPE=" + wrapQuote(rdo_Type.value)  + " TM_CD=" + wrapQuote(edt_Tm.userdata) + " UPJANG=" + wrapQuote(ds_upjang_out.getColumn(i, "UPJANG")) + " SUB_JOB_CD=" + wrapQuote(cbo_Subjob.value) + " LOC_CD=" + wrapQuote(cbo_Loc.value) + " SAUP_TYPE=" + wrapQuote(cbo_Saup.value) + " NEW_GUBUN=" + wrapQuote(cbo_New.value),"fn_afterSearch1");	  
			}
		}
	}
	http.Sync = false;
}

function fn_gridCreate(dsID)
{
	var ds = new Dataset;
	this.addChild(dsID, ds);
}

function fn_afterSearch1(errCode, errMsg)
{
	if(errCode != 0)
	{
		n_printCNT = 0;
		return;
	}
	n_dsCNT[n_printCNT-1] = "ds_out1_" + n_CNT;
	eval("Grid" + n_CNT).binddataset = n_dsCNT[n_printCNT-1];
}


