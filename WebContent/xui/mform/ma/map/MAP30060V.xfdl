function fn_searchSaupjang()
{
	// 업장 정보 조회 
    fsp_addSearch(this, "ma/mac:MAC_UPJANG_MST_S001");
    
    // 서버 호출 
	http.Sync = true;
	//2020.06.08
    //fsp_callService(this, "", "", "", "ds_upjang_out=ds_upjang_out","TM_CD=" + wrapQuote(edt_Tm.getTmCode()));
    var befYy = "";
    befYy = cbo_Yy.value -1 + "1231"; //전년말일
    if(chk_Except.value == "Y")
    {
		fsp_callService(this, "", "", "", "ds_upjang_out=ds_upjang_out","TM_CD=" + wrapQuote(edt_Tm.getTmCode()) + " BEF_YY=" + wrapQuote(befYy));
    }
    else
    {
		fsp_callService(this, "", "", "", "ds_upjang_out=ds_upjang_out","TM_CD=" + wrapQuote(edt_Tm.getTmCode()));
    }
    http.Sync = false;
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_Search()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 조회버튼 입력 시 처리되는 작업
 * 작  성  자   : 
 * 작성  일자   : 
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_search()
{
	if( gfn_isNull(edt_Tm.getTmCode()) )
	{
		g_Message("EE","팀을 선택하여 주십시오!");
		return;
	}





    // 초기화
	ds_in.clearData();
    gfn_clearGrid(grd_Main);
    grd_Main.binddataset = "";

	var strInDatasets = "";
	var strOutDatasets = "ds_out=ds_out";
	var strArgument  = "YY=" + wrapQuote(cbo_Yy.value);
	
	    if(chk_bonbu.value == true){
		   strArgument += " TM_CD=" + wrapQuote(gfn_subStr(edt_Tm.getTmCode(),0,3) );
		} else
		{
		   strArgument += " TM_CD=" + wrapQuote(edt_Tm.getTmCode());
		}
		strArgument += " UPJANG=" + wrapQuote(edt_Upjang.getUpjangCode());
		strArgument += " SUB_JOB_CD=" + wrapQuote(cbo_Subjob.value);
		strArgument += " LOC_CD=" + wrapQuote(cbo_Loc.value);
		strArgument += " SAUP_TYPE=" + wrapQuote(cbo_Saup.value);
		strArgument += " NEW_GUBUN=" + wrapQuote(cbo_New.value);
		strArgument += " S_TYPE=" + wrapQuote(rdo_gubun.value);

	if( (g_AuthLevel == "10") || (g_AuthLevel == "12") )	// 영양사, 리더영양사 
		strArgument += " CLOSE_TYPE=" + wrapQuote(rdo_Type_Temp.value);
	else
		strArgument += " CLOSE_TYPE=" + wrapQuote(rdo_Type.value);
		
	//블로킹사용자체크를위해추가
	strArgument += " UUSER=" + wrapQuote(gfn_getUserId());

	var strCallbackFunc = "fn_afterSearch";
	var strTrId = "search";

	fsp_clear(this);

	if( rdo_gubun.value == 1 )
	{
		ds_out.keystring = "";
		grd_Main.formatid = "format_1";
		fsp_addSearch(this, "ma/map:MAP30060V_S002");
	}
	else if( rdo_gubun.value == 2 )
	{
		ds_out.keystring = "";
		grd_Main.formatid = "format_2";
		fsp_addSearch(this, "ma/map:MAP30060V_S002");	
	}
	if( rdo_gubun.value == 3 )
	{
		ds_out.keystring = "G:+SKEY";
		grd_Main.formatid = "format_3";
		fsp_addSearch(this, "ma/map:MAP30060V_S002");
	}
	else if( rdo_gubun.value == 4 )
	{
		ds_out.keystring = "G:+SKEY";
		grd_Main.formatid = "format_4";
		fsp_addSearch(this, "ma/map:MAP30060V_S001");
	}
	
	
    fsp_callService(this, "", "", strInDatasets, strOutDatasets, strArgument, strCallbackFunc, true, true, false, strTrId);
}

