/*
 ******************************************************************************************
 * 함  수  명   : fn_OrgChange() 
 * 입      력   : 없음
 * 반      환   : 
 * 기      능   : 콤보박스 자료 생성
 * 작  성  자   : 
 * 작성  일자   :
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_OrgChange()
{
	fsp_clear(this);
   
	if (gfn_length(gfn_trim(edt_Upjang.getUpjangCode()))== 0 ) {  
		if (gfn_length(gfn_trim(edt_Tm.getTmCode()))== 0 ) {
			//지역코드
			fn_sql_common("C", strMAQuery_LOC_CD,""," ORDER BY CODE_NAME","non",cbo_Loc,-1,ds_cbo_Loc);
			 
			//SUB 직군코드
			fn_sql_common("C", strMAQuery_MASUBJOB,""," ORDER BY SUB_JOB_NAME","non",cbo_Subjob,-1,ds_cbo_SubJob);
			
			//사업코드
			fn_sql_common("C", strMAQuery_MASAUP_TYPE1,""," ORDER BY SAUP_NAME","non",cbo_Saup,-1,ds_cbo_saup);
			 
			cbo_New.userdata = "";
			cbo_New.text = "";
			 
			fsp_clear(this);
			// 업장 정보 조회 
			fsp_addSearch(this, "ma/mac:MAC_UPJANG_MST_S001");
			// 서버 호출 
			http.Sync = true;
			fsp_callService(this, "", "", "", "ds_upjang_out=ds_upjang_out","TM_CD=" + wrapQuote(edt_Tm.getTmCode()) + " SUB_JOB_CD=" + wrapQuote(cbo_Subjob.value) + " LOC_CD=" + wrapQuote(cbo_Loc.value) + " SAUP_TYPE=" + wrapQuote(cbo_Saup.value) + " YY=" + wrapQuote(cbo_Yy.value) + " NEW_GB=" + wrapQuote(cbo_New.value) );
			http.Sync = false;
		} else {
	  		//지역코드
			fn_sql_common("C", strMAQuery_MALOC_CD,""," AND TM_CD = '"+gfn_toString(edt_Tm.getTmCode())+ "' ORDER BY LOC_NAME","",cbo_Loc,-1,ds_cbo_Loc);
		 
			//SUB 직군코드
			fn_sql_common("C", strMAQuery_MASUBJOB1,""," AND TM_CD = '"+gfn_toString(edt_Tm.getTmCode())+ "' ORDER BY SUB_JOB_NAME","",cbo_Subjob,-1,ds_cbo_SubJob);
			 
			//사업코드
			fn_sql_common("C", strMAQuery_MASAUP_TYPE2,""," AND TM_CD = '"+gfn_toString(edt_Tm.getTmCode())+ "' ORDER BY SAUP_NAME","",cbo_Saup,-1,ds_cbo_saup);
			 
			cbo_New.userdata = "";
			cbo_New.text = "";
		}
	} else {
        //지역코드
		fn_sql_common("C", strMAQuery_MALOC_CD,""," AND UPJANG = '"+gfn_toString(edt_Upjang.getUpjangCode())+ "' ORDER BY LOC_NAME","",cbo_Loc,-1,ds_cbo_Loc);
		cbo_Loc.index = 0;
		 
		//SUB 직군코드
		fn_sql_common("C", strMAQuery_MASUBJOB1,""," AND UPJANG = '"+gfn_toString(edt_Upjang.getUpjangCode())+ "' ORDER BY SUB_JOB_NAME","",cbo_Subjob,-1,ds_cbo_SubJob);
		cbo_Subjob.index = 0;
		 
		//사업코드
		fn_sql_common("C", strMAQuery_MASAUP_TYPE2,""," AND UPJANG = '"+gfn_toString(edt_Upjang.getUpjangCode())+ "' ORDER BY SAUP_NAME","",cbo_Saup,-1,ds_cbo_saup);
		cbo_Saup.index = 0;
		
		// 업장 신규 정보 조회 
		fsp_addSearch(this, "ma/mac:MAC_UPJANG_NEW_S001");
		// 서버 호출 
		http.Sync = true;
		fsp_callService(this, "", "", "", "ds_old_out=ds_old_out","YY=" + wrapQuote(cbo_Yy.value) + " UPJANG=" + wrapQuote(edt_Upjang.getUpjangCode()));
		http.Sync = false;
		
		/*if (length(ds_old_out.GetColumn(ds_old_out.row, "NEW_GB")) == 0) {
		  cbo_New.UserData = "N";
		  cbo_New.Text = "신규";
		 }
		else
		  cbo_New.UserData = "O";
		  cbo_New.Text = "기존";
		 } */
		 
		if (ds_old_out.getColumn(ds_old_out.rowposition, "NEW_GB") == 0) {
			cbo_New.index = 2; 
		} else {
			cbo_New.index = 1;
		}
	}
}
   
