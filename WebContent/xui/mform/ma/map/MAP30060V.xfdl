/*----------------------------------------------------------------------------------------------
 * 설명      : 매출이익 합계
 *---------------------------------------------------------------------------------------------*/
function fn_sum(arg)
{
	var temp;
	if (gfn_isNull(arg)) {
		temp = 0;
	} else {
		if( grd_Main.formatid == "format_1" )
		{
			temp = Math.round(ds_out.getCaseSum("L_ACCT_CLASS== '매출액'",arg) 
							- ds_out.getCaseSum("L_ACCT_CLASS== '매출원가'",arg));
		}
		else
		{
			temp = Math.round(ds_out.getCaseSum("M_ACCT_CLASS== '매출액'",arg) 
							- ds_out.getCaseSum("M_ACCT_CLASS== '재료비'",arg) 
							- ds_out.getCaseSum("M_ACCT_CLASS== '경비'",arg) 
							- ds_out.getCaseSum("M_ACCT_CLASS== '인건비'",arg));		
		}

	}
	return temp;
}

