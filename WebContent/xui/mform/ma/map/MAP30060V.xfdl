function MAP30060V_onsize(obj:Form, e:SizeEventInfo)
{

	var nSpt  = sp_line.position.y;
	var nForm = this.position.bottom;
	
	if(nForm -35 <  nSpt && nSpt > 153)
	{
		sp_line.position.y = nForm -35;
	}
	sp_line.max = this.position.bottom - 35;
	sp_line.min = 133;
}


