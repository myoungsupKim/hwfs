 * 기      능   : callback 
 * 작  성  자   : 
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_afterSearch(nErrorCode, strErrorMsg, strSvcID)
{
   // 재료비율
    // 재료비 식음료 비율 
    // 재료비 상품 비율
    // 인건비 비율
    // 경비 비율
    // 매출이익율 
 
	if( grd_Main.formatid == "format_4" )
	{
    var row1 = fn_getRow("PLB10000");   // 재료비 PLB10000
    var row2 = fn_getRow("PLA00000");   // 매출액 PLA00000
   
    var row3 = fn_getRow("PLB11000");   // 식음료재료비 PLB11000
    var row4 = fn_getRow("PLA10000");   // 식음료매출 PLA10000
    
    var row5 = fn_getRow("PLB12000");   // 상품재료비 PLB12000
    var row6 = fn_getRow("PLA20000");   // 상품매출 PLA20000
    
    var row7 = fn_getRow("PLB20000");   // 인건비 PLB20000
    var row8 = fn_getRow("PLA00000");   // 매출액 PLA00000
    
    var row9 = fn_getRow("PLB30000");   // 경비 PLB30000
    var row10 = fn_getRow("PLA00000");  // 매출액 PLA00000
    
    var row11 = fn_getRow("PLC00000");   // 매출이익 PLC00000
    var row12 = fn_getRow("PLA00000");   // 매출액 PLA00000
    
    var tRow1 = fn_getRow("PLB10090");   // 재료비원가율 PLB10090
    var tRow2 = fn_getRow("PLB11090");   // 식음료원가율 PLB11090
    var tRow3 = fn_getRow("PLB12090");   // 상품원가율 PLB12090
    
    var tRow4 = fn_getRow("PLB20090");   // 인건비비율 PLB20090
    var tRow5 = fn_getRow("PLB30090");	 // 경비비율 PLB30090
    
    var tRow6 = fn_getRow("PLC00090");   // 영업이익율 PLC00090
     
     // 재료비 비율
    if (toNumber(ds_out.getColumn(row2, "SUM_AMT")) != 0 ) {
	    ds_out.setColumn(tRow1, "SUM_AMT" 
	         , Math.round(toNumber(ds_out.getColumn(row1, "SUM_AMT")) / toNumber(ds_out.getColumn(row2, "SUM_AMT")) * 100, 1));
	}
		
	// 식음료 재료비 비율
   	if (toNumber(ds_out.getColumn(row4, "SUM_AMT")) != 0 ) {
		ds_out.setColumn(tRow2, "SUM_AMT"
			, Math.round(toNumber(ds_out.getColumn(row3, "SUM_AMT")) / toNumber(ds_out.getColumn(row4, "SUM_AMT")) * 100 , 1));
	}	
		
	 // 상품 재료비 비율 
	if (toNumber(ds_out.getColumn(row6, "SUM_AMT")) != 0 ) {
		ds_out.setColumn(tRow3, "SUM_AMT"
			, Math.round(toNumber(ds_out.getColumn(row5, "SUM_AMT")) / toNumber(ds_out.getColumn(row6, "SUM_AMT")) * 100 , 1));
	}	
		
	// 인건비 비율 
	if (toNumber(ds_out.getColumn(row8, "SUM_AMT")) != 0 ) {	
		ds_out.setColumn(tRow4, "SUM_AMT"
			, Math.round(toNumber(ds_out.getColumn(row7, "SUM_AMT")) / toNumber(ds_out.getColumn(row8, "SUM_AMT")) * 100 , 1));
	}
	 		
	// 경비 비율
	if (toNumber(ds_out.getColumn(row10, "SUM_AMT")) != 0 ) {		 
		ds_out.setColumn(tRow5, "SUM_AMT"
			, Math.round(toNumber(ds_out.getColumn(row9, "SUM_AMT")) / toNumber(ds_out.getColumn(row10, "SUM_AMT")) * 100 , 1)); 
	}
	 		   
	// 매출이익율
	if (toNumber(ds_out.getColumn(row12, "SUM_AMT")) != 0 ) {		
		ds_out.setColumn(tRow6, "SUM_AMT"
			, Math.round(toNumber(ds_out.getColumn(row11, "SUM_AMT")) / toNumber(ds_out.getColumn(row12, "SUM_AMT")) * 100 , 1));    
    }

    var colNm = "";
    for( var i = 1; i <= 12; i++ ) {
        if ( i < 10 ) {
            colNm = "0" + i;
        } else {
            colNm = "" + i;
        }
        
		if (toNumber(ds_out.getColumn(row2, "MM_AMT_" + colNm)) != 0 ) {
			ds_out.setColumn(tRow1, "MM_AMT_" + colNm
				, Math.round(ds_out.getColumn(row1, "MM_AMT_" + colNm) / ds_out.getColumn(row2, "MM_AMT_" + colNm) * 100 , 1));
		}
     
		if (toNumber(ds_out.getColumn(row4, "MM_AMT_" + colNm)) != 0 ) {       
			ds_out.setColumn(tRow2, "MM_AMT_"+ colNm
				, Math.round(ds_out.getColumn(row3, "MM_AMT_"+ colNm) / ds_out.getColumn(row4, "MM_AMT_"+ colNm) * 100 , 1));
		}
        
		if (toNumber(ds_out.getColumn(row6, "MM_AMT_" + colNm)) != 0 ) {   
			ds_out.setColumn(tRow3, "MM_AMT_"+ colNm
				, Math.round(ds_out.getColumn(row5, "MM_AMT_"+ colNm) / ds_out.getColumn(row6, "MM_AMT_"+ colNm) * 100 , 1));
		}
            
		if (toNumber(ds_out.getColumn(row8, "MM_AMT_" + colNm)) != 0 ) {        
			ds_out.setColumn(tRow4, "MM_AMT_" + colNm
				, Math.round(ds_out.getColumn(row7, "MM_AMT_" + colNm) / ds_out.getColumn(row8, "MM_AMT_" + colNm) * 100 , 1));
		}
            
		if (toNumber(ds_out.getColumn(row10, "MM_AMT_" + colNm)) != 0 ) {        
			ds_out.setColumn(tRow5, "MM_AMT_"+ colNm
				, Math.round(ds_out.getColumn(row9, "MM_AMT_"+ colNm) / ds_out.getColumn(row10, "MM_AMT_"+ colNm) * 100 , 1));
		} 
       
		if (toNumber(ds_out.getColumn(row12, "MM_AMT_" + colNm)) != 0 ) {   
			ds_out.setColumn(tRow6, "MM_AMT_"+ colNm
				, Math.round(ds_out.getColumn(row11, "MM_AMT_"+ colNm) / ds_out.getColumn(row12, "MM_AMT_"+ colNm) * 100 , 1));    
		} 
    }
	}

    
    grd_Main.binddataset = "ds_out";
    gfn_showMsgGrid(grd_Main);
}
 
 /*
 ******************************************************************************************
 * 함  수  명   : fn_getRow
 * 입      력   : 
 * 반      환   : 없음
 * 기      능   : 처리할 row 찾기  
 * 작  성  자   : 
 * ----------------------------------------------------------------------------------------
