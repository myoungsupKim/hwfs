function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(obj, false);

	fsp_init(this);

	grd_Main.formatid = "format_4";

	edt_Tm.applyAuthLevel(cbo_mu);
	edt_Upjang.applyAuthLevel(edt_Tm);
	
	//Login 팀 정보 
// 	edt_Tm.userdata = g_Tm_Cd;
// 	edt_Tm.value = g_Tm_Nm;
// 	
// 	// Login 사업장 정보
// 	edt_Upjang.userdata = g_Upjang;
// 	edt_Upjang.value = g_UpjangNm;
	
	// 실적결산월 가져옴.
	cbo_Yy.value = gfn_left(gfn_today(),4);
	
	//사용자 권한에 따른 구분 선택 여부 셋팅########################################################
	if ((g_AuthLevel == "10") || (g_AuthLevel == "12")) { // 영양사, 리더영양사
		//st_Gubun.visible = false;
		rdo_Type.visible = false;
		rdo_Type.index = 0;
		
		//rdo_Type_Temp.visible = true;
        rdo_Type_Temp.index = 1;
    } else {
		//st_Gubun.visible = true;
		rdo_Type.visible = true;
		rdo_Type.index = 1;
		
		//rdo_Type_Temp.visible = false;
    } 
    
    //사용자 권한에 따른 구분 선택 여부 셋팅########################################################
	fsp_clear(this);
	
	// 업장 정보 조회 
    fn_searchSaupjang();
    
    var nRow = ds_upjang_out.findRow("UPJANG", edt_Upjang.getUpjangCode());
    ds_upjang_out.rowposition = nRow;
    
    fn_OrgChange();
    
     // 본부장 권한 추가 작업 진행 
	if (g_AuthLevel >= "17")   
	{
		chk_bonbu.visible = true;
	} else {
		chk_bonbu.visible = false;
	}
    //본부장 권한 추가 작업 종료       

    //권한에 따른 EXCEL DOWN세팅 #############################################################################
    fn_SetBlockExcelAuth(ds_cond, grd_Main);   
    //권한에 따른 EXCEL DOWN세팅 #############################################################################

	rdo_gubun.value = 4;

	//2019.12.09 김명섭: 엑셀다운로드 블로킹 대상자(공통코드: EXLDOWN_BLOCKING)
	LimsUtil.selectCodeBatchList(this, null, "EXLDOWN_BLOCKING");
}

