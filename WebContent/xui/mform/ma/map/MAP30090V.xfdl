/*
 ******************************************************************************************
 * 함  수  명   : fn_OrgChange() 
 * 입      력   : 없음
 * 반      환   : 
 * 기      능   : 콤보박스 자료 생성
 * 작  성  자   : 
 * 작성  일자   :
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_OrgChange()
{
	fsp_clear(this);
   
	if (gfn_length(gfn_trim(edt_upjang.getUpjangCode()))== 0 ) {  
		if (gfn_length(gfn_trim(edt_tm.getTmCode()))== 0 ) {
			fsp_clear(this);
			// 업장 정보 조회 
			fsp_addSearch(this, "ma/mac:MAC_UPJANG_MST_S001");
			// 서버 호출 
			http.Sync = true;
			fsp_callService(this, "", "", "", "ds_upjang_out=ds_upjang_out","TM_CD=" + wrapQuote(edt_tm.getTmCode()) );
			http.Sync = false;
		}
	} else {
		// 업장 신규 정보 조회 
		fsp_addSearch(this, "ma/mac:MAC_UPJANG_NEW_S001");
		// 서버 호출 
		http.Sync = true;
		fsp_callService(this, "", "", "", "ds_old_out=ds_old_out","YY=" + wrapQuote(yy.value) + " UPJANG=" + wrapQuote(edt_upjang.getUpjangCode()));
		http.Sync = false;
	}
}
