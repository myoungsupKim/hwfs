function fn_sum(arg){
	var temp;

	if( rdo_gubun.value == 1 )
	{
		temp = Math.round(ds_List.getCaseSum("L_ACCT_NM== '매출액'",arg) 
					    - ds_List.getCaseSum("L_ACCT_NM== '매출원가'",arg));
	}
	else
	{
		temp = Math.round(ds_List.getCaseSum("M_ACCT_NM== '매출액'",arg) 
					    - ds_List.getCaseSum("M_ACCT_NM== '재료비'",arg) 
						- ds_List.getCaseSum("M_ACCT_NM== '경비'",arg) 
						- ds_List.getCaseSum("M_ACCT_NM== '인건비'",arg));	
	}

	return temp;
}

