	ds_listInput.setColumn(ds_listInput.rowposition, "MGMT_SABUNNM",   edt_MGMT_SABUNQuery.value);	  
	
	ds_listInput.setColumn(ds_listInput.rowposition, "UPJANG",        edt_UPJANGQuery.userdata);	  
