function fn_GetBeforeCustInfo(objOutDs, str_GUBUN, str_MGMT_SABUN, real_CUST_ID)
{

 
    objOutDs.clear();
    objOutDs.addColumn("NAME","STRING",256);	       
    objOutDs.addColumn("DEPTNM","STRING",256);	       
    objOutDs.addColumn("JOB_GRADE","STRING",256);	       
    objOutDs.addColumn("HP_TEL","STRING",256);	       
    objOutDs.addColumn("INTERVIEW_CONTENTS","STRING",256);	       
    
    
	// action 정보 초기화 
    fsp_clear(this);  
	
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "ma/mak:MAK10020E_S003");
	
	//alert(str_ACCTCD);
	//alert(str_DATE_STD);
	// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", "", objOutDs.name + "=ds_list","GUBUN=" + wrapQuote(str_GUBUN) +
	                                                           " MGMT_SABUN=" + wrapQuote(str_MGMT_SABUN) +
	                                                           " CUST_ID=" + wrapQuote(real_CUST_ID)
	                                                           ,"");	
	http.Sync = false;
	                        
	
	if (objOutDs.rowcount > 0){
	
		ds_listInput.setColumn(0, "CUSTNM",             objOutDs.getColumn(0, "NAME"));
		ds_listInput.setColumn(0, "DEPTNM",             objOutDs.getColumn(0, "DEPTNM"));
		ds_listInput.setColumn(0, "JOB_GRADE",          objOutDs.getColumn(0, "JOB_GRADE"));
		ds_listInput.setColumn(0, "JOB_GRADENM",        objOutDs.getColumn(0, "JOB_GRADENM"));
		ds_listInput.setColumn(0, "HP_TEL", 		    objOutDs.getColumn(0, "HP_TEL"));
		ds_listInput.setColumn(0, "INTERVIEW_CONTENTS", objOutDs.getColumn(0, "INTERVIEW_CONTENTS"));
		
		edt_JOB_GRADE.userdata = objOutDs.getColumn(0, "JOB_GRADE");		
		
	}
	
}
