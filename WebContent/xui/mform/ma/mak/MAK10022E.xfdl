function fn_search()
{
	if (!before_SelectCheck())
		return;


	//리스트############################################################
    fsp_clear(this);
    
	fsp_addSearch(this, "ma/mak:MAK10022E_S001");	

	ds_cond.clear();
	
	ds_cond.addColumn("MGMT_SABUN","STRING",256);	
	ds_cond.addColumn("UPJANG","INT",256);
	ds_cond.addColumn("VISIT_DATE","STRING",256);
			
	ds_cond.addRow();

	var n_UPJANG = (edt_UPJANGQuery.userdata == 0) ? null: edt_UPJANGQuery.userdata;

	ds_cond.setColumn(ds_cond.rowposition, "MGMT_SABUN",   edt_MGMT_SABUNQuery.userdata);	
	ds_cond.setColumn(ds_cond.rowposition, "UPJANG",       n_UPJANG);
	ds_cond.setColumn(ds_cond.rowposition, "VISIT_DATE",   me_VISIT_DATEQuery.value);
					
	fn_DataSetAllClear();

    http.Sync = true;
	fsp_callService(this, "", "", "ds_cond=ds_cond","ds_list=ds_list","","");
	http.Sync = false;	
	//리스트############################################################
	
}

function fn_insert()
{
	if (!before_SelectCheck()) return;
	
