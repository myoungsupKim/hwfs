function fn_SaveTrxData(real_VISIT_ID)
{

    if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "MGMT_SABUN"))== 0){    

		g_Message("EE", "담당자를 선택하여 주십시요.\n" +
						"확인하여 주십시요.");        
        edt_MGMT_SABUNQuery.setFocus();
        return false;
    }
    
    
    if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"))== "0"){    

		g_Message("EE", "사업장을 선택하여 주십시요.\n" +
						"확인하여 주십시요.");        
        edt_UPJANGQuery.setFocus();
        return false;
    }
    
    if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_DATE"))== 0){    

		g_Message("EE", "방문일자가 선택되어 있지 않습니다.\n" +
						"확인하여 주십시요."); 						       
        me_VISIT_DATE.setFocus();
        return false;
    }    




    f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID");
    
    if (numFormat(real_VISIT_ID)== "0"){
    
		ds_cond.clear();
		ds_cond.addColumn("MAK_VISIT_MGMT_C_S","INT",256);
					
		// action 정보 초기화
		fsp_clear(this);

		// 서버에서 조회할 정보 추가
		fsp_addSearch(this, "ma/mak:MAK10022E_S002");

		// 서버 호출
		http.Sync = true;
		fsp_callService(this, "", "", "", ds_cond.name + "=ds_list","","");
		http.Sync = false;
		
		real_VISIT_ID = ds_cond.getColumn(ds_cond.rowposition, "MAK_VISIT_MGMT_C_S");
		ds_listInput.setColumn(ds_listInput.rowposition, "VISIT_ID", real_VISIT_ID);
		
		f_Saved_ID  = 0;
		f_NewDataCheck = true;
    }
    
  
    return true;
}
