function fn_GetBackColorClose(str_rowtype, ColorGubun, GUBUN)
{
   	if (str_rowtype == "logical")
   	{
		return "#fddcc9ff";
	}
	else if (GUBUN == "A") //본사표시
	{
	   
		return "cornsilk";	//floralwhite, 	#FFF9D6
	}
	else
	{
	   
		if (ColorGubun == "1")
			return "white";
		else
			return "#F6FCFC";
	}

}
