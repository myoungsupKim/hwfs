		// -- 트리를 펼친상태로 엑셀을 출력한다.
		grid_Slip.treeinitstatus = "expand,all";
		fn_excelDown();
	} else {
		// 하위사업장 제외
		fn_listCopy();
		fn_excelDown2();
	}

}


/*----------------------------------------------------------------------------------------------
 * 설명      : 엑셀다운로드 
 *---------------------------------------------------------------------------------------------*/
function fn_excelDown()
{
	var titleName = "거래처별 월매출 집계";
	var exportObj = new ExportObject();	
	exportObj.clear();
	exportObj.exporttype = ExportTypes.EXCEL;
	
	var fileName = titleName + ".xlsx";
	var sheetName = titleName;
	
	exportObj.activepagename = sheetName;
	exportObj.exportfilename = fileName;	
	exportObj.exportopenmode = "overwrite";
	
	var ret = exportObj.addExportItem(ExportItemTypes.GRID, grid_Slip, sheetName + "!A1", "" ,"" ,"" ,"" ,"onlyvalue");
	
	exportObj.exporteventtype = "itemrecord";
	exportObj.exportuitype = "exportprogress";
	exportObj.exportmessageprocess = "엑셀을 생성중 입니다."; 

	var result = exportObj.export();
	exportObj == null;
	grid_Slip.treeinitstatus = "collapse,all";
}




function fn_listCopy()
{
	ds_listDummy.clearData();
	
	for (var i = 0 ; i < ds_list.getRowCount() ; i++)
	{
		if (gfn_isNull(ds_list.getColumn(i, "UPJANG"))) {
			var iIdx = ds_listDummy.addRow();

			ds_listDummy.copyRow(iIdx, ds_list, i);
			
			//Next Row 
			var nfindRow = i + 1;
			
			//var nfindRow = ds_list.findRowExpr("BL_PLUS=='+'&&MAIN_UPJANG2=='" + ds_listDummy.getColumn(iIdx, "MAIN_UPJANG2") + "'");
			if (nfindRow != -1) {
				ds_listDummy.setColumn(iIdx, "MAIN_UPJANGNM", ds_list.getColumn(nfindRow, "MAIN_UPJANGNM"));
				ds_listDummy.setColumn(iIdx, "UPJANGNM", "(본사소계)");
			}
		}
	}
}

