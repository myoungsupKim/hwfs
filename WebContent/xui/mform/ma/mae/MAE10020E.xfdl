function ds_listInput_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
//alert(nPivotIndex);

	if (e.columnid == "CMP_CLASS"){
	
		edt_CMP_GRP.enable    = false;
		edtBtn_CMP_GRP.enable = false;
		
		edt_CMP_TYPE.enable      = false;
		edtBtn_CMP_TYPE.enable   = false;
		
		edt_CMP_ACCTCD.enable    = false;
		edtBtn_CMP_ACCTCD.enable = false;
	
		cbo_CMP_SPLS_YN.enable     = false;
		
		edt_CMP_SPLS_ACCTCD.enable    = false;
		edtBtn_CMP_SPLS_ACCTCD.enable = false;
		
		cbo_CMP_AUTO_YN.enable     = false;
		
		cbo_ALL_YN.enable          = false;	
		
	    if (-1 == -1){
	    
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_GRP", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_GRPNM", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_TYPE", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_TYPENM", "");			
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_ACCTCD", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_ACCTCDNM", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_SPLS_YN", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_SPLS_ACCTCD", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_SPLS_ACCTCDNM", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_AUTO_YN", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "ALL_YN", "");			
		}
				
		for (i=0;i<ds_cbo_CMP_CLASS.rowcount;i++){
		//alert("d");
			if (gfn_subStr(e.newvalue, i, 1)== "A"){
				edt_CMP_ACCTCD.enable    = true;
				edtBtn_CMP_ACCTCD.enable = true;
			}
			else if (gfn_subStr(e.newvalue, i, 1)== "T"){
				edt_CMP_TYPE.enable    = true;
				edtBtn_CMP_TYPE.enable = true;
			}
			else if (gfn_subStr(e.newvalue, i, 1)== "G"){
				edt_CMP_GRP.enable    = true;
				edtBtn_CMP_GRP.enable = true;
			}
		}
	
		if (e.newvalue != "N"){
	
			cbo_CMP_SPLS_YN.enable     = true;
			
			edt_CMP_SPLS_ACCTCD.enable    = true;
			edtBtn_CMP_SPLS_ACCTCD.enable = true;
			
			cbo_CMP_AUTO_YN.enable     = true;
			cbo_ALL_YN.enable          = true;
		}		
	}
	
	if (e.columnid == "CMP_SPLS_YN"){
	
		edt_CMP_SPLS_ACCTCD.enable    = false;
		edtBtn_CMP_SPLS_ACCTCD.enable = false;
	    
	    if (-1 == -1){
	    
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_SPLS_ACCTCD", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "CMP_SPLS_ACCTCDNM", "");
		}
	
		if (e.newvalue == "Y"){
	
			edt_CMP_SPLS_ACCTCD.enable    = true;
			edtBtn_CMP_SPLS_ACCTCD.enable = true;
		}		
	}
		
	if (e.columnid == "BIZ_CHK_YN"){
	
		if (-1 == -1){
		
			ds_listInput.setColumn(ds_listInput.rowposition, "BOND_CHK_YN", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_REG_YN", "");
		
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_ACCTCD", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_ACCTCDNM", "");
        }
        			
		if (e.newvalue == "T"){
	
			cbo_BOND_CHK_YN.enable     = true;
			cbo_FEE_REG_YN.enable      = false;
		}
		else if (e.newvalue == "R"){
	
			cbo_BOND_CHK_YN.enable     = false;
			cbo_FEE_REG_YN.enable      = true;
		}
		else {
	
			cbo_BOND_CHK_YN.enable     = false;
			cbo_FEE_REG_YN.enable      = false;
		}		
	}	
	
	if (e.columnid == "FEE_REG_YN"){
	
		if (-1 == -1){
		
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_ACCTCD", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_ACCTCDNM", "");
		}
	
		if (e.newvalue == "Y"){
	
			edt_FEE_ACCTCD.enable        = true;
			edtBtn_FEE_ACCTCD.enable     = true;
		}
		else {
			edt_FEE_ACCTCD.enable        = false;
			edtBtn_FEE_ACCTCD.enable     = false;
		}		
	}		

	if (e.columnid == "ADITEM1_CHK_YN"){
		
		if (-1 == -1){
			ds_listInput.setColumn(ds_listInput.rowposition, "ADITEM1_CD", "");
		}
	
	
		if (e.newvalue != "N"){
	
			cbo_ADITEM1_CD.enable     = true;
		}
		else {
			cbo_ADITEM1_CD.enable     = false;
		}		
	}		
	
	if (e.columnid == "ADITEM2_CHK_YN"){
	
		if (-1 == -1){
			ds_listInput.setColumn(ds_listInput.rowposition, "ADITEM2_CD", "");
		}
	
	
		if (e.newvalue != "N"){
	
			cbo_ADITEM2_CD.enable     = true;
		}
		else {
			cbo_ADITEM2_CD.enable     = false;
		}		
	}			

}

