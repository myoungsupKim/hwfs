/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";
	
	fsp_init(this);

	ds_search.addRow();
	trace("POP:=================================================");
	trace(arrSchVal[0]);
	//조회 셋팅값을 전달해 준다.			
	if(arrSchVal[0] == "채권대체"){
		trace("채권대체================================");
		ds_search.setColumn(0, "AR_NUM", arrSchVal[1]);
		ds_search.setColumn(0, "OCCUR_DATE", arrSchVal[2]);
		ds_search.setColumn(0, "TM_CD", arrSchVal[3]);
		ds_search.setColumn(0, "UPJANG", arrSchVal[4]);
		grd_list.setFormat("default");
		
		fn_search();
	}else if(arrSchVal[0] == "입금대체"){	
		trace("입금대체================================");
		ds_search.setColumn(0, "AR_NUM", arrSchVal[1]);
		ds_search.setColumn(0, "OCCUR_DATE", arrSchVal[2]);
		ds_search.setColumn(0, "TM_CD", arrSchVal[3]);
		ds_search.setColumn(0, "UPJANG", arrSchVal[4]);
		ds_search.setColumn(0, "T_GUBUN", "1");
		grd_list.setFormat("default_1");
		
		fn_search();
	}else if(arrSchVal[0] == "채권이관"){	
		trace("채권이관================================");
		ds_search.setColumn(0, "AR_NUM", arrSchVal[1]);
		ds_search.setColumn(0, "OCCUR_DATE", arrSchVal[2]);
		ds_search.setColumn(0, "TM_CD", arrSchVal[3]);
		ds_search.setColumn(0, "UPJANG", arrSchVal[4]);
		ds_search.setColumn(0, "T_GUBUN", "2");
		grd_list.setFormat("default_2");
		
		fn_search();			
	}else if(arrSchVal[0] == "선수금대체매출"){
		
		if(arrSchVal[6] == "식권"){
			ds_search.setColumn(0, "DATEFR", arrSchVal[1]);
			ds_search.setColumn(0, "DATETO", arrSchVal[2]);
			ds_search.setColumn(0, "TM_CD", arrSchVal[3]);
			ds_search.setColumn(0, "UPJANG", arrSchVal[4]);
			ds_search.setColumn(0, "OCCUR_ADVANCE_NUM", arrSchVal[5]);
