function edt_UPJANGQuery_OnKeyDown(obj:Edit, e:KeyEventInfo)
{
	fn_edt_OnKeyDown(obj,e.keycode);	
			
			
	if (e.keycode == 46){
	
		if (obj.name == "edt_UPDEPT_IDQuery"){
				
			fn_DataSetAllClear();
		
			//하위 업장정보  Clear#######################################################
			edt_UPJANGQuery.userdata = "";
			edt_UPJANGQuery.value     = "";
			//하위 업장정보  Clear#######################################################		
		}
		
		if (obj.name == "edt_UPJANGQuery"){
				
			fn_DataSetAllClear();				
		}	

	    //지역코드
		fn_sql_common("C", strMAQuery_LOC_CD,""," ORDER BY CODE_NAME","non",cbo_Loc,-1,ds_cbo_Loc);
	    //SUB 직군코드
		fn_sql_common("C", strMAQuery_MASUBJOB,""," ORDER BY SUB_JOB_NAME","non",cbo_Subjob,-1,ds_cbo_SubJob);			
	}
	else {
	
		if (obj.name == "edt_UPDEPT_IDQuery"){
			
			//지역코드
			fn_sql_common("C", strMAQuery_MALOC_CD,""," AND TM_CD = '"+gfn_toString(edt_UPDEPT_IDQuery.userdata)+ "' ORDER BY LOC_NAME","non",cbo_Loc,-1,ds_cbo_Loc);
			cbo_Loc.index = 0;
			 
			//SUB 직군코드
			fn_sql_common("C", strMAQuery_MASUBJOB1,""," AND TM_CD= '"+gfn_toString(edt_UPDEPT_IDQuery.userdata)+ "' ORDER BY SUB_JOB_NAME","non",cbo_Subjob,-1,ds_cbo_SubJob);
			cbo_Subjob.index = 0;	
		}
		
		if (obj.name == "edt_UPJANGQuery"){
				
			//지역코드
			fn_sql_common("C", strMAQuery_MALOC_CD,""," AND UPJANG = '"+gfn_toString(edt_UPJANGQuery.userdata)+ "' ORDER BY LOC_NAME","non",cbo_Loc,-1,ds_cbo_Loc);
			cbo_Loc.index = 0;
			 
			//SUB 직군코드
			fn_sql_common("C", strMAQuery_MASUBJOB1,""," AND UPJANG = '"+gfn_toString(edt_UPJANGQuery.userdata)+ "' ORDER BY SUB_JOB_NAME","non",cbo_Subjob,-1,ds_cbo_SubJob);
			cbo_Subjob.index = 0;				
		}			
	}
}


