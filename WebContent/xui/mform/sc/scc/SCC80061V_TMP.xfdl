 ******************************************************************************************
 */
function grd_prev_OnCellClick(obj:Grid, e:GridClickEventInfo)
{
    var conId = ds_prev.getColumn(0, "CON_ID");
    
	if ( conId == null || conId < 0 ) {
        return;
	}
	
	var dsObj = parent.div_list.ds_list;
	if ( F_LIST_SEARCH == "P" ) {
        var cRow = ds_prevList.findRow("CON_ID", toNumber( conId ));
        ds_prevList.setColumn(cRow, ds_prevList.getColumn(cRow, "READ_CNT") + 1 );
        
        Copy(dsObj, ds_prevList );
        dsObj.rowposition = dsObj.getRowCount() -1;
        parent.div_list.cbo_page.index = parent.div_list.cbo_page.index -1;
        parent.div_list.ds_cond.setColumn(0, "CURR_PAGE"
            , toNumber(parent.div_list.cbo_page.index) );
	}
	
	// 건수 증가 
    var cRow = dsObj.findRow("CON_ID", toNumber( conId ));
    dsObj.setColumn(cRow, "READ_CNT", dsObj.getColumn(cRow, "READ_CNT") + 1 );
    dsObj.rowposition = cRow;

	fn_init( ds_prev.getColumn(0, "BBS_ID") , conId );
}
