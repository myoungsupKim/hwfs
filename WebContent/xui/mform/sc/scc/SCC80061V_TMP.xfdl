 * 함  수  명   : fn_clearDetail()
 * 기      능   : 화면 초기화 처리 
 ******************************************************************************************
 */
function fn_clearDetail(bbsId) {
    //namoWeb.Value = "<html></html>"; 
    ds_prev.clearData();
    ds_next.clearData();
  
    ds_master.clearData();
    ds_file.clearData();

    // 팝업여부 , 공지여부 ,공지 종료일 - g_AuthLevel = 99 만 가능 
    //if ( MF_CURR_AUTH == F_ADMIN_LEVEL ) {  // 관리자 
    if (g_Board_Mst == "관리자" || g_Board_Mst == "공지자") {
        if ( st_tNotic.visible == false ) {
            st_tNotic.visible = true;
            ed_eDate.visible = true;
            st_noticEnd.visible = true;
            
            st_tPopup.visible = true;
            ed_popupSdate.visible = true;
            ed_popupEDate.visible = true;
            st_pUnder.visible = true;
        }  
        
        btn_write.visible = true;
		btn_update.visible = true;
		btn_delete.visible = true;
        
        // 답변 있는 게시판인지 유무 
        btn_reply.visible = mfn_isReply(bbsId);
        
    } 
    else 
    {
		if ( mfn_getCurrAuth( bbsId ) != "R" ) { // 해당 게시판에 권한 있음. 
			btn_write.visible = true;
			btn_update.visible = true;
			btn_delete.visible = true;
			
			btn_write.enable = true;
			btn_update.enable = true;
			btn_delete.enable = true;

			// 답변 있는 게시판인지 유무 
			btn_reply.visible = mfn_isReply(bbsId);
		} else {
			btn_write.visible = true;
			btn_update.visible = true;
			btn_delete.visible = true;

			btn_write.enable = false;
			btn_update.enable = false;
			btn_delete.enable = false;

			btn_reply.visible = false;
		}
	}
	
	if (g_SystemId == "PUBLIC") { //공개일 경우 by kksoo
			btn_write.visible = true;
			btn_update.visible = true;
			btn_delete.visible = true;

			btn_write.enable = false;
			btn_update.enable = false;
			btn_delete.enable = false;

			btn_reply.visible = false;
			btn_repSave.enable = false;
	}
	
    /* RowHeight은 지원되지 않는 속성입니다.*/ //grd_repList.RowHeight = 20;
    txt_reqDesc.value = "";
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_init()
 * 기      능   : 현재 게시판 초기화  -> 작성 모드로 들어올 경우 
 ******************************************************************************************
 */
function fn_init(bbsId, conId) {

try {
	namoWeb.ShowToolbar(0, false);  // 메뉴 감추기 
	namoWeb.ShowToolbar(1, false);  // 메뉴 감추기 
	namoWeb.ShowToolbar(2, false);  // 메뉴 감추기 
	namoWeb.ShowTab(false);  // tab 감추기
	//namoWeb.EditMode = 0;   // 읽기 전용 
	namoWeb.ActiveTab = 2;
}
catch (e) {
	gfn_alert(e.message);
}
    F_CURR_BBS_ID = bbsId;
    fn_clearDetail(bbsId);

	//압축 통신 설정1
	////http.Compress = true;
	////http.CompressMethod = "ZLIB";
	//압축 통신 설정	
	if (arguments.length > 2) {
		F_CURR_MOBILE_YN = arguments[2];
	}

    fsp_callService(this, "SCC80050E_ACT", "select"
            , ""
            , "ds_master=ds_master ds_repList=ds_repList"
            , "UPDATE_YN='Y' CON_ID=" + conId + " g_EmpNo=" + g_EmpNo
            , "fn_afterSelect"
            , true); 
    
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_afterSelect()
 * 입      력   : errCode 	= 에러코드
				  errMsg	= 에러메세지
 * 반      환   : 없음
 * 기      능   :  조회 후 callback 
 * 작  성  자   : 
 * 작성  일자   : 
 ******************************************************************************************
 */
function fn_afterSelect(errCode, errMsg)
{	
	//압축 통신 해제
	////http.Compress = false;
	////http.CompressMethod = "";
	//압축 통신 해제

    // 첨부 파일 처리 
    if ( ds_master.getRowCount() == 0 ) {
        gfn_alert("해당하는 게시물이 존재하지 않습니다.");
        return;
    }

    // namo 
    //trace(ds_master.saveXML());
try {
    namoWeb.MIMEValue = ds_master.getColumn(0, "N_CONT");
}
catch (e) {
	gfn_alert(e.messge);
}
    // 작성자 명 
    ed_writer.value = ds_master.getColumn(0, "NM_KOR") + "/" 
        + ds_master.getColumn(0, "UPJANGNM");
    
    // 파일 표시 
    mfn_makeFileDs( ds_file, ds_master );
    btn_fileSave.visible = (ds_file.getRowCount() > 0 );
    
	//첨부파일 표시###################################################
	Static4.text  = "첨부 파일";
	Static4.style.color = "default";
	Static4.style.font  = "굴림,9";
		    
    if (ds_file.getRowCount() > 0){
    
		Static4.text  = "첨부(" + ds_file.getRowCount() + "개)";
		Static4.style.color = "blue";
		Static4.style.font  = "굴림,9,Bold";
    }
	//첨부파일 표시###################################################
	
	
	//의견 갯수에 따른 size 동적 표시###############################
	fn_SetHeightByReplyCnt();
	//의견 갯수에 따른 size 동적 표시###############################
	    
	    
    // 수정 , 삭제버튼 처리 
    btn_delete.enable = false;
    btn_update.enable = false;    
    
    if (g_SystemId != "PUBLIC") { //공개일 경우 by kksoo
		if ( mfn_isWritable( ds_master.getColumn(0, "WRITE_SABUN")) ) {
			btn_delete.enable = true;
			btn_update.enable = true;
		}

		// 현재 권한 
		var auth = mfn_getCurrAuth(ds_master.getColumn(0, "BBS_ID") );


		// 공지사항일 경우 답변 불가 , 답변 불가 게시판 처리 
		if ( ds_master.getColumn(0, "NOTICE_YN") != "Y" && auth != "R")
		{
			btn_reply.enable = true;
		}
		else 
		{
			if (ds_master.getColumn(0, "END_NOTICE") == "Y")
			{
				btn_reply.enable = true;
			}
			else
			{
				btn_reply.enable = false;
			}
		}
	}
	
    // 이전 , 다음 글 조회 
    // 초기화 
    ds_prevList.clearData();
    F_LIST_SEARCH = "";
    
    var pDsObj = parent.div_list.ds_list;
    var pCboObj = parent.div_list.cbo_page;
    var prevRow = pDsObj.rowposition - 1;
    
    if ( prevRow < 0 ) {
        if ( pCboObj.value == "1" ) {
            ds_prev.clearData();
            ds_prev.addRow();
            ds_prev.setColumn(0, "V_TITLE", "없음.");
        } else {
            // 조회한다. 
            fn_selectList( "Prev" );
        }
    } else {
        ds_prev.clearData();
        ds_prev.addRow();
        ds_prev.copyRow(0, pDsObj, prevRow);
    }
    
    // 다음 글 
    var pageSize = parent.div_list.ds_cond.getColumn(0, "PAGE_SIZE");
    var nextRow = pDsObj.rowposition + 1;
    if ( nextRow >= pageSize || nextRow >= pDsObj.getRowCount() ) {
        if ( pCboObj.value == parent.div_list.ds_pageCd.getRowCount() ) {
            ds_next.clearData();
            ds_next.addRow();
            ds_next.setColumn(0, "V_TITLE", "없음.");
        } else {
            fn_selectList( "Next" );
        }
    } else {
        ds_next.clearData();
        ds_next.addRow();
        ds_next.copyRow(0, pDsObj, nextRow);
    }
    
	if (F_CURR_MOBILE_YN == "Y") {
		//Static14.visible = true;
		st_tNotic00.visible = true;
		Edit00.visible = true;
	}
	else {
		//Static14.visible = false;
		st_tNotic00.visible = false;
		Edit00.visible = false;
	}
			    
}
function fn_afterSearchPrev( errCode, errMsg ) {
    F_LIST_SEARCH = "P";
    ds_prev.clearData();
    ds_prev.addRow();
    ds_prev.copyRow(0, ds_prevList, ds_prevList.getRowCount() -1);
}
function fn_afterSearchNext( errCode, errMsg ) {
    F_LIST_SEARCH = "N";
    ds_next.clearData();
    ds_next.addRow();
    ds_next.copyRow(0, ds_prevList, 0);
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_selectList()
 * 입      력   : 
 * 반      환   : 없음
 * 기      능   : 목록 페이지 다시 조회 
 * 작  성  자   : 
 * 작성  일자   : 
 ******************************************************************************************
 */
function fn_selectList( postStr ) {
    Copy(ds_prevCond,parent.div_list.ds_cond);
    if ( postStr == "Prev") {
        ds_prevCond.setColumn(0, "CURR_PAGE", toNumber(parent.div_list.cbo_page.value) - 1);
    } else {
        ds_prevCond.setColumn(0, "CURR_PAGE", toNumber(parent.div_list.cbo_page.value) + 1);   
    }
    
    fsp_clear(this);
    if ( MF_IS_ALL_SEARCH ) {
        Copy(ds_myBbsList,parent.div_list.ds_myBbsList);
        Copy(ds_myBbsList2,parent.div_list.ds_myBbsList2);
        Copy(ds_myBbsListM,parent.div_list.ds_myBbsListM);
        fsp_addSearch(this, "sc/scc:SCC80060V_S005");	
        fsp_callService(this, "", ""
                , "ds_cond=ds_prevCond ds_myBbsList=ds_myBbsList ds_myBbsList2=ds_myBbsList2 ds_myBbsListM=ds_myBbsListM"
                , "ds_prevList=ds_list"
                , ""
                , "fn_afterSearch" + postStr
                , true); 
    } else {
        if ( mfn_isPrivOnlyBBSrepYN() &&  MF_CURR_AUTH != F_ADMIN_LEVEL ) { // 게시 대상 선택일 경우 , 관리자 모드도 아니다. 
            fsp_addSearch(this, "sc/scc:SCC80060V_S003");	
        } else {    
            fsp_addSearch(this, "sc/scc:SCC80060V_S001");	
        }     
        fsp_callService(this, "", ""
                , "ds_cond=ds_prevCond"
                , "ds_prevList=ds_list"
                , ""
                , "fn_afterSearch" + postStr
                , true);  
    }       
}
