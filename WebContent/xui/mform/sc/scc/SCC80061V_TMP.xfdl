 */
function btn_repSave_OnClick(obj:Button, e:ClickEventInfo)
{
	if ( gfn_isNull(txt_reqDesc.value, "의견", txt_reqDesc) ) {
        return;
    }
    
    http.Sync = true;
        
    ds_master_repList.clearData();
    ds_master_repList.addRow();
    ds_master_repList.setColumn(0, "CON_ID", ds_master.getColumn(0, "CON_ID"));
    
    // 의견 달기 
    fsp_clear(this); 
    fsp_addSingle(this, "sc/scc:SCC80061V_I001");	 
    fsp_addSearch(this, "sc/scc:SCC80061V_S001");	 
    

	//Timeout 설정처리###########################################
	var t_timeout;
	t_timeout = application.httptimeout;
	application.httptimeout=5000; //1800초
	
	//http.Compress = true;
	//http.CompressMethod = "ZLIB";
	//http.XmlFormat = false;			
	//Timeout 설정처리###########################################
			    
    fsp_callService(this, "", ""
            , "ds_master_repList=ds_master_repList"
            , "ds_repList=ds_repList"
            , "REP_CONT=" + wrapQuote( txt_reqDesc.value ) + " UPJANGNM=" + wrapQuote(g_UpjangNm)
            , "fn_afterSearchRep"
            , true);
            
	//Timeout 설정처리###########################################
	application.httptimeout=t_timeout; //60초
	
	//http.Compress = false;
	//http.CompressMethod = "";
	//http.XmlFormat = true;					
	//Timeout 설정처리###########################################

	http.Sync = false;		            
}
