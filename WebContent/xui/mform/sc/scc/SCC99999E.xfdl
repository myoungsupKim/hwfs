function fn_showFileAttach(ds)
{
	var strId       = "FileAttach";
	var strURL      = "U_SCC::SCC99999P.xfdl";
	var objArgument = {
		P_FILE_REF_SEQ : ds.getColumn(ds.rowposition, "fileRefSeq")
	};
	var nWidth      = 650;
	var nHeight     = 295;
	var strStyle    = "";
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";
	
	var fileRefNo = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);
	
	if (fileRefNo != undefined && fileRefNo != -1) ds.setColumn(ds.rowposition, "fileRefSeq", fileRefNo);
}

