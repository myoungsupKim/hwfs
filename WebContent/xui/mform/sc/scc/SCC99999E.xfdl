//=========================================================================================
function fn_Callback_Search(nErrorCode, strErrorMag)
{
	//처리결과에 따른 메세지 출력
	if (nErrorCode == 0)
	{
		div_condi.Edit16.value = ds_progress.getRowCount();
		
		//선택된 값으로 이동
		if(nRow < ds_progress.rowcount) 
		{
			ds_progress.rowposition = nRow;		
		}
		else {
			ds_progress.rowposition = 0;
		}
		
		if (ds_progress.getRowCount() > 0)
		{
			fn_searchTestList();
		}
	}
}

function fn_save_callback(nErrorCode, strErrorMag)
{
	//저장후 재조회
	fn_search();
}

