function fn_search()
{
	// 초기화
	ds_progress.clearData();
	ds_input.clearData();

	// 파라미터셋팅
	ds_input.addRow();
	ds_input.setColumn(0,"DEV_NAME", 		div_condi.Edit4.value);
	ds_input.setColumn(0,"DEV_SCRT_EDATE", 	div_condi.MaskEdit2.value);
	ds_input.setColumn(0,"DEV_SCRT_YN", 	div_condi.Combo4.value);
	ds_input.setColumn(0,"DEV_TEST_YN", 	div_condi.Combo8.value);
	ds_input.setColumn(0,"ITO_NAME", 		div_condi.Edit5.value);
	ds_input.setColumn(0,"ITO_TEST_DATE", 	div_condi.MaskEdit7.value);
	ds_input.setColumn(0,"ITO_CHK_YN", 		div_condi.Combo5.value);
	ds_input.setColumn(0,"ITO_NAME2", 		div_condi.Edit00.value);
	ds_input.setColumn(0,"ITO_TEST_DATE2", 	div_condi.MaskEdit00.value);
	ds_input.setColumn(0,"ITO_CHK_YN2", 	div_condi.Combo10.value);
	ds_input.setColumn(0,"PI_NAME", 		div_condi.Edit6.value);
	ds_input.setColumn(0,"PI_TEST_DATE", 	div_condi.MaskEdit8.value);
	ds_input.setColumn(0,"PI_CHK_YN", 		div_condi.Combo6.value);
	ds_input.setColumn(0,"PROGRAM_NAME", 	div_condi.Edit13.value);
	ds_input.setColumn(0,"PROGRAM", 		div_condi.Edit14.value);
	ds_input.setColumn(0,"PREFIX", 			div_condi.Edit15.value);
	ds_input.setColumn(0,"GUBUN", 			div_condi.cbo_gubun.value);
	ds_input.setColumn(0,"SUB_GUBUN",		div_condi.cbo_subGubun.value);
	ds_input.setColumn(0,"DEL_YN",			div_condi.chk_delYn.value);
	
	var formObj			= this;
	var strSvcID		= "SCC99998E_S001";
	var strURL			= "U_svc::" + "sc/scc/SCC99998E_S001.jsp";
	var strInDatasets	= "inDs_Cond=ds_input";
	var strOutDatasets	= "ds_progress=ds_List ds_percent=ds_List1";
	var strArgument;	//= "name1=park name2=yongdae";
	var strCallbackFunc	= "fn_Callback_Search";
	var isErrorNotCall	= false;
	var isNotShow		= false;	//false 메세지 보여줌 --> sync일때 표현 안됨.
	var isSync			= false;	//true 다실행하고 실행됨.(async)

	//서버호출
	jsp_transaction(formObj, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, isErrorNotCall, isNotShow, isSync);
}

