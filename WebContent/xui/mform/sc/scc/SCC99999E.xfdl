function ds_pl_test_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if (e.columnid == "cnfCode" && obj.getColumn(e.row, "cnfCode") == "F")
	{
		obj.setColumn(e.row, "cnfDt", gfn_today());
	}
	else
	{
		obj.setColumn(e.row, "cnfDt", "");
	}
}

