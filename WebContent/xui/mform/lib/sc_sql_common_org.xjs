*/
function fn_SetAuth2(strEdDept, strBtnDept, strEdUpjang, strBtnUpjang, strDataSet, strDeptId, strDeptNm, strUpjangCd, strUpjangNm)
{

	var bDept = false;
	var bUpjang = false;
	
	var objedDept = eval( strEdDept );
	var objbtnDept = eval( strBtnDept );
	
	var objedUpjang = eval( strEdUpjang );
	var objbtnUpjang = eval( strBtnUpjang );
	
	switch(g_AuthLevel)
	{
		case "00":  //일반관리자
			bDept = false;
			bUpjang = false;
			break;
		case "10":  //영양사
			bDept = false;
			bUpjang = false;
			break;
		case "12":  //리더영양사
			bDept = false;
			bUpjang = true;
			break;
		case "14": //소팀장
			bDept = false;
			bUpjang = true;
			break;
		case "16": //팀장
			bDept = false;
			bUpjang = true;
			break;
		case "18": //경영자
			bDept = true;
			bUpjang = true;
			break;	
		case "99": //전산관리자(현업)
			bDept = true;
			bUpjang = true;
			break;	
		case "999": //전산관리자(전산)
			bDept = true;
			bUpjang = true;
			break;				
		default:
			bDept = false;
			bUpjang = false;
			break;
	}
	
	if( !gfn_isNull( objedDept ) )
	{
		objedDept.enable = bDept;
		if(bDept == false) { eval(strDataSet).setColumn(0, strDeptNm, g_Tm_Nm); eval(strDataSet).setColumn(0, strDeptId, g_Tm_Cd);
		}
	}
	if( !gfn_isNull( objbtnDept ) )	
	{
		objbtnDept.enable = bDept;
	}
	if( !gfn_isNull( objedUpjang ) )
	{
		objedUpjang.enable = bUpjang;
		//리더영양사일 경우 업장Delete를 못하게 막는다.(선택은 가능)
		if (g_AuthLevel == "12") objedUpjang.enable = false;
		
		if ( (bUpjang == false) || (g_AuthLevel == "12") ) { eval(strDataSet).setColumn(0, strUpjangNm, g_UpjangNm); eval(strDataSet).setColumn(0, strUpjangCd, g_Upjang); }
	}
	if( !gfn_isNull( objbtnUpjang ) )
	{
		objbtnUpjang.enable = bUpjang;
	}
	
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_MakeUpjangSQL()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 권한에 따른 업장 팝업 SQL 생성
