/*
 ******************************************************************************************
 * 함  수  명   : fn_sql_common
 *                ex) combo -> fn_sql_common("C", strQuery_Upjang, "")
 *                ex) popup -> fn_sql_common("C", strQuery_Upjang, "str_where)
 * 입      력   : in_sql 	= 사용할 SQL Statement
 *                in_where  = 검색조건 
 *                in_type   = Combo 인지 Popup 인지 구분
 * 예제(Combo) 	: fn_sql_common("C", strQuery_Upjang,"");
 *				  ds_upjang.Clear();
 *				  ds_upjang.Copy(gds_common_code);
 * 예제(popup) 	: fn_sql_common("P", strQuery_Upjang, strQuery_Where);
 * 반      환   : v_ret = DataSet
 * 기      능   : 해당쿼리의 코드와 코드명을 Dataset으로 반환한다.
 * 작  성  자   : 
 * 작성  일자   : 2007-11-26
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
*/
	
function fn_sql_common(in_type, in_sql, in_where, in_OrderBy, in_Combokind, objCombo, grd_idx, objOutDs, chkIRIS)
{
	if ( in_type == "P" ) {
		ds_common.clear();
	}
	else {
		gds_common_code.clear();
	}	
	
	var ActioinVal	= "";
	var OutDS		= "";
	var InDS		= "";
	var v_ret		= "";

	ActionVal		 = "in_sql="+wrapQuote(gfn_trim(in_sql));
	ActionVal		+= " in_where="+wrapQuote(gfn_trim(in_where)+ " " + gfn_trim(in_OrderBy));
	
	if ( in_type == "P" ) {
		OutDS		= "ds_common=ds_common";
	}
	else {
		OutDS		= "gds_common_code=ds_common";
	}
	//alert(ActionVal);	
	http.Sync = true;
//http.Compress = true;
//http.CompressMethod = "ZLIB";
	if ( chkIRIS )
	  Transaction("SCC_COMMON_SELECT","U_svc::" + "sc/scc/SCC_COMMON_SELECT_IRIS.jsp",InDS,OutDS,ActionVal,"_fn_sql_searchCode");
	else
	  Transaction("SCC_COMMON_SELECT","U_svc::" + "sc/scc/SCC_COMMON_SELECT.jsp",InDS,OutDS,ActionVal,"_fn_sql_searchCode");
	http.Sync		= false;
//http.Compress = false;
//http.CompressMethod = "";

	//g_debug("ds_common");	
	if ( in_type == "P" ) {
		
		v_ret	= ds_common;
		
	}
	else {
	
		objOutDs.clear();
		Copy(objOutDs,gds_common_code);
		
		if ( grd_idx == -1 ) {
			objCombo.innerdataset = objOutDs.name;
			objCombo.codecolumn   = objOutDs.getColID(0);
			objCombo.datacolumn   = objOutDs.getColID(1);
		}
		else {
			objCombo.setCellProperty("body",grd_idx,"combodataset",objOutDs.name);
			objCombo.setCellProperty("body",grd_idx,"combocodecol",objOutDs.getColID(0));
			objCombo.setCellProperty("body",grd_idx,"combodatacol",objOutDs.getColID(1));
		}
		
	    if ( objOutDs.getRowCount() > 0 ) {
	    
			switch(in_Combokind)
			{
				case "all":
					objOutDs.insertRow(0);
					objOutDs.setColumn(0, 0, "");
					objOutDs.setColumn(0, 1, "전체");
					
					break;
				case "non":
					objOutDs.insertRow(0);
					objOutDs.setColumn(0, 0, "");
					objOutDs.setColumn(0, 1, "");
					break;
				default:

					break;
			}
	    }
	    
		v_ret	= objOutDs;	
	}
	return v_ret;
}

