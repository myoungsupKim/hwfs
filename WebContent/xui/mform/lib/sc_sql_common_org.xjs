/*
 ******************************************************************************************
 * 함  수  명   : fn_SetAuth
 * 입      력   : ed_Dept 	= 부서 텍스트 박스
 *                btn_Dept  = 부서 팝업 버튼명
 *                ed_Upjang   = 업장 텍스트 박스
 *                btn_Upjang   = 업장 팝업 버튼명
 * 반      환   : 없음
 * 기      능   : 권한에 따른 팝업 활성 설정
 * 작  성  자   : 김동표
 * 작성  일자   : 2008-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
*/
function fn_SetAuth(strEdDept, strBtnDept, strEdUpjang, strBtnUpjang)
{
	
	var bDept = false;
	var bUpjang = false;
	
	var objedDept = eval( strEdDept );	
	var objbtnDept = eval( strBtnDept );	
	var objedUpjang = eval( strEdUpjang );
	var objbtnUpjang = eval( strBtnUpjang );
	
	switch(g_AuthLevel)
	{
		case "00":  //일반관리자
			bDept = false;
			bUpjang = false;
			break;
		case "10":  //영양사
			bDept = false;
			bUpjang = false;
			break;
		case "12":  //리더영양사
			bDept = false;
			bUpjang = true;
			break;
		case "14": //소팀장
			bDept = false;
			bUpjang = true;
			break;
		case "16": //팀장
			bDept = false;
			bUpjang = true;
			break;
		case "18": //경영자
			bDept = true;
			bUpjang = true;
			break;	
		case "99": //전산관리자(현업)
			bDept = true;
			bUpjang = true;
			break;	
		case "999": //전산관리자(전산)
			bDept = true;
			bUpjang = true;
			break;				
		default:
			bDept = false;
			bUpjang = false;
			break;
	}
	
	if(!gfn_isNull(objedDept))
	{
		objedDept.enable = bDept;
		if(bDept == false) { objedDept.value = g_Tm_Nm; objedDept.userdata = g_Tm_Cd;}
	}
	if(!gfn_isNull(objbtnDept))	
	{
		objbtnDept.enable = bDept;
	}
	if(!gfn_isNull(objedUpjang))
	{
		objedUpjang.enable = bUpjang;
		//리더영양사일 경우 업장Delete를 못하게 막는다.(선택은 가능)
		if (g_AuthLevel == "12") objedUpjang.enable = false;
		
		if ( (bUpjang == false) || (g_AuthLevel == "12") ) { objedUpjang.value = g_UpjangNm; objedUpjang.userdata = g_Upjang;}		
	}
	if(!gfn_isNull(objbtnUpjang))
	{
		objbtnUpjang.enable = bUpjang;
	}
}

/*
 ******************************************************************************************
