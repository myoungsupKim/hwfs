	// 탭인덱스 설정
	if(NUT_TYPE!="") {
		Tab00.tabindex=toNumber(NUT_TYPE)-1;
	}
	else{
		Tab00.tabindex=toNumber(ed_NUT_TYPE.value)-1;
	}
*/	
    // 필터적용
    fn_Filter_Apply();
}

//어떤 탭이 활성화 되었는지에 따라서 필터 적용
function fn_Filter_Apply()
{    
    var tabIndex = Tab00.tabindex;
    switch(tabIndex)
	{
	  case 0 : 
		  // TAB1 필터적용    
		  ds_nut_intake_list.filter("NUT_TYPE=='1'");
		   ds_nut_intake_list.rowposition = 0;
		  //st_DISPLAY.Text="▶영양관리기준: 사업장>식당별로 영양기준이 동일하게 관리됩니다.";
		  break;
	  case 1 :
		  // TAB2 필터적용    
		  ds_nut_intake_list.filter("NUT_TYPE=='2'");
		  ds_nut_intake_list.rowposition = 0;
		  //st_DISPLAY.Text="▶영양관리기준: 사업장>식당>코너별로 영양기준이 동일하게 관리됩니다.";
		  break;
	  default :
		  ds_nut_intake_list.filter("");
	}
}

