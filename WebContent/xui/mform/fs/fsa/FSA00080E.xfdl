function fn_search()
{
    /*****************************************************************************************
      * 기      능   :  업장별 영양소 기준데이터 정보 조회
     *****************************************************************************************/
	if(!fn_Before_Search()){ return; }

	// 임시 데이터셋 생성
	var dsObj;
		
	create("Dataset", "ds_where080");
	
	dsObj = eval("ds_where080");
	dsObj.addColumn("UPJANG", "BIGDECIMAL", 100);
	dsObj.addColumn("HALL_CD", "STRING", 100);
	dsObj.addColumn("CORNER", "STRING", 100);
	dsObj.addColumn("MEAL", "STRING", 100);
		
	dsObj.addRow();
    dsObj.setColumn(0,"UPJANG", ed_UPJANG.value);
    dsObj.setColumn(0,"HALL_CD",ed_HALL_CD.value );
	dsObj.setColumn(0,"CORNER",ed_CORNER.value );
	dsObj.setColumn(0,"MEAL",ed_MEAL.value );
	

	// action 정보 초기화 
	fsp_clear(this);
	
   	// 표준 영양소 정보 조회
	fsp_addSearch(this, "fs/fsa:FSA00020E_S001");	
	
	// 업장 영양소 정보 조회
	fsp_addSearch(this, "fs/fsa:FSA00080E_S001");	
	
	// 서버 호출 
	fsp_callService(this, "", "", "ds_where080=ds_where080", "ds_nut_intake_list=ds_nut_intake_list ds_hall_nut_std=ds_hall_nut_std" , "", "fn_After_Search");
	
    // 임시 데이터셋 삭제 
	Destroy("ds_where080");
}

function fn_Before_Search(){
	var blnRtn = false;
	
	if(gfn_isNull(ed_UPJANG.value))
	{
		return;
	}
	
	//필수 입력사항 체크-----------------------------------------------------------------------
    //1.영양관리기준이 식당의 경우 업장+식당 
    if (!mfn_Chk_Obj_MustInput(ed_UPJANG)) return blnRtn;
	if (!mfn_Chk_Obj_MustInput(ed_HALL_CD)) return blnRtn;
    
    //2.영양관리기준이 코너의 경우 업장+식당+코너 
	if (ed_NUT_MGMT_TYPE.value=="2")
    {
    	if (!mfn_Chk_Obj_MustInput(ed_CORNER)){ return blnRtn; }
    }

	blnRtn = true;
	return blnRtn;
}

// 조회 후 콜백함수 
function fn_After_Search()
{
	// 영양소 정보 플래그 클리어
	ds_nut_intake_list.applyChange();
	
	// 업장 영양소 정보 플래그 클리어
	ds_hall_nut_std.applyChange();

	if(pSET2 == 1)
	{
		Tab00.tabindex = 0;
	}
	else
	{
		Tab00.tabindex = 1;
	}
	
/*	
	// 업장 영양기준이 있다면 
	var NUT_TYPE="";
