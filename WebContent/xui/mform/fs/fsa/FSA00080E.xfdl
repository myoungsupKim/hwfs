//Edit 컴포넌트 필수입력사항 확인 함수 
function mfn_Chk_Obj_MustInput(objEd) {
    var blnRtn = false;
   if(mfn_Chk_Obj_TextIsSame(objEd, "")){
	   g_Message("EE", objEd.tooltiptext + " 은(는) 필수 입력사항 입니다.");
	   objEd.setFocus();
	   blnRtn=false;
	} else {
	   blnRtn=true;
	}
	return blnRtn;
}
 
//컴포넌트 텍스트 프로퍼티 확인 함수  
function mfn_Chk_Obj_TextIsSame(obj, strCheckText){
	var blnRtn = false;	
	if(obj.text==strCheckText){
	  blnRtn=true;
	}
	else{
	  blnRtn=false;
	}
	return blnRtn;
}

