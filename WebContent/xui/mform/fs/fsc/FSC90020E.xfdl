 */
function grd_Detail1_OnCellClick(obj:Grid, e:GridClickEventInfo)
{
	var nCol,nValue,nPoint;
	if (e.cell == 5 || e.cell == 6 || e.cell == 7 || e.cell == 8)
	{
		nCol 		= e.cell-4;											// 현재선택된 배점위치(1,2,3,4)
		nValue		= ds_Detail.getColumn(e.row,"MARK_" + nCol);			// 현재선택된 배점

		if (gfn_length(gfn_toString(nValue))== 0)
		{
			return;
		}
		
		ds_Detail.setColumn(e.row,"CHK",nCol);
		ds_Detail.setColumn(e.row,"MARK",nValue);
		
		//(-)관리여부와 상관없이 점수는 배점*지적건이 아닌 그냥 배점이다.(즉, 점수=배점)
/*
		if ((ds_Detail.GetColumn(nRow,"MINUS_YN") == "Y") && (ds_Detail.GetColumn(nRow,"NOT_FIT_YN") == "Y"))
		{
			nPoint = toNumber(ds_Detail.GetColumn(nRow,"INDICATE_CNT")) * toNumber(ds_Detail.GetColumn(nRow,"MARK"));
		}
		else
		{
			nPoint = toNumber(ds_Detail.GetColumn(nRow,"MARK"));
		}
		ds_Detail.SetColumn(nRow,"CHK_POINT",nPoint);
*/
		ds_Detail.setColumn(e.row,"CHK_POINT",ds_Detail.getColumn(e.row,"MARK"));
		
		//최고점수
		if (toNumber(ds_Detail.getColumn(e.row,"MARK")) == toNumber(ds_Detail.getColumn(e.row,"MAX_MARK")))
		{
			ds_Detail.setColumn(e.row,"MAX_YN","Y");
		}
		else
		{
			ds_Detail.setColumn(e.row,"MAX_YN","N");
		}
/*		
		if (toNumber(nValue)<0 && fbAble)
		{
			grd_Detail1.SetCellPos(9);
		}
*/		
	}
	else if (e.cell == 12)
	{
	
		if(!fbAble) return;
	
		// 점검결과 등록 후 사진 등록 가능
		if(gfn_isNull(ds_Master.getColumn(0, "SCORE")))
		{
			g_Message("EE", "점검결과를 등록하셔야 사진등록이 가능합니다.");
			return;
		}
			
		trace(ds_Master.getColumn(0, "SCORE"));
		var HL_CLASS_NM = ds_Detail.getColumn(e.row, "LRG_CLASS_NAME");
		var HM_CLASS_NM = ds_Detail.getColumn(e.row, "MIDDLE_CLASS_NAME");
		var CLASS_NM = ds_Detail.getColumn(e.row, "CLASS_NAME");
		var CLASS_CD = ds_Detail.getColumn(e.row, "CLASS_CD");
		var CHK_CONTENTS = ds_Detail.getColumn(e.row, "EVAL_CONTENTS");
		var sParam = {
			  HL_CLASS_NM:HL_CLASS_NM
			, HM_CLASS_NM:HM_CLASS_NM
			, CLASS_NM:CLASS_NM
			, CLASS_CD:CLASS_CD
			, CHK_CONTENTS:CHK_CONTENTS
			, CALL_TYPE:"CHECK"
		};
		var vRtn = gfn_dialog("", "U_FSC::FSC90512P.xfdl", sParam);
		if(!gfn_isNull(vRtn))
		{
			ds_Detail.setColumn(e.row, "IMGE_CNT", vRtn);
		}	
	}
}

