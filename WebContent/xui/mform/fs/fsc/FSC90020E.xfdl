/*
 ******************************************************************************************
 * 함  수  명   : fn_FileDelete()
 * 입      력   : 없음
 * 반      환   : true/false : 파일삭제가 잘 됬는지 리턴
 * 기      능   : 파일삭제
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-24 
 ******************************************************************************************
 */
function fn_FileDelete()
{
	var btp;
	var nCnt = 0;
	
	// 파일 삭제(우수,지적사진 삭제)
	for (i=0;i<ds_file.rowcount;i++)
	{
		btp = tit_fileDelete(serverSaveDir, ds_file.getColumn(i,"SPHOTO"),true);
		
		if (!btp)
		{
			nCnt = nCnt + 1;
		}
	}
	
	// 파일 삭제(개선사진 삭제)
	for (i=0;i<ds_Detail.rowcount;i++)
	{
		if (gfn_length(ds_Detail.getColumn(i,"SIMPROV_PHOTO"))> 0)
		{
			btp = tit_fileDelete(serverSaveDir, ds_Detail.getColumn(i,"SIMPROV_PHOTO"),true);
			
			if (!btp)
			{
				nCnt = nCnt + 1;
			}
		}
	}
	
	if (nCnt>0)
	{
		g_Message("EE", nCnt + "건 파일삭제에 실패하였습니다.");
	}
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_SaveCheck()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 저상시 필수입력 체크
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-18  
 ******************************************************************************************
 */
function fn_SaveCheck(strGb)
{
	var nFRow,nRow;
	
	nRow = ds_Master.rowposition;
	
	// 필수 입력 사항 확인 
	if(gfn_length(ds_Master.getColumn(nRow,"TABLE_TYPE"))== 0)
	{
		g_Message("EE", "점검표구분을 입력해주십시오.");
		ed_TABLE_TYPE_NM.setFocus();
		return false;
	}
	if(gfn_length(ds_Master.getColumn(nRow,"FORM_CD"))== 0)
	{
		g_Message("EE", "양식을 입력해주십시오.");
		ed_FORM_NAME.setFocus();
		return false;
	}
	if(gfn_length(ds_Master.getColumn(nRow,"CHECK_TYPE"))== 0)
	{
		g_Message("EE", "점검구분을 선택해주십시오.");
		cbo_CHECK_TYPE.setFocus();
		return false;
	}
	if(gfn_length(ds_Master.getColumn(nRow,"CHECK_SABUN"))== 0)
	{
		g_Message("EE", "점검자를 입력해주십시오.");
		ed_CHECK_SABUN_NM.setFocus();
		return false;
	}
	if(gfn_length(ds_Master.getColumn(nRow,"CHECK_DATE"))== 0)
	{
		g_Message("EE", "점검일자를 입력해주십시오.");
		me_CHECK_DATE.setFocus();
		return false;
	}
	
	//============================================================================================
	//(2012-10-22 수정) 시작
	// 박은규 2012-10-22 CSR#(CH201210_00118) 요청자:홍호준(식품연구소)
	// 중간저장 가능케 수정
	//============================================================================================
	// Detail
	if (strGb == 'C')
	{
		nFRow = ds_Detail.findRowExpr("gfn_length(CHK)==0 || CHK==null");
		if (nFRow >= 0)
		{
			g_Message("EE", "배점을 선택해주십시오.");
			ds_Detail.rowposition = nFRow;
			grd_Detail1.setFocus();
			return false;
		}
	}
	//============================================================================================
	//(2012-10-22 수정) 끝
	//============================================================================================
	
    return true;
}

