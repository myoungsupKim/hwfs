	// 관리자의 경우 전체 업장내역 조회 가능
// 	if( toNumber(g_AuthLevel) < 99 ) {
// 		ed_UPJANG_NM.enable = false;
// 		btn_upjang.enable  = false;
// 		img_imp1.visible = true;
// 	}
	
