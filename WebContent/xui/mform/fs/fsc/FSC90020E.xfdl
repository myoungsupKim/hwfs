/*
 ******************************************************************************************
 * 함  수  명   : fn_Save()
 * 기      능   : [저장버튼] 저장 시 처리되는 작업
 ******************************************************************************************
 */
function fn_save(strGb)
{
	// 변경여부 확인
	if(gfn_dsIsUpdated(ds_Master)==false && gfn_dsIsUpdated(ds_Detail)==false)
	{
		g_Message("EE", "변경된 데이터가 없습니다.");
		return;
	}
	
	if(!fn_SaveCheck(strGb))
	{
		return;
	}
	if(! confirm(decode(strGb,'C','확정','저장')+ '하시겠습니까?')) return;
	
	// MasterData 셋팅
	var nNotFitCnt,strNotFit,nScore;
	
	nScore = ds_Detail.getSum("CHK_POINT");
	//부적합관리 하는 항목 중 최하점이면서 지적건이 있으면 전체적으로 부적합처리.
	nNotFitCnt	= ds_Detail.getCaseSum("MIN_MARK==MARK","NOT_FIT_CNT");
	//nNotFitCnt	= ds_Detail.Sum("NOT_FIT_CNT");
	//============================================================================================
	//(2012-04-16 수정) 시작
	// 박은규 2012-04-16 CSR#(SR201204_09204) 요청자:김희선(건강관리센터)
	// 특별점검 여부 추가 : 특별 점검인 경우는 점수가 없으므로 점수로 부적합을 판단하지 않는다.
	//============================================================================================
	//지적건이 하나라도 있거나 총점이 60점 미만이면 부적합
	//strNotFit	= iif(nNotFitCnt > 0 || nScore < 60 ,1,0);
	var notFitReason = '';
	// 정기, 지도, 보정
	if (fn_sql_common("P", "SELECT 1 FROM SCC_COMMON_CODE A WHERE A.GROUP_CODE = 'FS9042' AND A.CODE = '" + ds_Master.getColumn(0, "CHECK_TYPE") + "' AND A.SET1 = 'Y'").rowcount < 1 )
	{
		strNotFit = iif(nNotFitCnt > 0 || nScore < 60, 1, 0);
		if(nNotFitCnt > 0) notFitReason = "지적건이 1건 이상 존재, ";
		if(nScore < 60) notFitReason = notFitReason + "총점이 60점 미만,";
		ds_Master.setColumn(0, "ATTR01", notFitReason);
	}
	// 특별
	else
	{
		strNotFit = iif(nNotFitCnt > 0, 1, 0);
		if(nNotFitCnt > 0) notFitReason = "지적건이 1건 이상 존재,";
		ds_Master.setColumn(0, "ATTR01", notFitReason);
	}

	//trace('notFitReason : ' + notFitReason);
	
	//============================================================================================
	//(2012-04-16 수정) 끝
	//============================================================================================
	//
	ds_Master.setColumn(0,"SCORE",nScore);
	ds_Master.setColumn(0,"NOT_FIT_TOT_CNT",ds_Detail.getSum("NOT_FIT_CNT"));
	ds_Master.setColumn(0,"INDICATE_CNT",ds_Detail.getSum("INDICATE_CNT"));
	ds_Master.setColumn(0,"NOT_FIT_YN",strNotFit);
	
    // 서비스셋팅
    var actionName, cmdName, inData, outData, otherArg, callBackFnc;
    
	actionName		= "FSC90020E_T004";
	cmdName			= "U_svc::" + "fs/fsc/FSC90020E_T004.jsp";
	inData			= "ds_Master=ds_Master:U ds_Detail=ds_Detail:U";
    outData			= "";
    callBackFnc		= "fn_afterSave";
	
	//바이너리 통신 설정
	//http.Compress = true;
	//http.CompressMethod = "ZLIB";
	//
	jsp_transaction(this,actionName, cmdName ,inData ,outData,otherArg,callBackFnc,true,false,true);
}

