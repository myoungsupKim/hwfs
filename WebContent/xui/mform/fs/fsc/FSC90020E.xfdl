 ******************************************************************************************
 */
function fn_SearchSub(nRow)
{
	// 초기화
	ds_Master.clearData();
	ds_Detail.clearData();
	
	// 조회
	var actionName, cmdName, inData, outData, otherArg,callBackFnc;
	
	ds_input.setColumn(0,"TABLE_TYPE",ds_list.getColumn(nRow,"TABLE_TYPE"));
	ds_input.setColumn(0,"FORM_CD",ds_list.getColumn(nRow,"FORM_CD"));
	ds_input.setColumn(0,"MST_UPJANG",ds_list.getColumn(nRow,"UPJANG"));
	ds_input.setColumn(0,"CHECK_DATE",ds_list.getColumn(nRow,"CHECK_DATE"));
	
	// 정보조회
	actionName		= "FSC90020E_T002";
	cmdName			= "U_svc::" + "fs/fsc/FSC90020E_T002.jsp";
	inData			= "ds_input=ds_input";
	outData			= "ds_Master=ds_list";
	callBackFnc		= "fn_afterSearch";
	
	jsp_transaction(this,actionName, cmdName ,inData ,outData,otherArg,callBackFnc,true,false,false);
	
	// 평가항목조회
	actionName		= "FSC90020E_T003";
	cmdName			= "U_svc::" + "fs/fsc/FSC90020E_T003.jsp";
	inData			= "ds_input=ds_input";
	outData			= "ds_Detail=ds_list";
	callBackFnc		= "";
	
	jsp_transaction(this,actionName, cmdName ,inData ,outData,otherArg,callBackFnc,true,false,false);
}

