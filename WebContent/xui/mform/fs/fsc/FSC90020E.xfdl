/*
 ******************************************************************************************
 * 함  수  명   : ds_Master_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
 * 기      능   : 마스터 변경시
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-20  
 ******************************************************************************************
 */
function ds_Master_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if (e.columnid == "CHECK_TYPE")
	{
		if (e.newvalue == "002")	// 002 지도인경우 활성화
		{
			cbo_LEAD_CHECK_TYPE.enable = true;
			cbo_LEAD_CHECK_TYPE.index = 0;
		}
		else
		{
			cbo_LEAD_CHECK_TYPE.enable = false;
			ds_Master.setColumn(e.row,"LEAD_CHECK_TYPE","");
		}
	}
	
	if (e.columnid == "CHECK_DATE" && gfn_length(e.newvalue)>0)
	{
		// 값체크
		fn_CheckSearch(e.newvalue);
		if (ds_check.rowcount > 0)
		{
			g_Message("EE", "중복된 점검결과가 있습니다.\n확인해주십시오.");
			ds_Master.setColumn(e.row,"CHECK_DATE","");
			me_CHECK_DATE.setFocus();
			return;
		}
		
		// Detail셋팅
		for(i=0;i<ds_Detail.rowcount;i++)
		{
			ds_Detail.setColumn(i,"CHECK_DATE",e.newvalue);
		}
	}
}

/*
 ******************************************************************************************
 * 함  수  명   : ds_Detail_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
 * 기      능   : 디테일 변경시
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-20  
 ******************************************************************************************
 */
function ds_Detail_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var nValue;
	
	if (e.columnid == "INDICATE_CNT")	// 지적건
	{
		// 점수셋팅
		//(-)관리여부와 상관없이 점수는 배점*지적건이 아닌 그냥 배점이다.(즉, 점수=배점)
		//nValue = toNumber(ds_Detail.GetColumn(nRow,"MARK")) * toNumber(varNewValue);
		//ds_Detail.SetColumn(nRow,"CHK_POINT",nValue);
		
		// 부적합관리 항목인경우 갯수셋팅
		if (ds_Detail.getColumn(e.row,"NOT_FIT_YN") == "Y")
		{
			ds_Detail.setColumn(e.row,"NOT_FIT_CNT",toNumber(ds_Detail.getColumn(e.row,"INDICATE_CNT")));
		}
	}
	else if (e.columnid == "CHK_POINT") 
	{
		//최고점수
		if (toNumber(ds_Detail.getColumn(e.row,"MARK")) == toNumber(ds_Detail.getColumn(e.row,"MAX_MARK")))
		{
			ds_Detail.setColumn(e.row,"MAX_YN","Y");
