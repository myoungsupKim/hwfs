	);
}

/**
 * 전자결재 결과 정보를 업무테이블에 저장한 callback
 * @param
 * @return
 */
function fn_callBackDraft(errCode, errMsg) 
{
	if( errCode != 0 ) {
		g_Message("EE", errMsg);
		return;
	}
	else
	{
        // 의뢰정보 클릭 시 전자결재 상신 또는 상신상세 버튼 연동
        fn_gwUserButtonInit();
        
        g_Message("02", (strDraftGubun=='7'?"점검결과":"개선결과")+" 결재 상신 되었습니다!");
        
        strDraftGubun = '7';
        
        if (gfn_isNull(edt_upjang.getUpjangCode())) {
			ds_input.setColumn(0, "UPJANG", "");
        }
        
        ds_input.setColumn(0, "TABLE_TYPE", "");
		ds_input.setColumn(0, "FORM_CD", "");
		ds_input.setColumn(0, "CHECK_DATE", "");
		ds_input.setColumn(0, "CHECK_TYPE", "");
	
        ds_input.setColumn(0, "DRAFT_GUBUN", "");
        ds_input.setColumn(0, "SIGN_INSTANCE_ID1", "");
        ds_input.setColumn(0, "SIGN_INSTANCE_ID2", "");
        
        // 재조회
        fn_search() ;
	}
}

