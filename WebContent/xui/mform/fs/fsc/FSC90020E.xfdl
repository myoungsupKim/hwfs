/*
 ******************************************************************************************
 * 함  수  명   : btn_excelUpload_onclick(obj,  e)
 * 기      능   : 파일업로드
 * 작  성  자   : 지승훈
 * 작성  일자   : 2015-05-26  
 ******************************************************************************************
 */
function btn_excelUpload_onclick(obj:Button,  e:ClickEventInfo)
{
	this.setWaitCursor(true, true);

    // 파일 선택
    var strFileName = gfn_excelFileDialog("LOAD", "EXCEL");

    if (strFileName.length == 0)
    {
        this.setWaitCursor(false, true);
        return;
    }

    var rtn = gfn_importExcel(strFileName, "ds_DetailExcel", 2, 1);

    this.setWaitCursor(false, true);
    
    ds_DetailTmp.copyData(ds_Detail);
    
    var nRow = -1;
    var chkPointTmp = 0;
    var chk = 0;
    var className = "";
    var chkCnt = 0;
    //trace(ds_Detail.saveXML());
    ds_Detail.updatecontrol = false;
    for (var i = 0; i < ds_Detail.getRowCount(); i++)
    {
		nRow = ds_Detail.findRowExpr("CLASS_CD == '"+ds_Detail.getColumn(i, "CLASS_CD")+"'");	// 소분류코드
		ds_DetailExcel.rowposition = nRow;
		chkPointTmp = ds_DetailExcel.getColumn(nRow, "CHK_POINT");	// 배점
		if (gfn_isNull(chkPointTmp)) continue;
		if (ds_Detail.getColumn(i, "MARK_1") == chkPointTmp) {
			chk = "1";
		} else if (ds_Detail.getColumn(i, "MARK_2") == chkPointTmp) {
			chk = "2";
		} else if (ds_Detail.getColumn(i, "MARK_3") == chkPointTmp) {
			chk = "3";
		} else if (ds_Detail.getColumn(i, "MARK_4") == chkPointTmp) {
			chk = "4";
		} else {
			if (!gfn_isNull(chkPointTmp)) {
				gfn_alert("소분류 [" + ds_Detail.getColumn(i, "CLASS_NAME") + "]에\n해당하는 배점이 존재하지 않습니다.\n\n확인하시기 바랍니다.");
				chkCnt++;
				break;
			}
		}
		ds_Detail.setColumn(i, "CHK", chk);					// 조건에 해당하는 background, background2 색 지정
		ds_Detail.setColumn(i, "MARK", chkPointTmp);	    // 입력된 점수 세팅
		ds_Detail.setColumn(i, "MAX_YN", (ds_Detail.getColumn(i, "MAX_MARK")==chkPointTmp?'Y':'N'));	    // 입력된 점수 세팅
		ds_Detail.setColumn(i, "CHK_POINT", chkPointTmp);	// 입력된 점수 세팅
		ds_Detail.setColumn(i, "INDICATE_CNT", ds_DetailExcel.getColumn(nRow, "NOT_FIT_YN"));	// 지적건
		ds_Detail.setColumn(i, "EVAL_CONTENTS", ds_DetailExcel.getColumn(nRow, "EVAL_CONTENTS"));	// 평가내용
		
		// 입력버튼을 클릭했으면 insert. 수정이면 update 상태로 변경 한다.
		if (isInsert == true) {
			ds_Detail.setRowType(i, Dataset.ROWTYPE_INSERT);
		} else {
			ds_Detail.setRowType(i, Dataset.ROWTYPE_UPDATE);
		}
    }
    ds_Detail.updatecontrol = true;
    
    // 점수배점이 존재하지 않는경우에는 데이터를 원복 시킨다.
    if (chkCnt > 0) {
		ds_Detail.copyData(ds_DetailTmp);
    }
    
    ds_DetailExcel.clearData();
    ds_DetailTmp.clearData();
    
    //grd_Detail1.enableredraw = true;
}

