function fn_ChartData_Make2()
{
	var nMaxCnt,strXValue,strYValue,nFRow,nRCol,nValue;
	
	// 1. WORST 10 항목
	ds_list.copyData(this.opener.ds_list);
	ds_list1.copyData(ds_list);
	//Copy(ds_list,this.opener.ds_list);	
	//CopyF(ds_list1,ds_list);
	ds_list1.keystring = "G:+PL_UP_CD";
	
	var nTotValue = 0;
	var retCnt = 0;
	
	for(i = 0; i < ds_list1.getRowCount(); i++)
	{
		if(ds_list1.getRowLevel(i) == 1)
		{
			retCnt = ds_Chart1.addRow()
			ds_Chart1.copyRow(retCnt, ds_list1, i);
			ds_Chart1.setColumn(retCnt, "MIDDLE_CLASS_NAME", ds_list1.getColumn(i-1, "MIDDLE_CLASS_NAME"));
			ds_Chart1.setColumn(retCnt, "PL_UP_CD", ds_list1.getColumn(i-1, "PL_UP_CD"));
		}
	}
	ds_Chart1.keystring = "S:-CNT";
	
	// 2. 항목별 비율
	// 2.1 중분류별 지적건 비율
	
	ds_list2.copyData(ds_Chart1);
	ds_list2.addColumn("RATE","DECIMAL");

	for(i=0; i < ds_list2.getRowCount(); i++)
	{
		nTotValue	  = ds_list2.getSum("CNT");
		
		ds_list2.setColumn(i,"RATE",Math.round(toNumber(ds_list2.getColumn(i,"CNT"))/nTotValue*100,1));
	}
	
	ds_Chart2.copyData(ds_list2);
	
	
	// 2.2 평가항목별 지적건 비율
	
	ds_list3.copyData(ds_list1);
	//ds_list3.keystring = "G:+PL_UP_CD";
	ds_list3.addColumn("RATE","DECIMAL");

	var j = 0;
	for(i=0; i < ds_list1.getRowCount(); i++)
	{
		if(ds_list2.getColumn(j, "MIDDLE_CLASS_NAME") == ds_list3.getColumn(i, "MIDDLE_CLASS_NAME"))
		{
			nTotValue = ds_list2.getColumn(j, "CNT");
		}
		else
		{
			j++;
			nTotValue = ds_list2.getColumn(j, "CNT");

			//trace(nTotValue + ",    "+ ds_list3.getColumn(i, "MIDDLE_CLASS_NAME"));
		}

		ds_list3.setColumn(i,"RATE",Math.round(toNumber(ds_list3.getColumn(i,"CNT")) / nTotValue*100,1));
	}
	ds_Chart3.copyData(ds_list3);
	div_2.cbo_PL_UP_CD.index = 0;
	rdo_SType.enable = true;
}

