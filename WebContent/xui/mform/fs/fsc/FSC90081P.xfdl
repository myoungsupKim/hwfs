function fn_ChartData_Make()
{
	var nMaxCnt,strXValue,strYValue,nFRow,nRCol,nValue,nTotValue;
	
	// 1. 부모데이터 카피
	Copy(ds_list,this.opener.ds_list);
	
	// 1.1 중분류
	ds_list.filter("LEVEL_NO=='1'");
	CopyF(ds_list1,ds_list);
	CopyF(ds_list2,ds_list);
	
	// 1.2 소분류
	ds_list.filter("LEVEL_NO=='2'");
	CopyF(ds_list3,ds_list);

	// 2.Worst 10 항목
	nMaxCnt = least(ds_list1.rowcount,10);
	ds_list1.keystring = "G:+PL_CD";
	
	//Sort(ds_list1,"CNT:D,PL_CD");
	for(i=0; i<nMaxCnt; i++)
	{
		ds_Chart1.addRow();
		ds_Chart1.copyRow(i,ds_list1,i);
	}
	
	// 3. 항목별 비율
	// 3.1 중분류
	ds_list2.addColumn("RATE","DECIMAL");
	
	for(i=0; i<ds_list2.getRowCount(); i++)
	{
		nTotValue	  = ds_list2.getSum("CNT");
		
		ds_list2.setColumn(i,"RATE",Math.round(toNumber(ds_list2.getColumn(i,"CNT"))/nTotValue*100,1));
	}
	
	Copy(ds_Chart2, ds_list2);

	// 3.2 소분류
	ds_list3.addColumn("RATE","DECIMAL");
	
	for(i=0; i<ds_list3.getRowCount(); i++)
	{
		nTotValue	  = ds_list3.getSum("CNT");
		
		ds_list3.setColumn(i,"RATE",Math.round(toNumber(ds_list3.getColumn(i,"CNT"))/nTotValue*100,1));
	}

	rdo_SType.enable = true;
}
