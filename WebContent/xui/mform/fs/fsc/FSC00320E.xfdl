/******************************************************************************************
 * 함ㅡ수ㅡ명 : btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
 * 기ㅡㅡㅡ능 : 개선확정
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-03-23
 ******************************************************************************************/
function btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
{
	if (gfn_isNull(ds_resultMst.getColumn(0, "TABLE_TYPE")))
	{
		g_Message("EE", "점검 정보를 선택해 주세요.");
		return;
	}

	if (ds_resultMst.getColumn(0, "IMPROV_CONFIRM_YN") == "Y")
	{
		g_Message("EE", "이미 개선확정된 점검정보 입니다.");
		return;
	}

	trace(ds_resultMst.getColumn(0, "NOT_FIT_CNT"));
	trace(ds_resultMst.getColumn(0, "IMPROV_MRMT_CNT"));

	if (Number(ds_resultMst.getColumn(0, "NOT_FIT_CNT")) > Number(ds_resultMst.getColumn(0, "IMPROV_MRMT_CNT")))
	{
		g_Message("EE", "개선조치가 모두 완료된 경우에만 개선확정이 가능합니다.");
		return;
	}

	if(gfn_dsIsUpdated(ds_resultDtlList))
	{
		g_Message("EE", "변경된 데이터가 있습니다. 저장 후 개선확정하시기 바랍니다.");
		return false;
	}

	if (!g_Confirm("01", "확정 하시겠습니까?")) return;

	ds_resultMst.setColumn(0, "IMPROV_CONFIRM_YN", "Y");
	jsp_transaction(
