function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	var fa_JOINT_YYMM = ds_upjangList.getColumn(ds_upjangList.rowposition, "JOINT_YYMM");
	var fa_UPJANG = ds_upjangList.getColumn(ds_upjangList.rowposition, "UPJANG");

	var	v_arg = {fa_JOINT_YYMM:fa_JOINT_YYMM,fa_UPJANG:fa_UPJANG};

	if(e.col == 9)
	{
		// 공유이력 팝업
		gfn_dialog("", "U_FSM::FSM00600P.xfdl", v_arg);
	}
	else
	{
		// 공유정보조회 팝업
		gfn_dialog("", "U_FSM::FSM00621P.xfdl", v_arg);
	}
}

