	// 화면 초기화
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 초기화
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	v_ds_check = "";

	//시즌목록코드 불러오기
	fn_getSeasonList();
	cb_Season.index = 0;

	// 개인정보 보호 관련
	// 로그인한 사용자가 관리자이면 개인정보 보기
	if( SCAuth.isUserRole("특판03") )
	{
		fa_Special_Auth = "Y"
		gd_list_Detail.setFormat("Default");

		Static04.visible = true;
		cbo_gbn.visible = true;
	}
	else
	{
		fa_Special_Auth = "N";
		gd_list_Detail.setFormat("Default0");
	}

	// 주문일자
	me_PrDate.value    = fn_CurrDateWeek();
	me_PrDateT.value   = fn_CurrDateWeek();

	// 공통코드
	var v_fil_cond;
	var v_ds = fn_common_code("'022', '023', '024', '028'", "", "", "non", "Y");

	// 결제수단
	v_fil_cond = "GROUP_CODE == '022'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_Pay, v_ds.name);	

	// 할부기간
	v_fil_cond = "GROUP_CODE == '023'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_Inst, v_ds.name);	

	// 카드사
	v_fil_cond = "GROUP_CODE == '024'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_Card, v_ds.name);	

	// 특판판매구분
	v_fil_cond = "GROUP_CODE == '028'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_Sale, v_ds.name);

	// 판매구분(콤보) 설정
	ds_cbo_SALE_TYPE.copyData(ds_Sale);
	ds_cbo_SALE_TYPE.insertRow(0);
	ds_cbo_SALE_TYPE.setColumn(0, "CODE", "");
	ds_cbo_SALE_TYPE.setColumn(0, "CODE_NAME", "전체");
	cbo_SALE_TYPE.index = 0;

	// 주문자MU
	fn_sql_common("C", " SELECT MU_CD, MU_NM FROM HLDC_SC_MU WHERE (1=1)"	,"  "," ORDER BY MU_CD ", "all", cbo_PR_SABUN_MU, -1, ds_Pr_Mu);

	cbo_SALE_TYPE.index = 0;
	cbo_PR_SABUN_MU.index = 0;
}

