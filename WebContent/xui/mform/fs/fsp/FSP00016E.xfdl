	strLocalQuery_SPECIAL_ITEM			   += "     , C.ITEM_NAME || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       C.ITEM_SIZE || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       DECODE(C.TAX_CODE, '100', '과세', '210', '면세', '영세') || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       A.CONTRACT_PRICE || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       NVL(B.SALE_PRICE, A.CONTRACT_PRICE) || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       C.MAX_ORD_QTY AS ITEM_NAME \n";
	strLocalQuery_SPECIAL_ITEM			   += "  FROM HLDC_PO_CONTRACT_SPEC A \n";
	strLocalQuery_SPECIAL_ITEM			   += "     , FSP_PO_SPECIAL_ITEM B \n";
	strLocalQuery_SPECIAL_ITEM			   += "     , HLDC_PO_ITEM_MST C \n";
	strLocalQuery_SPECIAL_ITEM			   += " WHERE A.ITEM_CODE = B.ITEM_CODE \n";
	strLocalQuery_SPECIAL_ITEM			   += "   AND A.ITEM_CODE = C.ITEM_CODE \n";
