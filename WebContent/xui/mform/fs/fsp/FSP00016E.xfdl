 *---------------------------------------------------------------------------------------------*/
function fn_chkManNull()
{
	if( ffn_ChkNull(me_PrDate.value) || gfn_length(me_PrDate.value) <  8 )
	{
		g_Message("EE", "주문일자를 정확하게 입력하세요.");
		me_PrDate.setFocus();
		return true;
	}
	if( ffn_ChkNull(me_PrDateT.value) || gfn_length(me_PrDateT.value) < 8 )
	{
		g_Message("EE", "주문일자를 정확하게 입력하세요.");
		me_PrDateT.setFocus();
		return true;
	}
	
	return false;
}

