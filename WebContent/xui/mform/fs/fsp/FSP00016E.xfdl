 ************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수
 *---------------------------------------------------------------------------------------------*/
function fn_callback(nErrorCode, strErrorMsg, strSvcID)
{
	if( nErrorCode < 0 )
	{
		g_Message("EE", fn_alertMsg("002"));
		return;
	}

	switch( strSvcID )
	{
		case "SEARCH":
			if( ds_List_Detail.rowcount == 0 )
			{
				g_Message("EE", "조회대상이 없습니다.");
				me_PrDate.setFocus();
			}
			break;
		default:
			break;
	}
}

