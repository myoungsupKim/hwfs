function btn_custSearch_onclick(obj:Button,  e:ClickEventInfo)
{
    var aPram = {"p_need_date":ds_item_condi.getColumn(0, "NEED_DATE")}
    var vRtn = gfn_dialog("", "U_FSP::FSP00035P.xfdl",aPram);

    if (vRtn != null)
