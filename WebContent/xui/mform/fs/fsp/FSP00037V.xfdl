 *---------------------------------------------------------------------------------------------*/
function fn_item_search()
{
    var sSvcID;         // 서비스 ID
    var sUrl;           // jsp 경로
    var sInDs;          // 입력 데이터셋
    var sOutDs;         // 출력 데이터셋
    var sArg;           // 추가 파라미터
    var sCallFunc;      // 콜백함수명

    sSvcID    = "ITEM_SEARCH";
    sUrl      = "U_svc::" + "fs/fsp/FSP00035E_T002.jsp";
    sInDs     = "ds_item_condi=ds_item_condi";
    sOutDs    = "ds_item_list=ds_out";
    sCallFunc = "fn_item_callback";

    ds_item_condi.setColumn(0, "ITEM_CODE",     edt_item_code.value);
    ds_item_condi.setColumn(0, "SUBINV_CODE",   parent.parent.cbo_subInv.value);

    if(!fn_item_validation(sSvcID)) return;

    // 트랜잭션
    jsp_transaction(this, sSvcID, sUrl, sInDs, sOutDs, sArg, sCallFunc, false, false, false);
}

/*----------------------------------------------------------------------------------------------
 * 함  수  명   : fn_transation_validation()
 * 기      능   : 조회 전 유효성체크
*----------------------------------------------------------------------------------------------*/
function fn_item_validation(sSvcID)
{
    switch(sSvcID)
    {
        case "ITEM_SEARCH" :
            var sItemCls2 = ds_item_condi.getColumn(0, "ITEM_CLASS2");
            var sItemCls3 = ds_item_condi.getColumn(0, "ITEM_CLASS3");
            var sItemCls4 = ds_item_condi.getColumn(0, "ITEM_CLASS4");
            var sItemCd   = ds_item_condi.getColumn(0, "ITEM_CODE"  );
            var sBarcode  = ds_item_condi.getColumn(0, "BARCODE"    );
            var sCustCd   = gfn_trim(ds_item_condi.getColumn(0, "CUSTCD"));

            if(gfn_isNull(sItemCls2) && gfn_isNull(sItemCls3) && gfn_isNull(sItemCls4) && gfn_isNull(gfn_trim(sItemCd)) && gfn_isNull(sBarcode) && gfn_isNull(sCustCd)){
                g_Message("04"," ※ 조회조건을 2개 이상\n     입력 후 검색하시기  바랍니다.");

                cbo_item_class2.class = "cmb_WF_Error";
                cbo_item_class3.class = "cmb_WF_Error";
                cbo_item_class4.class = "cmb_WF_Error";
                edt_item_code.class   = "edt_WF_Error";
                edt_bar_code.class    = "edt_WF_Error";

                edt_item_code.setFocus();
                return false;
            }
            cbo_item_class2.class = "";
            cbo_item_class3.class = "";
            cbo_item_class4.class = "";
            edt_item_code.class   = "";
            edt_bar_code.class    = "";
            break;

    }

    return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 콜백함수
 *---------------------------------------------------------------------------------------------*/
function fn_item_callback(errCode, errMsg, sSvcID)
{
    if(toNumber(errCode) != 0)
    {
        g_Message("EE","조회 중 오류가 발생했습니다.\n에러메세지:" + errMsg);
        return;
    }

    if(ds_item_list.getCountNF() <= 0)
    {
        g_Message("EE","조회된 자료가 없습니다."); return;
    }
    parent.parent.cbo_subInv.enable = false;

    var strRemark = "";
    
    for(var i = 0; i < ds_item_list.rowcount; i++)
    {
        strRemark = "";
        if  (ds_item_list.getColumn(i,"SHP_MAP") == "N")
        {
            strRemark = "단가계약 안됨";
            ds_item_list.setColumn(i,"PR_REMARK",strRemark);
            ds_item_list.setColumn(i,"PR_YN" , "A");
            ds_item_list.setColumn(i,"PR_QTY", 0);
        }
        else if (ds_item_list.getColumn(i,"MTG_UNS") == "N")
        {
            strRemark = "불용자재";
            ds_item_list.setColumn(i,"PR_REMARK",strRemark);
            ds_item_list.setColumn(i,"PR_YN" , "B");
            ds_item_list.setColumn(i,"PR_QTY", 0);
        }
        
        else if (ds_item_list.getColumn(i,"MTG_STP") == "N")
        {
            strRemark = "중지자재";
            ds_item_list.setColumn(i,"PR_REMARK",strRemark);
            ds_item_list.setColumn(i,"PR_YN" , "B");
            ds_item_list.setColumn(i,"PR_QTY", 0);
        }
        
        else if (ds_item_list.getColumn(i,"REQ_WKD") == "N")
        {
            strRemark = "거래처 요일 제한";
            ds_item_list.setColumn(i,"PR_REMARK",strRemark);
            ds_item_list.setColumn(i,"PR_YN" , "A");
            ds_item_list.setColumn(i,"PR_QTY", 0);
        }
        
        else if(gfn_length(ds_item_list.getColumn(i,"RESTRICT_PERIOD")) > 0)
        {
            strRemark = "금지자재(" + ds_item_list.getColumn(i,"RESTRICT_PERIOD") + ", " + ds_item_list.getColumn(i,"RESTRICT_REASON") + ")";
            ds_item_list.setColumn(i,"PR_REMARK",strRemark);
            ds_item_list.setColumn(i,"PR_YN" ,"C");
            ds_item_list.setColumn(i,"PR_QTY",0);
        }
        else if(ds_item_list.getColumn(i,"SCH") ==  "N")
        {
            strRemark = "수발주제한";
            ds_item_list.setColumn(i,"PR_REMARK",strRemark);
            ds_item_list.setColumn(i,"PR_YN" , "A");
            ds_item_list.setColumn(i,"PR_QTY", 0);
        }
        else if(ds_item_list.getColumn(i,"D_DAY_YN") ==  "N")
        {
			strRemark = "D-" + ds_item_list.getColumn(i,"D_DAYS") + "일" + ds_item_list.getColumn(i,"D_TIMES") + "시 제한";
            ds_item_list.setColumn(i,"PR_REMARK",strRemark);
            ds_item_list.setColumn(i,"PR_YN" , "A");
            ds_item_list.setColumn(i,"PR_QTY", 0);
        }
        else
        {
            ds_item_list.setColumn(i,"PR_YN" , "Y");
        }       
    }

    fn_setInfoText();
    grd_ItemList.setFocus();
}

function fn_AddValidation()
{
    if(ds_item_list.getColumn(ds_item_list.rowposition,"CONTRACT_YN") == "N")
    {
        return false;
    }
    else if(ds_item_list.getColumn(ds_item_list.rowposition,"PR_YN") == "N")
    {
        return false;
    }
    else if(ds_item_list.getColumn(ds_item_list.rowposition,"USE_YN") == "N")
    {
        return false;
    }
    else if(gfn_length(ds_item_list.getColumn(ds_item_list.rowposition,"RESTRICT_PERIOD"))> 0)
    {
        return false;
    }
    else if( (toNumber(ds_item_list.getColumn(ds_item_list.rowposition,"DUTY_QTY")) > 0) && (toNumber(ds_item_list.getColumn(ds_item_list.rowposition,"DUTY_QTY")) <= toNumber(ds_item_list.getColumn(ds_item_list.rowposition,"REAL_USE_QTY"))) )
    {
        return false;
    }
    else if(ds_item_list.getColumn(ds_item_list.rowposition,"D_DAY_YN") == "N")
    {
        return false;
    }

    return true;
}

