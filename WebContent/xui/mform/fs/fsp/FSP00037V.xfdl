        case 2 : 
            SCSysIf.eprocPopup (SCSysIf.EPROC_ITEM_POPUP, ds_item_list.getColumn(e.row, "ITEM_CODE"));
            break;
            
		case 3 : 
            gfn_dialog("", "U_FSP::FSP00000P.xfdl",{fv_itemCode: ds_item_list.getColumn(e.row, "ITEM_CODE")}, "", "", "", "", "", "", "", "", true);
            break;
