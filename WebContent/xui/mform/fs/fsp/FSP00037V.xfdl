// 자재특성 콤보 값 변경시 그리드 필터 적용
function cbo_feature_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	switch(e.postindex)
	{
		case 0 :
			ds_item_list.filter("");
			break;
		default :
			ds_item_list.filter("FEATURE_NAME == '" + e.postvalue + "'");
			break;
		
	}
}

