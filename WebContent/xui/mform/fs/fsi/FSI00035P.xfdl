//=========================================================================================
// [ PART 3 ]
// Form Loading 시 작업 정의
//
//=========================================================================================
function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(this);

	// X-Framework : Action 정보 생성
	fsp_init(this);
	
	// 초기화
	fn_init();
}

/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_init()
 * 기ㅡㅡㅡ능 : 초기화
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-04-01
 ******************************************************************************************/
function fn_init()
{
	// 조회조건 초기화
	ds_cond.clearData();
	ds_cond.addRow();
	
	// 분류 조회
	fsp_clear(this);
	fsp_addSearch(this, "fs/fsi:FSI00010E_S006");	// 자재 대분류 조회  
	fsp_addSearch(this, "fs/fsi:FSI00010E_S007");	// 자재 중분류 조회  
	fsp_addSearch(this, "fs/fsi:FSI00010E_S008");	// 자재 소분류 조회
	fsp_callService(this, "", "", "", "ds_Big=ds_cbo_item_classM ds_Mid=ds_cbo_item_classS ds_Small=ds_cbo_item_classD ", "BLANK='Y' USE_YN='Y'", "fn_callback");
}

function fn_callback(errCode, errMsg)
{
	if( errCode != 0 ) {
		g_Message("EE", errMsg);
		return;
	}
	edt_itemName.setFocus();
}

//=========================================================================================
// [ PART 4 ]
// Main Process Event 정의(조회,입력,삭제,저장,출력)
//
//=========================================================================================
/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_search()
 * 기ㅡㅡㅡ능 : 자재조회
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-04-01
 ******************************************************************************************/
function fn_search()
{
	ds_itemList.clearData();
	fsp_clear(this);
	fsp_addSearch(this,"fs/fsi:FSI00035P_S001");
	fsp_callService(this,"","","ds_cond=ds_cond","ds_itemList=ds_itemList","","fn_callbackSearch","",true);
}

function fn_callbackSearch(errCode, errMsg)
{
	if( errCode != 0 ) {
		g_Message("EE", errMsg);
		return;
	}
	grd_itemList.setFocus();
}

/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_close(obj:Button, e:ClickEventInfo)
 * 기ㅡㅡㅡ능 : 닫기
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-04-01
 ******************************************************************************************/
function fn_close(obj:Button, e:ClickEventInfo)
{
	this.close();
}
 
