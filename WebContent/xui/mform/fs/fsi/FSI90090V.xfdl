function fn_search()
{
	// 유효성 검사
	//if (gfn_validCheck(ds_cond, ds_condValid, true) == false) return;
	
	// 본부의 팀전체 조회 옵션 추가 2016.02.16
	
	tmCd = edt_tm.getTmCode();
	
 	if(chk_bonbu.value == true &&  edt_tm.getTmCode() != "")  {
 	    tmCd = edt_tm.getTmCode();   
 		ds_cond.setColumn(0, "TM_CD", wrapQuote(gfn_subStr(tmCd ,0,3) ));
 		//alert(tmCd);
 	}
	
	var tabIdx = Tab00.tabindex;
	if(tabIdx == 0)
	{
		// 그리드 데이터 초기화
		ds_list11.clearData();
		ds_list12.clearData();

		fsp_clear(this);
		fsp_addSearch(this, "fs/fsi:FSI90090V_S001");  // 기획자재별 입고현황
		fsp_callService(this, "", "", "ds_cond=ds_cond", "ds_list11=ds_list11" , "", "fn_callbackSearch");
	} 
	else if(tabIdx == 1)
	{
		// 그리드 데이터 초기화	
		ds_list21.clearData();
		ds_list22.clearData();
		ds_list23.clearData();
		
		fsp_clear(this);	
		fsp_addSearch(this, "fs/fsi:FSI90090V_S003");  // 업장별 사용현황조회	
		fsp_callService(this, "", "", "ds_cond=ds_cond", "ds_list21=ds_list21" , "", "fn_callbackSearch");
	}
	else if(tabIdx == 2)
	{
		// 그리드 데이터 초기화	
		ds_list31.clearData();

		// 분석결과
		jsp_transaction(this, "FSI90090V_T001", "U_svc::fs/fsi/FSI90090V_T001.jsp", "ds_cond=ds_cond", "ds_list31=ds_list31 ds_tmList=ds_tmList", "", "fn_callbackSearchTab3", true, false, false);
	}
	else if(tabIdx == 3)
	{
		// 그리드 데이터 초기화	
		ds_list41.clearData();

		// 분석결과
		jsp_transaction(this, "FSI90090V_T002", "U_svc::fs/fsi/FSI90090V_T002.jsp", "ds_cond=ds_cond", "ds_list41=ds_list41", "", "fn_callbackSearch", true, false, false);
	}
	else if(tabIdx == 4)
	{
		// 그리드 데이터 초기화	
		ds_list51.clearData();

		// 분석결과
		jsp_transaction(this, "FSI90090V_T003", "U_svc::fs/fsi/FSI90090V_T003.jsp", "ds_cond=ds_cond", "ds_list51=ds_list51", "", "fn_callbackSearchTab5", true, false, false);
	}
}

function fn_callbackSearch(errCode, errMsg) 
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오!");
		return;
	}
	
	edt_tm.setTmCode(tmCd);
}

function fn_callbackSearchTab3(errCode, errMsg)
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오!");
		return;
	}

	//for (var i=0; i<10; i++)
	for (var i=0; i<11; i++)
	{
		if (i < ds_tmList.rowcount)
		{
			//Tab00.tabpage3.grd_list31.setCellProperty("head", i + 14, "text", ds_tmList.getColumn(i, "CODE_NAME"));
			//조직개편으로 팀 10개 -> 11개로 변경 (CH201606_00175)  처리자 : 맹수영
			Tab00.tabpage3.grd_list31.setCellProperty("head", i + 14, "text", ds_tmList.getColumn(i, "CODE_NAME"));
		}
		else
		{
			//Tab00.tabpage3.grd_list31.setFormatColProperty(i + 6, "size", 0);
			Tab00.tabpage3.grd_list31.setFormatColProperty(i + 6, "size", 0);
		}
	}
	Tab00.tabpage3.grd_list31.redraw();
	edt_tm.setTmCode(tmCd);
