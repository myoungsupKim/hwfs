function btn_ratio_OnClick(obj:Button, e:ClickEventInfo)
{
	if(ds_Recipe.getRowCount()==0)
	{
		return;
	}
	if(ds_Recipe.getColumn(0,"RECIPE_CLASS")=="H" )
	{
