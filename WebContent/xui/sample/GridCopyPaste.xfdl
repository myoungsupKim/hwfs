    <Script type="xscript4.0"><![CDATA[include "lib::lib_com.xjs";

function GridCopyPaste_onload(obj:Form, e:LoadEventInfo)
{
	var strMsg = "";
		strMsg += "- Operation \n";
		strMsg += "1. Grid body cell 영역 선택\n";
		strMsg += "2. 복사(Ctrl + C) 후 다른 Grid나 Excel파일에서 붙여넣기(Ctrl + V)\n";
		strMsg += "\n";
		strMsg += "- Property & Event \n";
		strMsg += "1. Grid onkeydown 이벤트에서 gfn_gridCopy(obj, e) or gfn_gridPaste 호출 \n";
	
	txtGuide.value = strMsg;
}

/* Grid 키다운 이벤트 */
function grd_copyPaste_onkeydown(obj:Grid, e:KeyEventInfo)
{
	// Ctrl + C
	if(e.ctrlKey == true && e.keycode == 67) 
	{
		//복사(Ctrl + C)
		gfn_gridCopy(obj, e);
	}
	// Ctrl + V	: Paste
	else if(e.ctrlKey == true && e.keycode == 86) {		
		//복사(Ctrl + C)
		gfn_gridPaste(obj, e);
	}
}]]></Script>
