function fn_clear(num)
{
	var nRow = ds_search.rowposition
	if(num >= 3)	ds_search.setColumn(nRow,'schMuCd','');
	if(num >= 2)	
	{
		ds_search.setColumn(nRow,'schUpjang','');
		ds_search.setColumn(nRow,'schUpjangNm','');
	}
	if(num >= 1)	
	{
		ds_search.setColumn(nRow,'schSubinv','');
		ds_subinv.clearData();
	}
}

