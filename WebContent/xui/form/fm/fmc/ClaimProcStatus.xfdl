 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	if(gfn_isNull(g_PersInfoMask)){
		gfn_alert("개인정보 처리 권한이 없습니다.\n관리자에게 문의 해 주세요.");
		return;
	}
	ds_search.setColumn(0, 'schItemCode', edt_returnItemSearch.userdata) 
	ds_search.setColumn(0, 'g_PersInfoMask', g_PersInfoMask);
    ds_search.setColumn(0, 'p_MaskAuth', SCAuth.getMaskAuth(this));
	
	// 변경사항 체크
	if (gfn_dsIsUpdated(ds_list) == true) 
	{
		var retValue = gfn_confirm("confirm.before.search");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
		if (retValue == false) return;
	}
	
	(g_PersInfoMask);

	// 조회시 Validation Check
	if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);

    var strSvcID    = "search";
    var strURL      = "fm/fmc/claimProcStatus/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
}

