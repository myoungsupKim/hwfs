function fn_init() {
	cal_fromDate.fn_initMonthCal(this, gfn_today().substr(0, 6)+"01", "", ds_search, "schFromDate", "N");
	comp_upjang.setRoleSetId("BrandUpjang");	// 브랜드 유통사업장 roleSetId
