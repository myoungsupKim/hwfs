 *---------------------------------------------------------------------------------------------*/
function fn_search(){		
	
	// 조회시 Validation Check
	if(!gfn_validCheck(ds_search, ds_searchValid, true)) return;
	
	
 	if(gfn_isNull(ds_search.getColumn(0, "schItem")) && gfn_isNull(ds_search.getColumn(0, "schUpjang"))){
 		gfn_alert("msg.err.validator.required", "검색어나 사업장"); // $1 은(는) 필수 입력 항목입니다.
		this.ed_item.setFocus();
 		return;
 	}
 	
	if(ds_search.getColumn(0, "schGbn") == "88" ){
		//바코드 조회시 6자리 이상 입력
		if (gfn_validCheck(ds_search, ds_searchValid2, true) == false) return;
	}

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);
    ds_list.clearData();
	
    var strSvcID    = "searchEventItem";
    var strURL      = "fm/fms/eventItemList/selectEventItemList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

