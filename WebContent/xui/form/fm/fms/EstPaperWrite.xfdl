			break;
    
	}	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 견적 목록의 DS 변경 이벤트 처리
 * 해당 Column의 값이 변경될 때에만 적용
 *---------------------------------------------------------------------------------------------*/
function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{

    switch(e.columnid)
    {
		// 마진율
        case "marginRate" :
			v_price = fn_calcAmt(e.row, ds_list.getColumn(e.row, "marginRate"));
			ds_list.setColumn(e.row, "estiPrice", v_price);
			// 단가 변경 시 금액 재계산
			v_amt = fn_calcAmt2(ds_list.getColumn(e.row, "estiQty"), ds_list.getColumn(e.row, "estiPrice"));
			ds_list.setColumn(e.row, "estiAmt", v_amt); 
			
            break;
	
		// 판매단가
        case "estiPrice" :
			v_rate = fn_calcRate(e.row, ds_list.getColumn(e.row, "estiPrice"));
			ds_list.setColumn(e.row, "marginRate", v_rate); 
			
			// 단가 변경 시 금액 재계산
			v_amt = fn_calcAmt2(ds_list.getColumn(e.row, "estiQty"), ds_list.getColumn(e.row, "estiPrice"));
			ds_list.setColumn(e.row, "estiAmt", v_amt); 
			
            break;
            
        // 수량
        case "estiQty" :
			// 매입금액 계산
			v_amt = fn_calcAmt2(ds_list.getColumn(e.row, "estiQty"), ds_list.getColumn(e.row, "unitPrice"));
			ds_list.setColumn(e.row, "unitAmt", v_amt); 
			// 판매금액 계산
			v_amt = fn_calcAmt2(ds_list.getColumn(e.row, "estiQty"), ds_list.getColumn(e.row, "estiPrice"));
			ds_list.setColumn(e.row, "estiAmt", v_amt); 
             
            break;
	
	}	
	
}

