function fn_selectCheck()
{
	var nRowCnt = ds_class.getRowCount();	// DataSet Count
	var sCond = "";
	var iCnt = 0;

	// CHECK 저장 여부 CHECK
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		// Dataset이 선택된 것만 적용
		if (ds_class.getColumn(i, "chk")=="1") {
			iCnt++;
			if (iCnt==1) sCond = ds_class.getColumn(i, "classCode");
			else sCond = sCond + "," + ds_class.getColumn(i, "classCode");
		}
	}

	if(iCnt == 0)
	{
		sMsg = "작업대상 대분류를 선택하세요.";
		gfn_alert(sMsg, "", "A");
		return false;
	}
	

	// 선택한 대분류에 대한 조회조건 설정
	ds_search.setColumn(ds_search.rowposition, "schCond", sCond);		// 분류조건
	
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 견적 목록의 마진율 등록 시 판매단가 자동계산 함수
 * 인자      : 선택한 ROW, 입력한 비율, 계산된 금액(Return)
 *---------------------------------------------------------------------------------------------*/
function fn_calcAmt(row, rate)
{
	var v_unitPrice = 0; 	// 매입단가
	var v_calcAmt = 0;
	
	v_unitPrice = ds_list.getColumn(row, "unitPrice");

	if (v_unitPrice == 0) return 0;
	
	if (rate=="") rate=0;
	
	v_calcAmt = v_unitPrice * (1 + (rate / 100));
	
	v_calcAmt = gfn_round(v_calcAmt, 0);

	return v_calcAmt;
} 

/*----------------------------------------------------------------------------------------------
 * 설명      : 견적 목록의 금액 등록 시 비율 자동계산 함수
 * 인자      : 선택한 ROW, 입력한 금액, 계산된 비율(Return)
 *---------------------------------------------------------------------------------------------*/
function fn_calcRate(row, amt)
{
	var v_unitPrice = 0; 	// 매입단가
	var v_calcRate = 0;
	
	v_unitPrice = ds_list.getColumn(row, "unitPrice");

	if (v_unitPrice==0) return 0;

	if (amt == "") return 0;
	
	v_calcRate = ((amt / v_unitPrice)-1) * 100;
	
	v_calcRate = gfn_round(v_calcRate, 1);

	return v_calcRate;
} 

/*----------------------------------------------------------------------------------------------
 * 설명      : 견적 목록의 합계 기준의 마진 계산
 * 인자      : 금액 기준으로 계산
 *---------------------------------------------------------------------------------------------*/
function fn_calcRateSum()
{
	var v_unitAmt = 0;
	var v_estiAmt = 0;
	var v_calcRate = 0;
	
	v_unitAmt = getSum('unitAmt');
	v_estiAmt = getSum('estiAmt');
		
	if (v_unitAmt==0) return 0;
	if (v_estiAmt == 0) return 0;
		
	v_calcRate = ((v_estiAmt / v_unitAmt)-1) * 100;
	
	v_calcRate = gfn_round(v_calcRate, 1);

	return v_calcRate;
}



/*----------------------------------------------------------------------------------------------
 * 설명      : 견적수량 * 단가 = 금액 자동계산
 * 인자      : 수량, 단가 = 금액
 *---------------------------------------------------------------------------------------------*/
function fn_calcAmt2(qty, price)
{
	var v_amt = 0; 	// 금액

	if (qty == "") return 0;
	if (price == "") return 0;
	
	v_amt = (qty * price);
	
	v_amt = gfn_round(v_amt, 0);

	return v_amt;
} 


