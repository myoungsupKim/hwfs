/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 * Multi 삭제 가능하게 처리
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	// 견적 목록에서 삭제 : 선택한 행의 자재를 바로 삭제
	var nRow = ds_list.rowposition;
	var nRowCnt = ds_list.getRowCount();	// DataSet Count
	var v_estiId = ds_search.getColumn(ds_search.rowposition, "schEstiId");		// 단가번호
	
	
	var iCnt = 0;
	var jCnt = 0;

	// 삭제할 자료 CHECK
	for (var i = 0 ; i < nRowCnt ; i++)
	{
 		// Dataset이 선택된 것만 적용
 		if (ds_list.getSelect(i))	
 		{
 			iCnt++;
 			if (ds_list.getRowType(i) <> Dataset.ROWTYPE_INSERT) {
				jCnt++;
 			}
 			
 		}
	}
	// 삭재 COUNT CHECK
	if(iCnt == 0)
	{
		sMsg = "삭제할 자재정보가 없습니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}	
	
	var sCustNm = ds_list.getColumn(nRow, "custNm");
	var sItemCode = ds_list.getColumn(nRow, "itemCode");
	var sItemName = ds_list.getColumn(nRow, "itemName");
	

	var sMsg = "선택한 자재를 삭제합니다.\n계속 진행 하시겠습니까?\n\n";
	if (jCnt > 0) {
		sMsg = sMsg + " - 기존 자재에 대한 삭제 후 자료를 재조회 합니다.\n";
	}
		sMsg = sMsg + " - 대상건수   : " + iCnt + "\n";
	
	
	var rtVal = gfn_confirm(sMsg, "", "C");	// 선택된 자료를 삭제 하시겠습니까?

	if (rtVal == false) return;


	// 신규생성일 경우에는 해당 ROW를 삭제
	if (v_estiId=="NEW") {
		// 선택되어진 Dataset에 대한 자료 일괄 삭제
		var bSucc = ds_list.deleteSelectedRows();
		
		if (bSucc) gfn_alert("msg.delete.success");	// 삭제 되었습니다.
		else gfn_alert("msg.err.grid.noselect");	// 선택된 항목이 없습니다.	
		return;
	}
	
	
	// 삭제용 dataset Clear
	ds_deleteItem.clearData();
		
		
	iCnt = 0;
	// 삭제할 대상 dataset 생성 : TR 대상
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		// Dataset이 선택된 것만 적용
			if (ds_list.getSelect(i))	
			{
				if (ds_list.getRowType(i) == Dataset.ROWTYPE_INSERT) {
					//ds_list.deleteRow(i);
					// 실제 TR 을 수행하게 되면 삭제하고자 하는 (추가)행은 조회 시 반영 안됨, 별도의 삭제를 할 필요가 없음
				} else {
					// 기존 자료인 경우
					iCnt++;
					ds_deleteItem.addRow();
					
					// 삭제용 dataset SET
					ds_deleteItem.setColumn(ds_deleteItem.rowposition, "estiId", v_estiId);									// 단가번호
					ds_deleteItem.setColumn(ds_deleteItem.rowposition, "itemCode", ds_list.getColumn(i, "itemCode"));		// 자재코드
				}
			}
	}
		
		
	if (iCnt > 0) {
		// Transaction 바로 호출해서 삭제 처리 함 
		var strSvcID    = "deleteItem";
		var strURL      = "fm/fms/estPaperWrite/deleteItem.xdo";
		var strInDs     = "ds_deleteItem=ds_deleteItem:A";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	} else {
		// 신규 추가행만 존재하는 경우
		var nSucc = ds_list.deleteSelectedRows();
		//trace("nSucc : " + nSucc);
		gfn_alert("msg.delete.success");	// 삭제 되었습니다.
		return;
	}
		
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete_old()
{
	// 견적 목록에서 삭제 : 선택한 행의 자재를 바로 삭제
	var nRow = ds_list.rowposition;
	var v_estiId = ds_search.getColumn(ds_search.rowposition, "schEstiId");		// 단가번호
	
	if(ds_list.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var sCustNm = ds_list.getColumn(nRow, "custNm");
	var sItemCode = ds_list.getColumn(nRow, "itemCode");
	var sItemName = ds_list.getColumn(nRow, "itemName");
	
	var sMsg = "선택한 자재를 삭제합니다.\n계속 진행 하시겠습니까?\n\n";
		sMsg = sMsg + " - 업체명   : " + sCustNm + "\n";
		sMsg = sMsg + " - 자재코드 : " + sItemCode + "\n";
		sMsg = sMsg + " - 자재명   : " + sItemName + "\n";

	var rtVal = gfn_confirm(sMsg, "", "C");	// 선택된 자료를 삭제 하시겠습니까?

	if (rtVal == true) 
	{	
		// 신규생성일 경우에는 해당 ROW를 삭제
		if (v_estiId=="NEW") {
			var bSucc = ds_list.deleteRow(nRow);
			if (bSucc) gfn_alert("msg.delete.success");	// 삭제 되었습니다.
			else gfn_alert("msg.err.grid.noselect");	// 선택된 항목이 없습니다.	
			return;
		}
	
	
		// 삭제용 dataset Clear
		ds_deleteItem.clearData();
		
		// 삭제용 dataset SET
		ds_deleteItem.addRow();
		ds_deleteItem.setColumn(ds_deleteItem.rowposition, "estiId", v_estiId);									// 단가번호
		ds_deleteItem.setColumn(ds_deleteItem.rowposition, "itemCode", ds_list.getColumn(nRow, "itemCode"));	// 자재코드

		
		// Transaction 바로 호출해서 삭제 처리 함 
		var strSvcID    = "deleteItem";
		var strURL      = "fm/fms/estPaperWrite/deleteItem.xdo";
		var strInDs     = "ds_deleteItem=ds_deleteItem:A";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 * N Transaction 처리
 *---------------------------------------------------------------------------------------------*/
function fn_save_old()
{
	var nRowCnt = ds_list.getRowCount();	// DataSet Count
	var nRow= ds_search.rowposition;
	var v_estiId = ds_search.getColumn(ds_search.rowposition, "schEstiId");		// 단가번호

	// 저장할 자료 여부 CHECK
	if(nRowCnt == 0) 
	{
		gfn_alert("msg.save.nodata", "저장할 데이터");	// 저장할 데이터가 없습니다.
		return;
	}

	 // v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_list) == false)
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}

	// 저장 시 조건 Check	
	if (v_estiId=="NEW") {
		fv_saveFlag = "I";
		if (gfn_validCheck(ds_search, ds_saveValid, true) == false) return;
	}
	else {
		fv_saveFlag = "U";
		if (gfn_validCheck(ds_search, ds_saveValid2, true) == false) return;
	}	

	// 저장 시 Validation Check : 중복 CHECK	
	if( !fn_saveCheck() ) 
	{
		return;
	}
	
	// 에러메시지 버튼 숨기기
	fn_errorView(false);
	
	
	var sManager = ds_search.getColumn(nRow, "schManagerNm");		// 담당자
	var sDate = cal_fromDate.text.substr(0,4) + "-" + cal_fromDate.text.substr(4,2) + "-" + cal_fromDate.text.substr(6,2);	// 견적기간_시작일
	
	var sMsg = "";
	if (ds_search.getColumn(nRow, "schEstiId")=="NEW") {
		fv_saveFlag = "I";
		sMsg = "견적서를 생성합니다.\n계속 진행 하시겠습니까?\n\n";
		sMsg = sMsg + " - 담당자 : " + sManager + "\n";
		sMsg = sMsg + " - 견적일자 : " + sDate;
	} else {
		fv_saveFlag = "U";
		sMsg = "견적서를 저장합니다.\n계속 진행 하시겠습니까?\n\n";
		sMsg = sMsg + " - 견적번호 : " + ds_search.getColumn(nRow, "schEstiId");
	}
	
	if (!gfn_confirm(sMsg, "", "C")) return;
	
	// 저장 프로세스 시작
	fn_processSet2();
}



