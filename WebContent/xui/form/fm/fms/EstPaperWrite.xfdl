	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
	
	if(gds_userInfo.getColumn(0,"introPage") == "IntroFrameType06.xfdl")
	{
		comp_sabun.enable = false;
		//ds_search.setColumn(0,"partSalesSabun",gds_userInfo.getColumn(0,"sabun"));
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";
	
	// 공통코드(DATA_SET) 정의 : 식재사업부 MU SET
	SCDataset.bind(this, "ComCode", "ds_dataSet", {groupCode:"DATA_SET", code:"DEF_FICS_MU_CODE", useYn:"Y"}, "fn_triggerDatasetEvent");
	

	// 센터정보 COMBO 초기화 : 조건
	fn_searchCenter();
	
	// 분류정보 조회
	fn_searchClass();
	
	//조회조건을 초기화한다.
	var nRow= ds_search.addRow();
	ds_search.setColumn(nRow, "schCenter", "");								// 센터
	ds_search.setColumn(nRow, "schDate1", gfn_today());						// 견적기간_FROM
	ds_search.setColumn(nRow, "schDate2", gfn_today());						// 견적기간_TO
	ds_search.setColumn(nRow, "schCond", "");								// 담당자검색팝업 호출 조건 : 1 (리조트 사업지원, 식재사업부)
	ds_search.setColumn(nRow, "schChk", "0");								// CHECK BOX 초기화
	ds_search.setColumn(nRow, "schChkPrice", "0");							// CHECK BOX 초기화

	// 일자 Obj SET
	cal_fromDate.fn_initMonthCal(this, "", "", ds_search, "schDate1");			// 견적기간 SET
	cal_toDate.fn_initMonthCal(this, "", "", ds_search, "schDate2");			// 견적기간 SET
	
	
	// 담당자정보 MU 초기화
	//comp_sabun.qoMuCd = fv_stdMu;
	
	// 로그인사용자 기본 SET
	ds_search.setColumn(nRow, "schManager", g_EmpNo);						// 담당자사번
	ds_search.setColumn(nRow, "schManagerNm", g_Name);						// 담당자명
	
	
	// 견적번호 기본 SET
	fn_estiId();
	
	// 확인여부 Hidden
	this.grd_list.setFormatColProperty(18,"size",0);
}

