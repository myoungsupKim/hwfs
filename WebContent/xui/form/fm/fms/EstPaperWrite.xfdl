	var sMsg = "평균 시뮬레이션을 적용합니다.\n계속 진행 하시겠습니까?\n\n";
		sMsg = sMsg + "진행 시 기존에 적용된 자료는 변경되어 다시 계산됩니다";
	
	if (!gfn_confirm(sMsg, "", "C")) return;
	

	ds_searchAvg.clearData();
	
	
	// 평균시뮬레이션 적용	
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		// 시뮬레이션이 평균인 자재만 작업 진행
		if (ds_list.getColumn(i, "simuGubun")=="0") {

			ds_searchAvg.addRow();
			ds_searchAvg.setColumn(ds_searchAvg.rowposition, "schCenter", ds_search.getColumn(nRow, "schCenter"));
			ds_searchAvg.setColumn(ds_searchAvg.rowposition, "schUpjang", ds_search.getColumn(nRow, "schUpjang"));
			ds_searchAvg.setColumn(ds_searchAvg.rowposition, "schDate1", ds_search.getColumn(nRow, "schDate1"));
			ds_searchAvg.setColumn(ds_searchAvg.rowposition, "schDate2", ds_search.getColumn(nRow, "schDate2"));
