			ds_searchAvg.setColumn(ds_searchAvg.rowposition, "schCustCd", ds_list.getColumn(i, "custCd"));
		}
	}	// end for

	
	// 작업진행_초기화
	fn_processInitAvg("1");

	fn_processSetAvg();
}




