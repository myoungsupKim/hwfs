/*----------------------------------------------------------------------------------------------
 * 설명      : 대분류별 이익율 적용 처리
 *              이익율 적용, 선택한 분류 기준
 *---------------------------------------------------------------------------------------------*/
function fn_classRateProc(iRow)
{
	var nRowCnt = ds_list.getRowCount();					// DataSet Count
	var sGubun = div_margin.rdo_price.value;				// 올림구분(1, 2, 3)  ===> 기본이 10원
	var v_saleRate = ds_class.getColumn(iRow, "saleRate");	// 적용할 이익율
	var v_itemClass2 = ds_class.getColumn(iRow, "classCode");// 적용할 분류정보
	var v_itemClass3 = ds_class.getColumn(iRow, "classCode2");// 적용할 대표코드정보
	var v_simuGubun = gfn_nvl(ds_class.getColumn(iRow, "simuGubun"), ""); // 시뮬레이션구분

	if (nRowCnt < 1) return;

	if (v_saleRate.valueOf() == "undefined" || v_saleRate == "") v_saleRate = 0;
	var arr = v_itemClass2.split(",");
	var v_estiPrice = 0;
	var v_rate = 0;
	
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		var itemClass = false;
		// 대분류가 같은 품목만 작업 진행
		for(var j = 0 ; j < arr.length; j++){
			if (ds_list.getColumn(i, "itemClass2")==arr[j]) 
				itemClass = true;
		}
		if(itemClass){
			// 1.견적(판매)단가 계산
			v_unitPrice = ds_list.getColumn(i, "unitPrice");
			v_estiPrice = v_unitPrice + ((v_unitPrice * v_saleRate) / 100);
			
			// 2.금액 재계산(올림 적용) : 적용 X
			//v_estiPrice = fn_amtRound(sGubun, v_estiPrice);
			ds_list.setColumn(i, "estiPrice", v_estiPrice);	// 판매금액 SET
			//trace(i + ".v_estiPrice : " + v_estiPrice);

			// 3.마진율 SET
			ds_list.setColumn(i, "marginRate", v_saleRate); 
			
			// 4. 시뮬레이션 구분 복사 : 선택 시
			if (v_simuGubun == "") {
				ds_list.setColumn(i, "simuYn", ""); 
				ds_list.setColumn(i, "simuGubun", ""); 
				ds_list.setColumn(i, "estiQty", "0"); 			//  수량 초기화
			} else {
				ds_list.setColumn(i, "simuYn", "Y"); 
				ds_list.setColumn(i, "simuGubun", v_simuGubun); 
			}
			
			// 5. 추가 : 실제 팝업창 처리 후 리턴 시 계산을 위한 대표 코드 SET
			ds_list.setColumn(i, "itemClass3", v_itemClass3); 
		}
	}	// end for
	
	//gfn_alert("msg.action.success");	// 처리 되었습니다.
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 시 CHECK 
 *---------------------------------------------------------------------------------------------*/
function fn_saveCheck()
{
	// 대분류 기준의 평균 및 시뮬레이션 적용시에는 저장 시 CHECK
	// CHECK가 되있으면 평균/유사 무조건 CHECK함
	var nRowCnt = ds_class.getRowCount();	// DataSet Count
	var nRowCnt1 = ds_list.getRowCount();	// DataSet Count
	var iCnt = 0;
	var sMsg = "";
	var v_itemClass2 = "";
	var v_className = "";
	var v_simuGubun = "";
	var v_simuGubunNm = "";

	// 대분류 기준 목록
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		// Dataset이 선택된 것만 적용
		if (ds_class.getColumn(i, "chk")=="1") {
			iCnt++;
			// 시물레이션 선택 CHECK
			v_itemClass2 = ds_class.getColumn(i, "classCode2");
			v_className = ds_class.getColumn(i, "className");
			v_simuGubun = gfn_nvl(ds_class.getColumn(i, "simuGubun"), "");
			
			
			// 시뮬레이션이 선택되있을 경우에만 CEHCK
			if (v_simuGubun!="") {
			
				if (v_simuGubun=="0") v_simuGubunNm = "평균";
				else v_simuGubunNm = "유사";

				// 자재목록 기준 CHECK
				for (var j = 0 ; j < nRowCnt1 ; j++)
				{
					// 동일한 분류기준의 자재이면
					if (ds_list.getColumn(j, "itemClass3")==v_itemClass2) {
						// 시뮬레이션 유형이 다를경우 오류 메시지 표시
						if (ds_list.getColumn(j, "simuGubun")!=v_simuGubun) {
							sMsg = "작업대상 대분류 " + v_className + " 은(는)\n\n";
							sMsg = sMsg + v_simuGubunNm + " 시뮬레이션으로 적용해야 합니다.\n";
							ds_list.rowposition = j;
							return false;
						}
					}
					
				}	// end for j	
			} //  end if v_simuGubun
		
		} // end if chk
	} // end for i


	return true;
}


