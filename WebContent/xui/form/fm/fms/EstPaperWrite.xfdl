		return;
	}

	// 유사시뮬레이션 여부 CHECK
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		if (ds_list.getColumn(i, "simuGubun")=="1") {
			iCnt++;
		}
	}
	if(iCnt == 0)
	{
		sMsg = "적용할 유사 시뮬레이션 대상 자재가 없습니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}
	

	
	// 대분류 CHECK	
	iCnt = 0;
	for (var i = 0 ; i < nRowCnt1 ; i++)
	{
		// Dataset이 선택된 것만 적용
		if (ds_class.getColumn(i, "chk")=="1") {
			if (ds_class.getColumn(i, "simuGubun")=="1") {
				iCnt++;
				if (ds_class.getColumn(i, "classCode2")=="F10") arrParam[4] = "1";
				else if (ds_class.getColumn(i, "classCode2")=="F12") arrParam[5] = "1";
				else if (ds_class.getColumn(i, "classCode2")=="F13") arrParam[6] = "1";
				else if (ds_class.getColumn(i, "classCode2")=="F14") arrParam[7] = "1";
				else if (ds_class.getColumn(i, "classCode2")=="F17") arrParam[8] = "1";
				else if (ds_class.getColumn(i, "classCode2")=="F21") arrParam[9] = "1";
				else if (ds_class.getColumn(i, "classCode2")=="F27") arrParam[10] = "1";
			}
		}
	}

	if(iCnt == 0)
	{
		sMsg = "작업대상 대분류를 선택하세요.";
		gfn_alert(sMsg, "", "A");
		return;
	}
	
	arrParam[0] = "estPaper";
	arrParam[1] = ds_search.getColumn(nRow, "schCenter");
	arrParam[2] = ds_search.getColumn(nRow, "schDate1");
	arrParam[3] = ds_search.getColumn(nRow, "schDate2");
	
	// 전송할 파라미터 1 : 작업구분(estPaper : 견적서 호출)	
	// 전송할 파라미터 2 : 센터코드
	// 전송할 파라미터 3 : 기준일자_FROM
	// 전송할 파라미터 4 : 기준일자_TO
	// 전송할 파라미터 5 : 양곡_CHK
	// 전송할 파라미터 6 : 채소_CHK
	// 전송할 파라미터 7 : 축산물_CHK
	// 전송할 파라미터 8 : 수산물_CHK
	// 전송할 파라미터 9 : 김치_CHK
	// 전송할 파라미터 10: 공산식품_CHK
	// 전송할 파라미터 11: 식재영업소모품_CHK

	var dsRtn = new Dataset();

	var dsObj = gfn_dialog("EstPaperWriteSimuSimilarPop"				// Dialog ID
							, "X_FMS::EstPaperWriteSimuSimilarPop.xfdl"	// Form URL
							, {fv_Contents:arrParam}					// 배열
							, null, null, null, null, null, false, true, -1);

	if(dsObj != null)
	{
		ds_searchSim.clearData();
		for(var i=0; i<dsObj.rowcount; i++)
		{	
			ds_searchSim.addRow();
			ds_searchSim.setColumn(ds_searchSim.rowposition, "schCenter", ds_search.getColumn(nRow, "schCenter"));		
			ds_searchSim.setColumn(ds_searchSim.rowposition, "schUpjang", dsObj.getColumn(i, "upjang"));
			ds_searchSim.setColumn(ds_searchSim.rowposition, "schDate1", ds_search.getColumn(nRow, "schDate1"));
			ds_searchSim.setColumn(ds_searchSim.rowposition, "schDate2", ds_search.getColumn(nRow, "schDate2"));
			ds_searchSim.setColumn(ds_searchSim.rowposition, "schItemClass", dsObj.getColumn(i, "itemClass"));
		}
		// 시뮬레이션 수량 적용 : 분류별
		// 
		//trace(ds_searchSim.saveXML());
		if (ds_searchSim.getRowCount() > 0 ) {
			// 작업진행_초기화
			fn_processInit("1");
			// 
			fn_processSet();
		}
		
	}

	
}


/*----------------------------------------------------------------------------------------------
 * 설명      : 견적번호 변경 시 이벤트 처리
 *              
 *---------------------------------------------------------------------------------------------*/
function cbo_esti_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	// 계약명 표시
	var v_estiId = cbo_esti.value; 

	// LIST Clear
	ds_list.clearData();

 	// KEY Clear
 	fv_key = "";

	if (v_estiId=="NEW")
	{	// 신규생성
		fn_estiCompSet("1");
	} else {
		// 기존
		fn_estiCompSet("0");
	}
	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 확인 버튼 클릭 시 이벤트 처리
 *              
 *---------------------------------------------------------------------------------------------*/
function btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRowCnt = ds_list.getRowCount();	// DataSet Count
	var nRow= ds_search.rowposition;
	var v_estiId = cbo_esti.value; 

	// 저장할 자료 여부 CHECK
	if(nRowCnt == 0) 
	{
		gfn_alert("msg.save.nodata", "저장할 데이터");	// 저장할 데이터가 없습니다.
		return;
	}

	// 견적번호 CHECK
	if(v_estiId=="NEW" || v_estiId=="") {
		sMsg = "견적번호를 선택하세요.";
		gfn_alert(sMsg, "", "A");
		cbo_esti.setFocus();
		return;
	}

	
	sMsg = "견적서를 확인처리 합니다.\n계속 진행 하시겠습니까?\n\n";
	sMsg = sMsg + " - 견적번호 : " + ds_search.getColumn(nRow, "schEstiId");
	
	if (!gfn_confirm(sMsg, "", "C")) return;
	
	// 저장 시 조건(KEY)을 ds_list의 KEY 값으로 복사
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		ds_list.setColumn(i, "estiId", ds_search.getColumn(nRow, "schEstiId"));		
	}
	
	// KEY SET
 	fv_key = ds_search.getColumn(nRow, "schEstiId");


	// Transaction 처리
	var strSvcID    = "saveConfirm";
	var strURL      = "fm/fms/estPaperWrite/saveConfirm.xdo";
	var strInDs     = "ds_list=ds_list:A";
	var strOutDs    = "";
	var strArg      = "";
	var strCallback = "fn_callBack";
	var bAsync      = true;
	
	gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

