/*----------------------------------------------------------------------------------------------
 * 설명      : 출력 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 * 견적서 출력 레포트 작성
 *---------------------------------------------------------------------------------------------*/
function fn_print()
{
	var sMsg = "";
	if (ds_search.getColumn(0, "schEstiId")=="NEW") {
		sMsg = "견적서는 저장 후에 출력이 가능합니다.";
