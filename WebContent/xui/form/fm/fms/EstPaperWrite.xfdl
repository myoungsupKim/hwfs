/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		if(strSvcID=="saveList2")
		{
			fv_errorFlag="Y";
			fv_errorMsg = strErrorMsg;
			fn_processStep2();
			return;
		}
		else 
		{
			return;
		}
	}
	
    switch(strSvcID)
    {
		// 목록 조회
        case "search" :
            //trace(ds_list.saveXML());
            if (ds_list.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_list, fv_objColInfo);
            }
            // 조회이후 COMP 관련 초기화
            break;

            
            
		// 센터 조회
        case "searchCenter" :
            //trace(ds_center.saveXML());
            fn_center_init();
            break;

		// 견적번호 조회
        case "searchEsti" :
            //trace(ds_esti.saveXML());
            fn_esti_init();
            break;

            
		// 자재분류 조회
		case "searchClass" :
			//trace(ds_class.saveXML());
			break;

        // 저장 : 1 Transaction
        case "saveList" :
			//trace("처리건수 : " + fv_processCnt);
			//gfn_alert("msg.save.success");	// 저장 되었습니다.
			var sMsg = "저장 되었습니다.\r\r";
			//sMsg = sMsg + "-. 전체건수 : " + ds_list.rowcount + " 건\r";
			sMsg = sMsg + "-. 처리건수 : " + fv_processCnt + " 건";
			gfn_alert(sMsg, "", "A");
			
			if (fv_key=="NEW") fv_key = fv_estiId;	// 신규일 경우 KEY값 전달

			// 견적번호 재조회
			fn_estiId();

			// 견적번호COMBO 설정에 따른 콤포넌트 처리(기존자료 조회)
			//fn_estiCompSet("0");
			
			// 견적목록 재조회
			//fn_search();
			
            break;    


        // 저장 : N Transaction
        case "saveList2" :
			if (fv_processCnt > 0) {
				fv_closeFlag = "OK";
				fv_errorFlag="";
				fv_errorMsg="";
			}
			//gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_processStep2();
            break;    




        // 확인
        case "saveConfirm" :
			//trace("확인건수 : " + fv_processCnt);
			// 확인건수가 있으면 선택한 견적서의 확인여부 FLAG SET
			if(fv_processCnt>0) {
				ds_search.setColumn(ds_search.rowposition, "schConfirmYn", "Y");		// 확인여부(견적서 출력 CHECK)
			}
			var sMsg = "확인 처리 되었습니다.\r\r";
			sMsg = sMsg + "-. 전체건수 : " + ds_list.rowcount + " 건\r";
			sMsg = sMsg + "-. 처리건수 : " + fv_processCnt + " 건";
			gfn_alert(sMsg, "", "A");
			// 견적목록 재조회
			fn_search();
            break;    

        // 삭제
        case "deleteItem" :
			//trace("삭제건수 : " + fv_processCnt);
			gfn_alert("msg.delete.success");	// 삭제 되었습니다.
			// 견적목록 재조회
			fn_search();
            break;    
            
        // 유사시뮬레이션 관련 수량 GET
        case "searchSimQty" :
			fn_processStep();
            break;    

        // 평균시뮬레이션 관련 수량 GET
        case "searchAvgQty" :
			fn_processStepAvg();
            break;    
            
            
    }
}


