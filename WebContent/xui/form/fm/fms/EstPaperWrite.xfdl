 * 평균수량을 가져와서 매입금액 및 판매금액을 계산           
 * 종전방식 : 조회 시 평균수량을 계산하여 미리 가져온다 ===> 향후 속도 문제 발생할 수 있음
 *---------------------------------------------------------------------------------------------*/
function btn_simuAvg2_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRowCnt = ds_list.getRowCount();	// DataSet Count
	var iCnt = 0;

	// 자료 여부 CHECK
	if(nRowCnt == 0) 
	{
		gfn_alert("msg.err.grid.nodata", "");	// 데이타가 없습니다.
		return;
	}

	// 평균시뮬레이션 여부 CHECK
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		if (ds_list.getColumn(i, "simuGubun")=="0") {
			iCnt++;
		}
	}

	if(iCnt == 0)
	{
		sMsg = "적용할 평균 시뮬레이션 대상 자재가 없습니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}
	
	var sMsg = "평균 시뮬레이션을 적용합니다.\n계속 진행 하시겠습니까?\n\n";
		sMsg = sMsg + "진행 시 기존에 적용된 자료는 변경되어 다시 계산됩니다";
	
	if (!gfn_confirm(sMsg, "", "C")) return;
	
	
	// 평균시뮬레이션 적용	
	var v_estiPrice = 0;
	var v_rate = 0;
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		// 시뮬레이션이 평균인 자재만 작업 진행
		if (ds_list.getColumn(i, "simuGubun")=="0") {

			// 1.평균수량을 견적수량으로 복사
			ds_list.setColumn(i, "estiQty", ds_list.getColumn(i, "avgQty"));	

			// 2.매입금액 계산
			v_amt = fn_calcAmt2(ds_list.getColumn(i, "estiQty"), ds_list.getColumn(i, "unitPrice"));
