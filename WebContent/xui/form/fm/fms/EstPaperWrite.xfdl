/*----------------------------------------------------------------------------------------------
 * 설명      : 유사시뮬레이션 로직 처리, 1 Row 씩 처리
 * arg : 처리 Index(row id)
 *---------------------------------------------------------------------------------------------*/
function fn_process(arg)
{
	// 최초 실행 시 첫번째 Row
	if (arg=="") arg = 0;

	// 선택 Row 가 최대 Row보다 클 경우 종료
	if ((arg+1) > fv_simRowCnt) {
		gfn_alert("msg.action.success");	// 처리 되었습니다.
		fn_processInit("2");
		return;
	}
	
	// ds_itemSimRow Dataset Clear
	ds_itemSimRow.clearData();
	
	// ds_itemSimRow Dataset 에 복사 
	var iIdx = ds_itemSimRow.addRow();
	
	ds_itemSimRow.setColumn(iIdx, "schCenter", ds_itemSim.getColumn(arg, "schCenter"));	// 센터코드
	ds_itemSimRow.setColumn(iIdx, "schUpjang", ds_itemSim.getColumn(arg, "schUpjang"));	// 업장코드
	ds_itemSimRow.setColumn(iIdx, "schDate1", ds_itemSim.getColumn(arg, "schDate1"));	// 기준일자_FROM
	ds_itemSimRow.setColumn(iIdx, "schDate2", ds_itemSim.getColumn(arg, "schDate2"));	// 기준일자_TO
	ds_itemSimRow.setColumn(iIdx, "schItemCode", ds_itemSim.getColumn(arg, "schItemCode"));	// 자재코드	
	ds_itemSimRow.setColumn(iIdx, "schCustCd", ds_itemSim.getColumn(arg, "schCustCd"));	// 납품업체코드
	fv_itemCode = ds_itemSim.getColumn(arg, "schItemCode");
	fv_custCd = ds_itemSim.getColumn(arg, "schCustCd");
	
	// 시작 ROW 표시
	var nRow = ds_list.findRowExpr("custCd=='"+fv_custCd+"' && itemCode=='"+fv_itemCode+"'");
	ds_list.rowposition = nRow;
	
	// Transaction 처리
	var strSvcID    = "searchSimQty";
