/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// 조건 SET
	var nRow = ds_search.rowposition;
	
	//ds_search.setColumn(nRow, "schManager", edt_sabun.userdata);	// 담당자
	ds_search.setColumn(nRow, "schEstiId", cbo_esti.value);			// 견적번호
	ds_search.setColumn(nRow, "schUpjang", edt_upjang.userdata);	// 고객사코드

	// 신규작성일 경우에는 대분류를 설정해야 함
	if (cbo_esti.value=="NEW") {
		// 조회시 Validation Check
		if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

		// 대분류 CHECK
		if( !fn_selectCheck() ) 
		{
			return;
		}
	} else {
		// 조회시 Validation Check : 고객사 CHECK 하지 않음
		if (gfn_validCheck(ds_search, ds_searchValid2, true) == false) return;
	}

	// 기간 CHECK, 평균/유사 시뮬레이션 용도
	if (cal_fromDate.text > cal_toDate.text) {
		gfn_alert("msg.err.validator.date.great");	// 종료일이 시작일보다 빠릅니다.
		cal_fromDate.setFocus();
		return;
	}
	
	var v_Pdate = "";						// 전략이익율 관련 날짜 SET

	// 전략이익율 기준일자 SET
	if (cal_fromDate.text.substr(6,2) > "15") v_Pdate = cal_fromDate.text.substr(0,4) + cal_fromDate.text.substr(4,2) + "16";
	else v_Pdate = cal_fromDate.text.substr(0,4) + cal_fromDate.text.substr(4,2) + "01";
	
	ds_search.setColumn(nRow, "schPdate", v_Pdate);					// 전략이익율 일자 SET
	ds_search.setColumn(nRow, "schCust",	edt_cust.userdata);
	
	// 에러메시지 버튼 숨기기
	fn_errorView(false);
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);

   	//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장 
	fv_objColInfo = gfn_getDatasetInfo (ds_list);
    
    var strSvcID    = "search";
	var strURL      = "fm/fms/estPaperWrite/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}

