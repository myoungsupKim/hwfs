/*----------------------------------------------------------------------------------------------
 * 설명      : String 타입의 날자값에 년월일을 붙여준다.
 *---------------------------------------------------------------------------------------------*/
function fn_strDate(strDate)
{
	//String 타입의 날자값에 년월일을 붙여 저장 ( ex)20150603 -> 2015년06월03일 )
	var str = strDate;	
	var str1 = str.substr(0,4); 
	var str2 = str.substr(4,2); 
	var str3 = str.substr(6,2); 
	return str = str1 + '년' + str2 + '월' + str3 + '일';
}

