/*----------------------------------------------------------------------------------------------
 * 설명      : 평균시뮬레이션 로직 처리, 1 Row 씩 처리
 * arg : 처리 Index(row id)
 *---------------------------------------------------------------------------------------------*/
function fn_processAvg(arg)
{
	// 최초 실행 시 첫번째 Row
	if (arg=="") arg = 0;

	// 선택 Row 가 최대 Row보다 클 경우 종료
	if ((arg+1) > fv_avgRowCnt) {
		gfn_alert("msg.action.success");	// 처리 되었습니다.
		fn_processInitAvg("2");
		return;
	}
	
	// ds_itemSimRow Dataset Clear
	ds_itemSimRow.clearData();
	
	// ds_itemSimRow Dataset 에 복사 
	var iIdx = ds_itemSimRow.addRow();
	
	ds_itemSimRow.setColumn(iIdx, "schCenter", ds_searchAvg.getColumn(arg, "schCenter"));	// 센터코드
	ds_itemSimRow.setColumn(iIdx, "schUpjang", ds_searchAvg.getColumn(arg, "schUpjang"));	// 업장코드
	ds_itemSimRow.setColumn(iIdx, "schDate1", ds_searchAvg.getColumn(arg, "schDate1"));	// 기준일자_FROM
	ds_itemSimRow.setColumn(iIdx, "schDate2", ds_searchAvg.getColumn(arg, "schDate2"));	// 기준일자_TO
	ds_itemSimRow.setColumn(iIdx, "schItemCode", ds_searchAvg.getColumn(arg, "schItemCode"));	// 자재코드	
	ds_itemSimRow.setColumn(iIdx, "schCustCd", ds_searchAvg.getColumn(arg, "schCustCd"));	// 납품업체코드
	fv_itemCode = ds_searchAvg.getColumn(arg, "schItemCode");
	fv_custCd = ds_searchAvg.getColumn(arg, "schCustCd");
	
	// 시작 ROW 표시
	var nRow = ds_list.findRowExpr("custCd=='"+fv_custCd+"' && itemCode=='"+fv_itemCode+"'");
	ds_list.rowposition = nRow;
	
	// Transaction 처리
	var strSvcID    = "searchAvgQty";
    var strURL      = "fm/fms/estPaperWrite/selectAvgQty.xdo";
	var strInDs     = "ds_itemSimRow=ds_itemSimRow:A";
    var strOutDs    = "ds_avgQty=ds_avgQty";
	var strArg      = "";
	var strCallback = "fn_callBack";
	var bAsync      = true;
	
	gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}



/*----------------------------------------------------------------------------------------------
 * 설명      : Callback 후 작업진행 여부 CHECK 및 프로세스 호출
 * 유사 시뮬레이션 처리
 *---------------------------------------------------------------------------------------------*/
function fn_processStep()
{
	var nRowCnt = ds_simQty.getRowCount();	// DataSet Count
	var v_simQty = 0;
	
	// 자료 존재여부 CHECK
	if (nRowCnt>0) 
	{
		v_simQty = ds_simQty.getColumn(0, "simQty");
	}
		
	var nRow = ds_list.findRowExpr("custCd=='"+fv_custCd+"' && itemCode=='"+fv_itemCode+"'");
	
	ds_list.setColumn(nRow, "simQty", v_simQty);			// 유사수량 SET
	
	// 1.유사수량을 견적수량으로 복사
	ds_list.setColumn(nRow, "estiQty", ds_list.getColumn(nRow, "simQty"));
	
	
	// Progress Bar SET
	pbr_status.text = "유사시뮬레이션 작업 ("+(fv_simRow+1)+"/"+ fv_simRowCnt+")";
	pbr_status.pos = fv_simRow+1;
	pbr_status.stepIt();
	this.updateWindow(); // 이 부분 추가
	//trace("iRow : " + iRow);

	//다음 Transaction 처리 실행
	fv_simRow++;
		
	//trace("fv_simRow : " + fv_simRow); 
	fn_process(fv_simRow);
}


/*----------------------------------------------------------------------------------------------
 * 설명      : Callback 후 작업진행 여부 CHECK 및 프로세스 호출
 * 평균 시뮬레이션 처리
 *---------------------------------------------------------------------------------------------*/
function fn_processStepAvg()
{
	var nRowCnt = ds_avgQty.getRowCount();	// DataSet Count
	var v_avgQty = 0;
	
	// 자료 존재여부 CHECK
	if (nRowCnt>0) 
	{
		v_avgQty = ds_avgQty.getColumn(0, "avgQty");
	}
		
	var nRow = ds_list.findRowExpr("custCd=='"+fv_custCd+"' && itemCode=='"+fv_itemCode+"'");

	// 평균시뮬레이션 적용	
	ds_list.setColumn(nRow, "avgQty", v_avgQty);			// 평균수량 SET
	
	// 1.평균수량을 견적수량으로 복사
	ds_list.setColumn(nRow, "estiQty", ds_list.getColumn(nRow, "avgQty"));

	// Progress Bar SET
	pbr_status.text = "평균시뮬레이션 작업 ("+(fv_avgRow+1)+"/"+ fv_avgRowCnt+")";
	pbr_status.pos = fv_avgRow+1;
	pbr_status.stepIt();
	this.updateWindow(); // 이 부분 추가
	//trace("iRow : " + iRow);

	//다음 Transaction 처리 실행
	fv_avgRow++;
		
	//trace("fv_avgRow : " + fv_avgRow); 
	fn_processAvg(fv_avgRow);
}


