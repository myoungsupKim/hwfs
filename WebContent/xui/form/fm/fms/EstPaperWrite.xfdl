/*----------------------------------------------------------------------------------------------
 * 설명      : 판매금액에 대한 올림처리
 * gubun (1:1원, 2:10원, 3:100원)  
 *---------------------------------------------------------------------------------------------*/
function fn_amtRound(gubun, amt)
{
	var v_calcAmt = 0;
	
	if (amt == 0) return 0;

	if(gubun=="1") {
		v_calcAmt = Math.ceil(amt / 1) * 1;
	} else if(gubun=="3") {
		v_calcAmt = Math.ceil(amt / 100) * 100;
	} else {
		v_calcAmt = Math.ceil(amt / 10) * 10;
	}
	
	return v_calcAmt;
}


