function fn_upjangPopup(obj:Button,  e:ClickEventInfo){
	var sql = "SELECT UPJANG, UPJANGNM FROM (SELECT A.UPJANG, A.UPJANGNM FROM ST_UPJANG A								";
	sql += " WHERE A.CUSTCD > 0 AND A.UPJANG = A.AP_UNITPRICE_UPJANG AND EXISTS ( 										";
		sql += " SELECT 1 FROM SCC_COMMON_CODE Z WHERE Z.GROUP_CODE = 'FM002' AND Z.CODE = A.UPJANG AND Z.USE_YN = 'Y'	";					
		sql += " ) UNION ALL																							";

	sql += " SELECT TO_NUMBER(A.CODE) AS UPJANG, A.CODE_NAME AS UPJANGNM FROM SCC_COMMON_CODE A							 ";
	sql += " WHERE A.USE_YN = 'Y' AND A.GROUP_CODE = 'FM002' 															 ";
	sql += " AND EXISTS (SELECT 1 FROM ST_CUST Z WHERE Z.CUSTCD = TO_NUMBER(A.CODE))									 ";
	sql += " ) A						 WHERE 1=1 																		 ";
	fn_sql_common_setup(sql,"UPJANG","UPJANGNM","사업장코드","사업장명","","","","사업장검색","UPJANG;UPJANGNM");
	if (gfn_length(gfn_trim(gds_sql_common.getColumn(0, "ret_code")))!= 0 && gfn_length(gfn_trim(gds_sql_common.getColumn(0, "ret_code")))!= null)
	{
		trace(gds_sql_common.getColumn(0,"ret_code"));
		trace(gds_sql_common.getColumn(0,"ret_name"));
		ds_search.setColumn(ds_search.rowposition,"schCust",gds_sql_common.getColumn(0,"ret_code"));
		ds_search.setColumn(ds_search.rowposition,"schCustName",gds_sql_common.getColumn(0,"ret_name"));
		ed_Upjang.value = gds_sql_common.getColumn(0,"ret_name");
	}
}
function fn_edBtn_OnKeyDown(obj:Edit, e:KeyEventInfo)
{
	if(e.keycode == 13)fn_upjangPopup();
}
