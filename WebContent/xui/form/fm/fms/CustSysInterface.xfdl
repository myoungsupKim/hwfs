 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
    // Grid sort 표시 초기화(필수)
    //gfn_clearSortMark(grd_list);
    
    var focusTabName = tab_custSys.tabpages.get_item(tab_custSys.tabindex).name;
    
    //ds_search.clearData();
    ds_searchValid.clearData();
    //ds_search.addRow();
    ds_searchValid.addRow();
    //ds_search.setColumn(0,'schCust',comp_upjang.getUpjangCode());
    //ds_search.setColumn(0,'schCustName',comp_upjang.getUpjangName());
    ds_searchValid.setColumn(0,'schCust','title:고객사,required:true,focus:ed_Upjang');
    
    switch (focusTabName)
    {
		case "tpg_subinv" : 
			ds_search.setColumn(0,'schType','tpg_subinv');
		
			var strURL		= "fm/fms/custSysInterface/selectSubinv.xdo"
			var strOutDs    = "ds_subinv=ds_list";
			break;
		
		case "tpg_item" : 
			ds_search.setColumn(0,'schType','tpg_item');
		
			ds_search.setColumn(0,'schDate1',tab_custSys.tpg_item.cal_priceDate.fn_getValue());
			ds_search.setColumn(0,'schItemType',tab_custSys.tpg_item.cbo_itemType.value);
			ds_search.setColumn(0,'schItem',tab_custSys.tpg_item.edt_item.text);
			if(tab_custSys.tpg_item.chk_isNotUse.value == true) ds_search.setColumn(0,'schIsNotUse',true);
				
			ds_searchValid.setColumn(0,'schDate1','title:단가일자,required:true,focus:tab_custSys.tpg_item.cal_priceDate');
			
			var strURL 		= "fm/fms/custSysInterface/selectItem.xdo"
			var strOutDs    = "ds_item=ds_list";
			break;
		
		case "tpg_order" : 
			ds_search.setColumn(0,'schType','tpg_order');
			
			ds_search.setColumn(0,'schDate1',tab_custSys.tpg_order.cal_orderFrom.fn_getValue());
			ds_search.setColumn(0,'schDate2',tab_custSys.tpg_order.cal_orderTo.fn_getValue());
			
			
			ds_searchValid.setColumn(0,'schDate1','title:시작일,required:true,comparemin:schDate2:종료일,focus:tab_custSys.tpg_order.cal_orderFrom');
			ds_searchValid.setColumn(0,'schDate2','title:종료일,required:true,comparemax:schDate2:시작일,focus:tab_custSys.tpg_order.cal_orderTo');
		
			var strURL 		= "fm/fms/custSysInterface/selectOrder.xdo"
			var strOutDs    = "ds_order=ds_list";
			break;
		
		case "tpg_trans" : 
