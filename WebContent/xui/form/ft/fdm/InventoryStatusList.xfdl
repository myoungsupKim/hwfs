function btn_warehouseMove_onclick(obj:Button,  e:ClickEventInfo)
{
    var deliverNum = gfn_nvl(ds_list.getColumn(ds_list.rowposition, "deliverNum")); 				

    var deliverAmt = toNumber(gfn_nvl(ds_list.getColumn(ds_list.rowposition, "deliverAmt")));	   //입고금액
    var deliverWgt = toNumber(gfn_nvl(ds_list.getColumn(ds_list.rowposition, "deliverWgt")));	   //입고중량
    var deliverQuantity = toNumber(gfn_nvl(ds_list.getColumn(ds_list.rowposition, "deliverQuantity")));	   //입고중량
    var whMoveQuantity = toNumber(gfn_nvl(ds_list.getColumn(ds_list.rowposition, "whMoveQuantity")));	   //이동수량
    var whMoveWgt = toNumber(gfn_nvl(ds_list.getColumn(ds_list.rowposition, "whMoveWgt")));	   //이동중량
    
    
    var unit = toNumber(gfn_nvl(ds_list.getColumn(ds_list.rowposition, "unit")));	   //입고중량
    var set1 = gfn_nvl(ds_list.getColumn(ds_list.rowposition, "set1"));	   //단위
    var wonwtrAmt = gfn_nvl(ds_list.getColumn(ds_list.rowposition, "wonwtrAmt"));	   //단위
    
    
    var uprice;
    if ( set1 == "KG" )
    {
		uprice = deliverAmt/(deliverWgt-whMoveWgt);
		//alert("uprice  :"+deliverAmt+"/"+deliverWgt +"="+uprice);
    }
    else
    {
		uprice =  deliverAmt/(deliverQuantity-whMoveQuantity);
		//alert("uprice  :"+deliverAmt+"/"+deliverQuantity+"-"+whMoveQuantity +"="+uprice+"//"+wonwtrAmt);
    }
    		
    		
	var  arrRtn = gfn_dialog("", "X_FDM::WarehouseMovePop.xfdl",{deliverNum:deliverNum, uprice:uprice});	
}

