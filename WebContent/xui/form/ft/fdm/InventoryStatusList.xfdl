function btn_deliver_onclick(obj:Button,  e:ClickEventInfo)
{
	var chkCnt = ds_list.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		return;
	}

	for ( var i=0; i<ds_list.rowcount; i++ ) 
	{
		if ( ds_list.getColumn(i, "chk") =='1' ) 
		{
			if ( ds_list.getColumn(i, "deliverCnt") > 0 )	  //입고된 내역이 있으면
			{
				gfn_alert("ft.fdm.validator.deliver", ds_list.getColumn(i, "entryNum") );	//수입문서번호 ["+ds_list.getColumn(i, "entryNum")+"]는 이미 입고되었습니다. 입고취소 후 가능합니다.
				ds_list.rowposition = i;
				return;
			}
			if ( ds_list.getColumn(i, "lastSaleEdate") == "undefined" || ds_list.getColumn(i, "lastSaleEdate") == "" )	  //최종판매종료일값이 없으면
			{
				gfn_alert("최종 판매종료일을 입력하세요" );	//
				
				ds_list.rowposition = i;
				cal_lastSaleEdate.setFocus();
				return;
			}
		}
	}	

	var retValue = gfn_confirm("ft.fdm.confirm.deliver", "입고");	//입고 처리하시겠습니까?
	if (retValue == false) return;

    var strSvcID    = "save";
    var strURL      = "ft/fdm/entryMst/saveDeliver.xdo";
    var strInDs     = "ds_search=ds_search ds_list=ds_list:U";
    var strOutDs    = "";
