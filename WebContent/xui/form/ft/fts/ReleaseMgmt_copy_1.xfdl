function btn_purchaseReturn_onclick(obj:Button,  e:ClickEventInfo)
{

	var chkCnt = ds_list2.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		grd_list2.setCellPos(0); // cellindex
		grd_list2.setCellProperty("body", 0, "background", "lightpink");		
		grd_list2.setCellProperty("body", 0, "background2", "lightpink");
	
		return;
	} else 
	{
		grd_list2.setCellProperty("body", 0, "background", "");		
		grd_list2.setCellProperty("body", 0, "background2", "");
	}

	for ( var i=0; i<ds_list2.getRowCount(); i++ )
	{
		if ( (ds_list2.getColumn(i, "chk") =="1" ) ) {
			if ( (ds_list2.getColumn(i, "releaseStatusCd") !="40" ) )	  //출고확정이 아니라면
			{
				gfn_alert("환입는 상태가 출고확정에서만 가능합니다.");
				ds_list2.rowposition = i;
				grd_list2.setCellPos( 0); // cellindex
				
				grd_list2.setFocus();
				grd_list2.showEditor(); 
				ds_list2.setColumn(i, "chk", 0);
				return;
			}
		}
	}

	for ( var i=21; i<=24; i++ ) 
	{
		if ( i == 23 )
		{
			grd_list2.setCellProperty("body", i, "edittype", "date");		//수정모드로
		}
		else
		{
			grd_list2.setCellProperty("body", i, "edittype", "masknumber");		//수정모드로
		}
		grd_list2.setCellProperty("body", i, "background", "lightpink");		
		grd_list2.setCellProperty("body", i, "background2", "lightpink");
	}

	grd_list2.setCellProperty("body", 16, "edittype", "none");		//수정모드로
	grd_list2.setCellProperty("body", 16, "background", "");		
	grd_list2.setCellProperty("body", 16, "background2", "");	

	ds_list2.rowposition = 0;
	grd_list2.setCellPos( 21); // cellindex
	grd_list2.setFocus();
	grd_list2.showEditor(); 

	fv_iProcessFlag = "RTN";
	
	
}
