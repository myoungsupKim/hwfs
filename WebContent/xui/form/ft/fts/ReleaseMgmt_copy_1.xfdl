/*------------------------------------------------------------
	1. Function ID : fn_Save()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_save()
{
	
	var chkCnt = ds_list2.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		grd_list2.setCellProperty("body", 0, "background", "lightpink");		
		grd_list2.setCellProperty("body", 0, "background2", "lightpink");

		grd_list2.setCellPos(0); // cellindex
		grd_list2.setFocus();
		grd_list2.showEditor(); 
		return;
	} 
	else
	{
		grd_list2.setCellProperty("body", 0, "background", "");		
		grd_list2.setCellProperty("body", 0, "background2", "");
	}

	for ( var i=0; i<ds_list2.getRowCount(); i++ )
	{
		if ( (ds_list2.getColumn(i, "chk") =="1" ) ) {
			if ( fv_iProcessFlag == "RTN")	//환입
			{
				if ( (ds_list2.getColumn(i, "purchaseReturnQuantity") =="" ) )
				{
					gfn_alert("환입수량은 필수 입력 항목입니다.");
					ds_list2.rowposition = i;
					grd_list2.setCellPos( 21); // cellindex
					grd_list2.setFocus();
					grd_list2.showEditor(); 
					return;
				}
				if ( (ds_list2.getColumn(i, "purchaseReturnWgt") =="" ) )
				{
					gfn_alert("환입중량은 필수 입력 항목입니다.");
					ds_list2.rowposition = i;
					grd_list2.setCellPos( 22); // cellindex
					grd_list2.setFocus();
					grd_list2.showEditor(); 
					return;
				}
				if ( (ds_list2.getColumn(i, "purchaseReturnDate") =="" ) )
				{
					gfn_alert("환입일자은 필수 입력 항목입니다.");
					ds_list2.rowposition = i;
					grd_list2.setCellPos( 23); // cellindex
					grd_list2.setFocus();
					grd_list2.showEditor(); 
					return;
				}
				/*
				if ( (ds_list2.getColumn(i, "purchaseReturnDeliverNum") =="" ) )
				{
					gfn_alert("환입금액은 필수 입력 항목입니다.");
					ds_list2.rowposition = i;
					grd_list2.setCellPos( 24); // cellindex
					grd_list2.setFocus();
					grd_list2.showEditor(); 
					return;
				}
				*/
			}	
			else	//중량변경
			{
				if ( (ds_list2.getColumn(i, "realWgt") =="" ) )
				{
					gfn_alert("변경될 실중량은 필수 입력 항목입니다.");
					ds_list2.rowposition = i;
					grd_list2.setCellPos(16); // cellindex
					grd_list2.setFocus();
					grd_list2.showEditor(); 
					return;
				}
			}
		}
	}
	if ( fv_iProcessFlag == "RTN")	//환입
	{
		var rtVal = gfn_confirm("환입 하시겠습니니까?");
	}
	else
	{
		var rtVal = gfn_confirm("중량변경 하시겠습니니까?");
	}

	if ( !rtVal == true) return;
	
	ds_search.setColumn(0,"fvIProcessFlag", fv_iProcessFlag);
	
    var strSvcID    = "releasePurchaseReturn";
    var strURL      = "ft/fts/releaseMgmt/saveList.xdo";
    var strInDs     = "ds_search=ds_search ds_list2=ds_list2:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);		
}

