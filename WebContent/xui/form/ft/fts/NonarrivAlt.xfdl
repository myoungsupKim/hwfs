/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0)
	{
		return;
	}

    switch(strSvcID)
    {
		// 조회
        case "selectList" :
			sta_totCnt.text = '총건수 : ' + gfn_numFormat(ds_list.rowcount) + '건';
            break;
        case "selectDetailList" :
            break;
        // 저장
        case "createFiscalSlip" :
			if (fv_iProcessCnt > 0) gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_searchDetail();
            break;
        case "cancelFiscalSlip" :
			if (fv_iProcessCnt > 0) gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_searchDetail();
            break;            
        case "createDraftFiscalSlip":
			fn_draft(); 
			break;            
        case "saveNonarrivAltDraft":
			gfn_alert("상신되었습니다.");	
			fn_search();
			break;			
    }
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 조회
 *---------------------------------------------------------------------------------------------*/
function fn_doSearch()
{
	if (gfn_dsIsUpdated(ds_detailList))
	{
		// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
        var retValue = gfn_confirm("confirm.before.search");
		if (retValue == false) return;
    }
    
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);
    gfn_clearSortMark(grd_detailList);

    // Dataset을 초기화한다.
    ds_list.clearData();
    ds_detailList.clearData();
    ds_detailList2.clearData();

    var strSvcID    = "selectList";
    var strURL      = "ft/fts/nonarrivAlt/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

