{
	//alert("instance id=[" + instanceId + "]"+" SCGw.GW_DRAFT_STATUS :"+SCGw.GW_DRAFT_STATUS);
	//ds_detailList.setColumn(ds_detailList.rowposition, "signInstanceId", instanceId);	 //결재InstanceId 설정
