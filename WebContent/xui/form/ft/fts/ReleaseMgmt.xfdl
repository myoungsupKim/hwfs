************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	//fn_setUserButton(this, fv_userBtnoption);
	
	fn_addFunctionClose();
	
	// 초기값 셋팅
	fn_init();

	//직인이미지명조회
	var v_fil_cond;
	var v_ds = fn_common_code("'COMPANY_INFO'","","","blk","Y");
	v_fil_cond = "SET1 == 'F' && ATTR01 <= '" + gfn_today() + "'" + " && ATTR02 >= '" + gfn_today() + "'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_JIKIN_NAME,v_ds.name);
	
	if(ds_JIKIN_NAME.rowcount == 1)
	{
		fv_jikin_name = ds_JIKIN_NAME.getColumn(0,"CODE_NAME");
	}
	else
	{
		fv_jikin_name = "";
	}

	//로고이미지명조회
	v_ds.filter("");
	v_fil_cond = "SET1 == 'G' && ATTR01 <= '" + gfn_today() + "'" + " && ATTR02 >= '" + gfn_today() + "'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_LOGO_NAME,v_ds.name);
	
	if(ds_LOGO_NAME.rowcount == 1)
	{
		fv_logo_name = ds_LOGO_NAME.getColumn(0,"CODE_NAME");
	}
	else
	{
		fv_logo_name = "";
	}
	//trace(fv_jikin_name+"-"+fv_logo_name);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// Grid내의 input field를 자동 선택 제어 함수
	FTCommon.setGridAutoSelect (this, true);
	
	ds_search.addRow();
	
	SCCombo.bind2(this, "ComCode", "cbo_term", null, {groupCode:"출고관리기간", useYn:"Y", defVal:""});	 //출고관리기간
	SCCombo.bind2(this, "ComCode", "cbo_search", null, {groupCode:"출고관리검색", useYn:"Y", defVal:""}, SCCombo.ALL);	 //출고관리검색
	SCCombo.bind2(this, "ComCode", "cbo_search00", null, {groupCode:"출고관리검색", useYn:"Y", defVal:""}, SCCombo.ALL);	 //출고관리검색
	SCCombo.bind2(this, "ComCode", "cbo_shipmentTypeCd", null, {groupCode:"SHIPMENT_TYPE_CD", useYn:"Y", defVal:""}, SCCombo.ALL);	 //출고유형
	SCCombo.bind2(this, "ComCode", "cbo_releaseStatusCd", null, {groupCode:"RELEASE_STATUS_CD", useYn:"Y", defVal:""}, SCCombo.ALL);	 //상태
	SCCombo.bind2(this, "ComCode", "cbo_saleAged", null, {groupCode:"SALE_AGED", useYn:"Y", defVal:""}, SCCombo.ALL);	 		 //판매경로
	SCCombo.bind2(this, "ComCode", "cbo_attachDept", null, {groupCode:"ATTACH_DEPT", useYn:"Y", defVal:"", isLast:true, lastEvent:"fn_combo2LastCallback"}, SCCombo.ALL);	 //귀속부서

	// calendar 초기화(해당Form, 값지정, 값변경시 실행할 callBack함수, dataset과 bind, 필수여부)
	var calStartTerm = gfn_subStr(gfn_today(),0,6)+"01";
	cal_startTerm.fn_initMonthCal(this, calStartTerm, "fn_calendarCallBack", ds_search, "startTerm");	
	cal_endTerm.fn_initMonthCal(this, gfn_today(), "fn_calendarCallBack", ds_search, "endTerm");	
	
	//그룹웨어 연동 초기화
    SCGw.init (this);
    
    //그룹웨어 연동 버튼을 설정
    //alert(ds_list.getColumn(ds_list.rowposition, "signProgressStatus"));	   
    SCGw.setUserButton (this, fv_option, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus2"),""));	
    SCGw.setUserButton (this, fv_option2, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus"),""), iOffsetX);	
    SCGw.setUserButton (this, fv_option3, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus4"),""), iOffsetX2);	
}

