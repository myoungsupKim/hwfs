function btn_salsConfirm_onclick(obj:Button,  e:ClickEventInfo)
{
 	if ( !gfn_dsIsUpdated(ds_list) )
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return; 
 	}

	var returnCnt = 0;	//환입마감건
	var normalCnt = 0;  //정상마감건
	for (var i = 0; i < ds_list.rowcount; i++) {
		if (ds_list.getColumn(i, "chk") == "1" && gfn_nvl(ds_list.getColumn(i, "seq"),"") != "") {
			returnCnt++;	
		}
		if (ds_list.getColumn(i, "chk") == "1" && gfn_nvl(ds_list.getColumn(i, "seq"),"") == "") {
			normalCnt++;	
		}
	}
	
	if ( returnCnt > 0 && normalCnt >0 )
	{
		gfn_alert("정상건 과 환입건을 같이 마감할수 없습니다.!");
		return;		
	}
	
	if ( normalCnt > 0 ) 	//정상건일떄 체크
	{
		if ( cbo_search.value != 'C' ||
			(cbo_search.value == 'C' && gfn_isNull(edt_searchContent.value))
			) {
			gfn_alert("판매처를 검색하여 주십시오!");
			return;
		}
		  
		
		var dupCd = "";
		var chkCnt = 0;
		var dupCnt = 0;
		//체크된 항목이 하나도 없으면 경고를 한다.
		for (var i = 0; i < ds_list.rowcount; i++) {
			if (ds_list.getColumn(i, "chk") == "1") {
				if (chkCnt == 0) {
					dupCd = ds_list.getColumn(i, "custCd");
				}
				if (dupCd != ds_list.getColumn(i, "custCd")) {
					dupCnt++;
					break;
				}
				chkCnt++;
			}
		}
		
		if (dupCnt > 0) {
			gfn_alert("다른 판매처가 존재합니다. \n하나의 판매처만 조회하세요.");
			return;
		}
		
		var nChkCnt = ds_list.getCaseCount("chk == 1");
		//alert(ds_list.getRowCount() +"//"+ nChkCnt);
		if ( ds_list.getRowCount() != nChkCnt )
		{
			gfn_alert("거래처별로 전체 매출확정 가능합니다.");
			return;		
		}	
	}
	

	var chkCnt = ds_list.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		grd_list.setCellPos(1); // cellindex
		grd_list.setCellProperty("body", 1, "background", "lightpink");		
		grd_list.setCellProperty("body", 1, "background2", "lightpink");
	
		return;
	} 
	else 
	{
		grd_list.setCellProperty("body", 1, "background", "");		
		grd_list.setCellProperty("body", 1, "background2", "");
	}
	var salsplceCd ="";
	var releaseNums ="";
	var seqs ="";
	var ii =0;
	for ( var i=0; i<ds_list.getRowCount(); i++ )
	{
		if ( (ds_list.getColumn(i, "chk") =="1" ) ) {
			if ( (ds_list.getColumn(i, "releaseStatusCd") !="40" ) )	  //출고확정이 아니면
			{
				gfn_alert("매출마감은 상태가 출고확정에서만 가능합니다.");
				ds_list.rowposition = i;
				grd_list.setCellPos( 0); // cellindex
				grd_list.setFocus();
				grd_list.showEditor(); 
				return;
			}
		
			if ( (ds_list.getColumn(i, "releaseStatusCd") =="50" ) )	  // 매출확정 상태
			{
				gfn_alert("이미 매출확정 되었습니다. 매출확정 취소 후 가능합니다.");
				ds_list.rowposition = i;
				grd_list.setCellPos( 0); // cellindex
				grd_list.setFocus();
				grd_list.showEditor(); 
				ds_list.setColumn(i, "chk", 0);
				return;
			}
			
// 			if ( (ds_list.getColumn(i, "releaseStatusCd") !="40" ) )	  // 출고확정승인이 아니라면
// 			{
// 				gfn_alert("매출확정은 출고확정 상태에서 가능합니다.");
// 				ds_list.rowposition = i;
// 				grd_list.setCellPos( 0); // cellindex
// 				grd_list.setFocus();
// 				grd_list.showEditor(); 
// 				ds_list.setColumn(i, "chk", 0);
// 				return;
// 			} 

			if ( ds_list.getColumn(i, "saleOrderType") =="50" ) {	//부서이관
				gfn_alert("부서이관 건으로 매출확정 대상이 아닙니다.");
				ds_list.rowposition = i;
				grd_list.setCellPos( 0); // cellindex
				grd_list.setFocus();
				grd_list.showEditor(); 
				return;
			}		
			
			releaseNums+= "'"+ds_list.getColumn(i, "releaseNum")+"',";
			seqs+= "'"+ds_list.getColumn(i, "seq")+"',";
		}
	}
	releaseNums = releaseNums.substring(0, releaseNums.length-1);
	seqs = seqs.substring(0, seqs.length-1);
	//alert(releaseNums+"//"+seqs);
	
	var rtVal = gfn_confirm("매출확정 하시겠습니니까?");
	if ( !rtVal == true) return;
	ds_search.setColumn(0, "releaseNums", releaseNums );
	ds_search.setColumn(0, "seqs", seqs );
	ds_search.setColumn(0, "salsConfirmFlag", "CONFIRM" );	//매출확정flag
    var strSvcID    = "save";
    var strURL      = "ft/fts/releaseMgmt/saveSalsConfirmList.xdo";
    var strInDs     = "ds_search=ds_search ds_list=ds_list:A";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);    						
}

