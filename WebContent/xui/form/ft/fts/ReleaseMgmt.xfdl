	}


	if ( chk_srcPrintChk.value == 1 ) //조회출력이면
	{
		var dupCd = "";
		var chkCnt = 0;
		var dupCnt = 0;	
		for (var i = 0; i < ds_list.rowcount; i++) {
			if (chkCnt == 0) {
				dupCd = ds_list.getColumn(i, "custCd");
			}
			if (dupCd != ds_list.getColumn(i, "custCd")) {
				dupCnt++;
				break;
			}
			chkCnt++;
		}
	}
	else
	{
		var dupCd = "";
		var chkCnt = 0;
		var dupCnt = 0;	
		var chkCnt = ds_list.findRow("chk", '1');
		var nChkCnt = ds_list.getCaseCount("chk == 1");

		//체크된 항목이 하나도 없으면 경고를 한다.
		if (chkCnt < 0) {
			gfn_alert("msg.noselect", "체크박스");
			grd_list.setCellPos(1); // cellindex
			grd_list.setCellProperty("body", 1, "background", "lightpink");		
			grd_list.setCellProperty("body", 1, "background2", "lightpink");
			return;
		} else { 
			grd_list.setCellProperty("body", 1, "background", "");		
			grd_list.setCellProperty("body", 1, "background2", "");
		}		
			
		for (var i = 0; i < ds_list.rowcount; i++) {
			if (nChkCnt > 1 && ds_list.getColumn(i, "chk") == "1") {
				if (chkCnt == 0) {
					dupCd = ds_list.getColumn(i, "custCd");
				}
				if (dupCd != ds_list.getColumn(i, "custCd")) {
					dupCnt++;
					break;
				}
				chkCnt++;
			}
		}
	}
	if (dupCnt > 0) {
		gfn_alert("다른 판매처가 존재합니다. \n하나의 판매처만 조회하세요.");
		return;
	}	

	var salsplceCd ="";
	var salsplceNm ="";
	var releaseNums ="";
	var returnStr ="";
	var ii = 0;
	for ( var i=0; i < ds_list.getRowCount(); i++ )
	{
		if (ds_list.getColumn(i, "chk") == "1") {
			if (ii == 0) {
				salsplceCd = ds_list.getColumn(i, "custCd");
				salsplceNm = ds_list.getColumn(i, "custNm");
				returnStr+= " AND A.RELEASE_NUM = '"+ds_list.getColumn(i, "releaseNum")
				         +"'  AND F.SEQ="+gfn_nvl(ds_list.getColumn(i, "seq"),"''") 
				         ;
			}
			releaseNums+= ds_list.getColumn(i, "releaseNum")+",";
			returnStr+= " OR A.RELEASE_NUM = '"+ds_list.getColumn(i, "releaseNum")+"'  AND F.SEQ="+gfn_nvl(ds_list.getColumn(i, "seq"),"''") ;
			ii++;
		}
	}


    var arrParam = new Array();
    arrParam[0] = ds_list.getColumn(0, "custCd");	// 거래처코드
    arrParam[1] = ds_list.getColumn(0, "custNm");	// 거래처명
    arrParam[2] = ds_list.getColumn(ds_list.rowposition, "saleFaxNo");	// Fax No
    arrParam[3] = "sales";	// 구분
	var rtnVal = gfn_dialog("ReleaseFaxSendPop"				// dialogId
                       , "X_FTS::ReleaseFaxSendPop.xfdl"			// Url
                       , {fv_Contents:arrParam}			// 배열
                       , null, null, null, null, null, false, true, -1);

    if (typeof rtnVal != "undefined" && rtnVal != null && gfn_getObjkeyLength(rtnVal) > 0) {
		var rtVal = gfn_confirm("거래명세서 Fax 발송 하시겠습니니까?");
		if (!rtVal == true) return;
	
		ds_search.setColumn(0, "faxNo", rtnVal.faxNo); 
		ds_search.setColumn(0, "salsplceCd", ds_list.getColumn(0, "custCd"));
		ds_search.setColumn(0, "releaseNums", releaseNums);
		ds_search.setColumn(0, "returnStr", returnStr);
		 
		
		var strSvcID    = "dealReqDocPublishFaxTransfer";
		var strURL      = "ft/fts/releaseMgmt/dealReqDocPublishFaxTransfer.xdo";
		var strInDs     = "ds_search=ds_search:A";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		//trace("ds_search >>> " + ds_search.saveXML());

		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
}

