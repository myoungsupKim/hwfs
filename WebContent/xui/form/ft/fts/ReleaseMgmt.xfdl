/*----------------------------------------------------------------------------------------------
 * 설명      : 거래명세서 버튼  클릭시
 *---------------------------------------------------------------------------------------------*/
function printReleaseReg()
{


	
	var salsplceCd ="";
	var releaseNums ="";
	var returnStr ="";
	var ii =0;
	
	//조회출력을 체크했을 경우 그리드에서 체크하지 않아 아래 로직에서 거래처 코드가 할당되지 않으므로 첫번째 Row의 거래처코드를 강제 할당해 준다
	if (gfn_nvl(chk_srcPrintChk.value,"0") == "1") salsplceCd = ds_list.getColumn(0, "custCd");
		
	for ( var i=0; i<ds_list.getRowCount(); i++ )
	{
		if ( (ds_list.getColumn(i, "chk") =="1" ) ) {
			ii++;
			if (ii==1) 
			{
				salsplceCd = ds_list.getColumn(i, "custCd");
				returnStr+= " AND A.RELEASE_NUM = '"+ds_list.getColumn(i, "releaseNum")
				         +"'  AND F.SEQ="+gfn_nvl(ds_list.getColumn(i, "seq"),"''") 
				         ;
			}
			else
			{
				returnStr+= " OR A.RELEASE_NUM = '"+ds_list.getColumn(i, "releaseNum")+"'  AND F.SEQ="+gfn_nvl(ds_list.getColumn(i, "seq"),"''") ;
			}
			if ( gfn_nvl(ds_list.getColumn(i, "seq"),"") == "") {
				releaseNums+= "'"+ds_list.getColumn(i, "releaseNum")+"',";
			}
			
		}
	}
	releaseNums = releaseNums.substring(0, releaseNums.length-1);
	

	//레포트에 전달할 파라메터 생성
	var arrParams	= SCReport.newParams ();
	
	//--------------------------------------------------
	//방법1)서버에서 Query에 의해 처리하는 경우 예
	//(필수) 레포트명
	//SCReport.setParams (arrParams, SCReport.RPT_NAME, "/hwfs/guide/sample/SampleList.ozr");
	SCReport.setParams (arrParams, SCReport.RPT_NAME, "/식재도매/거래명세서.ozr");

	//출력 옵션 설정
	//-창 타이틀 (디폴트는 "ReportViewer")
	SCReport.setParams (arrParams, SCReport.RPT_TITLE, "거래명세서");

	//-Zoom (디폴트는 100)
	//SCReport.setParams (arrParams, SCReport.RPT_ZOOM, "120");

	//레포트에 맞게 전달되는 개발자 정의 파라미터
	//alert(cbo_term.value+"//"+cal_startTerm.fn_getValue()+"//"+cal_endTerm.fn_getValue()+"//"+edt_searchContent.value );
	SCReport.setParams (arrParams, "PARAM1", cbo_term.value);	//기간
	SCReport.setParams (arrParams, "PARAM2", cal_startTerm.fn_getValue() );	//시작일자
	SCReport.setParams (arrParams, "PARAM3", cal_endTerm.fn_getValue() );	//종료일자
	//SCReport.setParams (arrParams, "PARAM4", application.services["svc"].url + "upload/ft/ftp/CompanyJikin.png");
	SCReport.setParams (arrParams, "PARAM4", application.services["svc"].url + "upload/ft/ftp/" + fv_jikin_name );
	SCReport.setParams (arrParams, "PARAM5", edt_searchContent.value);
	SCReport.setParams (arrParams, "PARAM6", salsplceCd); 
	SCReport.setParams (arrParams, "PARAM7", cbo_shipmentTypeCd.value);	//출고유형
