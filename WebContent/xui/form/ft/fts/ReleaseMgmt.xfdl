			{
				ds_list.setColumn(i, "signInstanceId4", instanceId);	 //결재InstanceId 설정
				ds_list.setColumn(i, "signProgressStatus4", SCGw.GW_DRAFT_STATUS);	 //결재진행상태를 상신으로 설정
