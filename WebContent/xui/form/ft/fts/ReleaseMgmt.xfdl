function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.columnid == "purchaseReturnQuantity" || e.columnid == "purchaseReturnWgt")
	{
		var purchaseReturnQuantity = toNumber(gfn_nvl(obj.getColumn(e.row, "purchaseReturnQuantity")));	   //주문잔여수량
		var orgPurchaseReturnQuantity = toNumber(gfn_nvl(obj.getOrgColumn(e.row, "purchaseReturnQuantity")));	   //주문잔여수량
		var releaseQuantity = toNumber(gfn_nvl(obj.getOrgColumn(e.row, "releaseQuantity")));	   //주문잔여수량
	
		//alert(purchaseReturnQuantity +"//"+releaseQuantity);
		if ( purchaseReturnQuantity > releaseQuantity )
		{
			gfn_alert("출고수량보다 큰 환입수량으로 환입할 수 없습니다.");
			obj.setColumn(e.row, "purchaseReturnQuantity", orgPurchaseReturnQuantity ); 	
			return;
		}
		var purchaseReturnWgt = toNumber(gfn_nvl(obj.getColumn(e.row, "purchaseReturnWgt")));	   //주문잔여수량
		var orgPurchaseReturnWgt = toNumber(gfn_nvl(obj.getOrgColumn(e.row, "purchaseReturnWgt")));	   //주문잔여수량
		var realWgt = toNumber(gfn_nvl(obj.getOrgColumn(e.row, "realWgt")));	   //주문잔여수량
		if ( purchaseReturnWgt > realWgt )
		{
			gfn_alert("출고중량보다 큰 환입중량으로 환입할 수 없습니다.");
			obj.setColumn(e.row, "purchaseReturnWgt", orgPurchaseReturnWgt ); 	
			return;
		}

// 		var purchaseReturnDeliverNum = toNumber(gfn_nvl(obj.getColumn(e.row, "purchaseReturnDeliverNum")));	   //주문잔여수량
// 		var orgPurchaseReturnDeliverNum = toNumber(gfn_nvl(obj.getOrgColumn(e.row, "purchaseReturnDeliverNum")));	   //주문잔여수량
// 		if ( purchaseReturnDeliverNum > orgPurchaseReturnDeliverNum )
// 		{
// 			gfn_alert("잔여 환입금액보다 큰 환입금액으로 환입할 수 없습니다.");
// 			obj.setColumn(e.row, "purchaseReturnDeliverNum", orgPurchaseReturnDeliverNum ); 	
// 			return;
// 		}

		//alert(unit+"//"+kgConvertRate+"//"+quantity+"//"+(kgConvertRate * quantity));
		//alert(ds_itemList.getColumn(e.row, "set1"));
		//계산 기준 : KG이면 중량 단위가 BOX나 BDL이면 수량	
		var orderRemainQuantity = toNumber(gfn_nvl(obj.getOrgColumn(e.row, "releaseQuantity")));	   //주문잔여수량
		var orderRemainWgt = toNumber(gfn_nvl(obj.getOrgColumn(e.row, "realWgt")));	   //주문잔여수량
		var saleUprice = toNumber(gfn_nvl(obj.getColumn(e.row, "saleUprice")));	   //판매단가
		var avgWgt = orderRemainWgt/orderRemainQuantity;	//평균중량
		var buyUprice = toNumber(gfn_nvl(obj.getColumn(e.row, "buyUprice")));	   //매입단가
		
				
		//alert(orderRemainWgt+"/"+orderRemainQuantity+"="+avgWgt);			
		if(e.columnid == "purchaseReturnQuantity" )
		{
			ds_list.setColumn(e.row, "purchaseReturnWgt", Math.round((purchaseReturnQuantity * avgWgt),2) ); 			
		}
		var set1 = gfn_nvl(obj.getColumn(e.row, "set1"));	   //단위
		if ( set1 == "KG"  )
		{
			ds_list.setColumn(e.row, "purchaseReturnDeliverNum", Math.round((saleUprice * toNumber(gfn_nvl(obj.getColumn(e.row, "purchaseReturnWgt"))))) ); 			
			ds_list.setColumn(e.row, "purchaseReturnCst", Math.round((buyUprice * toNumber(gfn_nvl(obj.getColumn(e.row, "purchaseReturnWgt"))))) );
		}
		else
		{
			ds_list.setColumn(e.row, "purchaseReturnDeliverNum", Math.round((saleUprice * toNumber(gfn_nvl(obj.getColumn(e.row, "purchaseReturnQuantity"))))) ); 			
			ds_list.setColumn(e.row, "purchaseReturnCst", Math.round((buyUprice * toNumber(gfn_nvl(obj.getColumn(e.row, "purchaseReturnQuantity"))))) ); 			
		}
	}
	if( e.columnid == "chk" )
	{
		if ( chk_srcPrintChk.value == 1 ) //조회출력이면
		{
			//gfn_alert("조회출력으로 체크되어 있으면 체크 할 수 없습니다.");
			if ( ds_list.getColumn( e.row, "chk") == "1" ) 
			{
				ds_list.setColumn(e.row, "chk", "0" );
			}
		}
	}
}

