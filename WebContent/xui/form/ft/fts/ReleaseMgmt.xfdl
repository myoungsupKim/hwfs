/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		//품의 조회
        case "select" :
			
			//그룹웨어 연동 버튼을 설정
			//alert(gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus2"),"")); 
			SCGw.setUserButton (this, fv_option, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus2"),""));	
			SCGw.setUserButton (this, fv_option2, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus"),""), iOffsetX);	
			SCGw.setUserButton (this, fv_option3, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus4"),""), iOffsetX2);	
            break;
        
        // 저장
        case "save" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				fn_search();
			}
            break; 
        case "releaseApprovReq" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("출고승인 요청 되었습니다.");	// 저장 되었습니다.
				fn_search();
			}
            break; 
        case "releaseApprovReqCancel" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("출고승인 요청취소 되었습니다.");	// 저장 되었습니다.
				fn_search();
			}
            break; 
        case "releaseApprov" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("팀장승인 되었습니다.");	// 저장 되었습니다.
				fn_search();
			}
            break; 
        case "releaseConfirm" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("출고확정 되었습니다.");	// 저장 되었습니다.
				fn_search();
			}
            break; 
        case "releaseConfirmCancel" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("출고확정 취소 되었습니다.");	// 저장 되었습니다.
				fn_search();
			}
            break; 
        case "releasePurchaseReturn" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("환입 되었습니다.");	// 저장 되었습니다.
				fn_search();
			}
            break; 
        case "releaseRollCancel" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("출고등록 취소되었습니다.");	// 저장 되었습니다.
				fn_search();
			}
            break; 
            
        case "delete" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.delete.success");	// 삭제되었습니다
				fn_search();
			}
            break;
        case "upriceChg" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("단가 변경 되었습니다.");	// 삭제되었습니다
				fn_search();
			}
            break;
        case "releaseWhgChg" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("중량 변경 되었습니다.");
				fn_search();
			}
            break;
        case "printDealReqDocPublish" :
			if (fv_iProcessCnt > 0) {
				printReleaseReg();
			}
            break;
        case "releaseApprovCancel" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("팀장승인 취소되었습니다.");
				fn_search();
			}
            break;            
        case "releaseBuyCostExcept" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("매입원가 차감 I/F 되었습니다.");
				fn_search();
			}
            break; 
        case "releaseBuyCostExceptCancel" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("매입원가 차감 취소 I/F 요청 되었습니다.");
				fn_search();
			}
            break; 
        case "releaseBuyCostInc" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("매입원가 증가 I/F 되었습니다.");
				fn_search();
			}
            break; 
        case "releaseBuyCostIncCancel" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("매입원가 증가 취소 I/F 요청 되었습니다.");
				fn_search();
			}
            break; 
        case "releaseDraftBuyCostExcept" :
			fn_draft(); 
            break; 
        case "saveBuyCostExceptDraft":
			gfn_alert("상신되었습니다.");	
			fn_search();
			break;            
        case "printReleaseReqCertificate" :
			if (fv_iProcessCnt > 0) {
				printReleaseReqCertificate();
			}
            break;
            
        case "releaseChgDept" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("부서이관 I/F 요청 되었습니다.");
				fn_search();
			}
			break;
        case "lendingChk" :		//여신체크
			//alert(fv_creditAmount+"//"+fv_rmnAmt+"//"+fv_preAmt+"//"+fv_reqAmt+"//"+fv_approvAmt+"//"+fv_confirmAmt);
			//2019.04.03 김명섭 총여신금액에 계약이행예치금(preAmt1) 제외 (컴플라이언스 이성희과장 전산의뢰 요청사항)
			//var totCreditAmount = toNumber(fv_creditAmount)+toNumber(fv_preAmt1)+toNumber(fv_preAmt2)+toNumber(fv_creditOverAmount)-toNumber(fv_rmnAmt);
			var totCreditAmount = toNumber(fv_creditAmount)+toNumber(fv_preAmt2)+toNumber(fv_creditOverAmount)-toNumber(fv_rmnAmt);
			var totInsideAmount = toNumber(fv_releaseAmt)+toNumber(fv_reqAmt)+toNumber(fv_approvAmt)+toNumber(fv_confirmAmt);
			//alert("gfn_numFormat(fv_preAmt1) :"+gfn_numFormat(fv_preAmt1)+"chk_preAmt1Yn:"+chk_preAmt1Yn.value);
			var str = "여신금액 :" + gfn_numFormat(fv_creditAmount);
				    if (chk_preAmt1Yn.value == "1") {
					    str += "\n"+"계약이행예치금 :" + gfn_numFormat(fv_preAmt1);
					}
					str += "\n"+"임시여신 :" + gfn_numFormat(fv_creditOverAmount)
					str += "\n"+"선수잔액 :" + gfn_numFormat(fv_preAmt2)
					+ "\n"+"외상매출잔액 :" + gfn_numFormat(fv_rmnAmt)
					+ "\n"+"총여신금액   :" + gfn_numFormat(totCreditAmount)
					+ "\n"
					
					+ "\n"+"출고요청금액 :" + gfn_numFormat(fv_releaseAmt)
					+ "\n"+"승인요청요금액 :" + gfn_numFormat(fv_reqAmt)
					+ "\n"+"팀장승인금액 :" + gfn_numFormat(fv_approvAmt)
					+ "\n"+"확정금액 :" + gfn_numFormat(fv_confirmAmt)
					+ "\n"+"기출고금액 :" + gfn_numFormat(totInsideAmount);
			if ( totCreditAmount < totInsideAmount )
			{
				var gap = totInsideAmount - totCreditAmount;
				str += "\n기출고금액 ["+gfn_numFormat(totInsideAmount)+"] 총여신금액["+gfn_numFormat(totCreditAmount)+"] 대비 여신["+gfn_numFormat(gap)+"]이 부족합니다.";
			}
			gfn_alert(str);
			
            break;
        case "releaseFaxTransfer" :	// 출고증 Fax발송
			gfn_alert("출고증 Fax 발송 완료 하였습니다.");
			break;
		case "dealReqDocPublishFaxTransfer" :	// 거래명세서증 Fax발송
			gfn_alert("거래명세서 Fax 발송 완료 하였습니다.");
			break;
    }
    fn_addFunctionClose();
    SCGw.setUserButton (this, fv_option, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus2"),""));	
    SCGw.setUserButton (this, fv_option2, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus"),""), iOffsetX);	
    SCGw.setUserButton (this, fv_option3, gfn_nvl(ds_list.getColumn(ds_list.rowposition, "signProgressStatus4"),""), iOffsetX2);	

	//체크된상태로 초기화
	ds_list.updatecontrol = false;
	for ( var i=0; i<ds_list.getRowCount(); i++ ) {
		if ( gfn_nvl(ds_list.getColumn(i, "chk"),"") ==  "1" )
		{
			bSucc = ds_list.setRowType(i, Dataset.ROWTYPE_UPDATE);
		}
	}
	ds_list.updatecontrol = true;	
}

