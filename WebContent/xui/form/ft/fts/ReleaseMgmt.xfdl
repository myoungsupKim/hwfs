	if (dupCnt > 0) {
		gfn_alert("다른 판매처가 존재합니다. \n하나의 판매처만 조회하세요.");
		return;
	}

	var nChkCnt = ds_list.getCaseCount("chk == 1");
	//alert(ds_list.getRowCount() +"//"+ nChkCnt);
	if ( ds_list.getRowCount() != nChkCnt )
	{
		gfn_alert("거래처별로 전체 취소가능합니다.");
		return;		
	}
		
	var msg = "";
	for ( var i=0; i<ds_list.getRowCount(); i++ )
	{
		if ( (ds_list.getColumn(i, "chk") =="1" ) ) {
			if ( toNumber(ds_list.getColumn(i, "receiptNumCnt")) > 1 )	  // 묶여서 마감된 건이 있으면
			{
				msg = "같이 묶여서 마감 처리된 건은 같이 묶여서 마감 취소 됨니다.\n\n";
				break;
			} 
		}
	} 
	
	for ( var i=0; i<ds_list.getRowCount(); i++ )
	{
		if ( (ds_list.getColumn(i, "chk") =="1" ) ) {
			if ( ( gfn_nvl(ds_list.getColumn(i, "ddddCloseYn")) =="Y" ) )
			{ 
			
				gfn_alert("출고일자["+gfn_getFormat(gfn_nvl(ds_list.getColumn(i, "releaseDate")),"@@@@-@@-@@") + "] 일마감 처리가 되었습니다. \n영업회계 일마감 화면에서 마감취소 후 매출확정 취소 가능합니다.");
				ds_list.rowposition = i;
				grd_list.setCellPos( 0); // cellindex
				grd_list.setFocus();
				grd_list.showEditor(); 
				return;
			} 
		}
	} 
			
	var rtVal = gfn_confirm(msg+"매출확정 취소 하시겠습니니까?");
	if ( !rtVal == true) return;
		
	ds_search.setColumn(0, "releaseNums", releaseNums );
	ds_search.setColumn(0, "salsConfirmFlag", "CANCEL" );	//매출확정 취소 flag
	
    var strSvcID    = "save";
    var strURL      = "ft/fts/releaseMgmt/saveSalsConfirmList.xdo";
    var strInDs     = "ds_search=ds_search ds_list=ds_list:A";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);    						
}
