/*----------------------------------------------------------------------------------------------
 * 설명      : 출고승인요청
 *---------------------------------------------------------------------------------------------*/
function btn_releaseApprovReq_onclick(obj:Button,  e:ClickEventInfo)
{
	var chkCnt = ds_list.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		grd_list.setCellPos(1); // cellindex
		grd_list.setCellProperty("body", 1, "background", "lightpink");		
		grd_list.setCellProperty("body", 1, "background2", "lightpink");
	
		return;
	} 
	else 
	{
		grd_list.setCellProperty("body", 1, "background", "");		
		grd_list.setCellProperty("body", 1, "background2", "");
	}

	var chkVal = false;		//손절매 체크
	var chkVal2 = false;	//저가판매 체크
	for ( var i=0; i<ds_list.getRowCount(); i++ )
	{
		if ( (ds_list.getColumn(i, "chk") =="1" ) ) {
			if ( (ds_list.getColumn(i, "releaseStatusCd") !="10" ) && ds_list.getColumn(i, "releaseStatusCd") !="15"  )	  //출고승인요청, 거래명세서 출력
			{
				gfn_alert("출고등록 상태에서만 출고승인 요청 가능합니다.");
				ds_list.rowposition = i;
				grd_list.setCellPos( 0); // cellindex
				grd_list.setFocus();
				grd_list.showEditor(); 
				return;
			}
			//손절매 체크 : 매입원가 > 입력한 판매가
			//alert(ds_list.getColumn(i, "buyCst") +"//"+ds_list.getColumn(i, "saleAmt")+"///"+ ds_list.getColumn(i, "realSaleAmt"));
			if ( ( ds_list.getColumn(i, "realSaleAmt")> 0 && ds_list.getColumn(i, "buyCst") > ds_list.getColumn(i, "realSaleAmt")  ) )	  
			{
				chkVal = true;
				break;
			}
			//저가판매 체크 : 계산된 판매가 > 입력한 판매가
			if ( ( ds_list.getColumn(i, "realSaleAmt")> 0 &&  ds_list.getColumn(i, "saleAmt") > ds_list.getColumn(i, "realSaleAmt")  ) )	  
			{
				chkVal2 = true;
				break;
			}
		} 
	}
	
	//alert(chkVal+"//"+chkVal2);
	var str="";
	if ( chkVal )
	{
		str += "매입원가가 입력한 판매가 보다 큽니다(손절매).";
	}
	if ( chkVal2 )
	{
		str += "\n계산된 판매가가 입력한 판매가 보다 큽니다.(저가판매).";
	}

	var dupCd = "";
	var chkCnt = 0;
	var dupCnt = 0;
	//체크된 항목이 하나도 없으면 경고를 한다.
	for (var i = 0; i < ds_list.rowcount; i++) {
		if (ds_list.getColumn(i, "chk") == "1") {
			if (chkCnt == 0) {
				dupCd = ds_list.getColumn(i, "custCd");
			}
			if (dupCd != ds_list.getColumn(i, "custCd")) {
				dupCnt++;
				break;
			}
			chkCnt++;
		}
	}
	
	if (dupCnt > 0) {
		gfn_alert("다른 판매처가 존재합니다. \n하나의 판매처만 조회하세요.");
		return;
	}	

	var rtVal = gfn_confirm(str+"\n출고승인요청 하시겠습니니까?");
	
	if ( !rtVal == true) return;
	
    var strSvcID    = "releaseApprovReq";
    var strURL      = "ft/fts/releaseMgmt/releaseApprovReq.xdo";
    var strInDs     = "ds_search=ds_search ds_list=ds_list:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
	ds_list.updatecontrol = false;
	bSucc = ds_list.setRowType(ds_list.rowposition, Dataset.ROWTYPE_UPDATE);
	ds_list.updatecontrol = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);		
}

