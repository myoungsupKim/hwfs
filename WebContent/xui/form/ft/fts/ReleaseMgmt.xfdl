			gfn_alert("msg.noselect", "체크박스");
			grd_list.setCellPos(1); // cellindex
			grd_list.setCellProperty("body", 1, "background", "lightpink");		
			grd_list.setCellProperty("body", 1, "background2", "lightpink");
			return;
		} else { 
			grd_list.setCellProperty("body", 1, "background", "");		
			grd_list.setCellProperty("body", 1, "background2", "");
		}		
		for (var i = 0; i < ds_list.rowcount; i++) {
			if (nChkCnt >1 && ds_list.getColumn(i, "chk") == "1") {
				if (chkCnt == 0) {
					dupCd = ds_list.getColumn(i, "whCd");
				}
				if (dupCd != ds_list.getColumn(i, "whCd")) { 
					dupCnt++;
					break;
				}
				chkCnt++;
			}
		}
	} 
	if (dupCnt > 0) {
		gfn_alert("다른 창고가 존재합니다. \n하나의 창고만 선택하여 주십시오.");
		return;
	}	
	 
	for ( var i=0; i<ds_list.getRowCount(); i++ )
	{
		if (chk_srcPrintChk.value == 1 )	//조회출력이면
		{
			if ( (Number(ds_list.getColumn(i, "releaseStatusCd")) < 30 ) )	  // 팀장승인이 아니라면
			{
				gfn_alert("출고증 발생은 상태가 팀장 승인 상태에서 가능합니다.");
				ds_list.rowposition = i;
				grd_list.setCellPos( 0); // cellindex
				grd_list.setFocus();
				grd_list.showEditor(); 
				ds_list.setColumn(i, "chk", 0);
				return;
			} 
		}
		else
		{
			if ( (ds_list.getColumn(i, "chk") == "1" ) ) {
				if ( (Number(ds_list.getColumn(i, "releaseStatusCd")) < 30 ) )	  // 팀장승인이 아니라면
				{
					gfn_alert("출고증 발생은 상태가 팀장 승인 상태에서 가능합니다.");
					ds_list.rowposition = i;
					grd_list.setCellPos( 0); // cellindex
					grd_list.setFocus();
					grd_list.showEditor(); 
					ds_list.setColumn(i, "chk", 0);
					return;
				} 
			}
		}
	}
	
	var whCds ="";
	var whNms ="";
	var releaseNums ="";
	var ii = 0;
	for ( var j = 0; j < ds_list.getRowCount(); j++ )
	{
		if (chk_srcPrintChk.value == 1 )	//조회출력이면
		{
			if (ii == 0) {
				whCds += ds_list.getColumn(j, "whCd") + ",";
				whNms += ds_list.getColumn(j, "whNm") + ",";
			}
			ii++;
			releaseNums += ds_list.getColumn(j, "releaseNum") + ",";
		}
		else
		{
			if ( ds_list.getColumn(j, "chk") == "1" ) {
				if (ii == 0) {
					whCds += ds_list.getColumn(j, "whCd") + ",";
					whNms += ds_list.getColumn(j, "whNm") + ",";
				}
				ii++;
				releaseNums += ds_list.getColumn(j, "releaseNum") + ",";
			}
		}
	}
	whCds = whCds.substring(0, whCds.length-1);
	whNms = whNms.substring(0, whNms.length-1);
	releaseNums = releaseNums.substring(0, releaseNums.length-1);
	
	// 검사목적과 해지사유를 입력하는 Popup을 호출 한다.
    var arrParam = new Array();
    arrParam[0] = whCds;	// 창고코드 
    arrParam[1] = whNms;	// 창고명
    arrParam[2] = ds_list.getColumn(ds_list.rowposition, "whFaxNo");	// Fax No
    arrParam[3] = "release";	// 구분
	var rtnVal = gfn_dialog("ReleaseFaxSendPop"				// dialogId
                       , "X_FTS::ReleaseFaxSendPop.xfdl"			// Url
                       , {fv_Contents:arrParam}			// 배열
                       , null, null, null, null, null, false, true, -1);

    if (typeof rtnVal != "undefined" && rtnVal != null && gfn_getObjkeyLength(rtnVal) > 0) {
		var rtVal = gfn_confirm("출고증 Fax 발송 하시겠습니니까?");
		if ( !rtVal == true) return;
		
		ds_search.setColumn(0, "faxNo", rtnVal.faxNo);
		
		ds_search.setColumn(0, "releaseReqCert", prtGubun);
		ds_search.setColumn(0, "releaseNums", releaseNums );
		ds_search.setColumn(0, "whCds", whCds ); 
		ds_search.setColumn(0, "whNm", whNms ); 
		ds_search.setColumn(0, "srcPrintChk", gfn_nvl(chk_srcPrintChk.value,"0")  ); 
		
		
		
		var strSvcID    = "releaseFaxTransfer";
		var strURL      = "ft/fts/releaseMgmt/releaseFaxTransfer.xdo";
		var strInDs     = "ds_search=ds_search:A";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		//trace("ds_search >>> " + ds_search.saveXML());

		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
}

