 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_list))
 	{
		// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		var retValue = gfn_confirm("confirm.before.search");
 		if (retValue == false) return;
 	}
	
	if (gfn_isNull(cal_startTerm.fn_getValue()) || gfn_isNull(cal_endTerm.fn_getValue()))
	{
		gfn_alert("조회일자를 입력하여 주십시오!");
		return;
	}
	if (cal_startTerm.fn_getValue() > cal_endTerm.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);
    
    //코드목록을 초기화한다.
    ds_list.clearData();

    var strSvcID    = "select";
    var strURL      = "ft/fts/releaseMgmt/releaseRegList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	//v_ds_check 미사용시 변경사항 체크
 	if ( !gfn_dsIsUpdated(ds_list) )
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	// 저장시 Validation Check
	//if (gfn_validCheck(ds_list, ds_listValid) == false)	return;

	for ( var i=0; i<ds_list.rowcount; i++ ) 
	{
		if (  ds_list.getColumn(i, "chk") =="1"  )	//체크
		{
			if ( ds_list.getColumn(i, "releaseDate") <=0 )
			{
				gfn_alert("출고일자를 입력하세요.");
				grd_list.setCellPos( 7) // cellindex
				grd_list.setFocus();
				grd_list.showEditor(); 
				return;
			}
			
			if ( ds_list.getColumn(i, "unit") != "KG" )
			{
				if ( ds_list.getColumn(i, "releaseExpectQuantity") <=0 )
				{
					gfn_alert("출고예정수량을 입력하세요.");
					ds_list.rowposition = i;
					grd_list.setCellPos( 15); // cellindex
					grd_list.setFocus();
					grd_list.showEditor(); 
					return;
				}
			}

			if ( ds_list.getColumn(i, "unit") == "KG" )
			{

				if ( ds_list.getColumn(i, "releaseExpectWgt") <=0 )
				{
					gfn_alert("출고예정중량을 입력하세요.");
					grd_list.setCellPos( 16); // cellindex
					grd_list.setFocus();
					grd_list.showEditor(); 
					return;
				}
			}
		}
	}	
	
	if (!gfn_confirm("confirm.before.save")) return false;
	
    var strSvcID    = "save";
    var strURL      = "ft/fts/releaseMgmt/saveReleaseReg.xdo";
    var strInDs     = "ds_search=ds_search ds_list=ds_list:A";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}


/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		//품의 조회
        case "select" :
            break;
        
        // 저장
        case "save" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				fn_search();
			}
            break;
        case "delete" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.delete.success");	// 삭제되었습니다
				fn_search();
			}
            break;
        case "releaseApprovReq" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				//fn_search();
			}
            break;
            
    }
}


