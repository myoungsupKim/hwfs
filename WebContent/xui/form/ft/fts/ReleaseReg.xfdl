/*----------------------------------------------------------------------------------------------
 * 설명      :같은 출고처끼리 체크
 *---------------------------------------------------------------------------------------------*/
function fn_chkDupReleaseplceCd(row)
{
	var focReleaseplceCd;
	if ( ds_list.getColumn(row, "chk") ==1 )
	{
		focReleaseplceCd = gfn_nvl(ds_list.getColumn(row, "releaseplceCd"),"") ;
	}
	
	var focRow = row;
	for ( var i=0; i<ds_list.rowcount; i++ ) 
	{	
		if ( ds_list.getColumn( i, "releaseStatusCd") == "" && ds_list.getColumn( i, "chk") == 1 && typeof focReleaseplceCd != "undefined") 
		{
			if ( focReleaseplceCd != gfn_nvl(ds_list.getColumn(i, "releaseplceCd"),"") )
			{
				gfn_alert("출고처가 서로 다릅니다. 같은 출고처만 체크 가능합니다.");
				
				ds_list.setColumn(focRow, "chk", 0);
				ds_list.setColumn(focRow, "releaseExpectQuantity", ds_list.getOrgColumn(focRow, "releaseExpectQuantity"));
				ds_list.setColumn(focRow, "releaseExpectWgt", ds_list.getOrgColumn(focRow, "releaseExpectWgt"));
				return;				
			}
		} 
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 그리드 컬럼 변경시
 *---------------------------------------------------------------------------------------------*/
function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.columnid == "releaseExpectQuantity" || e.columnid == "releaseExpectWgt")    //주문수량 변경시
	{	
		
		var releaseExpectQuantity = parseFloat(ds_list.getColumn(e.row, "releaseExpectQuantity"));
		var unit = gfn_nvl(ds_list.getColumn(e.row, "unit"));
		var kgConvertRate = gfn_nvl(ds_list.getColumn(e.row, "kgConvertRate"));

 		var orderRemainQuantity = gfn_nvl(ds_list.getColumn(e.row, "orderRemainQuantity"));
		var orderRemainWgt = gfn_nvl(ds_list.getColumn(e.row, "orderRemainWgt"));


		var orgReleaseExpectQuantity = parseFloat(ds_list.getOrgColumn(e.row, "releaseExpectQuantity"));
		
		var releaseExpectWgt = parseFloat(ds_list.getColumn(e.row, "releaseExpectWgt"));
		var orgReleaseExpectWgt = parseFloat(ds_list.getOrgColumn(e.row, "releaseExpectWgt"));
		
		var orderQuantity = parseFloat(ds_list.getColumn(e.row, "orderQuantity"));
		var orderWgt = parseFloat(ds_list.getColumn(e.row, "orderWgt"));
		if ( releaseExpectQuantity > orgReleaseExpectQuantity )
		{
			gfn_alert("출고예정수량이 주문수량보다 많습니다.");
			ds_list.setColumn( e.row, "releaseExpectQuantity", ds_list.getOrgColumn(e.row, "releaseExpectQuantity") )
			ds_list.setColumn( e.row, "releaseExpectWgt", ds_list.getOrgColumn(e.row, "releaseExpectWgt") )
			return;
		}

		if ( releaseExpectWgt > orgReleaseExpectWgt )
		{
			gfn_alert("출고예정중량이 주문중량보다 많습니다.");
			ds_list.setColumn( e.row, "releaseExpectQuantity", ds_list.getOrgColumn(e.row, "releaseExpectQuantity") )
			ds_list.setColumn( e.row, "releaseExpectWgt", ds_list.getOrgColumn(e.row, "releaseExpectWgt") )
			return;
		}
		
		if(e.columnid == "releaseExpectQuantity" )
		{
			//계산 기준 : KG이면 중량 단위가 BOX나 BDL이면 수량	
			var orgOrderRemainWgt = gfn_nvl(obj.getOrgColumn(ds_list.rowposition, "releaseExpectQuantity"));	   //출고예정수량
			var orgOrderRemainQuantity = gfn_nvl(obj.getOrgColumn(ds_list.rowposition, "releaseExpectWgt"));	   //출고예정중량
			
			var avgWgt = orgOrderRemainQuantity/orgOrderRemainWgt;	//평균중량
			//alert(orgOrderRemainQuantity+"/"+orgOrderRemainWgt+"="+avgWgt+"///"+releaseExpectQuantity);			
			ds_list.setColumn(e.row, "releaseExpectWgt", Math.round((releaseExpectQuantity * avgWgt),2) ); 		
		}

		if ( releaseExpectQuantity != orgReleaseExpectQuantity || releaseExpectWgt != orgReleaseExpectWgt )
		{
			ds_list.setColumn( e.row, "chk", "1")	
			//fn_chkDupReleaseplceCd(e.row);
				
		} else
		{
			ds_list.setColumn( e.row, "chk", "0")		
		}
		
		
	}
	
	if(e.columnid == "saleUprice" )	  //판매단가 변경시
	{
		var orgSaleUprice = parseFloat(ds_list.getOrgColumn(e.row, "saleUprice"));
		var saleUprice = parseFloat(ds_list.getColumn(e.row, "saleUprice"));
		var releaseExpectQuantity = parseFloat(ds_list.getColumn(e.row, "releaseExpectQuantity"));
		var releaseExpectWgt = parseFloat(ds_list.getColumn(e.row, "releaseExpectWgt"));
		
		//if ( orgSaleUprice != saleUprice )	  //판매단가가 변경 됬다면
		//{
			ds_list.setColumn( e.row, "orgSaleUprice", saleUprice )
			
			
			var unit = gfn_nvl(ds_list.getColumn(e.row, "unit"));
			if ( unit == 'KG') 
			{
				ds_list.setColumn( e.row, "saleCst", saleUprice * releaseExpectWgt)
			} 		
			else
			{
				ds_list.setColumn( e.row, "saleCst", saleUprice * releaseExpectQuantity)
			}
			
		//}
		
	}
	if(e.columnid == "saleCst" )	  //판매금액 변경시
	{
		var orgSaleCst = parseFloat(ds_list.getOrgColumn(e.row, "saleCst"));
		var saleCst = parseFloat(ds_list.getColumn(e.row, "saleCst"));
		//alert(orgSaleUprice +"//"+ saleUprice);
		
		if ( orgSaleCst != saleCst )	  //판매금액가 변경 됬다면
		{
			ds_list.setColumn( e.row, "orgSaleCst", saleCst )
			
		}
	}
}

