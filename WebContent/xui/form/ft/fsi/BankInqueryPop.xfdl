/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";
include "lib::LimsUtil.xjs";

/****************************************************************************************
 * FORM 변수 선언 영역
 ****************************************************************************************/
var fv_iProcessCnt = 0;  	// 저장건수를 저장한다.(Controller Class에서만 할당함.)
var argArgs;

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
    // Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
    gfn_formOnLoad(this);		// 검체유형 GRID에는 공통 이벤트 제외

    argArgs = this.getOwnerFrame().fv_Contents;
    if (typeof argArgs != "undefined" && argArgs != null)
    {
		edt_search.value = argArgs[1];
	}
    
    // 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 리턴받을 데이터셋 지정
	ds_search.setColumn(0, "groupCode", "FM0005");
	ds_search.setColumn(0, "useYn", "Y");
	ds_search.setColumn(0, "retDataSet", "ds_list");
	
	fn_search();
}

/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	fn_doSearch();	// 조회
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0)
	{
		return;
	}

    switch(strSvcID)
    {
		// 전체 조회
        case "selectList" :
			grd_list.setFocus();
            break;
    }
}

/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 처리
 *---------------------------------------------------------------------------------------------*/
function fn_doSearch()
{
 	// Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);

    //코드목록을 초기화한다.
 	ds_list.clearData();

 	//trace(ds_search.saveXML());

    var strSvcID    = "selectList";
    var strURL      = "ls/lcm/scArticleMgmt/selectCodeDtlList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 확인버튼 클릭
 *---------------------------------------------------------------------------------------------*/
function btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_Close();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 취소버튼 클릭
 *---------------------------------------------------------------------------------------------*/
function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	this.close();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 그리드 더블클릭 시
 *---------------------------------------------------------------------------------------------*/
function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	fn_Close();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 그리드 선택된 값을 부모창으로 전달한다.
 *---------------------------------------------------------------------------------------------*/
function fn_Close()
{
    var arr = LimsUtil.setPopupParam(ds_list);
    //trace(gfn_jsonObjectToString(arr));
	this.close(arr);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 닫기버튼 클릭
 *---------------------------------------------------------------------------------------------*/
function btn_close_onclick(obj:Button,  e:ClickEventInfo)
{
	this.close();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 소분류 입력 후 엔터 클릭 시
 *---------------------------------------------------------------------------------------------*/
function edt_search_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) {
		edt_search.updateToDataset();
		fn_doSearch();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 그리드에서 엔터 시 선택된 값을 부모창으로 전달한다.
 *---------------------------------------------------------------------------------------------*/
function grd_list_onkeydown(obj:Grid, e:KeyEventInfo)
{
	if (e.keycode==13) {
		fn_Close();	
	}
}
]]></Script>
