/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_list))
 	{
		// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		var retValue = gfn_confirm("confirm.before.search");
 		if (retValue == false) return;
 	}
 	
	if (gfn_isNull(cal_startTerm.fn_getValue()) || gfn_isNull(cal_endTerm.fn_getValue()))
	{
		gfn_alert("조회일자를 입력하여 주십시오!");
		return;
	}
	if (cal_startTerm.fn_getValue() > cal_endTerm.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);
    
    //코드목록을 초기화한다.
    ds_list.clearData(); 
    
    ds_search.setColumn(0, "maskAuth", SCAuth.getMaskAuth(this));

    var strSvcID    = "select";
    var strURL      = "ft/fsi/freezeWhCommMgmt/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*------------------------------------------------------------
	1. Function ID : fn_Save()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_save()
{
	//v_ds_check 미사용시 변경사항 체크
 	if ( !gfn_dsIsUpdated(ds_list))
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	
 	var chkCnt = 0;
	for ( var i = 0; i < ds_list.rowcount; i++ ) 
	{
		if ( ds_list.getColumn(i, "chk") == "1" )
		{
			chkCnt++;
		}
	}
	if (chkCnt == 0) {
		gfn_alert("대상을 선택하여 주십시오.");
		return;
	}
	
	// 저장시 Validation Check
	if (gfn_validCheck(ds_list, ds_listValid) == false) return;
	
	if (!gfn_confirm("confirm.before.save")) return false;
	
	ds_search.setColumn(0, "actionGubun", "SAVE");
	
    var strSvcID    = "save";
    var strURL      = "ft/fsi/freezeWhCommMgmt/saveList.xdo";
    var strInDs     = "ds_search=ds_search:A ds_list=ds_list:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*------------------------------------------------------------
	1. Function ID : fn_delete()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_delete()
{
	//v_ds_check 미사용시 변경사항 체크
 	if ( !gfn_dsIsUpdated(ds_list)) {
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	
 	var chkCnt = 0;
	for ( var i = 0; i < ds_list.rowcount; i++ ) 
	{
		if ( ds_list.getColumn(i, "chk") == "1" ) {
			chkCnt++;
		}
	}
	if (chkCnt == 0) {
		gfn_alert("대상을 선택하여 주십시오.");
		return;
	}
	
	if (!gfn_confirm("confirm.before.delete")) return false;
	
	//선택된 행에 대하여 삭제 혹은 사용여부의 값을 변경한다.
	var chkDelCnt = 0;
	for ( var i = ds_list.getRowCount() - 1; i >= 0; i-- ) 
	{
		if ( ds_list.getColumn(i, "chk") == "1" ) {
			if (ds_list.getRowType(i) == Dataset.ROWTYPE_INSERT) {
				ds_list.deleteRow(i);
				chkDelCnt++;
			}
		}
	}
	if (chkCnt == chkDelCnt) return;

	ds_search.setColumn(0, "actionGubun", "DELETE");
	
    var strSvcID    = "save";
    var strURL      = "ft/fsi/freezeWhCommMgmt/saveList.xdo";
    var strInDs     = "ds_search=ds_search:A ds_list=ds_list:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

