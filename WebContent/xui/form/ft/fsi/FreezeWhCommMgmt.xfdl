/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		//품의 조회
        case "select" :
			trace(ds_list.saveXML());
            break;
        // 저장
        case "save" :
			if (fv_iProcessCnt > 0) {
				if (ds_search.getColumn(0, "actionGubun") == "DELETE") {
					gfn_alert("msg.delete.success");	// 삭제 되었습니다.
				} else {
					gfn_alert("msg.save.success");	// 저장 되었습니다.
				}
				fn_search();
			}
            break;
        // 일괄저장
        case "saveAll" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				fn_search();
			}
            break;
        case "createDraftFiscalSlip":
			fn_draft();
			break;
	    case "saveDraft":
			gfn_alert("결재 상신 되었습니다.");
			fn_search();
			break;
        case "createFiscalSlip":
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
			break;
        case "cancelFiscalSlip":
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
			break;
    }
    
    //그룹웨어 연동 버튼을 설정
    SCGw.setUserButton (this, fv_option, ds_list.getColumn(ds_list.rowposition, "signProgressStatus"));
}

