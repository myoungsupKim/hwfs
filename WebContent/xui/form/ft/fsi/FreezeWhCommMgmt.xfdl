/*----------------------------------------------------------------------------------------------
 * 설명      : XML 파일 선택
 *---------------------------------------------------------------------------------------------*/
function btn_xmlFileSearch_onclick(obj:Button,  e:ClickEventInfo)
{
	if (!gfn_confirm("XML파일 작업을 위해 그리드의 내용을 Clear 하겠습니다.\n\nXML파일 선택 작업을 하시겠습니까?")) return false;
	
	// Dataset 초기화
	ds_list.clearData();
	
	var arrFile = FileDialog0.open ( "XML FileOpen", FileDialog.MULTILOAD );
	if (gfn_isNull(arrFile)) return;
	if (arrFile.length < 1) return;
	
	// 선택한 파일 오픈
	for (var i=0; i < arrFile.length; i++) 
	{
		arrFile[i].open( VirtualFile.openRead | VirtualFile.openText );
		var strBuf = arrFile[i].read(-1, "utf-8");
		fn_actionXml(strBuf);
		arrFile[i].close();
	}
}

