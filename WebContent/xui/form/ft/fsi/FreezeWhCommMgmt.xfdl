	ds_list.setColumn(nRowIdx, "amt", gfn_replace(gfn_replace(SCXml.getNodeValue(sXML, "SpecifiedMonetarySummation/GrandTotalAmount"), "<![CDATA[", ""), "]]]]><![CDATA[>", ""));		// 총 금액
