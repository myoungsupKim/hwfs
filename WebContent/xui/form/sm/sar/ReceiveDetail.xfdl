function fn_search()
{	
	// 조회시 Validation Check
	if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;
	
	/*
	if(gfn_isNull(ds_search.getColumn(0,"tmCd"))){
		if(gfn_isNull(ds_search.getColumn(0,"custCd"))){
			alert("부서/CC 미선택 시 고객사는 필수입니다. ");
			return;
		}
	}
	if(gfn_isNull(ds_search.getColumn(0,"ccCd"))){
		if(gfn_isNull(ds_search.getColumn(0,"custCd"))){
			alert("부서/CC 미선택 시 고객사는 필수입니다. ");
			return;
		}
	}
	
	if(gfn_isNull(ds_search.getColumn(0,"custCd"))){
			alert("고객사는 필수입니다. ");
			return;
	}
	*/
	
	if(gfn_isNull(ds_search.getColumn(0,"tmCd"))){
		if(gfn_isNull(ds_search.getColumn(0,"ccCd"))){	
			if(gfn_isNull(ds_search.getColumn(0,"custCd"))){
				alert("고객사는 필수입니다. ");
				return;
			}
		}
	}
	
	if(!gfn_isNull(ds_search.getColumn(0,"tmCd"))){
		if(!gfn_isNull(ds_search.getColumn(0,"ccCd"))){	
			if(gfn_isNull(ds_search.getColumn(0,"custCd"))){
				//기간제한 	
				var nDiffDays = gfn_getDiffDay(ds_search.getColumn(0,"receiveDateFr"),ds_search.getColumn(0,"receiveDateTo"));
				if (  nDiffDays > 365)
				{
					gfn_alert("CC전체 선택, 고객사 미선택 시 1년 초과는 제한합니다.");
					return;
				}
			}
		}
	}
	
	
	
	
	
	// Grid sort 표시 초기화(필수)
    //gfn_clearSortMark(ds_list);

    var strSvcID    = "search";
    var strURL      = "sm/sar/receiveMgnt/selectDetailList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list1=ds_list1 ds_list2=ds_list2 ds_list3=ds_list3";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

