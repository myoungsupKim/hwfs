 * 설명      : 가수금 팝업 데이터 적용 - 디버깅을 위해 copyData를 사용하지 않음
 *---------------------------------------------------------------------------------------------*/
function fn_setDataList(dsObj, cRow, rowNum){	
	var nRow = 0;
	var sumApplyAmt = 0;   //가수금잔액

	if(dsObj.rowcount > 0){
		gv_altNum	= "";

		for(var i=0; i<dsObj.rowcount; i++){
			nRow = ds_listSuspense.addRow();
			ds_listSuspense.setColumn(nRow, "r",                rowNum);
			ds_listSuspense.setColumn(nRow, "buCd",             dsObj.getColumn(i, "buCd"));
			ds_listSuspense.setColumn(nRow, "sapSuspenseNum",   dsObj.getColumn(i, "sapSuspenseNum"));
			ds_listSuspense.setColumn(nRow, "receiveDate",      dsObj.getColumn(i, "receiveDate"));    //입금일자
			ds_listSuspense.setColumn(nRow, "bankCd",           dsObj.getColumn(i, "bankCd"));
			ds_listSuspense.setColumn(nRow, "bankCdNm",         dsObj.getColumn(i, "bankCdNm"));
			ds_listSuspense.setColumn(nRow, "bankAcctNo",       dsObj.getColumn(i, "bankAcctNo"));     //계좌번호
			ds_listSuspense.setColumn(nRow, "currency",         dsObj.getColumn(i, "currency"));
			ds_listSuspense.setColumn(nRow, "receiveGuestNm",   dsObj.getColumn(i, "receiveGuestNm"));
			ds_listSuspense.setColumn(nRow, "ccCd",             dsObj.getColumn(i, "ccCd"));
			ds_listSuspense.setColumn(nRow, "ccNm",             dsObj.getColumn(i, "ccNm"));
			ds_listSuspense.setColumn(nRow, "muCd",             dsObj.getColumn(i, "muCd"));
			ds_listSuspense.setColumn(nRow, "muNm",             dsObj.getColumn(i, "muNm"));
			ds_listSuspense.setColumn(nRow, "sapSlipNum",       dsObj.getColumn(i, "sapSlipNum"));
			ds_listSuspense.setColumn(nRow, "classCd",          dsObj.getColumn(i, "classCd"));
			ds_listSuspense.setColumn(nRow, "notesPublishDate", dsObj.getColumn(i, "notesPublishDate"));
			ds_listSuspense.setColumn(nRow, "notesExpireDate",  dsObj.getColumn(i, "notesExpireDate"));
			ds_listSuspense.setColumn(nRow, "notesNum",         dsObj.getColumn(i, "notesNum"));
			ds_listSuspense.setColumn(nRow, "publisherNm",      dsObj.getColumn(i, "publisherNm"));
			ds_listSuspense.setColumn(nRow, "recdStatus",       dsObj.getColumn(i, "recdStatus"));
			ds_listSuspense.setColumn(nRow, "receiveAmt",       dsObj.getColumn(i, "receiveAmt"));     //입금금액
			ds_listSuspense.setColumn(nRow, "applyAmt",         dsObj.getColumn(i, "applyAmt"));       //적용금액
			ds_listSuspense.setColumn(nRow, "balAmt",           dsObj.getColumn(i, "balAmt"));
			ds_listSuspense.setColumn(nRow, "altNum",           dsObj.getColumn(i, "altNum"));         //대체번호
			ds_listSuspense.setColumn(nRow, "altSeq",           dsObj.getColumn(i, "altSeq"));         //대체순번
			ds_listSuspense.setColumn(nRow, "suspenseAltType",  dsObj.getColumn(i, "suspenseAltType"));
			ds_listSuspense.setColumn(nRow, "suspenseAltTypeNm",dsObj.getColumn(i, "suspenseAltTypeNm"));
			ds_listSuspense.setColumn(nRow, "altDate",          dsObj.getColumn(i, "altDate"));
			ds_listSuspense.setColumn(nRow, "sapCustCd",        dsObj.getColumn(i, "sapCustCd"));
			ds_listSuspense.setColumn(nRow, "sapSendYn",        dsObj.getColumn(i, "sapSendYn"));
			ds_listSuspense.setColumn(nRow, "remark",           dsObj.getColumn(i, "remark"));
			ds_listSuspense.setColumn(nRow, "rowStatus",        dsObj.getColumn(i, "rowStatus"));
			//가수금잔액합산
			sumApplyAmt += dsObj.getColumn(i, "applyAmt");
			
			if(gfn_isNull(dsObj.getColumn(i, "altNum"))){
				ds_listSuspense.setColumn(nRow, "altDate",          	fv_sToday);
				ds_listSuspense.setColumn(nRow, "suspenseAltType",  	"");
				ds_listSuspense.setColumn(nRow, "suspenseAltTypeNm",  	"");
			}
		}
		ds_list.setColumn(cRow, "chk", "1");
		ds_list.setColumn(cRow, "receiveAmt", sumApplyAmt);
	}
}

