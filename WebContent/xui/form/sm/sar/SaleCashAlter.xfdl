//대체처리
function btn_saveok_onclick(obj:Button,  e:ClickEventInfo)
{
    if (ds_list.rowcount > 0){
        for (var cnt=0;cnt<ds_list.rowcount;cnt++){
            var listOccurAmt     = ds_list.getColumn(cnt, "occurAmt");
            var listReceiveAmt   = ds_list.getColumn(cnt, "receiveAmt");
            var listAltType      = ds_list.getColumn(cnt, "suspenseAltType");        //영업대체유형
            var listSuspenseDate = ds_list.getColumn(cnt, "suspenseDate");   //입금일자
            var listRow          = ds_list.getColumn(cnt, "r");              //행번호
            
            if (ds_list.getColumn(cnt, "chk") == "1") {
				if (listReceiveAmt > listOccurAmt){
					alert(cnt + "행 입금액이 발생금액보다 큽니다");
					return;
				}
				if (listReceiveAmt < listOccurAmt){
					alert(cnt + "행 입금액이 발생금액보다 큽니다");
					return;
				}
				//계좌번호에 따른 입금금액 체크
				if (ds_listSuspense.rowcount > 0){
					for (var i=0;i<ds_listSuspense.rowcount;i++){
						//가수금대체위해 rowStatus 표기
						var sAltNum   = ds_listSuspense.getColumn(i, "altNum");     //대체번호
						var iApplyAmt = ds_listSuspense.getColumn(i, "applyAmt");   //대체금액
						
						if (iApplyAmt > 0){
							if (gfn_isNull(sAltNum)){
								ds_listSuspense.setColumn(i, "rowStatus", "INSERT");   //가수금대체 INSERT
							} else {
								ds_listSuspense.setColumn(i, "rowStatus", "UPDATE");   //가수금대체 UPDATE
							}
						} else {
							if(!gfn_isNull(sAltNum)){
								ds_listSuspense.setColumn(i, "rowStatus",	"DELETE");	//수정모드에서 적용금액이 0 이면 단건 대체취소
							}
						}
						
						//여기부터 입금액과 적용금액 비교체크
						var row        = ds_listSuspense.getColumn(i,"r");            //행번호
						var bankAcctNo = ds_listSuspense.getColumn(i,"bankAcctNo");   //계좌번호
						var receiveAmt = ds_listSuspense.getColumn(i,"receiveAmt");   //입금금액
						var applyAmt   = ds_listSuspense.getColumn(i,"applyAmt");     //적용금액
						
						//영업대체유형, 입금일자 가수금대체정보 셋팅
						 if (listRow == row){
							 ds_listSuspense.setColumn(i,"suspenseAltType", listAltType);     //가수금대체유형
							 ds_listSuspense.setColumn(i,"receiveDate", listSuspenseDate);   //입금일자
						 }
						
						var fRow;
						
						fRow = ds_listSuspenseValid.findRow("bankAcctNo", bankAcctNo, 0, -1);
						
						if (fRow == -1){
							var nRow = ds_listSuspenseValid.addRow();
							ds_listSuspenseValid.setColumn(nRow, "r"         , row);
							ds_listSuspenseValid.setColumn(nRow, "bankAcctNo", bankAcctNo);
							ds_listSuspenseValid.setColumn(nRow, "receiveAmt", receiveAmt);
							ds_listSuspenseValid.setColumn(nRow, "applyAmt"  , applyAmt);
						} else {
							var vRow        = ds_listSuspenseValid.getColumn(fRow,"r");            //행번호
							var vBankAcctNo = ds_listSuspenseValid.getColumn(fRow,"bankAcctNo");   //계좌번호
							var vReceiveAmt = ds_listSuspenseValid.getColumn(fRow,"receiveAmt");   //입금금액
							var vApplyAmt   = ds_listSuspenseValid.getColumn(fRow,"applyAmt");     //적용금액
							
							if (row == vRow && bankAcctNo == vBankAcctNo){
								ds_listSuspenseValid.setColumn(fRow,"receiveAmt", vReceiveAmt + receiveAmt);   //입금금액
								ds_listSuspenseValid.setColumn(fRow,"applyAmt"  , vApplyAmt + applyAmt);   //적용금액
							} else {
								ds_listSuspenseValid.setColumn(fRow,"applyAmt"  , vApplyAmt + applyAmt);   //적용금액
							}
						}
					}
					
					for (var j = 0;j<ds_listSuspenseValid;j++){
						var sumReceiveAmt = ds_listSuspenseValid.getColumn(j,"receiveAmt");   //입금금액
						var sumApplyAmt   = ds_listSuspenseValid.getColumn(j,"applyAmt");     //적용금액
						
						if(sumApplyAmt > sumReceiveAmt){
							alert("적용금액이 입금액보다 큽니다");
							return;
						}
					}
				}
			}
		}
	}
	fn_save();
}

