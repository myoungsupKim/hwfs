function btn_bankAcctNo_onclick(obj:Button,  e:ClickEventInfo){
	var strCcCd = UpjangSearchComponent01.getUpjangCode();
	var strCcNm = UpjangSearchComponent01.getUpjangName();
	
	if(gfn_isNull(strCcCd)){
		gfn_alert("CC를 선택하세요.");
		return false;
	}
	
	var strBuCdYn	= "N";
	var param = {pCcCd:strCcCd, pCcNm:strCcNm, pBuCdYn:strBuCdYn};
	
	var retVal = gfn_dialog("SuspenseReturnRequestPop"
							,"X_SAR::SuspenseReturnRequestPop.xfdl"
							,param	//objArgumentList
							,-1		//Width
							,-1		//Height
							,""		//Style
							,-1		//Left
							,-1		//Top
							,false	//Modeless
							,true	//ShowTitle
							,"-1"	//Align
							,true);	//Resizable
				
	if (retVal instanceof Object){
		edt_bankAcctNoSch.value = retVal.bankAcctNo;
		edt_bankAcctNoOrg.value = retVal.bankAcctNoOrg;
	}	
}

