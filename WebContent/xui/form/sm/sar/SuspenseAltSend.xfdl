	fv_objColInfo = gfn_getDatasetInfo (ds_list);

    gfn_transaction(this
					,"search"
					,"sm/sar/suspenseSapSend/selectList.xdo"
					,""
					,"ds_list=ds_list"
					,""
					,"fn_callBack"
					,true);
}

