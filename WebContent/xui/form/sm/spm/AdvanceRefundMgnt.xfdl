function fn_clearSearch(){
	var nRow= ds_search.addRow();
    //ds_search.setColumn(nRow, "altDateFrom",    fv_sToday);
    //ds_search.setColumn(nRow, "altDateTo",      fv_sToday);
    ds_search.setColumn(nRow, "altNum",  "");
    //ds_search.setColumn(nRow, "altTypeCd",      " ");
    //ds_search.setColumn(nRow, "custCd",         g_CustCd);	//227031
    //ds_search.setColumn(nRow, "custNm",         g_CustNm);	//한양식품
    ds_search.setColumn(nRow, "muCd",           g_Mu_Cd);
    ds_search.setColumn(nRow, "muNm",           g_Mu_Nm);
    ds_search.setColumn(nRow, "tmCd",           g_Tm_Cd);
    ds_search.setColumn(nRow, "tmNm",           g_Tm_Nm);
    ds_search.setColumn(nRow, "ccCd",           g_Cc_Cd);
    ds_search.setColumn(nRow, "ccNm",           g_Cc_Nm);
    //ds_search.setColumn(nRow, "upjang",         g_Upjang);
    //ds_search.setColumn(nRow, "upjangnm",       g_UpjangNm);
    ds_search.setColumn(nRow, "typeGrpCd",      "008");		//영업유형그룹
    
	cal_altDateFromSch.fn_initMonthCal(this, gfn_addDate(fv_sToday, -30), null, ds_search, "altDateFrom");
	cal_altDateToSch.fn_initMonthCal(this, fv_sToday, null, ds_search, "altDateTo");
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 선수금 대체 팝업 호출
 * gv_altNum : 선수금 대체번호 (전역변수)
 *---------------------------------------------------------------------------------------------*/
function fn_openPoup(arg){

	gv_altNum = "";		//전역변수 초기화
	
	var arrArg = new Array();
	
	//신규모드
	if(arg == "INIT"){
		arrArg[0] = ds_search.getColumn(0, "muCd");
		arrArg[1] = ds_search.getColumn(0, "muNm");
		arrArg[2] = ds_search.getColumn(0, "tmCd");
		arrArg[3] = ds_search.getColumn(0, "tmNm");
		arrArg[4] = ds_search.getColumn(0, "ccCd");
		arrArg[5] = ds_search.getColumn(0, "ccNm");
		arrArg[6] = ds_search.getColumn(0, "custCd");
		arrArg[7] = ds_search.getColumn(0, "custNm");
		arrArg[8] = "";
		arrArg[9] = "";
		gv_altNum = "";

	//수정모드
	}else if(arg == "ROW"){
		if(ds_list.rowcount > 0){
			arrArg[0] = "";
			arrArg[1] = "";
			arrArg[2] = "";
			arrArg[3] = "";
			arrArg[4] = "";
			arrArg[5] = "";
			arrArg[6] = "";
			arrArg[7] = "";
			arrArg[8] = ds_list.getColumn(ds_list.rowposition, "altNum");					//대체번호
			arrArg[9] = ds_list.getColumn(ds_list.rowposition, "advanceAltOccurClass");		//대체발생유형
			gv_altNum = ds_list.getColumn(ds_list.rowposition, "altNum");	//대체번호가 있을 경우
		}
	}else{
		return;
	}

	gfn_dialog(	"AdvanceRefundMgntPop"
				,"X_SPM::AdvanceRefundMgntPop.xfdl"
				,{arrSchVal:arrArg}	//objArgumentList
				,-1		//Width
				,-1		//Height
				,""		//Style
				,-1		//Left
				,-1		//Top
				,false	//Modeless
				,true	//ShowTitle
				,"-1"	//Align
				,true);	//Resizable
}
 
