/*----------------------------------------------------------------------------------------------
 * 설명      : 선수금 대체/대체취소 처리(PROC/CANCEL) - 저장
 *---------------------------------------------------------------------------------------------*/
function fn_saveAdvanceAlt(arg){

	//대체처리/취소 Vailidation 체크
	if(!fn_chkSaveInfo(arg)) return;

	//선수금대체 Master/Detail Setting
	fn_setAdvanceAltInfo(arg);
	
	//선수금대체처리
	if(arg == "PROC"){

		// v_ds_check 미사용시 변경사항 체크
		if(gfn_dsIsUpdated(ds_search) == false && gfn_dsIsUpdated(ds_list) == false){
			gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
			return false;
		}

		if(gfn_confirm("선수금 대체처리 하시겠습니까?")){
			//선수금 잔액체크
			gfn_getAdvanceAltYn(this, "getAdvanceAltYn", "ds_list");		
			//gfn_saveAdvanceAltProc(this, "saveAdvanceAltProc", "ds_search", "ds_list");
		}
		
	//선수금대체취소처리
	}else if(arg == "CANCEL"){
		//전표발행여부 체크
		for(var i=0; i<ds_list.rowcount; i++){
			if(ds_list.getColumn(i, "journalzYn") != "N"){
				gfn_alert("이미 전표가 발행된 선수금대체정보는 대체처리/취소할 수 없습니다.");
				ds_list.rowposition = i;
				return false;
			}
		}
			
		if(gfn_confirm("선수금 대체취소 하시겠습니까?")){
			gfn_saveAdvanceAltCancel(this, "saveAdvanceAltCancel", "ds_search", "ds_list");
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 대체처리/취소 Vailidation 체크
 *---------------------------------------------------------------------------------------------*/
function fn_chkSaveInfo(arg){
	var iOccurAmt = 0;
	var iApplyAmt = 0;
	
	if(ds_list.rowcount == 0){
		gfn_alert("msg.search.nodata");	// 조건에 해당하는 자료가 없습니다.
		return false;
	}
		
	if(arg == "PROC"){	//대체처리

		if(cmb_altTypeCd.value == "G02"){	//대체유형 G02 : +- 선수금 처리
			for(var i=0; i<ds_list.rowcount; i++){
				//ds_list.setColumn(i, "applyAmt", ds_list.getColumn(i, "occurAmt"));
				//ds_list.setColumn(i, "balAmt", 0);
				iOccurAmt += ds_list.getColumn(i, "occurAmt");
			}
			if(iOccurAmt != 0){
				gfn_alert("잔액의 합이 0 이 아니므로\n[+- 선수금 처리]를 할 수 없습니다.");
				ds_list.reset();
				return false;
			}
		}
		
		for(var i=0; i<ds_list.rowcount; i++){
			iApplyAmt += ds_list.getColumn(i, "applyAmt");
		}
		
		if(cmb_altTypeCd.value != "G02"){
			if(iApplyAmt == 0){
				gfn_alert("적용금액의 합이 0 이므로 대체처리할 수 없습니다.");
				//ds_list.reset();
				return false;
			}
		}
		/*
		if (gfn_isNull(cal_occurDateFromSch.text)){
			gfn_alert("msg.noinput", "발생일자");	// $1 을(를) 입력해 주십시요.
			cal_occurDateFromSch.setFocus();
			return false;
		}
		*/
		if (gfn_isNull(edt_custCd.value)){
			gfn_alert("msg.noselect", "고객사");	// $1 을(를) 선택해 주십시요.
			//edt_custCode.setFocus();
			return false;
		}
		if (gfn_isNull(MuSearchComponent01.getMuCode())){
			gfn_alert("msg.noselect", "사업부");	// $1 을(를) 선택해 주십시요.
			return false;
		}
		if (gfn_isNull(TmSearchComponent01.getTmCode())){
			gfn_alert("msg.noselect", "팀");	// $1 을(를) 선택해 주십시요.
			return false;
		}
		if (gfn_isNull(UpjangSearchComponent01.getUpjangCode())){
			gfn_alert("msg.noselect", "CC");	// $1 을(를) 선택해 주십시요.
			return false;
		}
		if (gfn_isNull(cal_altDate.text)){
			gfn_alert("msg.noselect", "대체일자");	// $1 을(를) 선택해 주십시요.
			cal_altDate.setFocus();
			return false;
		}
		if (gfn_isNull(cmb_altTypeCd.value)){
			gfn_alert("msg.noselect", "대체유형");	// $1 을(를) 선택해 주십시요.
			cmb_altTypeCd.setFocus();
			return false;
		}
		if (gfn_isNull(cmb_advanceAltOccurClass.value)){
			gfn_alert("msg.noselect", "대체발생구분");	// $1 을(를) 선택해 주십시요.
			cmb_advanceAltOccurClass.setFocus();
			return false;
		}
		if (gfn_isNull(edt_remark.value)){
			gfn_alert("msg.noinput", "적요");	// $1 을(를) 입력해 주십시요.
			edt_remark.setFocus();
			return false;
		}
	}
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 선수금대체(PROC)/취소(CANCEL) Setting
 *---------------------------------------------------------------------------------------------*/
function fn_setAdvanceAltInfo(arg){
	
	var iApplyAmt 	= 0;
	var sAltNum		= "";
	var sRowStatus	= "";
	
	//선수금대체
	if(arg == "PROC"){
		
		//신규/수정 구분
		if(gfn_isNull(gv_altNum)) sRowStatus = "INSERT";
		else sRowStatus = "UPDATE";
		
		//선수금대체마스터 세팅
		ds_search.setColumn(0, "rowStatus",	sRowStatus);
		ds_search.setColumn(0, "altAmt",	edt_applyAmtSum.value);
		ds_search.setColumn(0, "typeGrpCd",	"007");

		//선수금마스터, 선수금대체디테일 세팅
		for(var i=0; i<ds_list.rowcount; i++){
			iApplyAmt	= ds_list.getColumn(i, "applyAmt");
			sAltNum		= ds_list.getColumn(i, "altNum");

			if(ds_list.getRowType(i) > 1){	//행 타입(1:Normal, 2:insert, 4:Update, 8:Delete)
			
				if(iApplyAmt > 0){
					
					if(gfn_isNull(sAltNum)){
						ds_list.setColumn(i, "rowStatus",	"INSERT");
						ds_list.setColumn(i, "altNum", 		gv_altNum);
					}
					
					//입력 데이터 세팅
					ds_list.setColumn(i, "altDate",				cal_altDate.text);					// 대체일자
					ds_list.setColumn(i, "altTypeCd",			cmb_altTypeCd.value);				// 대체유형
					ds_list.setColumn(i, "advanceAltOccurClass",cmb_advanceAltOccurClass.value);	// 대체발생구분
					ds_list.setColumn(i, "remark",				edt_remark.value);					// 적요
				}else{
					if(!gfn_isNull(sAltNum)){
						ds_list.setColumn(i, "rowStatus",	"DELETE");	//수정모드에서 적용금액이 0 이면 단건 대체취소
					}
				}
			}
		}
		
		//신규일 경우 조회조건데이터를 복사한다.
		if(sRowStatus == "INSERT"){
			ds_search.setColumn(0, "muCd", 			ds_search.getColumn(0,"inqueryMuCd"));
			ds_search.setColumn(0, "tmCd", 			ds_search.getColumn(0,"inqueryTmCd"));
			ds_search.setColumn(0, "ccCd", 			ds_search.getColumn(0,"inqueryCcCd"));
			ds_search.setColumn(0, "upjangCd", 		ds_search.getColumn(0,"inqueryUpjangCd"));
			ds_search.setColumn(0, "custCd", 		ds_search.getColumn(0,"inqueryCustCd"));
			ds_search.setColumn(0, "custNm", 		ds_search.getColumn(0,"inqueryCustNm"));
			ds_search.setColumn(0, "advanceTypeCd",	ds_search.getColumn(0,"inqueryAdvanceTypeCd"));
			ds_search.setColumn(0, "advanceNum", 	ds_search.getColumn(0,"inqueryAdvanceNum"));
		}
		
	//선수금대체취소
	}else if(arg == "CANCEL"){
		ds_search.setColumn(0, "recdStatus", "D");
		for(var i=0; i<ds_list.rowcount; i++){
			if(!gfn_isNull(ds_list.getColumn(i, "altNum"))){
				ds_list.setColumn(i, "recdStatus", "D");
				ds_list.setColumn(i, "rowStatus",  "CANCEL");	//전체 대체취소
			}
		}
	}
}

