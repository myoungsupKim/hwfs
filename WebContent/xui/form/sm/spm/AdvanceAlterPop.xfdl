/*
//달력 초기화
*/
function fn_calendarFromCallBack()
{
	if(gfn_isNull(ds_search.getColumn(0,"inqueryCcCd"))){
		alert("업장 또는 CC를 선택해 주세요.");
		ds_search.setColumn(0, "altDate", fv_sToday);
		return false;
	}
	mfn_GetCcMonthClsYn("C", ds_search.getColumn(0,"inqueryCcCd"), gfn_subStr(ds_search.getColumn(0,"altDate"), 0, 6), "01");
	if (ds_common.getColumn(0, 0) == "Y")
	{
		g_Message("EE", "해당월은 월마감이 완료되어 작업할 수 없습니다.");
		ds_search.setColumn(0, "altDate", fv_taxDay);
	//	return false; 
	}
	ds_search.setColumn(0, "inqueryEndDate", ds_search.getColumn(0, "altDate"));	
}
/*달력 초기화*/
function fn_chgCalDate(){
	if (gfn_isNull(ds_search.getColumn(0,"inqueryCcCd"))){
		ds_search.setColumn(0, "altDate", fv_sToday);
		ds_search.setColumn(0, "inqueryEndDate", fv_sToday);
	}else{
		mfn_GetCcMonthClsYn("C", ds_search.getColumn(0,"inqueryCcCd"), gfn_subStr(gfn_addMonth(fv_sToday, -1), 0, 6), "01");

		if (ds_common.getColumn(0, 0) == "N")
		{
			fv_taxDay = gfn_getLastDate(gfn_addMonth(fv_sToday, -1));
			ds_search.setColumn(0, "altDate", fv_taxDay);
			ds_search.setColumn(0, "inqueryEndDate", fv_taxDay);
		}else{
			ds_search.setColumn(0, "altDate", fv_sToday);
			ds_search.setColumn(0, "inqueryEndDate", fv_sToday);
		}
	}
}
