function fn_custBeforeEvent(p){
    //FS사업부인경우 2002 : ScoUpjangCust
	if(MuSearchComponent01.getMuCode() == "2002"){
		if(gfn_isNull(ds_search.getColumn(0,"inqueryUpjangCd"))){
			gfn_alert("업장을 선택해 주세요.");
			return;
		}
		p.setQid("ScoUpjangCust");
		p.setUpjang(ds_search.getColumn(0,"inqueryUpjangCd"));
	}else{
		p.setQid("StCust");
		p.setUpjang("");
	}
	return true;
}

