/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg){
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0){
		return;
	}
	
    switch(strSvcID){

   		// 대체유형Combo 조회
        case "cmb_altTypeCd" :
			ds_altTypeCd.filter("typeCd != 'G05' && typeCd != 'G06' && typeCd != 'G07'");
			
            if(ds_altTypeCd.rowcount < 2){
				if(gfn_isNull(ds_altTypeCd.getColumn(0, "typeCd"))){
					
					//데이터가 없을 경우 전체조회
					ds_altTypeCd.clearData();
					gfn_setSalesTypeCd(this, "altTypeCdSch2", "ds_altTypeCd", "007");
				}
            }else{
				cmb_altTypeCd.index = 0;
            }
            break;

     	// 대체유형Combo 조회
        case "altTypeCdSch2" :
			ds_altTypeCd.filter("typeCd != 'G05' && typeCd != 'G06' && typeCd != 'G07'");
			ds_altTypeCd.insertRow(0);
			ds_altTypeCd.setColumn(0, "typeCd", "");
			ds_altTypeCd.setColumn(0, "typeNm", "선택");
			cmb_altTypeCd.index = 0;
            break;

        case "altClassCd" :
			ds_altClassCd.filter("code != 'TO03' && code != 'TO04' && code != 'TO05'");
			ds_altClassCd.insertRow(0);
			ds_altClassCd.setColumn(0, "code" ,"");
			ds_altClassCd.setColumn(0, "codeName" ,"선택");
			break;

        //조회
        case "search" :
			
			//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
			if(ds_list.getRowCount() == 0) gfn_loadDatasetInfo (ds_list, fv_objColInfo);
			fn_formSetInit(bCreateMode);
            break;

        //조회조건 조회 후 조회 (수정모드일때 사용)
        case "selectAdvanceAltMst" :

			var inqueryStartDate	= ds_search.getColumn(0, "inqueryStartDate");
			var inqueryEndDate		= ds_search.getColumn(0, "inqueryEndDate");
			var altDate				= ds_search.getColumn(0, "altDate");
			
			cal_occurDateFromSch.fn_initMonthCal(this, inqueryStartDate, null, ds_search, "inqueryStartDate", "Y");
			cal_occurDateToSch.fn_initMonthCal(this, inqueryEndDate, null, ds_search, "inqueryEndDate");
			cal_altDate.fn_initMonthCal(this, altDate, "fn_calendarFromCallBack", ds_search, "altDate", "Y");
			var orgAltTypeCd = ds_search.getColumn(0, "altTypeCd");
			
			//콤보조회
			fn_setCombo();
			
			fn_search();
			
			//조회 후 대체유형 세팅
			ds_search.setColumn(0, "altTypeCd", orgAltTypeCd);
			
			if (ds_search.getColumn(0, "inqueryMuCd") == "2002"){
				UpjangSearchComponent01.visible = false;
				stc_cccdTitle.visible = false;
				stc_upjangTitle.visible = true;
				UpjangSearchComponent00.visible = true;				
			}else{
				UpjangSearchComponent01.visible = true;
				stc_cccdTitle.visible = true;
				stc_upjangTitle.visible = false;
				UpjangSearchComponent00.visible = false;			
			}
            break;
           
        //선수금 잔액체크
		case "getAdvanceAltYn" :

			if(fv_advanceAltYn == "N"){
				gfn_alert("선수금 잔액이 부족합니다.");
				return;
			}else{
				//alert(ds_list.rowcount);
				ds_list.updatecontrol = false;
				for(var i=0; i<ds_list.rowcount; i++){
					ds_list.setRowType(i, Dataset.ROWTYPE_INSERT);
				}
				//trace(ds_list.saveXML);
				//선수금 대체 처리
				gfn_saveAdvanceAltProc(this, "saveAdvanceAltProc", "ds_search", "ds_list");
			}
			break;
			            
        //대체처리    
		case "saveAdvanceAltProc" :
			gfn_alert("선수금 대체처리가 완료 되었습니다.");
			ds_list.updatecontrol = true;
			this.opener.ds_search.setColumn(0, "altDateFrom",ds_search.getColumn(0, "inqueryStartDate"));
			this.opener.ds_search.setColumn(0, "altDateTo",ds_search.getColumn(0, "inqueryEndDate"));
			this.opener.ds_search.setColumn(0, "muCd",ds_search.getColumn(0, "inqueryMuCd"));
			this.opener.ds_search.setColumn(0, "muNm",ds_search.getColumn(0, "inqueryMuNm"));
			this.opener.ds_search.setColumn(0, "tmCd",ds_search.getColumn(0, "inqueryTmCd"));
			this.opener.ds_search.setColumn(0, "tmNm",ds_search.getColumn(0, "inqueryTmNm"));
			this.opener.ds_search.setColumn(0, "ccCd",ds_search.getColumn(0, "inqueryCcCd"));
			this.opener.ds_search.setColumn(0, "ccNm",ds_search.getColumn(0, "inqueryCcNm"));
			this.opener.fn_search();
			//parent.form.fn_search();
			this.close();
			break;
            
        //대체취소
		case "saveAdvanceAltCancel" :
			gfn_alert("선수금 대체취소가 완료 되었습니다.");
			this.opener.fn_search();
			//parent.form.fn_search();
			this.close();
			break;
            
    }
}

