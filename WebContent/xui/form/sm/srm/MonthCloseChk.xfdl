 *---------------------------------------------------------------------------------------------*/
function fn_tmBeforeEvent(obj)
{
	obj.qoMuCd = MuSearchComponent01.getMuCode();	//cbo_mu.value;
	obj.qoUseYn = "Y";
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 업장의 팝업을 호출하기 전에 발생되는 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_upjangBeforeEvent(obj, cls)
{
	cls.setTmCd(TmSearchComponent01.getTmCode());	//cls.setTmCd(edt_tm.getTmCode());
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 부서 변경시  CC 초기화 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_tmChanged()
{
    ds_search.setColumn(0, "ccCd", "");
    ds_search.setColumn(0, "ccNm", "");
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 마감, 미마감 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_monthClose(obj:Button,  e:ClickEventInfo)
{
	// 저장시 Validation Check
	if( !gfn_validCheck(ds_search, ds_searchValid, true) ) return;

	if( ds_list.findRow("chk", "1") < 0 )
	{
		gfn_alert("msg.err.grid.noselect", "" );	// 선택된 항목이 없습니다.
		return false;
	}

	var closeMonth = ds_search.getColumn(0, "closeMonth");

	var yy = closeMonth.substr(0, 4);
	var mm = closeMonth.substr(4, 2);

	if( obj.name == "btn_close" )
	{
		if( !gfn_confirm("선택한 업장의 " + yy + "년 " + mm + "월 마감처리 하시겠습니까?") ) return false;
	}
	else if( obj.name == "btn_closeCancel" )
	{
		if( !gfn_confirm("선택한 업장의 " + yy + "년 " + mm + "월 마감취소 하시겠습니까?") ) return false;
	}

	// 체크된 데이터를 전송할 데이터셋에 담는다.
	ds_saveList.clearData();
	for( var i = 0; i < ds_list.rowcount; i++ )
	{
		if( ds_list.getColumn(i, "chk") == "1" )
		{
			var nRow = ds_saveList.addRow();
			ds_saveList.copyRow(nRow, ds_list, i);
			if( obj.name == "btn_close" )
			{
				ds_saveList.setColumn(nRow, "type01", "Y");
			}
			else if( obj.name == "btn_closeCancel" )
			{
				ds_saveList.setColumn(nRow, "type01", "N");
			}

			ds_saveList.setColumn(nRow, "cuser", g_EmpNo);
			ds_saveList.setColumn(nRow, "uuser", g_EmpNo);
		}
	}

	// SAP 월마감 체크
	if( obj.name == "btn_closeCancel" )
	{
		if( !fn_sapCloseChk() )	 return false;
 

		if( ! fn_fiCloseChk() )  return false;
 
	}


	var strSvcID = "";
	if( obj.name == "btn_close" )
		strSvcID = "close";
	else if( obj.name == "btn_closeCancel" )
		strSvcID = "closeCancel";

    gfn_transaction(this
					,strSvcID
					,"sm/srm/monthCloseChk/saveList.xdo"
					,"ds_list=ds_saveList:A"
					,""
					,""
					,"fn_callBack"
					,true);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : sap 월마감 체크
 *---------------------------------------------------------------------------------------------*/
 function fn_sapCloseChk()
 {
	var rtn = true;
	for( var i = 0; i < ds_saveList.rowcount; i++ )
	{
		var args  = "year=" + wrapQuote(ds_saveList.getColumn(i, "closeMonth").substr(0, 4));
			args += " mm=" + wrapQuote(ds_saveList.getColumn(i, "closeMonth").substr(4, 2));
			args += " muCd=" + wrapQuote(ds_saveList.getColumn(i, "muCd"));
			args += " ccCd=" + wrapQuote(ds_saveList.getColumn(i, "ccCd"));

		gfn_transaction(this
						,"sapClose"
						,"sm/srm/sapSlipUpload/selectSapClose.xdo"
						,""
						,"ds_sapClose=ds_sapClose"
						,args
						,""
						,false);

		if( ds_sapClose.getColumn(0, "sapClose") == "Y" )
		{
			gfn_alert("SAP 회계 월마감 되었습니다. 회계팀에 문의하세요.");
			rtn = false;
			break;
		}
	}

	return rtn;
}


