function fn_setBackground(arg, arg1, arg2){
//alert(arg);
	//전표확정/임시전표
	if(arg == "CON"){
		return "#eeffee";	//"paleturquoise";	//#3a77a2ff
		
	//에러
	}else if(arg == "OK"){
		//return "aqua";	//"paleturquoise";	//#3a77a2ff
		if ((gfn_isNull(arg1)) && (arg2=="0")){
			return "aqua";	//"paleturquoise";	//#3a77a2ff
		}else{
			return "lightsalmon";	//"paleturquoise";	//#3a77a2ff		
		}
	//에러
	}else if(arg == "008" || arg == "011"){
		return "#ffeeee";	//"lightpink";	//#af5f5fff
	
	//고객 승인
	}else if(arg == "500"){
		return "#ececff";
		
	//기본
	}else{
		return "#ffffff";
	}
}

function cbo_recdStatus_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{	
	//사용이 아닌경우 버튼 초기화
	if(cbo_recdStatus.value=='L'){
	
		var progressStatus = ds_search.getColumn(0, "progressStatus");		
		
		//발행예정전송, 발행예정오류
		if(progressStatus == "010"  || progressStatus == "008" || progressStatus == "011" ){
			btn_delete.enable = true;			//삭제
			btn_publishRequest.enable = true;	//발행요청
			
		//발행예정 전송대기, 발행예정 전송
		}else if(progressStatus == "001" || progressStatus == "100"){
			btn_delete.enable = true;			//삭제
			btn_disuse.enable = true;			//발행예정폐기요청
		//발행예정 반려요청
		}else if(progressStatus == "200"){
			btn_returnDismiss.enable = true;	//반려기각
			btn_returnConfirm.enable = true;	//반려확인		
			
		//승인
		}else if(progressStatus == "500"){
			btn_definitePublish.enable = true;	//확정발행
			
		//발행예정 폐기(폐기된 세금계산서는 삭제해야함)
		}else if(progressStatus == "700"){
			btn_delete.enable = true;			//삭제
		}
	} else {
		fn_buttonInit();	//버튼 초기화
	}
 
}
