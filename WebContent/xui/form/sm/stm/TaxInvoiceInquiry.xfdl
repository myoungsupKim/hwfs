/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
        
			//생성된 수정세금계산서 발행요청 삭제된 세금계산서 수정세금계산서 발행 시 값 이 빠지는 문제로 추가				
			if(gfn_isNull(fv_arrRtn)){
				if(!gfn_isNull(arrRtn)){
					fv_arrRtn = arrRtn;
				}
			}     				
			
			if(!gfn_isNull(fv_arrRtn)){
				if(!gfn_isNull(fv_arrRtn[0])){
					var firstIdx = ds_list.findRow("taxShetNum", fv_arrRtn[0]); //계약의해제 수정세금계산서
					ds_list.setColumn(firstIdx, "chk", 1);
				}
				
				if(!gfn_isNull(fv_arrRtn[1])){
					var secondIdx = ds_list.findRow("taxShetNum", fv_arrRtn[1]); //정정 수정세금계산서
					ds_list.setColumn(secondIdx, "chk", 1);
				}
				
				fv_reqStatus = "001";
				fv_reqSuccMsg = "발행요청 성공";
				fv_reqFailMsg = "발행요청 실패";
				
				//발행요청
				fn_taxInvoiceRequest();
			}
			//내부변수 초기화
			fv_arrRtn = "";
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;    
    }
}


