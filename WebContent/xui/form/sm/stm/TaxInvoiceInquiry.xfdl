/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "";
	
	
	//검색조건을 초기화한다.
	var nRow = ds_search.addRow();
	fn_clearSearch();
	
	//Combo
	//SCCombo.bind(this, "ComCode", "rdo_publishClass", ds_publishClass, {groupCode:"PUBLISH_CLASS", useYn:"Y"}, SCCombo.ALL); 		//발행구분
	SCCombo.bind(this, "ComCode", "cbo_progressStatus", ds_progressStatus, {groupCode:"TAX_PROGRESS_STATUS", useYn:"Y"}, SCCombo.ALL); 	//진행상태
	SCCombo.bind(this, "ComCode", "cbo_journalzYn", null, {groupCode:"PO_CREATION_YN", useYn:"Y"}, SCCombo.ALL); 	//진행상태
	
	//Calendar
	cal_fiscalDateFr.fn_initMonthCal(this, gfn_addDate(fv_sToday, -30), "", ds_search, "fiscalDateFr", "Y");
	cal_fiscalDateTo.fn_initMonthCal(this, fv_sToday, "", ds_search, "fiscalDateTo", "Y");
	
	//사용자 버튼 위치 조정
	//role : src(조회권한), ins(입력권한), del(삭제권한), sav(저장권한), prt(출력권한), exl(엑셀권한)
	//ischange : true (공통버튼 무조건 숨김), false (공통버튼 권한에 따라 숨김)
	SCAuth.setUserButton(this, {buttons : [{id:"btn_taxPublish", role:"src", ischange:false}
											,{id:"btn_definitePublish", role:"src", ischange:false}
											,{id:"btn_returnConfirm", role:"src", ischange:false}
											,{id:"btn_returnDismiss", role:"src", ischange:false}
											,{id:"btn_disuse", role:"src", ischange:false}
											,{id:"btn_publishRequest", role:"src", ischange:false}
											,{id:"btn_delete", role:"src", ischange:false}
						]});
	
	//사업부, 부서(팀), CC 설정
	TmSearchComponent01.applyAuthLevel(MuSearchComponent01);
	UpjangSearchComponent01.setForcedRoleSet("CostCenter");
	UpjangSearchComponent01.applyAuthLevel(TmSearchComponent01);
	
	//삭제유무
	cbo_recdStatus.index = 1;
	
	edt_supplierInchargeId.value=g_EmpNo;
	edt_supplierInchargeNm.value=g_Name;
	
	
	fn_buttonInit();	//버튼 초기화
	
}
/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 사업부,팀,업장(CC) 공통처리
 *---------------------------------------------------------------------------------------------*/
function fn_clearSearch()
{
    ds_search.setColumn(0, "muCd", g_Mu_Cd);
	ds_search.setColumn(0, "muNm", g_Mu_Nm);
	ds_search.setColumn(0, "tmCd", g_Tm_Cd);
	ds_search.setColumn(0, "tmNm", g_Tm_Nm);
	ds_search.setColumn(0, "ccCd", g_Cc_Cd);
	ds_search.setColumn(0, "ccNm", g_Cc_Nm);
}

/*----------------------------------------------------------------------------------------------
 * 버튼 초기화
 *---------------------------------------------------------------------------------------------*/
function fn_buttonInit(){
	btn_delete.enable = false;				//삭제
	btn_disuse.enable = false;				//발행예정폐기
	btn_publishRequest.enable = false;		//발행요청
	btn_returnDismiss.enable = false;		//반려기각
	btn_returnConfirm.enable = false;		//반려확인
	btn_definitePublish.enable = false;	//확정발행
}

/*----------------------------------------------------------------------------------------------
 * 상태에 따른 버튼 제어
 *---------------------------------------------------------------------------------------------*/
function fn_buttonControl(){
	fn_buttonInit();	//버튼 초기화
	if(cbo_recdStatus.value=='L'){
	
		var progressStatus = ds_search.getColumn(0, "progressStatus");		
		
		//발행예정전송, 발행예정오류
		if(progressStatus == "010"  || progressStatus == "008" || progressStatus == "011" ){
			btn_delete.enable = true;			//삭제
			btn_publishRequest.enable = true;	//발행요청
			
		//발행예정 전송대기, 발행예정 전송
		}else if(progressStatus == "001" || progressStatus == "100"){
			btn_delete.enable = true;			//삭제
			btn_disuse.enable = true;			//발행예정폐기요청
		//발행예정 반려요청
		}else if(progressStatus == "200"){
			btn_returnDismiss.enable = true;	//반려기각
			btn_returnConfirm.enable = true;	//반려확인		
			
		//승인
		}else if(progressStatus == "500"){
			btn_definitePublish.enable = true;	//확정발행
			
		//발행예정 폐기(폐기된 세금계산서는 삭제해야함)
		}else if(progressStatus == "700"){
			btn_delete.enable = true;			//삭제
		}
	}
	fn_search();
}
/*----------------------------------------------------------------------------------------------
 * 설명      : 사업부 콤보 변경 이벤트
