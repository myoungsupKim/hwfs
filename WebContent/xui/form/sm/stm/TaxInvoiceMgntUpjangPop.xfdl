function ds_taxShetDtl_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.columnid == "itemNm"){
		if(gfn_isNull(ds_taxShetDtl.getColumn(e.row,"chk"))){
			ds_taxShetDtl.updatecontrol = false;
			ds_taxShetDtl.setRowType(e.row, 1);
			ds_taxShetDtl.setColumn(e.row, "itemNm", "");
			ds_taxShetDtl.updatecontrol = true;
		}
	}
}

