            //버튼제어
            fn_buttonControl();
            ds_taxShetDtlDummy.copyData(ds_taxShetDtl);
            
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			break;    
        
        //
        case "concellation" :
			gfn_alert(fv_reqSuccMsg);
			
 			//alert(fv_concellationTaxNum);
 			//alert(fv_correctionTaxNum);
// 			alert(fv_processCnt);
			
			var arrValue = new Array();
			
			if(!gfn_isNull(fv_concellationTaxNum)) arrValue.push(fv_concellationTaxNum);
			if(!gfn_isNull(fv_correctionTaxNum)) arrValue.push(fv_correctionTaxNum);
			
			//alert(arrValue[0]);
			opener.fn_search();
			this.close(arrValue);
			break;
		case "searchCust"	:
			ds_taxShetMst.setColumn(0, "custNm", ds_hanwha.getColumn(0, "custnm"));
			ds_taxShetMst.setColumn(0, "rcvrSubUpjangNum", ds_hanwha.getColumn(0, "subUpjangNum"));
			ds_taxShetMst.setColumn(0, "rcvrBizNo", ds_hanwha.getColumn(0, "saupno"));
			ds_taxShetMst.setColumn(0, "rcvrBizNm", ds_hanwha.getColumn(0, "custnm"));
			ds_taxShetMst.setColumn(0, "rcvrCeonm",ds_hanwha.getColumn(0, "drname1"));
			ds_taxShetMst.setColumn(0, "rcvrAddr", ds_hanwha.getColumn(0, "addr") +' '+ ds_hanwha.getColumn(0, "detailAddrEnc"));
			ds_taxShetMst.setColumn(0, "rcvrBizType", ds_hanwha.getColumn(0, "upTae"));
			ds_taxShetMst.setColumn(0, "rcvrBizCat", ds_hanwha.getColumn(0, "upJong"));		
			break;
    }
}

