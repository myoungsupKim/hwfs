/*----------------------------------------------------------------------------------------------
 * 수정세금계산서 생성 클릭 이벤트
 *---------------------------------------------------------------------------------------------*/
function btn_concellation_onclick(obj:Button,  e:ClickEventInfo)
{	
	if(!gfn_validCheck(ds_taxShetMst, ds_taxShetMstValid, true)) return;
	
    var modifyAmt = 0 ;
    
	if(ds_taxShetMst.getColumn(0, "modTaxCnt") > 0){
		alert("이미 발행된 수정 세금계산서가 있습니다.");
		return;
	}

	if(gfn_isNull(ds_combo05.value)){
		gfn_alert("수정세금계산서코드를 선택해주세요");
		ds_combo05.dropdown();
		return;
	}
	
	if(ds_combo05.value == "01" || ds_combo05.value == "02"){
		// v_ds_check 미사용시 변경사항 체크
		if ((gfn_dsIsUpdated(ds_taxShetDtl) == false) && (gfn_dsIsUpdated(ds_taxShetMst) == false))
		{
			gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
			return;
		}
	}

	//공급가액 변경 추가 20161215 맹수영
	if(ds_combo05.value == "02"){ 
	    modifyAmt = edt_modifyAmt.value;
	    ds_taxShetMst.setColumn(0,"supplyAmt",modifyAmt);
	    ds_taxShetMst.setColumn(0,"vatAmt",modifyAmt);
	    ds_taxShetMst.setColumn(0,"totAmt",modifyAmt);
	    //alert(modifyAmt);
	}
	
	//상세 품목에 대해서 번호를 지정한다.
	var itemSeq = 1;
	for(var idx = 0; idx<ds_taxShetDtl.rowcount; idx++){
		if(gfn_isNull(ds_taxShetDtl.getColumn(idx,"chk"))) continue;
		
		if(gfn_isNull(ds_taxShetDtl.getColumn(idx,"itemNm"))){
			ds_taxShetDtl.rowposition = idx;
			gfn_alert("품목명을 입력해주세요.");
			return;
		}
		
		ds_taxShetDtl.setColumn(idx, "itemSeq", itemSeq);
		itemSeq++;
	}
	
	
 	if ( cal_slipPeriodDate.text != "" ){
 	   ds_taxShetMst.setColumn(0,"publishDate",cal_slipPeriodDate.text);
	}
	
	//구분값 정의(세금계산서 일련번호 생성시 사용)
	fn_setSysClass();
		
	if(confirm("수정세금계산서을 생성 하시겠습니까?")){
		fv_reqSuccMsg = "정상적으로 생성되었습니다."
		fn_concellation();
	}
}


