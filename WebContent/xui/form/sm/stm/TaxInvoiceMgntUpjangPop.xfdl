function grd_list_onexpanddown(obj:Grid, e:GridMouseEventInfo)
{
	//채권, 선수금, 매출 팝업호출
	//ds_taxShetDtl, ds_taxShetDtlLine - 추후 수정해야함.
	var dsObj = fn_callOccurObjPop(ds_taxShetDtl, ds_taxShetDtlLine, (e.row+1));
	
	
	if(gfn_isNull(dsObj) || dsObj.rowcount <= 0) return;
	
	var taxShetDtlIdx = ds_taxShetDtl.rowposition;
	var today = gfn_today();
	var totApplyAmt = 0;
	var totVatAmt = 0;
	
	for(var idx = 0; idx<dsObj.rowcount; idx++){
		totApplyAmt += dsObj.getColumn(idx,"supplyAmt");
		totVatAmt += dsObj.getColumn(idx, "vatAmt");
		
	}
	ds_taxShetDtl.setColumn(taxShetDtlIdx, "chk", "1");
 	ds_taxShetDtl.setColumn(taxShetDtlIdx, "itemSeq", taxShetDtlIdx+1);
 	ds_taxShetDtl.setColumn(taxShetDtlIdx, "itemSupplyDate", gfn_today());
 	ds_taxShetDtl.setColumn(taxShetDtlIdx, "itemSupplyDateMm", gfn_today().substr(4,2));
 	ds_taxShetDtl.setColumn(taxShetDtlIdx, "itemSupplyDateDd", gfn_today().substr(6,2));
 	ds_taxShetDtl.setColumn(taxShetDtlIdx, "itemNm", "");
 	ds_taxShetDtl.setColumn(taxShetDtlIdx, "itemSupplyAmt", totApplyAmt);
 	ds_taxShetDtl.setColumn(taxShetDtlIdx, "itemVatAmt", totVatAmt);
 	
 	totApplyAmt = 0;
 	totVatAmt = 0;
 	
 	ds_amt.clearData();
 	ds_tax.clearData();
 	ds_amt.addRow();
 	ds_tax.addRow();
 	
 	for(var idx = 0; idx<ds_taxShetDtl.rowcount; idx++){
		if(gfn_isNull(ds_taxShetDtl.getColumn(idx, "itemSupplyAmt"))) continue;
		totApplyAmt += ds_taxShetDtl.getColumn(idx, "itemSupplyAmt");
		totVatAmt += ds_taxShetDtl.getColumn(idx, "itemVatAmt");
 	}
 	
 	//공급가액
	for(var i=1; i<=totApplyAmt.toString().length; i++){
		var rslt = 0;
		if(i == 1){
			rslt = totApplyAmt.toString().substring(totApplyAmt.toString().length-1, totApplyAmt.toString().length);
		}else{
			rslt = totApplyAmt.toString().substring(totApplyAmt.toString().length-i, totApplyAmt.toString().length-i+1);
		}
		ds_amt.setColumn(0, "amt"+i, rslt);
	}
	
	//세액
	for(var i=1; i<=totVatAmt.toString().length; i++){
		var rslt = 0;
		if(i == 1){
			rslt = totVatAmt.toString().substring(totVatAmt.toString().length-1, totVatAmt.toString().length);
		}else{
			rslt = totVatAmt.toString().substring(totVatAmt.toString().length-i, totVatAmt.toString().length-i+1);
		}
		ds_tax.setColumn(0, "tax"+i, rslt);
	}
	
}

/*----------------------------------------------------------------------------------------------
 * 채권, 선수금, 매출 팝업
 * selDs : 선택한 라인의 발행 대상 Dataset
 * lineNum : 선택한 라인의 번호
 *---------------------------------------------------------------------------------------------*/
function fn_callOccurObjPop(lineDs:Dataset,selDs:Dataset,lineNum){
	var dsObj;															//팝업에서 전달받는 발행대상 Dataset
	var vatCd = ds_taxShetMst.getColumn(0, "vatCd");					//부가세코드
	var vatClass = ds_vatCd.getColumn(cbo_vatCd.index, "sapVatCd");		//SAP부가세코드
	var vatRate = ds_vatCd.getColumn(cbo_vatCd.index, "vatRate");		//부가세율
	var vatRateYn = ds_vatCd.getColumn(cbo_vatCd.index, "vatRateYn");	//부가세율여부
	var ccCd = ds_taxShetMst.getColumn(0, "ccCd");						//업장(CC)코드
	var ccNm = ds_taxShetMst.getColumn(0, "ccNm");						//업장(CC)명
	var mainUpjangCd = ds_taxShetMst.getColumn(0, "upjangCd");			//메인 업장코드
	var mainUpjangNm = ds_taxShetMst.getColumn(0, "upjangNm");			    //메인 업장명
	var subUpjang = ds_taxShetMst.getColumn(0, "subUpjangCd");			//서브 업장코드
	var subUpjangNm = ds_taxShetMst.getColumn(0, "subUpjangNm");	//서브 업장명
	var custCd = ds_taxShetMst.getColumn(0, "custCd");			        //고객사 코드		
	
	if(gfn_isNull(vatCd)){
		alert("부가세구분을 먼저 선택해 주세요");
		return dsObj;
	}
	
	var strId       = "TaxInvoiceOccurObjPop";
	var strURL      = "X_STM::TaxInvoiceOccurObjPop1.xfdl";	//Default 외식사업부
	var muCd        = ds_taxShetMst.getColumn(0,"muCd");
	var ccCd        = ds_taxShetMst.getColumn(0,"ccCd");
	var ccNm        = ds_taxShetMst.getColumn(0,"ccNm");
	
	if(muCd == "2002"){
		strURL      = "X_STM::TaxInvoiceOccurObjPop2.xfdl";	//FS사업부(급식)
	}else if(muCd == "2003"){
		strURL      = "X_STM::TaxInvoiceOccurObjPop1.xfdl";	//외식사업부
	}else{
		strURL      = "X_STM::TaxInvoiceOccurObjPop3.xfdl";	//식재사업부(외)
	}
	var objArgument = {dataSet:"",		    //팝업에서 선택된 전체목록
					   selDataSet:"",		//클릭한 라인에 선택된 목록
					   pCustCd:custCd,
					   pVatCd:vatCd,
					   pVatClass:vatClass,
					   pVatRate:vatRate,
					   pVatRateYn:vatRateYn,
					   pLineNum:lineNum,
					   pMuCd:muCd,
					   pTmCd:"",
					   pTmNm:"",
					   pCcCd:ccCd,
					   pCcNm:ccNm,
					   pMainUpjangCd:mainUpjangCd,
					   pMainUpjangNm:mainUpjangNm,
					   pSubUpjang:subUpjang,
					   pSubUpjangNm:subUpjangNm					   
					   };
	var nWidth      = -1;
	var nHeight     = -1;
	var strStyle    = "";	
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";
	var bResizable  = true;

	// 리턴값을  Dataset으로 받을 경우
 	dsObj = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);

	//순발행일경우 팝업에서 받은 데이터 추가
// 	if(dsObj != null && rdo_publishClass.value == "1"){
// 		ds_lineData.appendData(dsObj);
// 	}
	
	return dsObj;
}

