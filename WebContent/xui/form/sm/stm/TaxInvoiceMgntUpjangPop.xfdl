			btn_disuse.position.left = 665;
			btn_disuse.position.right = 758;

			
		//발행예정 폐기(폐기된 세금계산서는 삭제해야함)
		}else if(progressStatus == "700"){
			btn_delete.visible = true;			//삭제
			
		}else if(progressStatus == "OK"){
			//if (ds_taxShetMst.getColumn(0, "modTaxCnt") == "0") {		
				if (gfn_isNull(ds_taxShetMst.getColumn(0, "orgnlTaxShetNum"))){
