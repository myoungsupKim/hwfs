/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	//trace(ds_list.saveXML());
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			break;
        
        case "searchHanwha" :
			if(ds_hanwha.rowcount > 0){
				ds_search.setColumn(0, "supplierBizNo", ds_hanwha.getColumn(0, "saupno"));
				ds_search.setColumn(0, "supplierBizNm",ds_hanwha.getColumn(0, "custnm"));
				ds_search.setColumn(0, "supplierCeonm", ds_hanwha.getColumn(0, "drname1"));
				ds_search.setColumn(0, "supplierAddr", ds_hanwha.getColumn(0, "addr") +' '+ ds_hanwha.getColumn(0, "detailAddrEnc"));
				ds_search.setColumn(0, "supplierBizType", ds_hanwha.getColumn(0, "upTae"));
				ds_search.setColumn(0, "supplierBizCat", ds_hanwha.getColumn(0, "upJong"));
			}        
			break;
			
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break;    			
    }
}


