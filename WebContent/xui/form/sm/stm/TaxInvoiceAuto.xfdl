/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	// 저장시 Validation Check	
	for(var i=0; i<ds_list.rowcount; i++){
		if(ds_list.getColumn(i, "rcvrBizNm") == "1") {
			if(gfn_isNull(ds_list.getColumn(i, "rcvrBizNm")) || gfn_replace(ds_list.getColumn(i, "rcvrBizNm"), " " , "")== ""){
				alert("공급받는자 상호를 확인해주시기 바랍니다.");
				return;
			}
			if(gfn_isNull(ds_list.getColumn(i, "rcvrCeonm")) || gfn_replace(ds_list.getColumn(i, "rcvrCeonm"), " " , "")== ""){
				alert("공급받는자 대표자명을 확인해주시기 바랍니다.");
				return;
			}
				if(gfn_isNull(ds_list.getColumn(i, "rcvrAddr")) || gfn_replace(ds_list.getColumn(i, "rcvrAddr"), " " , "")== ""){
				alert("공급받는자 주소를 확인해주시기 바랍니다.");
				return;
			}
		}
	}


	//세금계산서 타입코드(종류 + 분류
	ds_search.setColumn(0, "taxShetType", ds_search.getColumn(0, "taxShetKind") + ds_search.getColumn(0, "taxShetClsfi"));	
	ds_search.setColumn(0, "newhghtUpjang", ds_search.getColumn(0, "ccCd"));	

	
	var strSvcID    = "save";
    var strURL      = "sm/stm/taxInvoiceAuto/saveList.xdo";
    var strInDs     = "ds_search=ds_search:A ds_list=ds_list:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

