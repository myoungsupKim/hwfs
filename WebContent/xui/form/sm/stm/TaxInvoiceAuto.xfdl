 * 부가세구분 onitemchanged 이벤트
 *---------------------------------------------------------------------------------------------*/
 function cbo_vatCd_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	var idx = obj.index;

	//세금계산서 종류 선택
	if(ds_vatCd.getColumn(idx, "vatCd")=="40" || ds_vatCd.getColumn(idx, "vatCd")=="43"){
		cbo_taxShetKind.index=1;
	}else{
		cbo_taxShetKind.index=0;
	}
	//영세
	if(ds_vatCd.getColumn(idx, "vatCd")=="13"){
		cbo_taxShetClsfi.index=1;
	}else{
		cbo_taxShetClsfi.index=0;
	}
}

