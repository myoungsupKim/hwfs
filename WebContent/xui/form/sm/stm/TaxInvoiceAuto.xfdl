function fn_init()
{

	//검색조건을 초기화한다.
	var nRow = ds_search.addRow();
	fn_clearSearch();

	if(parseInt(fv_sToday.substr(6, 2)) < 10){
		fv_taxDay = gfn_getLastDate(gfn_addMonth(fv_sToday, -1));
	}
	cal_publishDate.fn_initMonthCal(this, fv_taxDay, "", ds_search, "publishDate", "Y");						//발행일자
	//Calendar
	cal_fiscalDateFr.fn_initMonthCal(this, gfn_addMonth(fv_sToday, -1), "", ds_search, "inqueryStartDate", "Y");
	cal_fiscalDateTo.fn_initMonthCal(this, fv_sToday, "", ds_search, "inqueryEndDate", "Y");
	ds_search.setColumn(0, "sysClass", "2100");
	SCCombo.bind(this, "ComCode", "cbo_rcptBillingClass", ds_rcptBillingClass, {groupCode:"RCPT_BILLING_CLASS", useYn:"Y", defVal:"02"}); 	//영수/청구구분
