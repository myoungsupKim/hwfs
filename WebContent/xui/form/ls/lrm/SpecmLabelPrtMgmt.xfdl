 * 설명   : 검체명 입력 후 엔터
 ***************************************************************************/
function edt_specmNm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13)
	{
		edt_specmNm.updateToDataset();
		fn_search();
	}
}

