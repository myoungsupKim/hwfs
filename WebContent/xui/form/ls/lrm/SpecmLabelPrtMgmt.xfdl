/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";
include "lib::LimsUtil.xjs";

/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_iProcessCnt = 0;  	// 저장건수를 저장한다.(Controller Class에서만 할당함.)
var curDate = gfn_today();

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
    // Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
    gfn_formOnLoad(this);
    
    // 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
    cal_fromDay.fn_initMonthCal(this, gfn_addDate(curDate, -7), "", ds_search, "fromDay", "Y");
    cal_toDay.fn_initMonthCal(this, curDate, "", ds_search, "toDay", "Y");
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{
	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}

/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	fn_doSearch();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장(승인) 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제(반려) 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 출력 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_print()
{
	var chkCnt = ds_testRegList.findRow("chk", '1');
	// 체크된 항목이 하나도 없으면 경고를 한다.
	if (chkCnt < 0) {
		gfn_alert("인쇄하고자 하는 대상 검체를 선택하여 주십시오.");
		return;
	}
	
	var rtVal = gfn_confirm("검체에 대한 라벨을 인쇄 하시겠습니니까?");
	if ( !rtVal == true) return;
	
	var acceptNums = "";
	for ( var j = 0; j < ds_testRegList.getRowCount(); j++ )
	{
		if ( ds_testRegList.getColumn(j, "chk") =="1" ) {
			acceptNums += "'" + ds_testRegList.getColumn(j, "acceptNum") + "',";
		}
	}
	acceptNums = acceptNums.substring(0, acceptNums.length-1);
	
	//레포트에 전달할 파라메터 생성
	var arrParams	= SCReport.newParams ();
	
	//--------------------------------------------------
	//방법1)서버에서 Query에 의해 처리하는 경우 예
	//(필수) 레포트명
	//SCReport.setParams (arrParams, SCReport.RPT_NAME, "/hwfs/guide/sample/SampleList.ozr");
	SCReport.setParams (arrParams, SCReport.RPT_NAME, "/hwfs/ls/lrm/SpecmLabelPrt.ozr");

	//출력 옵션 설정
	//-창 타이틀 (디폴트는 "ReportViewer")
	SCReport.setParams (arrParams, SCReport.RPT_TITLE, "시료확인표");

	//-Zoom (디폴트는 100)
	//SCReport.setParams (arrParams, SCReport.RPT_ZOOM, "120");

	//레포트에 맞게 전달되는 개발자 정의 파라미터
	SCReport.setParams (arrParams, "PARAM1", acceptNums);
	
	SCReport.viewReport (this, arrParams, false);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0)
	{
		gfn_alert(strErrorMsg);
		return;
	}

    switch(strSvcID)
    {
		// 조회
        case "selectList" :
			sta_totCnt.text = "총건수 : " + gfn_numFormat(ds_testRegList.rowcount) + "건";
            break;
        // 검체 폐기 저장
        case "saveDisuseYn" :
			if (fv_iProcessCnt > 0) gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_doSearch();
            break;
    }
}

/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 조회
 *---------------------------------------------------------------------------------------------*/
function fn_doSearch()
{
	if (gfn_isNull(cal_fromDay.fn_getValue()) || gfn_isNull(cal_toDay.fn_getValue()))
	{
		gfn_alert("의뢰일자를 입력하여 주십시오!");
		return;
	}

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_testRegList);

    // Dataset을 초기화한다.
    ds_testRegList.clearData();

    var strSvcID    = "selectList";
    var strURL      = "ls/lrm/testRegInquery/selectSpecmLabelPrtList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_testRegList=ds_testRegList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/***************************************************************************
 * 설명   : 시험접수 상세 화면 Popup
 ***************************************************************************/
function btn_searchTestReqDtlPop_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_testRegList.rowposition == -1)
	{
		gfn_alert("목록을 선택하여 주십시오!");
		return;
	}
	
	var arrParam = new Array();

	arrParam[0] = ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum");	// 전송할 파라미터 1
	arrParam[1] = false;	// 전송할 파라미터 2

	var rtnVal = gfn_dialog("TestInfoDtlInqueryPop"				// dialogId
					   , "X_LRM::TestInfoDtlInqueryPop.xfdl"			// Url
					   , {fv_Contents:arrParam}			// 배열
					   , null, null, null, null, null, false, true, -1);
}

