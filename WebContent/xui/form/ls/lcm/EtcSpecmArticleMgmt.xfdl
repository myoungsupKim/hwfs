************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	fn_doSearch();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{
	var iIdx = ds_etcSpecmArticleList.addRow();
	ds_etcSpecmArticleList.setColumn(iIdx, "specmClass", cbo_specmClass.value);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	// 년간계획에 등록된 데이터인지 체크
	if (ds_etcSpecmArticleList.getColumn(ds_etcSpecmArticleList.rowposition, "yearAnnuExists") == "1") {
		if (!gfn_confirm("년간계획에 등록된 데이터 입니다.\n삭제 하시 겠습니까?")) return false;
	}
	// 월계획에 등록된 데이터인지 체크
	if (ds_etcSpecmArticleList.getColumn(ds_etcSpecmArticleList.rowposition, "monthAnnuExists") == "1") {
		if (!gfn_confirm("월계획에 등록된 데이터 입니다.\n삭제 하시 겠습니까?")) return false;
	}
	
	ds_etcSpecmArticleList.deleteRow(ds_etcSpecmArticleList.rowposition);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (ds_etcSpecmArticleList.rowposition == -1)
	{
		gfn_alert("기타검체유형 데이터를 먼저 입력하여 주십시오.");
		return;
	}

	// 자료를 저장 하시겠습니까?
	if (!gfn_confirm("confirm.before.save")) return false;

	if (!gfn_dsIsUpdated(ds_etcSpecmArticleList))
	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return;
	}
	
	// 저장시 Validation Check
	if (gfn_validCheck(ds_etcSpecmArticleList, ds_etcSpecmArticleListValid) == false)	return;

    var strSvcID    = "saveList";
    var strURL      = "ls/lcm/etcSpecmArticleMgmt/saveList.xdo";
    var strInDs     = "ds_etcSpecmArticleList=ds_etcSpecmArticleList:U";
    var strOutDs    = "ds_retval=ds_retval";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

