/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0)
	{
		return;
	}

    switch(strSvcID)
    {
		// 조회
        case "selectList" :
            break;
        // 저장
        case "saveList" :
			if (fv_iProcessCnt > 0) gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_doSearch();
            break;
    }
}

/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회
 *---------------------------------------------------------------------------------------------*/
function fn_doSearch()
{
	// v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_etcSpecmArticleList))
 	{
		// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		var retValue = gfn_confirm("confirm.before.search");
 		if (retValue == false) return;
 	}
 	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_etcSpecmArticleList);

    // 장비, 수리목록 Dataset을 초기화한다.
    ds_etcSpecmArticleList.clearData();
    
    // Service단을 접수/상세 등 에서 같이 사용하기 때문에 기타검체유형 구분을 별도로 넘긴다.
    // inqueryYn : Y 인 경우는 기타검체유형에서 조회 시 사용한다.
    //             '' 이나 'N'인 경우는 접수/상세에서 자재 품목 조회 조건으로 사용한다.
    ds_search.setColumn(0, "inqueryYn", "Y");

    var strSvcID    = "selectList";
    var strURL      = "ls/lcm/etcSpecmArticleMgmt/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_etcSpecmArticleList=ds_etcSpecmArticleList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체유형 팝업 클릭
 *---------------------------------------------------------------------------------------------*/
function btn_specmTypePop_onclick(obj:Button,  e:ClickEventInfo)
{
	if (cbo_specmType.index == -1) 
	{
		gfn_alert("검체유형을 선택하여 주십시오!");
		return;
	}
	
	var arrParam = new Array();
	arrParam[0] = cbo_specmType.value;

	var rtnVal = gfn_dialog("SpecmTypeInqueryPop"				// dialogId
					   , "X_LCM::SpecmTypeInqueryPop.xfdl"			// Url
					   , {fv_Contents:arrParam}			// 배열
					   , null, null, null, null, null, false, true, -1);

	if (typeof rtnVal != "undefined" && rtnVal != null && gfn_getObjkeyLength(rtnVal) > 0) 
	{
		edt_specmTypeNm.value = rtnVal.mgrpNm + " / " + rtnVal.mcalssNm + " / " + rtnVal.sclassNm;
		
		// Popup에서 넘어온 객체의 값을 DataSet에 Binding 시킨다.
        LimsUtil.setArrParamBind(ds_etcSpecmArticleList, rtnVal);

// 		ds_etcSpecmArticleList.setColumn(ds_etcSpecmArticleList.rowposition, "mgrpCd", rtnVal.mgrpCd);
// 		ds_etcSpecmArticleList.setColumn(ds_etcSpecmArticleList.rowposition, "mcalssCd", rtnVal.mcalssCd);
// 		ds_etcSpecmArticleList.setColumn(ds_etcSpecmArticleList.rowposition, "sclassCd", rtnVal.sclassCd);
	}
}

