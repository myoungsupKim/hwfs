/***************************************************************************
 * 설명    		: 검체목록 다음행에 복사
 ***************************************************************************/
function btn_coppySpecm_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_etcSpecmArticleList.rowposition == -1) return;
	if (!gfn_confirm("중복데이터에 주의하여 검체정보를 입력하여 주십시오!\n복사 하시겠습니까?", "", "C")) return;

    var nRow = ds_etcSpecmArticleList.rowposition;
    var nIdx = ds_etcSpecmArticleList.addRow();
    ds_etcSpecmArticleList.copyRow(nIdx, ds_etcSpecmArticleList, nRow);

    ds_etcSpecmArticleList.setColumn(nIdx, "specmCd", "");
    ds_etcSpecmArticleList.setColumn(nIdx, "specmNm", "");
}
