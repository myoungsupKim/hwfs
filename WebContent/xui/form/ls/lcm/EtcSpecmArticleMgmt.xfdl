var codeList = "SPECM_CLASS,SPECM_TYPE,COST_CLASS,INSPT_CLASS,INSPT_PURP_1,INSPT_PURP_2,INSPT_PURP_3,LONGSTORAGE_YN,INSPTBEFORE_SPECM_STATUS,PICK_STATUS,SPECM_WTRLST_NM,INSPT_OBJ_1,INSPT_OBJ_2,INSPT_OBJ_3,INSPT_OBJ_4,CHARG_BASIS,URGENT_YN";

