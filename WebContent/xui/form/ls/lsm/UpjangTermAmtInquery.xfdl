	var bDiaryWriteYn = false;
	var objParam = {
		"reqNum" : ds_detailViewList.getColumn(ds_detailViewList.rowposition, "reqNum")
	  , "acceptNum" : ds_detailViewList.getColumn(ds_detailViewList.rowposition, "acceptNum")
	  , "articleCd" : ds_detailViewList.getColumn(ds_detailViewList.rowposition, "articleCd")
	  , "sabun" : ds_detailViewList.getColumn(ds_detailViewList.rowposition, "sabun")
	  , "completeYn" : ds_detailViewList.getColumn(ds_detailViewList.rowposition, "completeYn")
	  , "diaryWriteYn" : bDiaryWriteYn
	  , "modifyYn" : false
	};

	var rtnVal = gfn_dialog("TestDiaryRegPop"				// dialogId
				   , "X_LTM::TestDiaryRegPop.xfdl"			// Url
				   , {fv_Contents:objParam}			// 파라미터
				   , null, null, null, null, null, false, true, -1);
	if (rtnVal != null && rtnVal != "undefined" && rtnVal == "OK")
	{
	}
}

/***************************************************************************
 * 설명   : 성적서발급 이력 조회
 ***************************************************************************/
function btn_rtpIssueHistory_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_detailViewList.rowposition == -1)
	{
		gfn_alert("목록을 선택하여 주십시오!");
		return;
	}
	
	if (Number(ds_detailViewList.getColumn(ds_detailViewList.rowposition, "publishCnt")) == 0)
	{
		gfn_alert("성적서 발급 이력이 없습니다!");
		return;
	}
	
	var arrParam = new Array();

	arrParam[0] = ds_detailViewList.getColumn(ds_detailViewList.rowposition, "reqNum");	// 전송할 파라미터 1

	var rtnVal = gfn_dialog("InspectRptIssueHistPop"				// dialogId
				   , "X_LEM::InspectRptIssueHistPop.xfdl"			// Url
				   , {fv_Contents:arrParam}			// 배열
				   , null, null, null, null, null, false, true, -1);
}

/***************************************************************************
 * 설명   : 조회목록 그리드 클릭
 ***************************************************************************/
function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if (gfn_isNull(cal_fromDay.fn_getValue()) || gfn_isNull(cal_toDay.fn_getValue()))
	{
		gfn_alert("의뢰일자를 입력하여 주십시오!");
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_detailList);
    gfn_clearSortMark(grd_detailViewList);

    // Dataset을 초기화한다.
    ds_detailList.clearData();
    ds_detailViewList.clearData();
    
    ds_search.setColumn(0, "acceptClassSub", ds_list.getColumn(ds_list.rowposition, "acceptClass"));
    
    var strSvcID    = "selectDetailList";
    var strURL      = "ls/lsm/upjangTermAmtInquery/selectDetailList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_detailList=ds_detailList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/***************************************************************************
 * 설명   : 상세 조회목록 그리드 클릭
 ***************************************************************************/
function grd_detailList_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if (gfn_isNull(cal_fromDay.fn_getValue()) || gfn_isNull(cal_toDay.fn_getValue()))
	{
		gfn_alert("의뢰일자를 입력하여 주십시오!");
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_detailViewList);

    // Dataset을 초기화한다.
    ds_detailViewList.clearData();
    
    ds_search.setColumn(0, "reqNum", ds_detailList.getColumn(ds_detailList.rowposition, "reqNum"));
    
    var strSvcID    = "selectDetailViewList";
