	}
	
	var arrParam = new Array();

	arrParam[0] = ds_detailViewList.getColumn(ds_detailViewList.rowposition, "reqNum");	// 전송할 파라미터 1
	arrParam[1] = false;	// 전송할 파라미터 2

	var rtnVal = gfn_dialog("TestInfoDtlInqueryPop"				// dialogId
					   , "X_LRM::TestInfoDtlInqueryPop.xfdl"			// Url
					   , {fv_Contents:arrParam}			// 배열
					   , null, null, null, null, null, false, true, -1);
