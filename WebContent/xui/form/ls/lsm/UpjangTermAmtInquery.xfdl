************************************************************************************************/
var fv_iProcessCnt = 0;  	// 저장건수를 저장한다.(Controller Class에서만 할당함.)
var curDate = gfn_today();
var codeList = "ACCEPT_CLASS,INSPT_PURP_1,INSPT_PURP_2,INSPT_PURP_3,INSPT_OBJ_1,INSPT_OBJ_2,INSPT_OBJ_3,INSPT_OBJ_4,INSPT_METHOD,PROC_CLASS";
var strCodeGubun = "ALL";
var v_arg;

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
    // Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
    gfn_formOnLoad(this);
    
    // 파라미터 Get
    v_arg = gfn_getScreenParams();
    
    // 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
    cal_fromDay.fn_initMonthCal(this, LimsUtil.getFirstDate(curDate), "", ds_search, "fromDay", "Y");
    cal_toDay.fn_initMonthCal(this, curDate, "", ds_search, "toDay", "Y");
    
    fn_selectInsptUser();	// 검사항목리스트에 보여질 검사자 조회 후 DataSet Setting
    
    // 사용 코드를 한번에 가져와서 Dataset에 순차적으로 binding 시킨다.
	LimsUtil.selectCodeBatchList(this, null, codeList, null, strCodeGubun);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 공통코드 binding 조회 후 callback function
 *---------------------------------------------------------------------------------------------*/
function fn_codeBatchCallback()
{
	//fn_getScreenParams(v_arg);
	cbo_gubun.value = '101';	// Default 완료
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검사자 검사준수 현황에서 호출 시 실행
 *---------------------------------------------------------------------------------------------*/
function fn_getScreenParams(arrArg)
{
    v_arg = arrArg;

    if (typeof v_arg != "undefined" && v_arg != null && v_arg != "")
    {
		cbo_userList.value = v_arg[0];	// 담당자 사번
		cal_fromDay.fn_setValue(v_arg[1]);	// 의뢰시작일
		cal_toDay.fn_setValue(v_arg[2]);	// 의뢰종료일
		cbo_gubun.value = v_arg[3];	// 처리구분
		
		fn_doSearch();
    }
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
