/***************************************************************************
 * 설명   : MultiCombo Popup Layer 호출
 ***************************************************************************/
function fn_multiComboPopup(obj)
{
	var objArg = {
		sObjInputFuc : "fn_setParamValue",		// callback function
		sSccGroupCd  : "",						// 공통코드 GroupCode
		outDs        : ds_progressStatusTmp,				// DataSet을 넘길경우 사용
		sParam1      : gfn_nvl(ds_search.getColumn(0, "progressStatus"), ""),
		sParam2      : strCodeGubun	// "ALL" or ""	멀티콤보 헤더 체크용
	};
	var sPlayerPopup = {
		 objGubun	   : 'comp'								// Object구분 : comp -> component, grid -> Grid(미 테스트:사용금지)
		,evtGrid       : null								// Grid에서 사용, comp인 경우 null세팅
		,objInput	   : obj
		,nPwidth       : 280								// 요건 고정
		,nPheight      : 289								// 요거도 고정
		,sPpopupDivNm  : 'ObjComboTestPop'					// LayerDiv명
		,sPopupUrl     : 'X_LSM::CodeMultiComboPop.xfdl'
		,objArg        : objArg
		,objForm       : this
	};
	LimsUtil.gfn_objMultiComboLayerPopup(sPlayerPopup);
}

/***************************************************************************
 * 설명   : MultiCombo Popup Layer 에서 넘어오는 값
 ***************************************************************************/
function fn_setParamValue(codeArr, codeNameArr)
{
	var strCodeArr = codeArr;
	var strCodenameArr = codeNameArr;
	edt_progressStatusMulti.value = "";
	// 구분자 : ∮
	ds_search.setColumn(0, "progressStatus", strCodeArr);
	edt_progressStatusMulti.value = strCodenameArr.replace(/∮/gi, ",");
}

/***************************************************************************
 * 설명   : 검체유형 항목 Del Key 클릭 시
 ***************************************************************************/
function edt_specmType_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode <> "46") { return false; }
	
	ds_search.setColumn(0, "specmType", "");
	ds_search.setColumn(0, "mgrpArr", "");
	ds_search.setColumn(0, "mclassArr", "");
	ds_search.setColumn(0, "sclassArr", "");
	
	edt_specmType.value = "";
}

/***************************************************************************
 * 설명   : 진행상황 항목 Del / Enter Key 클릭 시
 ***************************************************************************/
function edt_progressStatusMulti_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == "46") {
		ds_search.setColumn(0, "progressStatus", "");
		edt_progressStatusMulti.value = "전체";		
		
		//fn_doSearch();
	} else if (e.keycode == 13) {
		fn_doSearch();
	} else {
		return false;
	}
}
