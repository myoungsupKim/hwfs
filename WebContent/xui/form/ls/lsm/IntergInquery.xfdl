************************************************************************************************/
include "lib::lib_com.xjs";
include "lib::LimsUtil.xjs";

/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_iProcessCnt = 0;  	// 저장건수를 저장한다.(Controller Class에서만 할당함.)
var curDate = gfn_today();
//var codeList = "SPECM_TYPE,INSPT_CLASS,INSPT_PURP_1,INSPT_PURP_2,INSPT_PURP_3,LONGSTORAGE_YN,INSPT_OBJ_1,INSPT_OBJ_2,INSPT_OBJ_3,INSPT_OBJ_4,INSPT_FLD,INSPT_METHOD,LAST_DECISION,PROGRESS_STATUS";
var codeList = "INSPT_CLASS,INSPT_PURP_1,INSPT_PURP_2,INSPT_PURP_3,LONGSTORAGE_YN,INSPT_OBJ_1,INSPT_OBJ_2,INSPT_OBJ_3,INSPT_OBJ_4,INSPT_FLD,INSPT_METHOD,LAST_DECISION,PROGRESS_STATUS";
var excludeCodeList = "PROGRESS_STATUS";
var strCodeArr = "";	// MultiComboBox 코드값 저장 (구분자 : ∮)
var strCodeGubun = "ALL"; // MultiComboBox 초기 체크값
var v_arg;

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
    // Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
    gfn_formOnLoad(this);
    
    // 파라미터 Get
    v_arg = gfn_getScreenParams();
    
    // 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
    cal_fromDay.fn_initMonthCal(this, gfn_addDate(curDate, -7), "", ds_search, "fromDay", "Y");
    cal_toDay.fn_initMonthCal(this, curDate, "", ds_search, "toDay", "Y");
    
    cal_acceptdd.fn_initMonthCal(this, "", "", ds_search, "acceptdd");
    cal_completedd.fn_initMonthCal(this, "", "", ds_search, "completeDd");
    
    fn_selectInsptUser();	// 검사항목리스트에 보여질 검사자 조회 후 DataSet Setting
    
    // 사용 코드를 한번에 가져와서 Dataset에 순차적으로 binding 시킨다.
	LimsUtil.selectCodeBatchList(this, null, codeList, excludeCodeList, strCodeGubun);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 공통코드 binding 조회 후 callback function
 *---------------------------------------------------------------------------------------------*/
function fn_codeBatchCallback()
{
	// 배열객체의 값에 해당하는 값에 대하여 DataSet Binding 함수
	LimsUtil.setMultiComboDataSetBind(ds_progressStatus, ds_progressStatusTmp, ["code","codeName"], ["code","codeName"]);
	edt_progressStatusMulti.value = "전체";
	
	chk_disuseYn.value = '1';
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검사자 검사준수 현황에서 호출 시 실행
 *---------------------------------------------------------------------------------------------*/
function fn_getScreenParams(arrArg)
{
    v_arg = arrArg;

    if (typeof v_arg != "undefined" && v_arg != null && v_arg != "")
    {
		ds_search.setColumn(0, "upjang", v_arg[2]);
		edt_upjangNm.value = v_arg[3];
		
		if (v_arg[4] == undefined) {
			edt_reqNum.value = "";	// 의뢰번호
			cal_fromDay.fn_setValue(gfn_getThisYearBegin());	// 금년 1월 1일
		} else {
			edt_reqNum.value = v_arg[0];	// 의뢰번호
			cal_fromDay.fn_setValue(v_arg[1]);	// 의뢰일자
		}
		
		fn_doSearch();
    }
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
