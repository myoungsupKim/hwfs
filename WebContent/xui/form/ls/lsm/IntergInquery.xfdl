/***************************************************************************
 * 설명   : 검체유형 팝업 버튼 클릭 시
 ***************************************************************************/
function btn_specmType_onclick(obj:Button,  e:ClickEventInfo)
{
	var mgrpArr = "";
	var mclassArr = "";
	var sclassArr = "";
	var objParam = {
		specmType : ds_search.getColumn(0, "specmType")
	  , mgrpArr : ds_search.getColumn(0, "mgrpArr")
	  , mclassArr : ds_search.getColumn(0, "mclassArr")
	  , sclassArr : ds_search.getColumn(0, "sclassArr")
	};
	
	var rtnVal = gfn_dialog("IntergSpecmTypeInqueryPop"				// dialogId
				   , "X_LSM::IntergSpecmTypeInqueryPop.xfdl"			// Url
				   , {fv_Contents:objParam}			// 파라미터
				   , null, null, null, null, null, false, true, -1);
	if (rtnVal != null && rtnVal != "undefined" && gfn_getObjkeyLength(rtnVal) > 0)
	{
		mgrpArr = rtnVal.mgrp.code;
		mclassArr = rtnVal.mclass.code;
		sclassArr = rtnVal.sclass.code;
		ds_search.setColumn(0, "specmType", rtnVal.specmType.code);
		
		//trace(mgrpArr + " >>> " + mclssArr  + " >>> " + sclassArr);
		
		ds_search.setColumn(0, "mgrpArr", mgrpArr);
		ds_search.setColumn(0, "mclassArr", mclassArr);
		ds_search.setColumn(0, "sclassArr", sclassArr);
		
		// 선택된 레벨의 가장 마지막 명을 보여준다.
		if (mgrpArr != undefined && mgrpArr != "" && (mclassArr == undefined || mclassArr == "") && (sclassArr == undefined || sclassArr == "")) {
			edt_specmType.value = rtnVal.mgrp.name;
		} else if (mclassArr != undefined && mclassArr != "" && (sclassArr == undefined || sclassArr == "")) {
			edt_specmType.value = rtnVal.mclass.name;
		} else if (sclassArr != undefined && sclassArr != "") {
			edt_specmType.value = rtnVal.sclass.name;
		} else {
			edt_specmType.value = rtnVal.specmType.name;
		}
	}
}

