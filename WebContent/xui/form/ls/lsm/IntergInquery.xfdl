function fn_selectInsptUser()
{
    ds_search.setColumn(0, "authCd", "101");	// 담당자권한 코드중 검사자
    
    var strSvcID    = "selectInsptUserList";
    var strURL      = "ls/lcm/userMgmt/selectUserList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_userList=ds_userList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync, false);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 조회
 *---------------------------------------------------------------------------------------------*/
function fn_doSearch()
{
	if (gfn_isNull(cal_fromDay.fn_getValue()) || gfn_isNull(cal_toDay.fn_getValue()))
	{
		gfn_alert("의뢰일자를 입력하여 주십시오!");
		return;
	}
	if (cal_fromDay.fn_getValue() > cal_toDay.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
	
	if (!gfn_isNull(edt_acceptNum.value) && gfn_length(edt_acceptNum.value) < 6)
	{
		gfn_alert("6자리 이상으로 입력하여 주십시오!");
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_testRegList);

    // Dataset을 초기화한다.
    ds_testRegList.clearData();
    
    // TextBox에 값이 없으면 전체 조회로 간주하여 값을 넘기지 않는다.
    if (gfn_isNull(edt_specmType.value)) {
		ds_search.setColumn(0, "specmType", "");
		ds_search.setColumn(0, "mgrpArr", "");
		ds_search.setColumn(0, "mclassArr", "");
		ds_search.setColumn(0, "sclassArr", "");
    }

    var strSvcID    = "selectList";
    var strURL      = "ls/lsm/intergInquery/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_testRegList=ds_testRegList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

