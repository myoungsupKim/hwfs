    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/***************************************************************************
 * 설명   : 시험항목 일지 상태권한 체크
 ***************************************************************************/
 function fn_testDiaryWriteViewCheck(statusGubun)
{
	if (statusGubun == "VIEW")
	{
		// 시험일지조회 버튼 권한
		return (ds_testRegList.getColumn(ds_testRegList.rowposition, "progressStatus") >= '150');
	}
	else if (statusGubun == "WRITE")
	{
		// 시험항목 그리드 일지 작성 권한
		return (ds_testRegList.getColumn(ds_testRegList.rowposition, "progressStatus") == '150' || 
				ds_testRegList.getColumn(ds_testRegList.rowposition, "progressStatus") == '160');
	}
}

