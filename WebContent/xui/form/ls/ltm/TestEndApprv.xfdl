		if ( gds_limsAuthLevels.findRow('authCd', '103') != -1 || 
		     gds_limsAuthLevels.findRow('authCd', '105') != -1 ) {
			div_cmnBtn.btn_delete.enable = true;
			div_cmnBtn.btn_save.enable = true;
		}
	}
