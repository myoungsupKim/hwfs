function div_barCd_btn_barDel_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRow = ds_outletMenuBarcd.rowposition;
	
	if(ds_outletMenuBarcd.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "delete row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		var bIsProcRow = false;
		if (ds_outletMenuBarcd.getRowType(nRow) == Dataset.ROWTYPE_INSERT)
		{
			ds_outletMenuBarcd.deleteRow(nRow);
		} else
		{
			ds_outletMenuBarcd.setColumn(nRow, "useYn", 0);
			bIsProcRow = true;
		}
		
		if (!bIsProcRow) return;
		
		sMsg = gfn_getMessage("msg.action.success", "");
		sMsg = sMsg += "\r\r" + gfn_getMessage("rc.sle.save.data", "");
		
		//실제 저장처리를 수행할 지를 알려준다.
		if (gfn_confirm(sMsg, "", "C"))
		{
			fn_save();
		}
		else
		{
			sMsg = gfn_getMessage("rc.sle.resave.data","");  //"변경된 자료는 나중에 저장하시기 바랍니다.";
			gfn_alert(sMsg, "", "A");
		}
	}
}
 
//바코드 등록시 같은 바코드가 등록되는 버그를 막는다.
function ds_outletMenuBarcd_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{

	
	
		//신규 추가행에 대해서만 처리한다.
	if (obj.getRowType(obj.rowposition) == Dataset.ROWTYPE_INSERT)
	{
		if (e.columnid == "barCd")
		{
		
			//DB에 이미 입력 되어 있는가 확인한다.
			ds_outletMenuBarcdSearch.clearData();
			var strSvcID    = "searchBarCd";
			var strURL      = "rc/sle/menuMgmt/searchBarCd.xdo";
			var strInDs     = "";
			var strOutDs    = "ds_outletMenuBarcdSearch=ds_outletMenuBarcdSearch";
			var strArg      = "outletCd=" + gfn_nvl(edt_upjang.getUpjangCode(),"")
							+ " barCd="   + gfn_nvl(obj.getColumn(e.row, "barCd"));
			var strCallback = "fn_callBack";
			var bAsync      = false;
			
			gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
			
			if (ds_outletMenuBarcdSearch.rowcount > 0)
			{
				sMsg = "이미 등록된 바코드가 존재합니다.\r\r메뉴코드 : " + ds_outletMenuBarcdSearch.getColumn(0, "menuCd") + "\r메뉴명 : " + ds_outletMenuBarcdSearch.getColumn(0, "menuNm");
				gfn_alert(sMsg, "", "A");
				obj.setColumn(e.row, "barCd", "");
				//그리드의 Focus 이동
				div_barCd.grd_outletMenuBarcd.setCellPos(div_barCd.grd_outletMenuBarcd.getBindCellIndex("Body", "barCd"));
				div_barCd.grd_outletMenuBarcd.showEditor(true);
				div_barCd.grd_outletMenuBarcd.setFocus();
				return;	
			}
			
			//이미 등록된 그룹코드가 존재하는지를 확인한다.
			var iFindRowIdx = obj.findRow("barCd", obj.getColumn(e.row, "barCd"), 0, obj.rowcount);
			var sMsg = "";
			
			//자기 자신을 제외하고 검색된 행이 존재할 시에는 중복 메시지를 표시한다.
			if (iFindRowIdx != e.row && iFindRowIdx != -1)
			{
				sMsg = "이미 등록된 바코드가 존재합니다.\r\r행 번호 : " + (iFindRowIdx+1) + "\r입력 값 : " + obj.getColumn(e.row, "barCd");
				gfn_alert(sMsg, "", "A");
				obj.setColumn(e.row, "barCd", "");
				//그리드의 Focus 이동
				div_barCd.grd_outletMenuBarcd.setCellPos(div_barCd.grd_outletMenuBarcd.getBindCellIndex("Body", "barCd"));
				div_barCd.grd_outletMenuBarcd.showEditor(true);
				div_barCd.grd_outletMenuBarcd.setFocus();
				return;	
			}
		}
	}
	
	//가장 첫번째인것중 사용중인것을 찾아  메뉴의 바코드에 같이 넣어 둔다.
	if ((obj.getRowType(obj.rowposition) == Dataset.ROWTYPE_INSERT) &&  (obj.getRowType(obj.rowposition) == Dataset.ROWTYPE_UPDATE))	
	{
		for (var i=0; i < obj.rowcount; i++)
		{
			// trace("useYn:" + obj.getColumn(i , "useYn") + " barCd:" + gfn_nvl(obj.getColumn(i , "barCd")));
			ds_outletMenu.setColumn(ds_outletMenu.rowposition, "barCd", "");
			if ((gfn_nvl(obj.getColumn(i , "useYn")) == "1") && (gfn_nvl(obj.getColumn(i , "barCd")) !=""))
			{
				 
				var menuBarCd = gfn_nvl(obj.getColumn(i , "barCd"));
				ds_outletMenu.setColumn(ds_outletMenu.rowposition, "barCd", menuBarCd);
				break;
			}	
		}
	}
}



 
