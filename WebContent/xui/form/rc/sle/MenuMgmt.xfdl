function fn_menuGrpMgmt()
{
 	//업장 선택은
 	if (gfn_nvl(edt_upjang.getUpjangCode(),"") == "") 
	{ 
		gfn_alert("msg.noselect","영업장");
		return;
	}
 
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_gropList);
	ds_outletMenuGrp.clearData();
	gfn_clearSortMark(grd_menuPool);
	ds_menuPool.clearData();
	ds_outletMenu.clearData();
	
	//왼쪽 트리구조 검색
	var strSvcID    = "searchList";
    var strURL      = "rc/sle/menuGrpMgmt/searchList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_outletMenuGrp=ds_outletMenuGrp";
    var strArg      = "outletCd="  + gfn_nvl(edt_upjang.getUpjangCode(),"")  + " maskAuth=" + SCAuth.getMaskAuth(this);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
    //업장을 읽어 온다.
    ds_outletMst.clearData();
  
    var strSvcID    = "searchOutlet";
    var strURL      = "rc/sle/outletMgmt/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_outletMst=ds_outletMst";
    var strArg      = "muCd="      + gfn_nvl(cbo_mu.getMuCode(),"")
					+ " tmCd="     + gfn_nvl(edt_tm.getTmCode(),"")
					+ " outletCd=" + gfn_nvl(edt_upjang.getUpjangCode(),"")
					+ " maskAuth=" + SCAuth.getMaskAuth(this) ;
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
    
}
/*----------------------------------------------------------------------------------------------
 * 설명      : 하단에 업장메뉴 리스트를 검색 한다 상단의 검색 조건에 맞게 검색한다.
 *---------------------------------------------------------------------------------------------*/
 function fn_searchMenuList()
 {
	 
 	 
	if (gfn_nvl(edt_upjang.getUpjangCode(),"") == "") 
	{
		gfn_alert("msg.noselect","영업장");
		return;
	}
	
	// Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_outletMenu);
    ds_outletMenu.clearData();
    
    var chk_use = "";
    
    if(chk_useYn.value == true) {
		chk_use = "Y";
	} else {
		chk_use = "N";	
	}
	 
    var strSvcID    = "selectListMenuList";
    var strURL      = "rc/sle/menuMgmt/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_outletMenu=ds_outletMenu";
    var strArg      = "outletCd="    + gfn_nvl(edt_upjang.getUpjangCode(),"")
					+ " menuClass="  + edt_menuClass.menuClassCd
					+ " menuClsfi="  + edt_menuClsfi.menuClsfiCd
					+ " menuSclass=" + edt_menuSclass.menuSclassCd
					+ " chkUseYn=" + chk_use					
					+ " menuNm="     + edt_menuNm.value;
    
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
 }
 
 
/*----------------------------------------------------------------------------------------------
 * 설명      : fn_search 에서 검색된 내용을 클릭하는 경우 상세 내역을 검색 한다.
 *---------------------------------------------------------------------------------------------*/
function fn_searchList()
{
	 //같은 분류를 선택 하는 경우 계속 검색 하지 않는다.
	var nRow = ds_outletMenuGrp.rowposition;
    var menuClass  = ds_outletMenuGrp.getColumn(nRow, "menuClass");
    var menuClsfi  = ds_outletMenuGrp.getColumn(nRow, "menuClsfi");
