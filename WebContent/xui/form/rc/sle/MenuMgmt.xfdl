/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{

	
	if (gfn_dsIsUpdated(ds_outletMenu)==false && gfn_dsIsUpdated(ds_outletMenuBarcd)==false )
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	//저장시 입력되지 않는 바코드 목록은 모두 삭제 한다.
	//역순으로 삭제 해야 한다. 아니면 인덱스가 맞지 않는 버그가 생긴다.
	//알자나요.
	for (var i=ds_outletMenuBarcd.rowcount - 1; i>=0; i--)
	{	
		if(gfn_nvl(ds_outletMenuBarcd.getColumn(i, "barCd"),"") == "")
		{
			ds_outletMenuBarcd.deleteRow(i);
		}
	}	
	
	// 저장시 Validation Check	
	if (gfn_validCheck(ds_outletMenu, ds_outletMenuValid) == false)	return;
	
	// 저장시 Validation Check	
	if (gfn_validCheck(ds_outletMenuBarcd, ds_outletMenuBarcdValid) == false)	return;
	
	
	//변경한 자료는 모두 변경 불가 처리 한다.
	for (var i=ds_outletMenu.rowcount - 1; i>=0; i--)
	{	
		var nRowType = ds_outletMenu.getRowType(i);
		if (nRowType ==  Dataset.ROWTYPE_UPDATE )
		{
			ds_outletMenu.setColumn(i, "modifyYn", "N");
		}
	}	

    var strSvcID    = "save";
    var strURL      = "rc/sle/menuMgmt/saveData.xdo";
    var strInDs     = "ds_outletMenu=ds_outletMenu:U ds_outletMenuBarcd=ds_outletMenuBarcd:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

