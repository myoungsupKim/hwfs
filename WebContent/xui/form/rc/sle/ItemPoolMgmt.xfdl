/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            //trace("===" + ds_amList.getRowCount());
            break;
        case "searchList":
			fv_searchYn=true;
			break;
		case "searchNewFmItem":
			//trace(ds_menuPool.saveXML());
			ds_menuPool.updatecontrol = false;
			for(var i = 0; i < ds_menuPool.rowcount; i++){
				ds_menuPool.setRowType(i, Dataset.ROWTYPE_INSERT);  
				trace("ROWTYPE_UPDATE ===>>> "+ds_menuPool.getRowType(i));
			}
			ds_menuPool.updatecontrol = true;
			break;
		
        // 저장 시 아이템명중복체크
        case "saveSearch" :
			if (fv_itemNm != "") {
				alert("[" +fv_itemNm +"] 아이템명이 중복됩니다. \n 아이템명을 수정해주세요" );
				div_master.edt_menuNm.setFocus();
			} else {
				//중복아이템명이 없으면 저장
				var strSvcID    = "save";
				var strURL      = "rc/sle/itemPoolMgmt/saveData.xdo";
				var strInDs     = "ds_menuPool=ds_menuPool:U";
				var strOutDs    = "";
				var strArg      = "";
				var strCallback = "fn_callBack";
				var bAsync      = true;
				gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
			}
            break;    
        // 저장
        case "save" :
        
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_searchList();				
		
		    break;    
    }
}


