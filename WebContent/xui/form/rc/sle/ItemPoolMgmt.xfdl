 }
 
 
/*----------------------------------------------------------------------------------------------
 * 설명      : 왼꼭 그룹 리스트를 검색 한다.
 *---------------------------------------------------------------------------------------------*/
function fn_searchGroup()
{
	 
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_gropList);
    gfn_clearSortMark(grd_menuPool);
	ds_menuClsfiList.clearData();
	ds_menuPool.clearData();
	
    var strSvcID    = "searchGroup";
    var strURL      = "rc/sle/itemGrpMgmt/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_menuClsfiList=ds_menuClsfiList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}
/*----------------------------------------------------------------------------------------------
 * 설명      : fn_search 에서 검색된 내용을 클릭하는 경우 상세 내역을 검색 한다.
 *---------------------------------------------------------------------------------------------*/
function fn_searchList()
{
	
	//입력 중인경우
	if (gfn_dsIsUpdated(ds_menuPool))
 	{
		var retValue = gfn_confirm("confirm.before.moveropos");	// 해당 row의 위치를 이동하면 변경된 데이터가 사라집니다.@계속 진행 하시겠습니까?
		if (retValue == false) return;
	}

	 //같은 분류를 선택 하는 경우 계속 검색 하지 않는다.
    var menuClass = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuClass");
    var menuClsfi = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuClsfi");
    var menuSclass = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuSclass");
   
	 //최하위가 아닌경우 해당 내용을 읽을 필요가 없다.
	 
	 var treeKey = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "treeKey");
	 if (treeKey =="0" ) 
	 {
		ds_menuPool.clearData();
		return;
	 }
	 
	 
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_menuPool);
    ds_menuPool.clearData();
    fv_searchYn=false;
    var strSvcID    = "searchList";
    var strURL      = "rc/sle/itemPoolMgmt/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_menuPool=ds_menuPool";
    var strArg      = "menuClass="  + menuClass
					+ " menuClsfi=" + menuClsfi
					+ " menuSclass=" + menuSclass;
    
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}
 
/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	
	var treeKey = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "treeKey");
	if (treeKey =="0" ) 
	{
		gfn_alert("rc.sle.no.data"); //$1 을(를) 선택해 주십시요.
		return;
	}
	
	if (gfn_validCheck(ds_menuPool, ds_menuPoolValid) == false)	return;
	
	
    var menuClass = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuClass");
    var menuClsfi = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuClsfi");
    var menuSclass = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuSclass"); 
    //대,중,소 없는 경우 등록 불가
    if (gfn_nvl(menuClass,"") == "00")
    {
		gfn_alert("msg.noselect","대분류");
		return;
    }
    if (gfn_nvl(menuClsfi,"") == "00")
    {
		gfn_alert("msg.noselect","중분류");
		return;
    }
    if (gfn_nvl(menuSclass,"") == "00")
    {
		gfn_alert("msg.noselect","소분류");
		return;
    }

	var nRowIdx = ds_menuPool.addRow();	//행추가
	ds_menuPool.setColumn(nRowIdx, "useYn", 1);
	ds_menuPool.setColumn(nRowIdx, "useClass", 1);
	
	 
    
    var menuClassNm = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuClassNm");
    var menuClsfiNm = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuClsfiName");	//임시 명칭 좀 애매하네...
    var menuSclassNm = ds_menuClsfiList.getColumn(ds_menuClsfiList.rowposition, "menuClsfiNm"); 	//원필드명

    ds_menuPool.setColumn(nRowIdx, "menuClass", menuClass);
    ds_menuPool.setColumn(nRowIdx, "menuClsfi", menuClsfi);
    ds_menuPool.setColumn(nRowIdx, "menuSclass", menuSclass);
    
   // trace("menuClass:"+menuClass+" menuClsfi:"+menuClsfi+" menuSclass:"+menuSclass);
    
    ds_menuPool.setColumn(nRowIdx, "menuClassNm", menuClassNm);
    ds_menuPool.setColumn(nRowIdx, "menuClsfiNm", (menuClsfi=="00"?"":menuClsfiNm));
    ds_menuPool.setColumn(nRowIdx, "menuSclassNm", (menuSclass=="00"?"":menuSclassNm));
    
    div_master.edt_menuNm.setFocus();
    
    //trace("menuClassTo:"+menuClass+" menuClsfiTo:"+menuClsfi+" menuSclassTo:"+menuSclass);
    //trace("menuClassNm:" + menuClassNm + " menuClsfiNm:"+ menuClsfiNm+ " menuSclassNm:"+menuSclassNm);
    
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_menuPool.rowposition;
	
	if(ds_menuPool.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	var upjang_nm = ds_menuPool.getColumn(nRow,"uNm");
	//alert(upjang_nm);
	if(upjang_nm == "")
	{
		var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
		if (rtVal == true) 
		{	
			var bIsProcRow = false;
			if (ds_menuPool.getRowType(nRow) == Dataset.ROWTYPE_INSERT)
			{
				ds_menuPool.deleteRow(nRow);
			} else
			{
				ds_menuPool.deleteRow(nRow);
				sMsg = "임시 삭제되었습니다!\n\n저장 후에 실제 삭제가 되오니 착오없으시기 바랍니다.";
				gfn_alert(sMsg, "", "A");
			}
			 
		}
	} else {
		alert("업장에서 사용중인 아이템은 삭제불가합니다.");
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{

	
	if (gfn_dsIsUpdated(ds_menuPool)==false )
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	
 	// 저장시 Validation Check	 "rc/sle/itemGrpMgmt/searchList.xdo";
	if (gfn_validCheck(ds_menuPool, ds_menuPoolValid) == false)	return;
	
	//중복아이템 검색하기 위해 데이타셋 복사
	ds_menuPoolSearch.clear();
 	ds_menuPoolSearch.assign(ds_menuPool);
 	
 	var strSvcID    = "saveSearch";
    var strURL      = "rc/sle/itemPoolMgmt/saveSearch.xdo";
    var strInDs     = "ds_menuPoolSearch=ds_menuPoolSearch:U";
