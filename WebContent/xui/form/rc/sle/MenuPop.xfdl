//그리드 더블클릭
function grd_outletMenu_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if (ds_outletMenuList.getRowCount() > 0) 
	{ 
		ds_outletMenuList.setColumn(ds_outletMenuList.rowposition,"chkYn","1");
		fn_Ok();
	 }
}
//그리드 엔터
function grd_outletMenu_onkeydown(obj:Grid, e:KeyEventInfo)
{
	if(e.keycode =="13") 
	{
		ds_outletMenuList.setColumn(ds_outletMenuList.rowposition,"chkYn","1");
		fn_Ok();
	}
}

//선택반환
function fn_Ok()
{

	var retVal = {
		dataRow : 0,
		datas : null
	};
	var arr = new Array();
	 
	if (ds_outletMenuList.getRowCount() > 0)
	{
		var n = 0;
		for (var i=0; i<ds_outletMenuList.getRowCount(); i++)
		{
			if (ds_outletMenuList.getColumn(i, "chkYn") == true)
			{
				var arrCol = new Array();
				arrCol["outletCd"] = ds_outletMenuList.getColumn(i, "outletCd");
				arrCol["outletNm"] = ds_outletMenuList.getColumn(i, "outletNm");
				arrCol["menuCd"]   = ds_outletMenuList.getColumn(i, "menuCd");
				arrCol["menuNm"]   = ds_outletMenuList.getColumn(i, "menuNm");
				
				arrCol["menuClass"]    = ds_outletMenuList.getColumn(i, "menuClass");
				arrCol["menuClassNm"]  = ds_outletMenuList.getColumn(i, "menuClassNm");
				arrCol["menuClsfi"]    = ds_outletMenuList.getColumn(i, "menuClsfi");
				arrCol["menuClsfiNm"]  = ds_outletMenuList.getColumn(i, "menuClsfiNm");
				arrCol["menuSclass"]   = ds_outletMenuList.getColumn(i, "menuSclass");
				arrCol["menuSclassNm"] = ds_outletMenuList.getColumn(i, "menuSclassNm");
				
				arrCol["applyPrice"] = ds_outletMenuList.getColumn(i, "applyPrice");
				 
				arr[n] = arrCol;
				n++;
			}
		}
		if(n==0)
		{
			var i = ds_outletMenuList.rowposition;
			var arrCol = new Array();
			arrCol["outletCd"] = ds_outletMenuList.getColumn(i, "outletCd");
			arrCol["outletNm"] = ds_outletMenuList.getColumn(i, "outletNm");
			arrCol["menuCd"]   = ds_outletMenuList.getColumn(i, "menuCd");
			arrCol["menuNm"]   = ds_outletMenuList.getColumn(i, "menuNm");
			
			arrCol["menuClass"]    = ds_outletMenuList.getColumn(i, "menuClass");
			arrCol["menuClassNm"]  = ds_outletMenuList.getColumn(i, "menuClassNm");
			arrCol["menuClsfi"]    = ds_outletMenuList.getColumn(i, "menuClsfi");
			arrCol["menuClsfiNm"]  = ds_outletMenuList.getColumn(i, "menuClsfiNm");
			arrCol["menuSclass"]   = ds_outletMenuList.getColumn(i, "menuSclass");
			arrCol["menuSclassNm"] = ds_outletMenuList.getColumn(i, "menuSclassNm");
			
			arrCol["applyPrice"] = ds_outletMenuList.getColumn(i, "applyPrice");
			 
			arr[n] = arrCol;
		}
		
		retVal.dataRow = arr.length;
		retVal.datas = arr;
		this.close(retVal);
		 
	}


}
  
