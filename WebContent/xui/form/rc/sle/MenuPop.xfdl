 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	//업장 선택은
	if (gfn_nvl(edt_outletNm.outletCd) == "") 
	{
		gfn_alert("msg.noselect","영업장");
		return;
	}
	
	// Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_outletMenu);
    ds_outletMenuList.clearData();

	 
    var strSvcID    = "selectListMenuList";
    var strURL      = "rc/sle/menuMgmt/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_outletMenuList=ds_outletMenu";
    var strArg      = "outletCd="    + gfn_nvl(edt_outletNm.outletCd,"")
					+ " menuClass="  + gfn_nvl(edt_menuClass.menuClassCd,"")
					+ " menuClsfi="  + gfn_nvl(edt_menuClsfi.menuClsfiCd,"")
