	edt_menuClass.value = "";
	edt_menuClsfi.value = "";
	edt_menuSclass.value = "";
	edt_menuNm.value = "";
	
	//SCCombo.bind(this, "ComCode", "cbo_serchUseClass",      null, {groupCode:"SLE_0003", useYn:"Y"}, SCCombo.ALL);	//사용유무
	
