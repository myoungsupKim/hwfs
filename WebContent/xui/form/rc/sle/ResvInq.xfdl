    var strInDs     = "";
    var strOutDs    = "ds_reservMst=ds_reservMst";
    var strArg      = "outletCd="     + gfn_nvl(edt_upjang.getUpjangCode(),"") 			//업장
					+ " tableGrp="    + tableGrp
					+ " tableCd="     + tableCd
					+ " reservDate="  + cal_serchMonth.text + nDay
					+ " reservClass=" + reservClass
					+ " maskAuth="  + SCAuth.getMaskAuth(this);
