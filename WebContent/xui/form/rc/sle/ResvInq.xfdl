/************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역 (필수)
************************************************************************************************/
 
function grd_rsrvMast_onselectchanged(obj:Grid, e:GridSelectEventInfo)
{ 
	/*----------------------------------------------------------------------------------------------
	 * 설명      : 검색된 그리드를 클릭 하는 경우 해당 내역을 설정 한다.
	 *---------------------------------------------------------------------------------------------*/
	var nDay ="";
	var tableGrp = "";
	var tableCd ="";
	var reservClass="";
	var nData="";
	
	tableGrp = ds_reservList.getColumn(ds_reservList.rowposition,"tableGrp");
	tableCd = ds_reservList.getColumn(ds_reservList.rowposition,"tableCd");
	
	if (e.cell >= 4 && e.cell <= 34)
	{
		
		var sDay = e.cell - 3;
		nDay = (sDay.toString().length==1?"0"+sDay.toString():sDay.toString());
		nData = ds_reservList.getColumn(ds_reservList.rowposition,"b"+nDay );
		reservClass = ds_reservList.getColumn(ds_reservList.rowposition,"reservBclass");
		
	}
	if (e.cell >= 36 && e.cell <= 66)
	{
		
		var sDay = e.cell - 35;
		nDay = (sDay.toString().length==1?"0"+sDay.toString():sDay.toString());
		nData = ds_reservList.getColumn(ds_reservList.rowposition,"l"+nDay );
		reservClass =  ds_reservList.getColumn(ds_reservList.rowposition,"reservLclass");
	}
	if (e.cell >= 68 && e.cell <= 98)
	{
		var sDay = e.cell - 67;
		nDay = (sDay.toString().length==1?"0"+sDay.toString():sDay.toString());
		nData = ds_reservList.getColumn(ds_reservList.rowposition,"d"+nDay);
		reservClass =  ds_reservList.getColumn(ds_reservList.rowposition,"reservDclass");
	}
	
	if (nData =="")
	{
		ds_reservMst.clearData();
		ds_reservGoods.clearData();
	} else
	{
		fn_reservMst(nDay,tableGrp,tableCd,reservClass);
	}
	
}

function btn_sizeGrid_onclick(obj:Button,  e:ClickEventInfo)
{
	/*----------------------------------------------------------------------------------------------
	 * 설명      : 날자 부분을 크기를 1씩 증가 한다.
	 *---------------------------------------------------------------------------------------------*/
// 	var varProperty = grd_rsrvMast.getFormatColProperty(4,"size");  
// 	trace(varProperty);
// 	trace(Number(varProperty) + 1);
	if(gfn_isDigit(edt_size.value) == false) edt_size.value="170"
	
	for(var i=4; i < grd_rsrvMast.getCellCount("Head"); i++)
	{
		var bSucc = grd_rsrvMast.setFormatColProperty(i, "size",Number(edt_size.value) );
		
	}
}
  



