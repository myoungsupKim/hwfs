	SCCombo.bind(this, "ComCode", "cbo_calMethod",  null, {groupCode:"SLE_0002", useYn:"Y",defVal:"firstrow"});	//계산방법
	SCCombo.bind(this, "ComCode", "cbo_vatPrint",   null, {groupCode:"SLE_0004", useYn:"Y",defVal:"firstrow"});	//세금출력여부
	SCCombo.bind(this, "ComCode", "cbo_svcPrint",   null, {groupCode:"SLE_0004", useYn:"Y",defVal:"firstrow"});	//봉사료출력
	SCCombo.bind(this, "ComCode", "cbo_useYn",      null, {groupCode:"SLE_0003", useYn:"Y",defVal:"firstrow"});	//사용유무
	
	SCCombo.bind(this, "ComCode", "grd_outletList.outletGrp",  null, {groupCode:"SLE_0110", useYn:"Y"},SCCombo.CHOOSE);	//업장그룹
	SCCombo.bind(this, "ComCode", "grd_outletList.outletBgn",  null, {groupCode:"SLE_0112", useYn:"Y"},SCCombo.CHOOSE);	//업장구분 (대분류)
