function fn_delete()
{
	var nRow = ds_outletMst.rowposition;
	
	if(ds_outletMst.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		var bIsProcRow = false;
		if (ds_outletMst.getRowType(nRow) == Dataset.ROWTYPE_INSERT)
		{
			ds_outletMst.deleteRow(nRow);
		} else
		{
			var sMsg = "사용유무를 체크 하시고 저장 처리 하십시요!";
			gfn_alert(sMsg, "", "A");
		}
		 
	}
}

