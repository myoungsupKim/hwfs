function fn_search()
{
	 
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_outletList);
	ds_outletMst.clearData();
  
    var strSvcID    = "search";
    var strURL      = "rc/sle/outletMgmt/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_outletMst=ds_outletMst";
    var strArg      = "muCd="      + gfn_nvl(cbo_mu.getMuCode(),"")
					+ " tmCd="     + gfn_nvl(edt_tm.getTmCode(),"")
					+ " outletCd=" + gfn_nvl(edt_upjang.getUpjangCode(),"")
					+ " useYn="    + (chk_useYn.value?1:0)
					+ " maskAuth=" + SCAuth.getMaskAuth(this) ;
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}
/*----------------------------------------------------------------------------------------------
 * 설명      : fn_search 에서 검색된 내용을 클릭하는 경우 상세 내역을 검색 한다.
 차후 다시 작업 해야함 현재는 사용 안함....
 *---------------------------------------------------------------------------------------------*/
 
 
/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	


	for (var i=0; i < ds_outletMst.rowcount ; i++)
	{
		var bStat = ds_outletMst.selectRow(i,false);
	}
	
	var nRow = ds_orgzloctree.rowposition;
	var orgUpjangCd = ds_orgzloctree.getColumn(nRow,"upjang");
	trace("orgUpjangCd:" + orgUpjangCd + " Row:" + nRow);
	
	if (orgUpjangCd <= 0)
	{
		return;
	}
	
	for (var i=0; i < ds_outletMst.rowcount ; i++)
	{
		
		var upjangCd = ds_outletMst.getColumn(i,"outletCd");
		if (orgUpjangCd == upjangCd)
		{
			var sMsg = "이미 등록된 업장 코드가 존재합니다.\r\r행 번호 : " + (i+1) + "\r입력 값 : " + ds_outletMst.getColumn(i, "outletCd") + "(" +  ds_outletMst.getColumn(i, "outletNm")   + ")" ;
			gfn_alert(sMsg, "", "A");
			return;
		}
	}

	//검색을 해야함
	//자료을 읽어서 존재 여부를 확인 해야 한다.
	ds_outletMstCheck.clearData();
	var strSvcID    = "searchCheck";
    var strURL      = "rc/sle/outletMgmt/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_outletMstCheck=ds_outletMst";
    var strArg      = "outletCd="  + gfn_nvl(orgUpjangCd,"")
					+ " useYn="    + "0"
					+ " maskAuth=" + SCAuth.getMaskAuth(this) ;
    var strCallback = "fn_callBack";
	var bAsync      = false;
	
	gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	if (ds_outletMstCheck.rowcount > 0)
	{
		var sMsg = "이미 등록된 업장 코드가 존재합니다. " ;
		gfn_alert(sMsg, "", "A");
		return;
	}

	var nRowIdx = ds_outletMst.addRow();	//행추가
	ds_outletMst.setColumn(nRowIdx, "useYn", 1);
	 
	cbo_calMethod.index=0;
	cbo_vatPrint.index=0;
	cbo_svcPrint.index=0;
	cbo_outletType.index=1;
	 

	ds_outletMst.setColumn(nRowIdx, "outletCd", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"upjang"));
	ds_outletMst.setColumn(nRowIdx, "outletNm", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"upjangnm"));
	ds_outletMst.setColumn(nRowIdx, "calMethodNm", cbo_calMethod.text);
	ds_outletMst.setColumn(nRowIdx, "vatPrintNm", cbo_vatPrint.text );
	ds_outletMst.setColumn(nRowIdx, "svcPrintNm", cbo_svcPrint.text );
	ds_outletMst.setColumn(nRowIdx, "outletTypeNm", cbo_outletType.text);
	
	ds_outletMst.setColumn(nRowIdx, "startDate", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"startDate"));
	ds_outletMst.setColumn(nRowIdx, "endDate", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"endDate"));
	ds_outletMst.setColumn(nRowIdx, "startTime", "0001" );
	ds_outletMst.setColumn(nRowIdx, "endTime", "2359" );

	ds_outletMst.setColumn(nRowIdx, "ceoNm", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"drname"));		//대표자
	ds_outletMst.setColumn(nRowIdx, "telNum", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"telNum"));		//전화번호
	ds_outletMst.setColumn(nRowIdx, "telNum1", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"telNum1"));
	ds_outletMst.setColumn(nRowIdx, "telNum2", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"telNum2"));
	ds_outletMst.setColumn(nRowIdx, "telNum3", ds_orgzloctree.getColumn(ds_orgzloctree.rowposition,"telNum3"));
	
