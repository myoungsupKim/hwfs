function fn_TriggerEvent(p) 
{
	if(cbo_outletSub.getCount() > 1 ) 
	{
		cbo_outletSub.visible =true;
		stat_outletSub.visible=true;
	}else
	{
		cbo_outletSub.visible =false;
		stat_outletSub.visible=false;
	}
}

