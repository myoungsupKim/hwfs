function ds_data_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
alert(e.eventid);
	if(e.eventid == "useAmt"){
		ds_data.setColumn(0, "exchAmt", Number(ds_data.getColumn(0, "couponAmt")) - Number(ds_data.getColumn(0, "useAmt")));
	}
}

function ds_data_onvaluechanged(obj:Dataset,  e:DSColChangeEventInfo)
{
	
	if(!gfn_isNull(ds_data.getColumn(0, "couponAmt")) && !gfn_isNull(ds_data.getColumn(0, "useAmt"))){
		if(Number(ds_data.getColumn(0, "couponAmt")) < Number(ds_data.getColumn(0, "useAmt"))){
			alert("사용금액을 정확히 입력해 주세요.");
			ds_data.setColumn(0, "useAmt", 0)
			return;
		}
		ds_data.setColumn(0, "exchAmt", Number(ds_data.getColumn(0, "couponAmt")) - Number(ds_data.getColumn(0, "useAmt")));
	}
}
