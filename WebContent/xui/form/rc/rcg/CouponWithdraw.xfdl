************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";
	
	ds_search.clearData();
	ds_search.addRow();
	
	SCPopup.bind(this, "CcCode", 	"edt_ccNm", 	"btn_ccCdDtl", 		null, {}, {after:"fn_popCcCdAfter"}); 	//CC코드팝업(입력)
	SCCombo.bind(this, "ComCode", "cbo_posType", null, {groupCode:"POS_TYPE_CD", useYn:"Y"}, SCCombo.ALL);		//POS 유형
	SCCombo.bind(this, "ComCode", "cbo_focClass", null, {groupCode:"FOC_CLASS", useYn:"Y"}, SCCombo.ALL);		//유무상 구분
	SCCombo.bind(this, "ComCode", "cbo_couponGrpCd", null, {groupCode:"COUPON_GRP_CD", useYn:"Y"}, SCCombo.ALL);	//쿠폰그룹
	SCCombo.bind(this, "ComCode", "cbo_couponWithdrawClass", null, {groupCode:"COUPON_WITHDRAW_CLASS", useYn:"Y"}, SCCombo.ALL);			//상품권 유형
	
	//달력 초기화
	cal_useDateFr.fn_initMonthCal(this, fv_sToday, null, ds_search, "useDateFr", "Y");
	cal_useDateTo.fn_initMonthCal(this, fv_sToday, null, ds_search, "useDateTo", "Y");
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 업장CC Combo 조회 후 처리
 *---------------------------------------------------------------------------------------------*/
function fn_popCcCdAfter(p, data){
	//if(data.code != null && data.code != undefined){
	if(gfn_transNullToEmpty(data.code)){
		edt_ccCd.value = data.code;
		edt_ccNm.value = data.codeName;
	}
}

