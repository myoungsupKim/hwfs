/*----------------------------------------------------------------------------------------------
 * 설명      : 업장의 팝업을 호출한 후에 발생되는 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_upjangAfterEvent(obj, cls)
{
	if(!gfn_isNull(cls.tmCd)){
		ds_data.setColumn(0,"tmCd", cls.tmCd);
		ds_data.setColumn(0,"tmNm", cls.tmNm);
	}
	ds_data.setColumn(0,"ccCd", cls.ccCd);
	ds_data.setColumn(0,"ccNm", cls.ccNm);
	return true;
