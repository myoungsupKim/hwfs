/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            break;
        // 상세내역 조회
        case "searchDetail" :
            //trace(ds_list.saveXML());
			if(ds_listDetail.getColumn(0, "couponWithdrawClass")=="2"){
				btn_cDel.visible = true;
			}else{
				btn_cDel.visible = false;
			}	
            break;
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			ds_data.clearData();
			fn_search();
            break;    
        case "cancel" :
			gfn_alert("쿠폰 회수 취소 처리가 완료 되었습니다.");	// 저장 되었습니다.
			ds_listDetail.clearData();
			fn_search();
            break;    
    }
}


