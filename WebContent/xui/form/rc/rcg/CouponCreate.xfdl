function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	//쿠폰 번호가 생성된 이후에는 발행 매수 수정 불가처리
	if(ds_list.getRowType(e.row) == 2){
		btn_couponKind.enable=true;	//기존 데이터에 대해서는 쿠폰종류 선택안되게 처리
	}else{
		btn_couponKind.enable=false;	//기존 데이터에 대해서는 쿠폰종류 선택안되게 처리
	}
	
	if(edt_couponNumFr.value == ""){
		edt_couponPublishPcnt_onkillfocus();
	}

	if(ds_list.getColumn(e.row, "couponCreationYn")=="N" || ds_list.getColumn(e.row, "couponCreationYn") == undefined){
		edt_publishClass.readonly=false;
		edt_couponPublishPcnt.readonly=false;
	}else{
		edt_publishClass.readonly=true;
		edt_couponPublishPcnt.readonly=true;
	}
	
	//cal_deliverDate00.fn_initMonthCal(this, ds_list.getColumn(e.row, "publishDate"), null, ds_list, "deliverDate", "Y");
}

function btn_couponCreation_onclick(obj:Button,  e:ClickEventInfo)
{
	if(edt_couponCreationYn.value!='N'){
		alert("쿠폰은 미발행 상태에서만 번호를 생성할수 있습니다.");
		return;
	}
	if(!gfn_transNullToEmpty(edt_couponPublishPcnt.value)){
		alert("발행 매수를 입력해 주세요.");
		edt_couponPublishPcnt.setFocus();
		return;
	}
	if(!gfn_transNullToEmpty(edt_muCd.value)){
		alert("원가 부서를 다시 선택해 주세요.");
		return;
	}
	
	var nRow = ds_list.rowposition;
	ds_data.clearData();
	ds_data.addRow();
	ds_data.copyRow(0, ds_list, nRow);
	
	ds_data.setColumn(0, "couponCreationYn", "Y");
		
    var strSvcID    = "create";
    var strURL      = "rc/rcg/couponCreate/createProc.xdo";
    var strInDs     = "ds_data=ds_data:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);	
}

function edt_couponPublishPcnt_onkillfocus(obj:MaskEdit, e:KillFocusEventInfo)
{
	if(edt_couponKindCd.value == null || edt_couponKindCd.value == undefined){
		alert("쿠폰 종류를 선택해 주세요.");return;
	}
	if(edt_publishClass.value == null || edt_publishClass.value == undefined){
		alert("발행 구분을 입력해 주세요.");return;
	}
	if(edt_couponPublishPcnt.value=="" || edt_couponPublishPcnt.value==undefined){
		alert("발행 매수를 입력해 주세요.");return;
	}
//	if(edt_publishReason.value=="" || edt_publishReason.value==undefined){
//		alert("발행 사유를 입력해 주세요.");return;
//	}
	
	if(edt_deliverSeq.value == null || edt_deliverSeq.value == undefined){
		edt_deliverSeq.value="";
	}
	ds_couponNum.clearData();
    var strSvcID    = "searchCouponNum";
    var strURL      = "rc/rcg/couponCreate/selectCouponNum.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_couponNum=ds_couponNum";
    var strArg      = "publishClass=" + edt_publishClass.value +" couponPublishPcnt=" + edt_couponPublishPcnt.value +" couponKindCd=" + ds_list.getColumn(ds_list.rowposition, "couponKindCd") +" deliverDate=" + ds_list.getColumn(ds_list.rowposition, "deliverDate") +" deliverSeq=" + ds_list.getColumn(ds_list.rowposition, "deliverSeq");
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);	
}

function fn_procUserPop()
{
	if(ds_list.rowcount==0){
		alert("조회를 먼전 실행 한후 진행해 주시기 바랍니다.");
		return;
	}
}
