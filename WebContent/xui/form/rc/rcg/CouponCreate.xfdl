/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_list.rowposition;
	trace(ds_list.saveXML());
	if(ds_list.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	if(ds_list.getColumn(nRow , "delCnt") > 0){
		gfn_alert("판매된 쿠폰이 있거나\n\n쿠폰상태가 변경되어 삭제가 불가능 합니다.");
		return;
	}
	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		ds_list.deleteRow(nRow);
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	for(i=0; i<= ds_list.getRowCount(); i++){
		if(ds_list.getRowType(i) != 0){
			if(edt_publishReason.value=="" || edt_publishReason.value==undefined){
				alert("발행 사유를 입력해 주세요.");return;
			}
		}
	}
	// 저장시 Validation Check	
	if (gfn_validCheck(grd_list, ds_listValid) == false)	return;
    var strSvcID    = "save";
    var strURL      = "rc/rcg/couponCreate/saveList.xdo";
    var strInDs     = "ds_list=ds_list:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

