/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.	
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			if(ds_list.getRowCount() == 0){
				gfn_loadDatasetInfo (ds_list, fv_objColInfo);
			}
			trace("fv_processFail : " + fv_processFail);
			if(gfn_length(fv_processFail) > 0 && fv_processFail == "fail"){
				alert("삭제하지 못한 쿠폰이 존재합니다.\n\n재 조회 후 삭재 해 주세요.");
				fv_processFail = "";//reSet
			}
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break; 
               
        // 번호생성
        case "create" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.			
			fn_search();
            break;    
            
        // 번호조회
        case "searchCouponNum" :
			if(ds_couponNum.getRowCount() > 0){
				ds_list.setColumn(ds_list.rowposition, "couponNumFr", ds_couponNum.getColumn(0,"couponNumFr"));
				ds_list.setColumn(ds_list.rowposition, "couponNumTo", ds_couponNum.getColumn(0,"couponNumTo"));
				ds_list.setColumn(ds_list.rowposition, "maxCouponNumSeq", ds_couponNum.getColumn(0,"maxCouponNumSeq"));
			}else{
				alert("입력된 발행구분으로 쿠폰 번호를 생성하지 않은 데이터가 있습니다.");
				ds_list.setColumn(ds_list.rowposition, "couponNumFr", "");
				ds_list.setColumn(ds_list.rowposition, "couponNumTo", "");
				ds_list.setColumn(ds_list.rowposition, "maxCouponNumSeq", "");
			}
    }
}


