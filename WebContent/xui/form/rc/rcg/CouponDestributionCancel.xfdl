function btn_deploy_onclick(obj:Button,  e:ClickEventInfo)
{
	// 배포처리시  Validation Check
	if (gfn_validCheck(ds_search, ds_dataValid, true) == false) return;
	
 	// v_ds_check 사용시 공통버튼에서 변경사항 체크함
 	if (gfn_isDataChange() == false) 
 	{		
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	fn_save();
}

