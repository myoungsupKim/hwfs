/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.	
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break; 
               
        // 번호생성
        case "create" :
			gfn_alert("할인권이 생성되었습니다.");	// 저장 되었습니다.
			SCCombo.bind(this, "ComCode", "cbo_posDisc", null, {groupCode:"POS_DISC_PRM_CD", useYn:"Y"}, SCCombo.ALL);
			//fn_search();
            break;    
            
    }
}


