/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            edt_couponKindCd.enable=false;
            fn_init();
            break;
        
		// 코드분류 조회
        case "upjangsearch" :
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;   
             
        // 삭제
        case "delete" :
			gfn_alert("msg.delete.success");	// 삭제 되었습니다.
			fn_close();
            break;    
            
 		// 쿠폰 코드 중복 조회
        case "selectKindCount" :
            if(ds_kindcount.getColumn(0, "kindcount")=="0"){
				edt_couponKindCd.enable=false;
			}else{
				alert("종류 코드에 이미 입력된 코드가 있습니다.");
				edt_couponKindCd.value="";
			}

            break;
           
    }
}


