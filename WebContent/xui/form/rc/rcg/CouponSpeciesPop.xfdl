 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_list.rowposition;
	
	if(ds_list.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		ds_list.deleteRow(nRow);
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
// 	// v_ds_check 미사용시 변경사항 체크
 	if ((gfn_dsIsUpdated(ds_list) == false) && (gfn_dsIsUpdated(ds_upjang) == false))
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}

	if (gfn_validCheck(ds_list, ds_listValid) == false)	return;
	
	if (cbo_recdStatus.value == "Y"){
	    ds_list.setColumn(0,"recdStatus","L");
	} else if (cbo_recdStatus.value == "N"){
	    ds_list.setColumn(0,"recdStatus","D");
	}
		
    var strSvcID    = "save";
    var strURL      = "rc/rcg/couponSpeciesPop/saveList.xdo";
    var strInDs     = "ds_list=ds_list:A ds_upjang=ds_upjang";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

