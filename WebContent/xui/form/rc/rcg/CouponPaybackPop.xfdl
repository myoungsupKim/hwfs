function cbo_payType_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(obj == null)return;
	
	var val = obj.value;
	
	var nRow  = ds_typeDetail.findRow("typeCd", val);
	
	if(ds_view.rowcount == 0){
	    ds_view.clearData();
	    ds_view.addRow();
	}
	
	var acctCd = ds_typeDetail.getColumn(nRow, "acctCd");   //계정코드
	var acctNm = ds_typeDetail.getColumn(nRow, "acctNm");   //계정명
	
	edt_acctCd.value = acctCd;
	edt_acctNm.value = acctNm;
}

