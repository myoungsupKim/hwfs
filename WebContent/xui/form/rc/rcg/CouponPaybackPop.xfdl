{
    var strSvcID    = "search";
    var strURL      = "rc/rcg/couponPaybackPop/selectInfo.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_view=ds_view ds_couponSelect=ds_couponSelect";
    var strArg      = "";
