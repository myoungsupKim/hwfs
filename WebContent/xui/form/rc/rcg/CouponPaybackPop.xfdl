function fn_setCombo(){
						
	// 입력:지불그룹	SCCombo.CHOOSE					
	SCCombo.bind(	this
					,"ComCode"
					,"cmb_payGroup"
					,null
					,{groupCode:"PAY_GROUP", useYn:"Y"}
					,SCCombo.CHOOSE
					,false);
					
	if(!gfn_isNull(cmb_payGroup.value)){

		// 입력:지불형태	SCCombo.CHOOSE					
		SCCombo.bind(	this
						,"ComCode"
						,"cmb_payType"
						,null
						,{groupCode:"PAY_TYPE", set1:cmb_payGroup.value, useYn:"Y"}
						,SCCombo.CHOOSE
						,false);
	}
}
/*----------------------------------------------------------------------------------------------
 * 지불그룹 변경시 발생 EVENT
