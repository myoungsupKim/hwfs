/*----------------------------------------------------------------------------------------------
 * 설명      : 첨부파일 DB저장
 *---------------------------------------------------------------------------------------------*/
function fn_saveRecipeFile()
{
	var strSvcID    = "fileSave";
    var strURL      = "rc/rrm/standardRecipeMasterMgnt/saveRecipeFile.xdo";
    var strInDs     = "ds_file=ds_file:A";
    var strOutDs    = "fv_RefSeq=fv_RefSeq ds_file=ds_file";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = false;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역 (필수)
 ************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : ds_list RowPos 이벤트 (입력필드 키값 활성화 비활성화)
 *---------------------------------------------------------------------------------------------*/
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	var rowType = obj.getRowType(e.newrow);

	// 표준레시피에 등록된 가상레시피는 수정불가 상태로 둔다.
	if( rowType == 2 || obj.getColumn(e.newrow, "useYn") == "Y" )
	{
		cbo_upjangType.enable = true;
		edt_stdRecipeCd.enable = true;
		edt_recipeNm.enable = true;
		edt_recipeNmEnglish.enable = true;
		edt_recipeNmBynm.enable = true;
		cbo_stand.enable = true;
		cbo_mcalssCd.enable = true;
		cbo_sclassCd.enable = true;
		edt_oferNumber.enable = true;
		cbo_oferUnit.enable = true;
		edt_recipeDvlpBy.enable = true;
		cal_regDate.enable = true;
		edt_salePrice.enable = true;
		edt_remark.enable = true;
	}
	else
	{
		cbo_upjangType.enable = false;
		edt_stdRecipeCd.enable = false;
		edt_recipeNm.enable = false;
		edt_recipeNmEnglish.enable = false;
		edt_recipeNmBynm.enable = false;
		cbo_stand.enable = false;
		cbo_mcalssCd.enable = false;
		cbo_sclassCd.enable = false;
		edt_oferNumber.enable = false;
		cbo_oferUnit.enable = false;
		edt_recipeDvlpBy.enable = false;
		cal_regDate.enable = false;
		edt_salePrice.enable = false;
		edt_remark.enable = false;
	}

	if( rowType == 1 || rowType == 4 )
	{
		if( !gfn_isNull(obj.getColumn(e.newrow, "relPath")) )
		{
			if( obj.getColumn(e.newrow, "imgModifyYn") == "Y" )
				img_viewer.image = obj.getColumn(obj.rowposition, "relPath");
			else
				img_viewer.image = gv_webImgRoot + obj.getColumn(e.newrow, "relPath") + "/" + obj.getColumn(e.newrow, "saveFileNm");
		}
		else
		{
			img_viewer.image = "";
		}

		cbo_upjangType.enable = false;
	}
	else
	{
		cbo_upjangType.enable = true;

		if( !gfn_isNull(obj.getColumn(e.newrow, "relPath")) )
			img_viewer.image = obj.getColumn(obj.rowposition, "relPath");
		else
			img_viewer.image = "";
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 레시피명 onkeydown 이벤트
