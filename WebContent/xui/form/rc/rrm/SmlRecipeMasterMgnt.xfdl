*/

/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
 ************************************************************************************************/
include "lib::lib_com.xjs";

/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
 ************************************************************************************************/
var fv_RefSeq;
var fv_userRole;
/************************************************************************************************
 * FORM EVENT 영역 (필수)
 ************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);

	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";

	// 검색조건을 초기화 한다.
	var nRow = ds_search.addRow();
	ds_search.setColumn(nRow, "muCd", "2003");	// 사업부는 외식사업부 고정
	ds_search.setColumn(nRow, "upjangType", "");
	ds_search.setColumn(nRow, "recipeNm", "");

	// 외식 담당자 권한 체크
	if( SCAuth.isUserRole("담당자_외식메뉴") )
	{
		fv_userRole = "Y";
		ds_search.setColumn(nRow, "smlUpjang", "");
	}
	else
	{
		fv_userRole = "N";
		ds_search.setColumn(nRow, "smlUpjang", g_Upjang);
	}

	var nRow2 = ds_searchDtl.addRow();
	ds_searchDtl.setColumn(nRow2, "muCd", "");

	SCCombo.bind(this, "ComCode", "cbo_schUpjangType", ds_upjangType, {groupCode:"UPJANG_TYPE", useYn:"Y", defVal:"11"}, SCCombo.ALL);	// 업장유형
	SCCombo.bind(this, "ComCode", "cbo_stand", ds_standType, {groupCode:"STAND_TYPE", useYn:"Y", defVal:"01"});							// 레시피단계
	SCCombo.bind(this, "ComCode", "cbo_oferUnit", ds_oferUnit, {groupCode:"OFER_UNIT", useYn:"Y", defVal:"A"});							// 투입단위
	SCCombo.bind(this, "ComCode", "cbo_mcalssCd", ds_mcalssCd, {groupCode:"MCALSS_CD", useYn:"Y"});										// 중분류코드
	SCCombo.bind(this, "ComCode", "cbo_sclassCd", ds_sclassCd, {groupCode:"SCLASS_CD", useYn:"Y"});										// 소분류코드
	SCCombo.bind(this, "ComCode", "cbo_schSclassCd", null, {groupCode:"SCLASS_CD", useYn:"Y"}, SCCombo.ALL);							// 소분류코드(검색조건)

	SCDataset.bind(this, "ComCode", "ds_muCd", {groupCode:"DATA_SET", code:"DEF_MU_CODE", useYn:"Y"}, "fn_triggerDatasetEvent");

	fn_searchRsr();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{
	// v_ds_check 사용시 변경사항 체크
	if( gfn_length(v_ds_check) != 0 )
	{
		// v_ds_check 설정기준 변경사항 체크
		if( gfn_isDataChange() )
		{
			// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
			if( !gfn_confirm("confirm.before.movepage") ) return false;
		}
	}

	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}

/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
 ************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// 조회시 Validation Check
	if( !gfn_validCheck(ds_search, ds_searchValid, true) )	return;

	var strSvcID    = "search";
	var strURL      = "rc/rrm/smlRecipeMasterMgnt/selectList.xdo";
	var strInDs     = "ds_search=ds_search:A";
	var strOutDs    = "ds_list=ds_list";
	var strArg      = "";
	var strCallback = "fn_callBack";
	var bAsync      = true;

	gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{
	var nRow = ds_list.addRow();
	ds_list.setColumn(nRow, "upjangType", ds_upjangType.getColumn(0, "code"));	// 업장유형
	ds_list.setColumn(nRow, "upjangCd", g_Upjang);								// 업장
	ds_list.setColumn(nRow, "muCd", "2003");									// 사업부
	ds_list.setColumn(nRow, "useYn", "Y");										// 사용여부
	ds_list.setColumn(nRow, "cookUnit", "1");									// 조리단위(Def:1)
	ds_list.setColumn(nRow, "offerUnit", "B");									// 조리단위(Def:1)
	ds_list.setColumn(nRow, "regDate", gfn_today());							// 등록일자
	ds_list.setColumn(nRow, "useYn", "Y");										// 상태

	if( fv_userRole == "Y" )
	{
		ds_list.setColumn(nRow, "recipeDvlpBy", "RnD");
		ds_list.setColumn(nRow, "smlUpjang" , "118216");
		ds_list.setColumn(nRow, "smlUpjangNm" , "RnD");
	}
	else
	{
		ds_list.setColumn(nRow, "recipeDvlpBy", g_UpjangNm);
		ds_list.setColumn(nRow, "smlUpjang" , g_Upjang);
		ds_list.setColumn(nRow, "smlUpjangNm" , g_UpjangNm);
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_list.rowposition;

	if( ds_list.rowposition == -1 || ds_list.findRow("chk", "1") == -1 )
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if( rtVal == true )
	{
		for( var i = ds_list.rowcount; i >= 0; i-- )
		{
			if( ds_list.getColumn(i, "chk") == "1" )
				ds_list.deleteRow(i);
		}

		gfn_alert("저장 후 반영 됩니다.");
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	// v_ds_check 미사용시 변경사항 체크
 	if( !gfn_dsIsUpdated(ds_list) )
	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return;
	}

	// 저장시 Validation Check
	if( !gfn_validCheck(ds_list, ds_listValid) )	return;

	var strSvcID    = "save";
    var strURL      = "rc/rrm/smlRecipeMasterMgnt/saveList.xdo";
    var strInDs     = "ds_list=ds_list:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 출력 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_print(){}

/*----------------------------------------------------------------------------------------------
 * 설명      : 도움말 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_help(){}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
 ************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if( nErrorCode < 0 )
	{
		return;
	}

    switch(strSvcID)
    {
		// 조회
        case "search" :
            break;

        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break;

		// 반찬세트
        case "searchRsr" :
			ds_setRecipe.insertRow(0);
			ds_setRecipe.setColumn(0,"stdRecipeCd", "");
			ds_setRecipe.setColumn(0,"recipeNm", "");
			break;

		// 레시피 분류코드
		case "sClass" :
			ds_sClassCdSch.copyData(ds_sClassCd);
			ds_sClassCdSch.insertRow(0);
			ds_sClassCdSch.setColumn(0, "sclassCd", "");
			ds_sClassCdSch.setColumn(0, "sclassCdNm", "전체");
			ds_search.setColumn(0, "sclassCd", "");
			break;
    }
}

/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
 ************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 사업부Combo 조회 후처리 함수
 *---------------------------------------------------------------------------------------------*/
function fn_triggerDatasetEvent(p, ds)
{
	if( p.oid == "ds_muCd" )
	{
		SCCombo.bind(this, "MU", "cbo_schMuCd",	null, {defVal:"2003"}); 	//조회:사업부
		SCCombo.bind(this, "MU", "cbo_muCd",	null, {defVal:"2003"}); 	//입력:사업부
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 반찬세트 목록 조회
 *---------------------------------------------------------------------------------------------*/
function fn_searchRsr()
{
	var strSvcID    = "searchRsr";
	var strURL      = "rc/rrm/standardRecipeMasterMgnt/selectRsr.xdo";
	var strInDs     = "ds_search=ds_search";
	var strOutDs    = "ds_setRecipe=ds_setRecipe";
