						   , "X_FSI::SalesCustPop.xfdl"		// 매출처 팝업 호출
						   , {fv_Contents:arrParam}			// 배열 
						   , null, null, null, null, null, false, true, -1);
		if ( arrRtn instanceof Object )
		{
		//alert(arrRtn["releaseplceCd"]+"//"+arrRtn["releaseplceNm"]);
			ds_list.setColumn(e.row, "salsplceCd", arrRtn["salsplceCd"]  );
			ds_list.setColumn(e.row, "salsplceNm", arrRtn["salsplceNm"]  );	
			//alert(arrRtn["ccCd"]);
			if ( arrRtn["ccCd"] != ds_list.getColumn(e.row, "attachDeptSap") && ds_list.getColumn(e.row, "saleOrderType") !="50" )
			{
				gfn_alert("부서이관 대상입니다.");	
				ds_list.setColumn(e.row, "saleOrderType", "50");
				ds_list.setColumn(e.row, "chgAttachDeptSap", arrRtn["ccCd"]  );	
			}
		}			
	}
	
	if (sColumn == "salePrcgNm")
	{	
		SCPopup.doProcessGridPopupEvent(obj, e);
	}
	
}

function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	//수정시 통관완료수량 + 통관수량이 PO수량보다 클 수는 없다
	
	if(e.columnid == "orderRemainQuantity" || e.columnid == "orderRemainWgt") {
		var entryQuantity = gfn_nvl(obj.getColumn(ds_list.rowposition, "orderRemainQuantity"));	   //주문잔여수량
		var orgEntryQuantity = gfn_nvl(obj.getOrgColumn(ds_list.rowposition, "orderRemainQuantity"));	   //원주문잔여수량
		var unit = gfn_nvl(ds_list.getColumn(e.row, "unit"));
		var kgConvertRate = gfn_nvl(ds_list.getColumn(e.row, "kgConvertRate"));
		
		if ( parseInt(entryQuantity) > parseInt(orgEntryQuantity) )
		{
			gfn_alert("주문잔여수량 보다 클수 없습니다.");
			obj.setColumn(ds_list.rowposition, "orderRemainQuantity", orgEntryQuantity);
			return;
		}

		var entryWgt = gfn_nvl(obj.getColumn(ds_list.rowposition, "orderRemainWgt"));	   //주문잔여수량
		var orgOrderRemainWgt = gfn_nvl(obj.getOrgColumn(ds_list.rowposition, "orderRemainWgt"));	   //원주문잔여수량
		var orgOrderRemainQuantity = gfn_nvl(obj.getOrgColumn(ds_list.rowposition, "orderRemainQuantity"));	   //원주문잔여수량

		if ( parseInt(entryWgt) > parseInt(orgOrderRemainWgt) )
		{
			gfn_alert("주문잔여중량 보다 클수 없습니다.");
			obj.setColumn(ds_list.rowposition, "orderRemainWgt", orgOrderRemainWgt);
			return;
		}
		//계산 기준 : KG이면 중량 단위가 BOX나 BDL이면 수량	
		var avgWgt = orgOrderRemainWgt/orgOrderRemainQuantity;	//평균중량
		//alert(orgOrderRemainWgt+"/"+orgOrderRemainQuantity+"="+avgWgt+"///"+entryQuantity);			
		if(e.columnid == "orderRemainQuantity" )
		{
			ds_list.setColumn(e.row, "orderRemainWgt", Math.round((entryQuantity * avgWgt),2) ); 		
		}

	}
	

	
}

