function fn_search()
{
	
	// Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);
    grd_list.setCellProperty("body", 0, "background", "");		
	grd_list.setCellProperty("body", 0, "background2", "");
	
	
	if(gfn_isNull(edt_muNm.getMuCode())){
		alert("사업부는 필수 입력 사항입니다.");
		return;
	}
	
	if(gfn_isNull(edt_upjangNm.getUpjangCode())){
		alert("사업장은 필수 입력 사항입니다.");
		return;
	}
	
	if (!before_SelectCheck())
		return;
    
    //코드목록을 초기화한다.
    ds_list.clearData();
    ds_mmPrice.clearData();
    
    ds_search.setColumn(0, "muCd",      edt_muNm.getMuCode() );
	ds_search.setColumn(0, "tmCd",      edt_tmNm.getTmCode() );
	ds_search.setColumn(0, "upjang",     edt_upjangNm.getUpjangCode() );
    ds_search.setColumn(0, "occurDate",      me_Date.value);
    

	//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장 
	fv_objColInfo = gfn_getDatasetInfo (ds_mmPrice);


    var strSvcID    = "select";
    var strURL      = "rc/rrm/saleEstFrm/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_mmPrice=ds_mmPrice ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

