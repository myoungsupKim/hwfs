/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		//품의 조회
        case "select" :
        trace("조회후:"+ds_mmPrice.getRowCount());
        if (ds_mmPrice.getRowCount() == 0) 
        {
			//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
			gfn_loadDatasetInfo (ds_mmPrice, fv_objColInfo);
        }
			
//     trace("조회후:"+ds_mmPrice.saveXML());
//     if(ds_mmPrice.rowcount == 0)
//     {
// 		ds_mmPrice.addRow();
//     }
            break;
        
        // 저장
        case "save" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				//fv_flag = "UPDATE";
				fn_search();
			}
            break; 
    }
}

