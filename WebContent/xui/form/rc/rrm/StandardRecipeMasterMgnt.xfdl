/*----------------------------------------------------------------------------------------------
 * 설명      : ds_list RowPos 이벤트 (입력필드 키값 활성화 비활성화)
 *---------------------------------------------------------------------------------------------*/
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	var rowType = obj.getRowType(obj.rowposition);
	
	if(rowType == 1 || rowType == 4){
		if(obj.getColumn(obj.rowposition, "relPath") != null && obj.getColumn(obj.rowposition, "relPath") != ""){
			if(obj.getColumn(obj.rowposition, "imgModifyYn") == "Y"){
				img_viewer.image = obj.getColumn(obj.rowposition, "relPath");
			}else{
				img_viewer.image = gv_webImgRoot + obj.getColumn(obj.rowposition, "relPath") + "/" + obj.getColumn(obj.rowposition, "saveFileNm");
			}
			
		}else{
			img_viewer.image = "";
		}
		cbo_upjangType.enable = false;
	}else{
		cbo_upjangType.enable = true;
		//cbo_muCd.enable = false;
		
		if(obj.getColumn(obj.rowposition, "relPath") != null && obj.getColumn(obj.rowposition, "relPath") != undefined){
			img_viewer.image = obj.getColumn(obj.rowposition, "relPath");
		}else{
			img_viewer.image = "";
		}
	}
}

