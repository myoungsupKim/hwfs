/*----------------------------------------------------------------------------------------------
 * 설명      : SAP 거래처 검색후
 *---------------------------------------------------------------------------------------------*/
function fn_custAfterEvent(obj, data)
{
	var nRow = ds_custMst.rowposition;
	ds_custMst.setColumn(nRow, "sapCustCd", data.sapCustCd);
	
	var bizType = gfn_nvl(ds_custMst.getColumn(nRow, "bizType"),"");
	var bizCat  = gfn_nvl(ds_custMst.getColumn(nRow, "bizCat"),"");
	var bsnNo   = gfn_nvl(ds_custMst.getColumn(nRow, "bsnNo"),"");
	var corpNum = gfn_nvl(ds_custMst.getColumn(nRow, "corpNum"),"");
	
	if (bizType =="") ds_custMst.setColumn(nRow, "bizType", data.upTae);	//업태
	if (bizCat =="")  ds_custMst.setColumn(nRow, "bizCat", data.upJong);	//업장
	if (bsnNo =="")   ds_custMst.setColumn(nRow, "bsnNo", data.saupno);		//사업자번호
	if (corpNum =="") ds_custMst.setColumn(nRow, "corpNum", data.bubinno);	//법인번호

	var  intergNum = ds_custMst.getColumn(nRow, "intergNum")
	trace(intergNum);
}
 
