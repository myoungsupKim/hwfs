/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
	
    switch(strSvcID)
    {
		case "searchAmEmpNo":
			if (ds_amEmpNo.rowcount>0)
			{
				ds_custMst.setColumn(ds_custMst.rowposition, "amCd", ds_amEmpNo.getColumn(ds_amEmpNo.rowposition,"amCd"));
				ds_custMst.setColumn(ds_custMst.rowposition, "amNm", ds_amEmpNo.getColumn(ds_amEmpNo.rowposition,"amNm"));
			}
			else
			{
				ds_custMst.setColumn(ds_custMst.rowposition, "amCd","");
				ds_custMst.setColumn(ds_custMst.rowposition, "amNm","");
			}
						
			break;
		// 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            //trace("===" + ds_amList.getRowCount());
            break;
        
        // 저장
        case "save" :
			//trace("RetVal:" + RetVal);
			
			//ds_amList.setColumn(ds_amList.rowposition, "amcd", 0);
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break;    
        
    }
}


