/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	 
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_custList);
    ds_custMst.clearData();
	 
    var strSvcID    = "search";
    var strURL      = "rc/bms/basCustMgmt/selectCustList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_custMst=ds_custMst";
    var strArg      = "custNm=" + edt_serch_CustNm.value 
					+ " custCd=" 
					+ " prcgNm=" + edt_serch_PrcgNm.value
                    + " amCd=" + edt_serch_AmNm.amCd 
                    + " amNm=" + edt_serch_AmNm.value
                    + " useYn="    + (chk_useYn.value?1:0)
                    + " maskAuth=" + SCAuth.getMaskAuth(this);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	

	if (gfn_validCheck(ds_custMst, ds_custMstValid) == false)	return;
	var nRowIdx = ds_custMst.addRow();	//행추가
	ds_custMst.setColumn(nRowIdx, "addrInfo", cmp_addr.newData());	//주소처리을위해 해당 필드 초기화
	ds_custMst.setColumn(nRowIdx, "useYn", 1);
	cbo_prcgDept.index=0;	//부서명
	cbo_jobPosition.index=0;	//직위
	cbo_custClass.index=0;	//거래처구분
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_custMst.rowposition;
	
	if(ds_custMst.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		var bIsProcRow = false;
		if (ds_custMst.getRowType(nRow) == Dataset.ROWTYPE_INSERT)
		{
			ds_custMst.deleteRow(nRow);
		} else
		{
			ds_custMst.setColumn(nRow, "useYn", "0");
			bIsProcRow = true;
		}
		
		if (!bIsProcRow) return;
		
		sMsg = gfn_getMessage("msg.action.success", "");
		sMsg = sMsg += "\r\r처리된 결과를 저장하시겠습니까?";
		
		//실제 저장처리를 수행할 지를 알려준다.
		if (gfn_confirm(sMsg, "", "C"))
		{
			fn_save();
		}
		else
		{
			sMsg = "변경된 자료는 나중에 저장하시기 바랍니다.";
			gfn_alert(sMsg, "", "A");
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (gfn_dsIsUpdated(ds_custMst)==false )
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	// 저장시 Validation Check	
	if (gfn_validCheck(ds_custMst, ds_custMstValid) == false)	return;
	
    var strSvcID    = "save";
    var strURL      = "rc/bms/basCustMgmt/saveCustMgmt.xdo";
    var strInDs     = "ds_custMst=ds_custMst:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

