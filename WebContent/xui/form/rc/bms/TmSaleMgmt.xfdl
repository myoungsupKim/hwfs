function ds_tmSale_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var befSum:int=0;
	var conSum:int=0;
	var befBamt:int=0;
	var conBamt:int=0;
	var befEamt:int=0;
	var conEamt:int=0;
	
	if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "befFamt"))) {
		befSum = befSum + ds_tmSale.getColumn(obj.rowposition, "befFamt");
	} else if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "befBamt"))) {
		befSum = befSum + ds_tmSale.getColumn(obj.rowposition, "befBamt");
	} else if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "befEamt"))) {
		befEamt = ds_tmSale.getColumn(obj.rowposition, "befEamt");
		befSum = befSum + befEamt;
	}
	
	//alert(famtSum);
	//총매출(확정전)
	ds_tmSale.setColumn(obj.rowposition,"befSum",befSum);
	
	if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "conFamt"))) {
		conSum = conSum + ds_tmSale.getColumn(obj.rowposition, "conFamt");
	} else if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "conBamt"))) {
		conSum = conSum + ds_tmSale.getColumn(obj.rowposition, "conBamt");
	} else if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "conEamt"))) {
		conEamt = ds_tmSale.getColumn(obj.rowposition, "conEamt");		
		conSum = conSum + conEamt;
	}
	
	//alert(eamtSum);
	//총매출(확정후)
	ds_tmSale.setColumn(obj.rowposition,"conSum",conSum);
	
	//형태가 변경되면 연회장 초기화
	if (e.columnid == "gubun01") {		
		ds_tmSale.setColumn(obj.rowposition,"banquetCd","");
		ds_tmSale.setColumn(obj.rowposition,"banquetNm","");
	}
	
	//확정전 음료매출 자동계산 식음료총매출 입력 시에만 계산	
	if (e.columnid == "befSamt") {			
		if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "befSamt"))) {
			//0이 아닌 경우만
			if (ds_tmSale.getColumn(obj.rowposition, "befSamt") > 0) {
				if (ds_tmSale.getColumn(obj.rowposition, "befSamt") < ds_tmSale.getColumn(obj.rowposition, "befFamt")) {
					alert("식음료총매출은 식료매출보다 작을 수 없습니다");
					ds_tmSale.setColumn(obj.rowposition,"befSamt",0);
				} else {
					befBamt = ds_tmSale.getColumn(obj.rowposition, "befSamt") - ds_tmSale.getColumn(obj.rowposition, "befFamt");	
				}
			} else {
				befBamt = 0;
			}
		}
		ds_tmSale.setColumn(obj.rowposition,"befBamt",befBamt);
	}	
		
	//확정후 음료매출 자동계산 식음료총매출 입력 시에만 계산
	if (e.columnid == "conSamt") {		
		if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "conSamt"))) {
			//0이 아닌 경우만			
			if (ds_tmSale.getColumn(obj.rowposition, "conSamt") > 0) {
				if (ds_tmSale.getColumn(obj.rowposition, "conSamt") < ds_tmSale.getColumn(obj.rowposition, "conFamt")) {
					alert("식음료총매출은 식료매출보다 작을 수 없습니다");
					ds_tmSale.setColumn(obj.rowposition,"conSamt",0);
				} else {
					conBamt = ds_tmSale.getColumn(obj.rowposition, "conSamt") - ds_tmSale.getColumn(obj.rowposition, "conFamt");	
				}
			} else {
				befBamt = 0;
			}
		}
		ds_tmSale.setColumn(obj.rowposition,"conBamt",conBamt);	
	}	
	
	var wday = "";
	var dc = 0;
	
	//요일
	if (!gfn_isNull(obj.getColumn(obj.rowposition, "occurDate"))) {
		wday = gfn_getDayName(ds_tmSale.getColumn(obj.rowposition, "occurDate"));
		ds_tmSale.setColumn(obj.rowposition, "day",wday);
	}
	
	//DC변경 시 식료매출 자동계산 
	if (e.columnid == "dc") {
		dc = (100-ds_tmSale.getColumn(obj.rowposition, "dc"))/100;
		ds_tmSale.setColumn(obj.rowposition,"befFamt",ds_tmSale.getColumn(obj.rowposition, "befPrice") * ds_tmSale.getColumn(obj.rowposition, "befPsn")*dc);							
		ds_tmSale.setColumn(obj.rowposition,"conFamt",ds_tmSale.getColumn(obj.rowposition, "conPrice") * ds_tmSale.getColumn(obj.rowposition, "conPsn")*dc);									
	}
	
	if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "befPrice"))) {
		if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "befPsn"))) {
			if ((e.columnid == "befPrice") || (e.columnid == "befPsn")) {
				if (ds_tmSale.getColumn(obj.rowposition, "befPrice") >= 0) {
					if (ds_tmSale.getColumn(obj.rowposition, "befPsn") >= 0) {
						//alert(ds_tmSale.getColumn(obj.rowposition, "befPrice") * ds_tmSale.getColumn(obj.rowposition, "befPsn"));
						if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "dc"))) {
							dc = (100-ds_tmSale.getColumn(obj.rowposition, "dc"))/100;
							ds_tmSale.setColumn(obj.rowposition,"befFamt",ds_tmSale.getColumn(obj.rowposition, "befPrice") * ds_tmSale.getColumn(obj.rowposition, "befPsn")*dc);
						} else {
							ds_tmSale.setColumn(obj.rowposition,"befFamt",ds_tmSale.getColumn(obj.rowposition, "befPrice") * ds_tmSale.getColumn(obj.rowposition, "befPsn"));
						}
					}
				} 
			}
		}
	}
	//확정후 식료매출 자동계산 추가 20190827 김호석
	if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "conPrice"))) {
		if (!gfn_isNull(ds_tmSale.getColumn(obj.rowposition, "conPsn"))) {
			if ((e.columnid == "conPrice") || (e.columnid == "conPsn")) {
