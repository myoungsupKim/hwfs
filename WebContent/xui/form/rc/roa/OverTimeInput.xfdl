				//trace("::::::::::::::::::::: gfn_round(iNghtOccur,2):"+gfn_round(iNghtOccur,2));
			}

		//Strat 22:01 ~ 24:00
		}else if(iSTime > iStdTime22 && iSTime <= iStdTime24){
			//End 22:01 ~ 24:00
			if(iETime > iStdTime22 && iETime <= iStdTime24){
				//trace("Strat 22:01 ~ 24:00   End 22:01 ~ 24:00  ::::::::::::::::: sDutyStartTime=" + sDutyStartTime+",  sDutyEndTime="+sDutyEndTime);
				iNghtOccur += fn_getDiffTime(sDutyStartTime, sDutyEndTime, "HOUR");
			}
		}
		
		/*********************************************************************************************************
		 * 연장 발생 시간 (iExtensionOccur) 계산 - 8시간 이후 근무
		 *********************************************************************************************************/
		//trace("================= iOccurTime="+iOccurTime);
		//if(iOccurTime > 8){
			iExtensionOccur = gfn_round(iOccurTime - 8, 2);	//- iNghtOccur 
		//}
		//trace("================= iExtensionOccur="+iExtensionOccur);
		/*********************************************************************************************************
		 * 휴일근무,공휴근무
		 *********************************************************************************************************/
		if(sWorkAttType == "002" || sWorkAttType == "003"){
			iHolidayOccur = iOccurTime;
		}

		/*********************************************************************************************************
		 * 휴무일경우 09:00 ~ 18:00 공제1시간 세팅
		 * 001:정상근무
		 * 002:휴일근무
		 * 003:공휴근무
		 * 004:연차
		 * 005:자기개발
		 * 006:유급휴가
		 * 007:기타휴가
		 * 008:출산휴가
		 * 009:주휴일
		 * 010:유급휴무
		 * 011:추가비번
		 * 012:공휴대체
		 * 013:무급휴가
		 * 014:훈련

		 *********************************************************************************************************/
		//trace("["+row+"] ================= sWorkAttType="+sWorkAttType);
		//if( sWorkAttType == "004" || sWorkAttType == "005" || sWorkAttType == "006" || sWorkAttType == "007" ||
		//	sWorkAttType == "008" || sWorkAttType == "009" || sWorkAttType == "010" || sWorkAttType == "011" || sWorkAttType == "013" || sWorkAttType == "014"){
		  if( sWorkAttType == "006" || sWorkAttType == "007" || sWorkAttType == "009" || sWorkAttType == "010" || sWorkAttType == "011" ||
			   sWorkAttType == "013" || sWorkAttType == "014" || sWorkAttType == "015" || sWorkAttType == "017"   ){
			//trace("["+row+"] ================= readonly");
			eval("edt_dutyStartTime"+row).readonly	= true;
			eval("edt_dutyEndTime"+row).readonly	= true;
			eval("cbo_deductionTime"+row).readonly	= true;
			//eval("edt_dutyStartTime"+row).mask		= "";
			//eval("edt_dutyEndTime"+row).mask		= "";
			//eval("cbo_deductionTime"+row).displaynulltext = "";

			sDutyStartTime	= "0900";
			sDutyEndTime	= "1800";
			sDeductionTime	= "60";
						
			iExtensionOccur	= 0;	//연장 발생 시간
			iHolidayOccur	= 0;	//휴일 발생 시간
			iNghtOccur		= 0;	//야근 발생 시간
			iOccurTime		= 0;	//총근무시간
			
			//추가비번, 운영비번(0.5대상)
			if(sWorkAttType == "011" || sWorkAttType == "017"){
				iExtensionOccur = -8;	//연장 발생 시간
				iDayOffTime		= -8;	//비번시간
				iOccurTime		= 0;	//총근무시간
				//iExtensionOccur = 0;	//추가비번
				//iDayOffTime		= 0;	//추가비번
				//iOccurTime		= 0;	//추가비번
			}
			//비번
			if(sWorkAttType == "015"){
				iExtensionOccur = 0;	//연장 발생 시간
				iDayOffTime		= -8;	//비번시간
				iOccurTime		= 0;	//총근무시간
			}
		} else if (sWorkAttType == "015" ) {
			eval("edt_dutyStartTime"+row).readonly	= false;
			eval("edt_dutyEndTime"+row).readonly	= false;
			eval("cbo_deductionTime"+row).readonly	= false;
		
		} else if (sWorkAttType == "017" ) {
			eval("edt_dutyStartTime"+row).readonly	= false;
			eval("edt_dutyEndTime"+row).readonly	= false;
			eval("cbo_deductionTime"+row).readonly	= false;
		
		} else {
			//trace("["+row+"] ================= false");
			eval("edt_dutyStartTime"+row).readonly	= false;
			eval("edt_dutyEndTime"+row).readonly	= false;
			eval("cbo_deductionTime"+row).readonly	= false;

			//eval("edt_dutyStartTime"+row).mask		= "##:##";
			//eval("edt_dutyEndTime"+row).mask		= "##:##";
			//eval("cbo_deductionTime"+row).displaynulltext = "선택";
			
			//if(eval("edt_dutyStartTime"+row).value == "") 	sDutyStartTime	= "0900";
			//if(eval("edt_dutyEndTime"+row).value == "") 	sDutyEndTime	= "1800";
			//if(eval("cbo_deductionTime"+row).value == "") 	sDeductionTime	= "60";			
		}
		eval("edt_dutyStartTime"+row).value	= sDutyStartTime;
		eval("edt_dutyEndTime"+row).value	= sDutyEndTime;
		eval("cbo_deductionTime"+row).value	= sDeductionTime;
		eval("cbo_workAttType"+row).value	= sWorkAttType;
		//추가비번과 운영비번(0.5대상)은 연장근 -8표시
		eval("edt_extensionTime"+row).value	= gfn_round(iExtensionOccur,2);
		eval("edt_holidayTime"+row).value	= gfn_round(iHolidayOccur,2);
		eval("edt_nghtTime"+row).value		= gfn_round(iNghtOccur,2);
		eval("edt_totalTime"+row).value		= gfn_round(iOccurTime,2);

		var iExtensionOccurTot	= 0;
		var iHolidayOccurTot	= 0;
		var iNghtOccurTot		= 0;
		var iTotalTimeTot		= 0;
		for(var i=0; i<ds_list.rowcount; i++){
			//iExtensionOccurTot	+= eval("edt_extensionTime"+i).value;
			iHolidayOccurTot	+= eval("edt_holidayTime"+i).value;
			iNghtOccurTot		+= eval("edt_nghtTime"+i).value;
			iTotalTimeTot		+= eval("edt_totalTime"+i).value;
		}
		edt_extensionTimeTot.value	= gfn_round(iExtensionOccurTot,2);
		edt_holidayTimeTot.value	= gfn_round(iHolidayOccurTot,2);
		edt_nghtTimeTot.value		= gfn_round(iNghtOccurTot,2);
		edt_totalTimeTot.value		= gfn_round(iTotalTimeTot,2);
		
		//주단위 총 연장시간을 구한다
		var std_time = cbo_overTime.value;
		//alert(std_time);
		//if(iTotalTimeTot > 40){
		if(iTotalTimeTot > std_time){
			edt_extensionTime6.value	= gfn_round(iTotalTimeTot - std_time,2);
			edt_extensionTimeTot.value	= edt_extensionTime6.value;
			//edt_extensionTimeTot.value	= gfn_round(iTotalTimeTot - std_time,2);
		}else{
			//edt_extensionTime6.value	= 0;
			//edt_extensionTimeTot.value	= 0;
			edt_extensionTime6.value = gfn_round(iTotalTimeTot - std_time,2);
			edt_extensionTimeTot.value	= edt_extensionTime6.value;
			//edt_extensionTimeTot.value	= gfn_round(iTotalTimeTot - std_time,2);
		}
