/*----------------------------------------------------------------------------------------------
 * @class 두 시간간의 차이 계산
 * @param sDutyStartTime - HHmmss형태의 From 시간 ( 예 : "033025" )
 * @param sDutyEndTime - HHmmss형태의 To 시간 ( 예 : "034025" )
 * @param strDiv - 구분 
 * @return 
 * 		구분이 "HOUR" 이면 소수점 첫째자리까지의 시간(반올림) - 315분 => 5.25시간 => return '5.3'
 *      구분이 "HH24MI" 이면 시분(HH24MI) ex) 8시간 15분 => return '0815'
 *---------------------------------------------------------------------------------------------*/
function fn_getDiffTime(sDutyStartTime, sDutyEndTime, strDiv){
    var nRetVal = 0;
    var nFrom_hh = toNumber(sDutyStartTime.substring(0, 2));
    var nFrom_mm = toNumber(sDutyStartTime.substring(2, 4));
    var nFrom_ss = toNumber(sDutyStartTime.substring(4, 6));
    
    var nTo_hh = toNumber(sDutyEndTime.substring(0, 2));
    var nTo_mm = toNumber(sDutyEndTime.substring(2, 4));
    var nTo_ss = toNumber(sDutyEndTime.substring(4, 6));
    
    var nFromTotal_ss = (nFrom_hh * 3600) + (nFrom_mm * 60) + nFrom_ss;
    var nToTotal_ss   = (nTo_hh * 3600) + (nTo_mm * 60) + nTo_ss;
    var iSec	= 0;
    
    iSec = nToTotal_ss - nFromTotal_ss;

	if(strDiv == "SECOND"){
		nRetVal = iSec;
	}else if(strDiv == "HOUR"){
		//nRetVal = gfn_round(iSec / 3600,2);
		nRetVal = iSec / 3600;
	}else if(strDiv == "HH24MI"){
		nRetVal = fn_getHhmibySec(iSec);
	}
    return nRetVal;
}

/*----------------------------------------------------------------------------------------------
 * "초"를 "1200" 형태로 변환
 *---------------------------------------------------------------------------------------------*/
function fn_getHhmibySec(sec){
    var iTime 	= 0;
    var iHour 	= 0;
    var iMin 	= 0;
    var iSec	= 0;
    
    iSec	= toNumber(sec);
    
    if(iSec == 0){
		return "0000";
	}else{
		iTime	= gfn_round(iSec / 3600, 2);
		iHour	= gfn_floor(iSec / 3600, 0);
		
		iMin	= gfn_round((iTime - iHour) * 60, 0);
		
		if(iMin == 60){
			iMin	= 0;
			iHour	= iHour + 1;
		}
		return gfn_lPad(iHour.toString(),"0",2) + gfn_lPad(iMin.toString(),"0",2);
	}
}

/*----------------------------------------------------------------------------------------------
 * "1200" 형태를 "초"로 변환
 *---------------------------------------------------------------------------------------------*/
function fn_getSecbyHhmi(sTime){
    var iTime 	= 0;
    var iHour 	= 0;
    var iMin 	= 0;
    var iSec	= 0;
    
    iHour	= parseInt(sTime.substr(0, 2),10);
    iMin	= parseInt(sTime.substr(2, 4),10);
    
    if(sTime == "0000"){
		iSec	= 0;
    }else{
		iSec	= (iHour * 3600) + (iMin * 60);
    }
    return iSec;
}

