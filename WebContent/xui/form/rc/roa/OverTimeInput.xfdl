/*----------------------------------------------------------------------------------------------
 * 설명      : Input 데이터 세팅 (Dataset > Input)
 *---------------------------------------------------------------------------------------------*/
function fn_setInputValue(){
	var cnt = 0;
	if(ds_list.getRowCount() > 0){
		for(var i=0; i< ds_list.getRowCount();i++){
		
			//rowType 변경
			ds_list.setRowType(i,ds_list.getColumn(i,"rowType"));

			eval("edt_dutyStartDate"+i).value	= ds_list.getColumn(i,"dutyStartDate");
			eval("edt_dutyStartTime"+i).value	= ds_list.getColumn(i,"dutyStartTime");
			eval("edt_dutyEndTime"+i).value		= ds_list.getColumn(i,"dutyEndTime");
			eval("cbo_deductionTime"+i).value	= ds_list.getColumn(i,"deductionTime");
			eval("cbo_workAttType"+i).value		= ds_list.getColumn(i,"workAttType");
			
			if(!gfn_isNull(ds_list.getColumn(i,"dutyStartTime")) || !gfn_isNull(ds_list.getColumn(i,"dutyEndTime"))){
				fn_setOccurCal(i);
			}else{
				eval("edt_dutyStartTime"+i).readonly    = false;
				eval("edt_dutyEndTime"+i).readonly      = false;
				eval("cbo_deductionTime"+i).readonly    = false;
				eval("cbo_workAttType"+i).readonly      = false;
				eval("edt_extensionTime"+i).value		= 0;
				eval("edt_holidayTime"+i).value			= 0;
				eval("edt_nghtTime"+i).value			= 0;
				eval("edt_totalTime"+i).value			= 0;
				edt_extensionTimeTot.value				= 0;
				edt_holidayTimeTot.value				= 0;
				edt_nghtTimeTot.value					= 0;
				edt_totalTimeTot.value					= 0;
			}
			
			if(ds_list.getColumn(i,"closeYn") == "Y"){
				eval("edt_dutyStartTime"+i).readonly    = true;
				eval("edt_dutyEndTime"+i).readonly      = true;
				eval("cbo_deductionTime"+i).readonly    = true;
				eval("cbo_workAttType"+i).readonly      = true;
				cnt++;
			}
		}

		if(cnt > 0){
			fv_closeYn = "Y";
		}else{
			fv_closeYn = "N";
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장시 Validation Check
 *---------------------------------------------------------------------------------------------*/
function fn_validCheck(){
	var sWorkAttType = "";
	for(var i=0; i<ds_list.rowcount; i++){
	
		sWorkAttType = ds_list.getColumn(i, "workAttType");
		if(sWorkAttType == "001" || sWorkAttType == "002" || sWorkAttType == "003"){
			
			if(gfn_isNull(ds_list.getColumn(i, "dutyStartTime"))){
				gfn_alert("msg.noinput", "근무시작시간");	// $1 을(를) 입력해 주십시요.
				eval("edt_dutyStartTime"+i).setFocus();
				return false;
			}
			if(gfn_isNull(ds_list.getColumn(i, "dutyEndTime"))){
				gfn_alert("msg.noinput", "근무종료시간");	// $1 을(를) 입력해 주십시요.
				eval("edt_dutyEndTime"+i).setFocus();
				return false;
			}
			if(gfn_isNull(ds_list.getColumn(i, "deductionTime"))){
				gfn_alert("msg.noselect", "공제시간");	// $1 을(를) 선택해 주십시요.
				eval("cbo_deductionTime"+i).setFocus();
				return false;
			}
		}
		if(gfn_isNull(ds_list.getColumn(i, "workAttType"))){
			gfn_alert("msg.noselect", "근태유형");	// $1 을(를) 선택해 주십시요.
			eval("cbo_workAttType"+i).setFocus();
			return false;
		}
		if(ds_list.getColumn(i, "dutyStartTime") != ds_list.getOrgColumn(i, "dutyStartTime")){
			if(gfn_isNull(ds_list.getOrgColumn(i, "dutyStartTime"))){
				ds_sub.setColumn(i, "logYn", "I");
			}else{
				ds_sub.setColumn(i, "logYn", "U");
			}
		}
		if(ds_list.getColumn(i, "dutyEndTime") != ds_list.getOrgColumn(i, "dutyEndTime")){
			if(gfn_isNull(ds_list.getOrgColumn(i, "dutyEndTime"))){
				ds_sub.setColumn(i, "logYn", "I");
			}else{
				ds_sub.setColumn(i, "logYn", "U");
			}
		}
		if(ds_list.getColumn(i, "deductionTime") != ds_list.getOrgColumn(i, "deductionTime")){
			if(gfn_isNull(ds_list.getOrgColumn(i, "deductionTime"))){
				ds_sub.setColumn(i, "logYn", "I");
			}else{
				ds_sub.setColumn(i, "logYn", "U");
			}
		}
		if(ds_list.getColumn(i, "workAttType") != ds_list.getOrgColumn(i, "workAttType")){
			if(gfn_isNull(ds_list.getOrgColumn(i, "workAttType"))){
				ds_sub.setColumn(i, "logYn", "I");
			}else{
				ds_sub.setColumn(i, "logYn", "U");
			}
		}
	}	
	return true;	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 특근 발생 시간을 계산한다.
 *---------------------------------------------------------------------------------------------*/
function fn_setOccurCal(row){
	var sDutyStartDate	= "";	// 근무일자
	var sDutyStartTime	= "";	// 근무시작시간
	var sDutyEndTime	= "";	// 근무종료시간
	var sOccurTime		= "";	// 근무시간
	var sDeductionTime	= "";	// 공제시간
	var sWorkAttType	= "";	// 근태유형
	var sStimeOrg		= "";
	var sEtimeOrg		= "";
	
	var iDeductionTime	= 0;	// 공제시간
	var iOccurTime 		= 0;	//총근무시간
	var iSTime          = 0;
	var iETime          = 0;
	var iStdTime0       = 0;    //야간근무시간(총근무시간 중 22:00 ~ 06:00)
	var iStdTime6       = 600;
	var iStdTime22      = 2201;
	var iStdTime24      = 2400;
	var iNghtOccur      = 0;	//야근 발생 시간
	var iExtensionOccur = 0;	//연장 발생 시간
	var iHolidayOccur	= 0;	//휴일 발생 시간
	var iDayOffTime		= 0;	//비번시간
	
	if(ds_list.getRowCount() > 0){

		sDutyStartDate		= eval("edt_dutyStartDate"+row).value;
		sStimeOrg			= ds_list.getColumn(row,"dutyStartTime");
		sEtimeOrg			= ds_list.getColumn(row,"dutyEndTime");
		
		if(gfn_isNull(sStimeOrg)) sStimeOrg = "0900";
		if(gfn_isNull(sEtimeOrg)) sEtimeOrg = "1800";
		
		//trace("++++++++++++++++++++++++++"+sStimeOrg+":"+sEtimeOrg);
		if(gfn_isNull(eval("edt_dutyStartTime"+row).value))	sDutyStartTime	= "0900";
		else 												sDutyStartTime	= gfn_rPad(eval("edt_dutyStartTime"+row).value,"0",4);
		
		if(gfn_isNull(eval("edt_dutyEndTime"+row).value))	sDutyEndTime	= "1800";
		else 												sDutyEndTime	= gfn_rPad(eval("edt_dutyEndTime"+row).value,"0",4);
		
		if(gfn_isNull(eval("cbo_deductionTime"+row).value))	sDeductionTime	= "60";
		else 												sDeductionTime	= eval("cbo_deductionTime"+row).value;
		
		if(gfn_isNull(eval("cbo_workAttType"+row).value))	sWorkAttType	= "001";
		else 												sWorkAttType	= eval("cbo_workAttType"+row).value;

		if(toNumber(sDutyStartTime.toString().substr(0, 2)) > 23 || toNumber(sDutyStartTime.toString().substr(2, 2)) > 59) {
			gfn_alert("근무시작시간은 00~23 , 분은 00~59 사이에 입력해 주세요.");
			eval("edt_dutyStartTime"+row).value	= sStimeOrg;
			eval("edt_dutyEndTime"+row).value	= sEtimeOrg;
			eval("edt_dutyStartTime"+row).setFocus();
			return false;
		}
    
		if(toNumber(sDutyEndTime.toString().substr(0, 2)) > 23 || toNumber(sDutyEndTime.toString().substr(2, 2)) > 59) {
			gfn_alert("근무종료시간은 00~23 , 분은 00~59 사이에 입력해 주세요.");
			eval("edt_dutyStartTime"+row).value	= sStimeOrg;
			eval("edt_dutyEndTime"+row).value	= sEtimeOrg;
			eval("edt_dutyEndTime"+row).setFocus();
			return false;
		}
    
		//trace("++++++++++++++++++++++++++"+sDutyStartTime+":"+sDutyEndTime);
		iDeductionTime	= gfn_round(Number(sDeductionTime) / 60, 2);
		iOccurTime		= gfn_round(fn_getDiffTime(sDutyStartTime, sDutyEndTime, "HOUR") - iDeductionTime, 2);
		
		iSTime			= Number(sDutyStartTime);
		iETime			= Number(sDutyEndTime);
		
		if(iSTime > iETime){
			gfn_alert("근무시작시간은 근무종료시간보다 클 수 없습니다.");
			eval("edt_dutyStartTime"+row).value	= ds_list.getOrgColumn(row,"dutyStartTime");
			eval("edt_dutyEndTime"+row).value	= ds_list.getOrgColumn(row,"dutyEndTime");
			eval("edt_dutyStartTime"+row).setFocus();
			return false;	
		}
		
		/*********************************************************************************************************
		 * 야근 발생 시간(iNghtOccur) 계산
		 *********************************************************************************************************/
		//trace("["+row+"] Strat:"+iSTime+", End:"+iETime);

		//Strat 00:00 ~ 06:00
		if(iSTime >= iStdTime0 && iSTime <= iStdTime6){

			//End 00:00 ~ 06:00
			if(iETime >= iStdTime0 && iETime <= iStdTime6){
				//trace("Strat 00:00 ~ 06:00    End 00:00 ~ 06:00  ::::::::::::::::: sDutyStartTime=" + sDutyStartTime+",  sDutyEndTime="+sDutyEndTime);
				iNghtOccur += fn_getDiffTime(sDutyStartTime, sDutyEndTime, "HOUR");

			//End 06:01 ~ 22:00
			}else if(iETime > iStdTime6 && iETime <= iStdTime22){
				//trace("Strat 00:00 ~ 06:00    End 06:01 ~ 22:00  ::::::::::::::::: sDutyStartTime=" + sDutyStartTime+",  sDutyEndTime="+sDutyEndTime);
				iNghtOccur += fn_getDiffTime(sDutyStartTime, "0600", "HOUR");

			//End 22:01 ~ 24:00
			}else if(iETime > iStdTime22 && iETime <= iStdTime24){
				//trace("Strat 00:00 ~ 06:00   End 22:01 ~ 24:00  ::::::::::::::::: sDutyStartTime=" + sDutyStartTime+",  sDutyEndTime="+sDutyEndTime);
