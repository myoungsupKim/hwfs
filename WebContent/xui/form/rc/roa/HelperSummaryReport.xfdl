/*----------------------------------------------------------------------------------------------
 * 설명      : 지원받는 업장 합계
 *---------------------------------------------------------------------------------------------*/
function fn_getSum(arg){
	var oTime = 0;
	var nTime = 0;
	var oAmt = 0;
	var nAmt = 0;
	var nRetVal = 0;
	
	for(var i=0; i<ds_list.rowcount; i++){
	
		oTime	= ds_list.getColumn(i, "time");
		oAmt	= ds_list.getColumn(i, "amt");
		
		if(arg == "supTime"){
			if(oTime != nTime){
				nRetVal += oTime;
			}
		}
		
		if(arg == "supAmt"){
			if(oTime != nTime){
				nRetVal += oAmt;
			}
		}
		nTime	= ds_list.getColumn(i, "time");
		nAmt	= ds_list.getColumn(i, "amt");
	}
	return nRetVal;
}

