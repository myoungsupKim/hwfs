/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";
	
	//조회조건을 초기화한다.
	var nRow= ds_search.addRow();
	ds_search.setColumn(nRow, "useYn", "");
	
	//Combo
	SCCombo.bind(this, "ComCode", "cbo_useYn", ds_useYn, {groupCode:"USE_YN", useYn:"Y", defVal:"Y"});			//사용여부
	SCCombo.bind(this, "ComCode", "cbo_schUseYn", null, {groupCode:"USE_YN", useYn:"Y"}, SCCombo.ALL); 			//사용여부(조회조건)
	
	fn_search();
}

/*----------------------------------------------------------------------------------------------
