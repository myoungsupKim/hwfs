 *---------------------------------------------------------------------------------------------*/
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	var rowType = obj.getRowType(obj.rowposition);
	
	if(rowType == 1 || rowType == 4){
		edt_helperGradeCd.enable = false;
	}else{
		edt_helperGradeCd.enable = true;
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : HELPER 등급 코드 중복체크(동기로 실행)
 *---------------------------------------------------------------------------------------------*/
function fn_existsSearch(){
	
	var nRow = ds_list.rowposition;
	var helperGradeCd = ds_list.getColumn(nRow, "helperGradeCd");
	
	if(helperGradeCd ==  null){
		return true;
	}
	
	var strSvcID    = "search";
    var strURL      = "/rc/roa/helperGradeCd/selectExists.xdo";
    var strInDs     = "";
    var strOutDs    = "fv_existsCnt=fv_existsCnt";
    var strArg      = "helperGradeCd="+helperGradeCd;
    var strCallback = "";
    var bAsync      = false;		//동기실행
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
    if(fv_existsCnt > 0){
		gfn_alert("msg.err.duplicatedata", "등급 코드");	// $1 은(는) 이미 등록된 자료입니다.
		fv_existsCnt = 0;
		
		return false;
    }
    
    return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : HELPER 등급 코드 onkillfocus 이벤트(코드 중복체크)
 *---------------------------------------------------------------------------------------------*/
function edt_helperGradeCd_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	if(!fn_existsSearch()){
		obj.value = null;
		obj.setFocus();
		return;
	}
}
