/*----------------------------------------------------------------------------------------------
 * 설명      : 지원등급 itemChange 이벤트(등급 변경시 지원금액 변경)
 *---------------------------------------------------------------------------------------------*/
function cbo_helperGradeCd_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(ds_list.getColumn(ds_list.rowposition, "slipCreationYn") == "Y"){
		alert("전표가 생성된 내역이므로 수정할 수 없습니다.");
		ds_list.setColumn(ds_list.rowposition, "helperGradeCd", ds_list.getOrgColumn(ds_list.rowposition, "helperGradeCd"))
		//ds_list.setColumn(ds_list.rowposition, "helperAmt", ds_list.getOrgColumn(ds_list.rowposition, "helperAmt"))
		return;
	}
	
	var helperIdx = obj.index;
	
	ds_list.setColumn(ds_list.rowposition, "helperAmt", ds_helperGrade.getColumn(helperIdx, "supAmt"));
	
	//지원금액 계산
	if(ds_list.getColumn(ds_list.rowposition, "supTime") != null && ds_list.getColumn(ds_list.rowposition, "supTime") != ""){
		var supAmt = ds_list.getColumn(ds_list.rowposition, "supTime") * ds_list.getColumn(ds_list.rowposition, "helperAmt");
		ds_list.setColumn(ds_list.rowposition, "supAmt", supAmt);
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : ds_list RowPos 이벤트 (입력필드 키값 활성화 비활성화)
 *---------------------------------------------------------------------------------------------*/
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	var rowType = obj.getRowType(obj.rowposition);
	
	if(rowType == 1 || rowType == 4){
 		MuSearchComponent00.enable = false;
 		edt_upjang.enable = false;
		edt_emp.enable = false;
		cal_occurDate.enable = false;
		edt_supUpjang.enable = false;
	}else{
 		MuSearchComponent00.enable = true;
 		edt_upjang.enable = true;
		edt_emp.enable = true;
		cal_occurDate.enable = true;
		edt_supUpjang.enable = true;
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : HELPER 지원 등록 중복체크(동기로 실행)
 *---------------------------------------------------------------------------------------------*/
function fn_existsSearch(){
	
	var nRow = ds_list.rowposition;
	var mucd 		= ds_list.getColumn(nRow, "muCd");
	var upjang 		= ds_list.getColumn(nRow, "upjang");
	var empno 		= ds_list.getColumn(nRow, "empno");
	var supUpjang 	= ds_list.getColumn(nRow, "supUpjang");
	var occurDate 	= ds_list.getColumn(nRow, "occurDate");
	
	if(mucd == null || upjang == null || empno == null || supUpjang == null || occurDate == null){
		return true;
	}
	
	ds_existsSearch.addRow();
	ds_existsSearch.setColumn(0, "muCd", mucd);
	ds_existsSearch.setColumn(0, "upjang", upjang);
	ds_existsSearch.setColumn(0, "empno", empno);
	ds_existsSearch.setColumn(0, "supUpjang", supUpjang);
	ds_existsSearch.setColumn(0, "occurDate", occurDate);
	

	var strSvcID    = "search";
    var strURL      = "/rc/roa/helperSupport/selectExists.xdo";
    var strInDs     = "ds_search=ds_existsSearch:A";
    var strOutDs    = "fv_existsCnt=fv_existsCnt";
    var strArg      = "";
    var strCallback = "";
    var bAsync      = false;		//동기실행
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
    if(fv_existsCnt > 0){
		gfn_alert("msg.err.duplicatedata", "사원");	// $1 은(는) 이미 등록된 자료입니다.
		fv_existsCnt = 0;
		
		return false;
    }
    
    return true;
}

