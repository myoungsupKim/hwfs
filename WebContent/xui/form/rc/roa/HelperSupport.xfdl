 *---------------------------------------------------------------------------------------------*/
function msk_supTime_onkeyup(obj:MaskEdit, e:KeyEventInfo)
{
	if(ds_list.getColumn(ds_list.rowposition, "slipCreationYn") == "Y"){
		alert("전표가 생성된 내역이므로 수정할 수 없습니다.");
		ds_list.setColumn(ds_list.rowposition, "supTime", ds_list.getOrgColumn(ds_list.rowposition, "supTime"))
		//ds_list.setColumn(ds_list.rowposition, "supAmt", ds_list.getOrgColumn(ds_list.rowposition, "supAmt"))
		return;
	}

	if(gfn_isNull(obj.value)){
		ds_list.setColumn(ds_list.rowposition, "supAmt", 0);
		return;
	}
	
	var supAmt = obj.value * ds_list.getColumn(ds_list.rowposition, "helperAmt");
	ds_list.setColumn(ds_list.rowposition, "supAmt", supAmt);
}

