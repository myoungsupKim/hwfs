/**
 * @class 값이 변경될 시 마다. 해당 값을 할당한다.
 * @param p - 발생 Event에 대한정보
 * @param data - 선택된 결과에 대한 정보
 * @return 
 */
function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if (e.columnid == "bsnNo" || e.columnid == "subBsnNo")
	{
		var sBsnNo = "";
		var sSubBsnNo = "";
		
		if (!gfn_isNull(obj.getColumn(e.row, "bsnNo")))
		{
			sBsnNo = obj.getColumn(e.row, "bsnNo");
		}
		
		if (!gfn_isNull(obj.getColumn(e.row, "subBsnNo")))
		{
			sSubBsnNo = obj.getColumn(e.row, "subBsnNo");
		}
		
		//개인거래처인 경우 주민번호라 사업자번호가 길어서 발생		
		if (sBsnNo.length == 13) {
			obj.setColumn(e.row, "saupno", sBsnNo);
			//alert(sBsnNo);
		} else {
			obj.setColumn(e.row, "saupno", sBsnNo + sSubBsnNo);		
		}
	}
	
	if (e.columnid == "shopTypCd")
	{
		obj.setColumn(e.row, "shopTypNm", div.cbo_shopTypCd.text);
	}
	
	if(ds_list.getRowType(e.row) == 4)  // 수정일 경우 CC코드 변경 불가
	{
		if (e.columnid == "ccCd")
		
		{
			//alert(obj.getColumn(e.row, "cdate"));
			//alert(obj.getColumn(e.row, "udate"));
			if (!gfn_isNull(obj.getColumn(e.row, "attr02")) && (obj.getColumn(e.row, "cdate") == obj.getColumn(e.row, "udate"))) {
				//리조트&외식 고객업장이면서 첫 수정만 허용 20200121 김호석
			} else {
				gfn_alert("신규 업장이 아닌 경우 CC는 전산팀에서만 변경할 수 있습니다.");
				obj.setColumn(e.row, "ccCd", e.oldvalue);
			}
		}
		
		if (e.columnid == "ccNm")
		{
			//gfn_alert("신규 업장이 아닌 경우 CC는 전산팀에서만 변경할 수 있습니다.");
			if (!gfn_isNull(obj.getColumn(e.row, "attr02")) && (obj.getColumn(e.row, "cdate") == obj.getColumn(e.row, "udate"))) {
				//리조트&외식 고객업장이면서 첫 수정만 허용 20200121 김호석
			} else {
				obj.setColumn(e.row, "ccNm", e.oldvalue);
			}
		}	
		
	}	
}

