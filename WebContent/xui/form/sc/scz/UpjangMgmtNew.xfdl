	}
	
	var approvalCancelUpjang = "";
	var j=0;
	
	for (var i=0,len=ds_list.getRowCount(); i<len; i++) {
		if (ds_list.getColumn(i, "chk") == "1") {
			if (j==0) approvalCancelUpjang = ds_list.getColumn(i, "upjang");
