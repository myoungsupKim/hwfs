/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            sta_upjangCnt.text = "총 " + ds_list.getRowCount() + "건";
            ds_procList.clearData();
            //div.AddrSearchComponent00.clearAll();
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			
			var isGwProc = false;
			
			for (var i=0,len=ds_procList.getRowCount(); i<len; i++) {
				if (ds_procList.getColumn(i, "bankAcctRequestYn") == "Y") {
					isGwProc = true;
				}
			}
			
			if (isGwProc) {
				if (gfn_confirm("결재를 진행하시겠습니까?\n결재는 추후 일괄적으로 진행할 수 있습니다.")) {
					fn_gwDraftAction();
				}
				else {
					fn_saveList();
				}
			}
			else {			
				fn_saveList();
			}
			
            break;
		case "updateGwId" :
			fn_search();
			break;
		case "selectSaveUpjangList" :
			ds_procList.clearData();
			break;
		case "selectIncludeCS" :
		    //if (Number(ds_includeCS.getColumn(0, "cnt")) > 0) {
		    //직영조회 및 저장 권한 팀장 이상만 가능하도록 수정 20160831 처리자:맹수영  (CH201608_00616)
			if ((Number(ds_includeCS.getColumn(0, "cnt")) > 0)  || (g_AuthLevel < "16") ){
				cbo_s_shopTypCd.readonly = true;
			}
			cbo_s_shopTypCd.value = "02";
			
			break;
        case "approvalCancel" :
			fn_search("TMP");
			break;
    }
}


