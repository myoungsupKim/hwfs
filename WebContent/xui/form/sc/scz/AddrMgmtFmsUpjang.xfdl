	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	TmSearchComponent00.applyAuthLevel(MuSearchComponent00);
	UserSearchComponent00.setSabun(g_EmpNo);
	UserSearchComponent00.setUserName(g_Name);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
