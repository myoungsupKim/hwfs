 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
 	// v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_list)) 
 	{
 		if (!gfn_confirm("confirm.before.search")) return false;
 	}

	var nm = "";
	
	if (edt_nm.value != undefined) nm = edt_nm.value;
	
	var csSabun = UserSearchComponent00.getSabun();
	
	
    var strSvcID    = "select";
    var strURL      = "sc/scz/addrMgmtFmsUpjang/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "muCd="+wrapQuote(MuSearchComponent00.getMuCode())+" tmCd="+wrapQuote(TmSearchComponent00.getTmCode())+" nm="+wrapQuote(nm)+" confirmYn="+wrapQuote(Combo00.value)+" csSabun="+wrapQuote(csSabun);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

