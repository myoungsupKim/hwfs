/**
 * @class 사용자구분에 따라서 해당 사용자군을 조회한다.
 * @param p - Callback Parameter
 */
function fn_bindingUserGroup(userType)
{
	if ( userType == "내부" ) {
		var nPos = ds_mainType.findRow("code", "INTRO_12");
		if ( nPos != -1 ) {
			ds_mainType.setColumn(nPos, "codeName", "기본화면2");
		}
	} else {
		ds_mainType.reset();
	}
	
	ds_userGroup.filter("set1=='" + userType + "'");
	ds_mainType.filter("set2=='" + userType + "' || code == 'INTRO_12' ");
	
	if (!gfn_isNull(userType))
	{
		if (userType == "외부") {
			ds_userRole_sysType.filter("set3=='" + userType + "' || set3=='내부/외부'");
			ds_exptRole_sysType.filter("set3=='" + userType + "' || set3=='내부/외부'");
			ds_boardRole_sysType.filter("set1=='FICS'");
		}
		else {
			ds_userRole_sysType.filter("");
			ds_exptRole_sysType.filter("");
			ds_boardRole_sysType.filter("");
		}
	}
	else
	{
		ds_userRole_sysType.filter("set3==''");
		ds_exptRole_sysType.filter("set3==''");
		ds_boardRole_sysType.filter("");
	}

	ds_unUserAuth.clearData();
	ds_userAuth.clearData();
	ds_exptAuth.clearData();

	if (userType == "내부")
	{
        tab_Info.tpg_userRole.cbo_sysType.value = "IFIS";
        tab_Info.tpg_exptRole.cbo_sysType.value = "IFIS";
        tab_Info.tpg_boardRole.cbo_sysType.value = "100";
		uc_upjang.setRoleSetId("ScoUpjang");
		tab_Info.tpg_userRole.btn_userAuthSearch.click();
		fn_searchSysId(tab_Info.tpg_exptRole.cbo_sysType.value);
		
		if (arguments.length == 1) {
			//fn_searchAuthLevel("", userType);
			cbo_mainType.value = "INTRO_01";
		}
	}
	else if (userType == "외부")
	{
        tab_Info.tpg_userRole.cbo_sysType.value = "FICS";
        tab_Info.tpg_exptRole.cbo_sysType.value = "FICS";
        tab_Info.tpg_boardRole.cbo_sysType.value = "200";
		uc_upjang.setRoleSetId("FmsUpjang");
		tab_Info.tpg_userRole.btn_userAuthSearch.click();
		fn_searchSysId(tab_Info.tpg_exptRole.cbo_sysType.value);
		
		if (arguments.length == 1) {
			//fn_searchAuthLevel("", userType);
			cbo_mainType.value = "INTRO_03";
		}
	}
	
	
}

