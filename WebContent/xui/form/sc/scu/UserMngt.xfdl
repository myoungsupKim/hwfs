		tab_Info.tpg_userRole.btn_rightToLeft.enable = false;
		
		tab_Info.tpg_exptRole.btn_exptAuthSave.enable = false;
		
		tab_Info.tpg_boardRole.btn_bbsAuthCopy.enable = false;
