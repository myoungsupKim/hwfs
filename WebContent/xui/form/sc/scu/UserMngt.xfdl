				//SCCombo.bind(this, "ComCode", "tab_Info.tpg_exptRole.cbo_sysType", null, {groupCode:"시스템구분", useYn:"Y", set3:"내부"}, SCCombo.CHOOSE, "fn_triggerEventSysType");
