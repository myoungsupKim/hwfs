/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	// 변경사항 체크
	if (gfn_dsIsUpdated(ds_master) == false) 
	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return;
	}
	
	// 저장시 Validation Check	
	//if (gfn_validCheck(grd_list, ds_listValid) == false)	return;
	
	if (gfn_isNull(ds_master.getColumn(ds_master.rowposition, "userid"))) {
		gfn_alert("아이디는 필수 입력항목입니다.");
		edt_userid.setFocus();
		return;
	}
	
	if (gfn_isNull(ds_master.getColumn(ds_master.rowposition, "nmKor"))) {
		gfn_alert("성명은 필수 입력항목입니다.");
		edt_nmKor.setFocus();
		return;
	}
	
	if (ds_master.getColumn(ds_master.rowposition, "userType") == "내부" && gfn_isNull(ds_master.getColumn(ds_master.rowposition, "userGroup"))) {
		gfn_alert("사용자군(FC/리조트) 구분은 필수 입력항목입니다.");
		cbo_userGroup.setFocus();
		return;
	}
	
	if (ds_master.getColumn(ds_master.rowposition, "userType") == "외부" && gfn_isNull(ds_master.getColumn(ds_master.rowposition, "userGroup"))) {
		gfn_alert("사용자군(식재고객/공급업체 등) 구분은 필수 입력항목입니다.");
		cbo_userGroup.setFocus();
		return;
	}
	
	if (ds_master.getColumn(ds_master.rowposition, "userGroup") != "공급업체" && gfn_isNull(ds_master.getColumn(ds_master.rowposition, "upjang")))
	{
		gfn_alert("사업장은 필수 선택항목입니다.");
		return;
	}
	
	if (ds_master.getColumn(ds_master.rowposition, "userGroup") == "공급업체" && gfn_isNull(ds_master.getColumn(ds_master.rowposition, "otcustcd")))
	{
		gfn_alert("공급업체는 필수 선택항목입니다.");
		return;
	}
	
	if (ds_master.getRowType(ds_master.rowposition) == Dataset.ROWTYPE_INSERT) {
		if (ds_master.getColumn(ds_master.rowposition, "userType") == "내부" && gfn_isNull(ds_master.getColumn(ds_master.rowposition, "sabun"))) {
			gfn_alert("내부 사용자인 경우에는 사번은 필수 항목입니다.\n사원찾기 버튼을 클릭하시기 바랍니다.");
			return;
		}
		
		if (ds_master.getColumn(ds_master.rowposition, "userType") == "외부" && fv_userIdCheck == "Y")
		{
			gfn_alert("아이디 중복확인을 클릭하여 유효성을 검증하시기 바랍니다.");
			return;
		}
	}
	
	if (ds_master.getColumn(ds_master.rowposition, "userType") == "외부" && (ds_master.getColumn(ds_master.rowposition, "userid") != ds_master.getColumn(ds_master.rowposition, "oldUserId")) && fv_userIdCheck == "Y")
	{
		gfn_alert("변경한 아이디에 대하여 중복체크를 수행하지 않았습니다.\n중복확인을 클릭하여 유효성을 검증하시기 바랍니다.");
		return;
	}
	
	if (gfn_isNull(cbo_userAuthLevel.value)) {
		gfn_alert("사용자권한은 필수항목입니다.");
		cbo_userAuthLevel.setFocus();
		return;
	}

    var strSvcID    = "save";
    var strURL      = "sc/scu/userMngt/save.xdo";
    var strInDs     = "ds_master=ds_master:U ds_authLevel=ds_authLevel:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

