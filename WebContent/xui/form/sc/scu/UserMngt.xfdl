			}
			
			cbo_userType.readonly = true;
			btn_searchPersonal.visible = false;
			
			
			if (ds_master.getColumn(ds_master.rowposition, "userType") == "외부")
			{
				//SCCombo.bind(this, "ComCode", "tab_Info.tpg_userRole.cbo_sysType", null, {groupCode:"시스템구분", useYn:"Y", set4:"외부"}, SCCombo.CHOOSE);
