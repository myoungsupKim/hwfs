		ds_authLevel.filter("set1=='내부'");
		cbo_systemMst.value = "";
		cbo_systemMst.enable = true;
		cbo_boardMst.value = "";
		cbo_boardMst.enable = true;
	}
	else
	{
		btn_searchPersonal.visible = false;
		//chk_attr06.readonly = false;
