		cbo_userGroup.value = "식재영업";
		
		if ( ds_master.getColumn(ds_master.rowposition, "mainYn") == "Y" ) {
			this.objects[cbo_boardMst.innerdataset].filter("code != '관리자'");
			cbo_boardMst.enable = true;
		} else {
			this.objects[cbo_boardMst.innerdataset].filter("");
			cbo_boardMst.enable = false;
		}
	}
	edt_upjang.class = "edt_WF_Essential";
	uc_upjang.setClass("edt_WF_Essential");
	uc_cust.setClass("");
}


