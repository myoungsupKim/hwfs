			if(e.newvalue == "Y")
			{
				//LOCK, NEW, NORMAL, TEMP_RETIRE 상태가 아닌 경우만 변경 가능(20171102 유선미)
				var strAcctStatus = obj.getColumn(0, "acctStatus");
				if( gfn_isNull(strAcctStatus) ) {
					return true;
				} else {
					if(strAcctStatus != "LOCK" && strAcctStatus != "NEW" && strAcctStatus != "NORMAL" && strAcctStatus != "TEMP_RETIRE")
					{
						gfn_alert("사용여부를 변경할 수 없는 계정상태입니다.");
						return false;
					}
				}	
			}
				
			break;
	}
}
