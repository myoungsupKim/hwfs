function fn_sosokBeforeEvent(obj)
{
	if (gfn_isNull(ds_master.getColumn(ds_master.rowposition, "userType")))
	{
		gfn_alert("사용자군이 선택되지 않았습니다.");
		return false;
	}
	else
	{
		if (obj.name == "uc_upjang")
		{
			if (ds_master.getColumn(ds_master.rowposition, "userType") == "내부")
			{
				return true;
			}
			else if (ds_master.getColumn(ds_master.rowposition, "userType") == "외부" && ds_master.getColumn(ds_master.rowposition, "userGroup") == "식재영업")
			{
				return true;
			}
			else
			{
				return false;
			}
		}
		else
		{
			if (ds_master.getColumn(ds_master.rowposition, "userType") == "외부" && ds_master.getColumn(ds_master.rowposition, "userGroup") == "공급업체")
			{
				obj.setQid("OtCust");
				return true;
			}
			else
			{
				return false;
			}
		}
	}
}

/**
 * @class 조회조건의 사번을 클리어할 때에 모든 화면의 자료를 Clear한다.
 * @param obj
 */
function fn_searchSabunClear(obj)
{
	// 변경사항 체크
	if (gfn_dsIsUpdated(ds_master) == true) 
	{
		var retValue = gfn_confirm("변경된 자료가 있습니다.\n화면을 클리어하시겠습니까?");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
		if (retValue == false) return;
	}

	// 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
	if (gfn_dsIsUpdated(ds_userAuth) == true) 
	{
		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
        if (!gfn_confirm("변경된 자료가 있습니다.\n화면을 클리어하시겠습니까?")) return false;
    }

	// 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
	if (gfn_dsIsUpdated(ds_exptAuth) == true) 
	{
		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
        if (!gfn_confirm("변경된 자료가 있습니다.\n화면을 클리어하시겠습니까?")) return false;
    }
    
	// 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
	if (gfn_dsIsUpdated(ds_bbsAuth) == true) 
	{
		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
        if (!gfn_confirm("변경된 자료가 있습니다.\n화면을 클리어하시겠습니까?")) return false;
    }

	// 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
	if (gfn_dsIsUpdated(ds_dualUpjang) == true) 
	{
		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
        if (!gfn_confirm("변경된 자료가 있습니다.\n화면을 클리어하시겠습니까?")) return false;
    }

	ds_master.setColumn(ds_master.rowposition, "userType", "");
	ds_master.clearData();
	ds_unUserAuth.clearData();
	ds_loginHistory.clearData();
	ds_userAuth.clearData();
	tab_Info.tpg_userRole.cbo_sysType.value = "";
	ds_exptAuth.clearData();
	tab_Info.tpg_exptRole.cbo_sysId.value = "";
	tab_Info.tpg_exptRole.cbo_sysType.value = "";
	ds_unBbsAuth.clearData();
	ds_bbsAuth.clearData();
	ds_dualUpjang.clearData();
	//fn_searchAuthLevel("", "");
	ds_authLevel.filter("");
	btn_searchPersonal.visible = false;
	
	edt_userid.position.width = 211;
	edt_userid.readonly = true;
	btn_useridChk.visible = false;
	
	ds_userRole_sysType.filter("set3==''");
	ds_exptRole_sysType.filter("set3==''");
	ds_boardRole_sysType.filter("set1=''");
	tab_Info.tpg_userRole.cbo_sysType.value = "";
	tab_Info.tpg_exptRole.cbo_sysType.value = "";
	tab_Info.tpg_boardRole.cbo_sysType.value = "";
	
	try {
		var ds = eval(tab_Info.tpg_exptRole.cbo_sysId.innerdataset);
		ds.clearData();
	}
	catch(e) {}
	return true;
}

