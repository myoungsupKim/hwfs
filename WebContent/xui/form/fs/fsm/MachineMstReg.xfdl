/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            break;
        case "machineMstSearch" :
            //trace(ds_list.saveXML());
            // 사업부별 콤보 조회
            fn_muItemChangedSearch();
            break;   
        case "codeUseCnt" :
			var machineMasterCnt = ds_codeUse.getColumn(ds_codeUse.rowposition,"machineMasterCnt");
			var machineReqCnt = ds_codeUse.getColumn(ds_codeUse.rowposition,"machineReqCnt");
			var machineAsdisuseCnt = ds_codeUse.getColumn(ds_codeUse.rowposition,"machineAsdisuseCnt");
			var cateringCnt = ds_codeUse.getColumn(ds_codeUse.rowposition,"cateringCnt");
			var eventCnt = ds_codeUse.getColumn(ds_codeUse.rowposition,"eventCnt");
			
			if(machineMasterCnt == 0 && machineReqCnt == 0 && machineAsdisuseCnt == 0 && cateringCnt == 0 && eventCnt == 0 )
			{
				if(ds_machineMst.getColumn(ds_machineMst.rowposition, "lev")==2)
				{
					ds_search.setColumn(ds_search.rowposition,"mclassCd",ds_machineMst.getColumn(ds_machineMst.rowposition, "mclassCd"));
					
					for(var i=0; i < ds_machineMst.getRowCount(); i++)
					{	
						if(ds_machineMst.getColumn(i, "mclassCd") == ds_search.getColumn(ds_search.rowposition,"mclassCd"))
						{
							ds_machineMst.deleteRow(i);
						}
					}
				}else{
					ds_machineMst.deleteRow(ds_machineMst.rowposition);
				}
				
			}else{
				gfn_alert("사용중인 코드 입니다. 삭제할 수 없습니다.");
				return;
			}	
            break;        
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break;    
    }
}
 

