function btn_machineMstDel_onclick(obj:Button,  e:ClickEventInfo)
{
    if(fv_authLvl < 70){
		gfn_alert("권한이 없습니다.");
		return;
	}
	
	var nRow = ds_machineMst.rowposition;
	
	if(nRow == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	if(ds_machineMst.getColumn(ds_machineMst.rowposition, "lev") == 1){
		gfn_alert("대분류는 삭제할 수 없습니다.");
		return;
	}
	
	if(ds_machineMst.getRowType(nRow) == 2){
		ds_machineMst.deleteRow(nRow);
	}else{
		var rtVal = gfn_confirm("등록되어 있는 코드입니다. 삭제 하시겠습니까?");	// 선택된 자료를 삭제 하시겠습니까?
		if (rtVal == true) 
		{	
			ds_search.setColumn(0, "lclassCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "lclassCd"));
			ds_search.setColumn(0, "mclassCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "mclassCd"));
			ds_search.setColumn(0, "sclassCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "sclassCd"));
			
			trace("lev : " +ds_machineMst.getColumn(ds_machineMst.rowposition, "lev"));
			trace("lclassCd : " +ds_machineMst.getColumn(ds_machineMst.rowposition, "lclassCd"));
			trace("mclassCd : " +ds_machineMst.getColumn(ds_machineMst.rowposition, "mclassCd"));
			trace("sclassCd : " +ds_machineMst.getColumn(ds_machineMst.rowposition, "sclassCd"));
			trace("parentCd : " +ds_machineMst.getColumn(ds_machineMst.rowposition, "parentCd"));

			if(ds_machineMst.getColumn(ds_machineMst.rowposition, "lev") == 2){
				if(ds_machineMst.findRow("parentCd" , ds_machineMst.getColumn(ds_machineMst.rowposition, "mclassCd")) != -1){
					gfn_alert("등록된 소분류가 있습니다.\n삭제할 수 없습니다.");
					return;
				}
			}
			
			if(ds_machineMst.getColumn(ds_machineMst.rowposition, "lev") == 3){
				if(ds_grd.getRowCount() > 0){
					gfn_alert("등록된 기기/기물이 있습니다.\n삭제할 수 없습니다.");
					return;
				}
			}
			trace("삭제!!");
			var strSvcID    = "codeUseCnt";
			var strURL      = "fs/fsm/machineMstReg/selectCodeUse.xdo";
			var strInDs     = "ds_search=ds_search:A";
			var strOutDs    = "ds_codeUse=ds_codeUse";
			var strArg      = "";
			var strCallback = "fn_callBack";
			var bAsync      = true;
			
			gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
		}
	}
	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 기기/기물 등록 [+]{-]
 *---------------------------------------------------------------------------------------------*/
function btn_grdAdd_onclick(obj:Button,  e:ClickEventInfo)
{
	if(fv_authLvl < 70){
		gfn_alert("권한이 없습니다.");
		return;
	}
	var cnt = 0;
	if(gfn_isNull(ds_machineMst.getColumn(ds_machineMst.rowposition, "lclassCd")) ||
	gfn_isNull(ds_machineMst.getColumn(ds_machineMst.rowposition, "mclassCd")) ||
	gfn_isNull(ds_machineMst.getColumn(ds_machineMst.rowposition, "sclassCd"))){
		gfn_alert("소분류에만 등록 할 수 있습니다.\n\n 왼쪽 분류를 확인 해 주세요.");
		return;
	}
	
	if(ds_machineMst.rowcount > 0)
	{
		for(var i=0; i < ds_machineMst.getRowCount(); i++)
		{
			if(ds_machineMst.getRowType(i) != 1)
			{
				cnt++;
			}
		}
		
		if(cnt == 0)
		{
			var nRow = ds_grd.addRow();
			//행추가시 자동세팅 되는 컬럼들
			ds_grd.setColumn(nRow, "lclassCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "lclassCd"));
			ds_grd.setColumn(nRow, "mclassCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "mclassCd"));
			ds_grd.setColumn(nRow, "sclassCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "sclassCd"));
			ds_grd.setColumn(nRow, "useYn", "Y");
			ds_grd.setColumn(nRow, "mgmtDeptCd", edt_muNm.getMuCode() );
			ds_grd.setColumn(nRow, "mgmtDeptNm",edt_muNm.getMuName() );
			ds_grd.setColumn(nRow, "cuserId", g_EmpNo);
			ds_grd.setColumn(nRow, "cuserNm", g_Name);
			ds_grd.setColumn(nRow, "cdate", fv_sToday);
		}else{
			gfn_alert("기기/기물 목록에 저장되지 않은 데이터가 있습니다.\n저장 후 행추가가 가능합니다.");
		}
	}else{
		gfn_alert("기기/기물 목록이 없습니다.");
		return;
	}
}
function btn_grdListDel_onclick(obj:Button,  e:ClickEventInfo)
{
	if(fv_authLvl < 70){
		gfn_alert("권한이 없습니다.");
		return;
	}
	var nRow = ds_grd.rowposition;
	//gfn_alert(ds_grd.getRowType(nRow));
	
	if(nRow == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	//RowType  이 INSERTED 인경우는 행삭제
	if(ds_grd.getRowType(nRow) == 2){
		ds_grd.deleteRow(nRow);
	}
	
	//기기/기물목록 잔여수량과  입고수량 비교하여 기기/기물이 사용중이면 삭제불가(잔여수량과 입고수량이 다르면 사용중으로 판단)
	var vResidualQty     = ds_machineMst.getColumn(ds_machineMst.rowposition,"residualQty");    //잔여수량
	var vSumQtyDelivered = ds_grd.getColumn(ds_grd.rowposition,"sumQtyDelivered");              //입고수량계
	var vQtyDelivered    = ds_grd.getColumn(ds_grd.rowposition,"qtyDelivered");                 //입고수량

	
	//이벤트등록건이 있는 경우는 삭제 불가.
	if (vSumQtyDelivered > 0){
	    alert("사용된 기기/기물은 행삭제 불가합니다");
	    return;
	}
	
	////잔여수량이 NULL이 아니여야 하고  잔여수량과 입고수량이 다르면 사용중으로 본다
	//if (!gfn_isNull(vResidualQty) && vResidualQty != vSumQtyDelivered){
	//    alert("해당 기기/기물은 사용중이므로 행삭제 불가합니다");
	//    return;
	//}  
	// KJH 이벤트관리 MASTER COUNT > 0 일 경우 처리 한다.
	
	//선택한 행 삭제
	ds_grd.deleteRow(nRow);
	
	var vResultQty = 0;
	
	vResultQty = vSumQtyDelivered - vQtyDelivered;
	
	//기기/기물 목록 잔여수량 차감 및 기기/기물 등록 입고수량계 차감
	ds_machineMst.setColumn(ds_machineMst.rowposition, "residualQty", vResultQty);
	
	for (var i=0;i<ds_grd.rowcount;i++){
	    ds_grd.setColumn(i, "sumQtyDelivered", vResultQty);
	}
	
	//RowType  이 INSERTED 인경우는 행삭제
// 	if(ds_grd.getRowType(nRow) == 2){
// 		ds_grd.deleteRow(nRow);
// 	}
// 	else{
// 		var rtVal = gfn_confirm("등록되어 있는 자료는 삭제가 불가능 합니다.\n사용하지 않으려면 사용여부를 미사용으로 변경하시기 바랍니다.");	// 선택된 자료를 삭제 하시겠습니까?
