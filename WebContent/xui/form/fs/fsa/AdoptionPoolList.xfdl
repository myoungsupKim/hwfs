//이메일 형식체크
function edt_email_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	var email = edt_email.value;

	if(email == ""){
		ds_poolList.setColumn(ds_poolList.rowposition, "email", ""); 	// 이메일
	
	
	}else if(!gfn_isEmail(email)) {
		gfn_alert("msg.err.validator.email");  //e-mail이 잘못된 형태로 입력 되었습니다.		
		ds_poolList.setColumn(ds_poolList.rowposition, "email","");
		return;
	}
}


//생년 형식체크
function cal_brthDate_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{		
	var dBrthDate = obj.value;
	var nAge = gfn_parseInt(gfn_getYear()) - gfn_parseInt(dBrthDate);
			
	if(dBrthDate == ''){
		gfn_alert("msg.err.validator.required", "생년");	// 생년 은(는)필수 입력 항목입니다.
		return;
		
	}else if( nAge < 0){
		gfn_alert(nAge+" 세 입니다. \n0 세 이상으로 입력해 주십시요." );
		ds_poolList.setColumn(ds_poolList.rowposition, "brthDate","");
		ds_poolList.setColumn(ds_poolList.rowposition, "age","");
		return;
		
	}else if(nAge > 100){
		gfn_alert(nAge+" 세 입니다. \n100 세 이하로 입력해 주십시요." );
		ds_poolList.setColumn(ds_poolList.rowposition, "brthDate","");
		ds_poolList.setColumn(ds_poolList.rowposition, "age","");
		return;
	
	}else{		
		ds_poolList.setColumn(ds_poolList.rowposition, "brthDate", dBrthDate );
		ds_poolList.setColumn(ds_poolList.rowposition, "age", nAge );
		return;	
	}
}


