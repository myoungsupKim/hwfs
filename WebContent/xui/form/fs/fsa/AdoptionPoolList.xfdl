/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            if(ds_poolList.rowcount == 0)
            {
				AddrSearchComponent01.clearAll();
            }
            break;
        
        // 저장
        case "save" :
			if( gfn_isNull(fv_adoptionNum) == '' ){
				ds_poolList.rowposition = 0;
			}else{
				var nSaveRowNum =  ds_poolList.findRowExpr("adoptionNum == '"+fv_adoptionNum+"'")
				if( nSaveRowNum > 0  ){				
					ds_poolList.rowposition = nSaveRowNum;
				}else{
					ds_poolList.rowposition = 0;
				}
			}
			
			
			gfn_alert("msg.save.success");	// 저장 되었습니다.
		
            break;    
        case "sidoCombo":
			ds_search.setColumn(0,'sido'  ,'');
			cbo_searchSido.index = 0;
			ds_sido1.copyData(ds_sido);
			
			break;
    }
}


