/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
    // 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
    if (nErrorCode < 0) {
        return;
    }

    switch(strSvcID) {
        // 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
			
			// 조회 전 dataset 초기화
			ds_evenInfo.clearData();
			ds_progress.clearData();
			ds_inforeg.clearData();
			ds_utensil.clearData();
			ds_file.clearData();
			ds_utensilMst.clearData();
			ds_fileMst.clearData();
            
            break;
        case "searchMaster" :
            //trace(ds_list.saveXML());
            //trace("EventProcessApprvMgnt 화면 >> fn_callBack(searchMaster) : ds_evenInfo.saveXML()-------------");
            //trace(ds_evenInfo.saveXML());
            //trace("EventProcessApprvMgnt 화면 >> fn_callBack(searchMaster) : ds_fileMst.saveXML()-------------");
            //trace(ds_fileMst.saveXML());
            
            div_evntDtl.visible     = false;
            div_evntMst.visible     = true;
            div_evntTrsf.visible    = false;
            
            break;
        case "searchRequest" :
            //trace(ds_list.saveXML());
            //trace("EventProcessApprvMgnt 화면 >> fn_callBack(searchRequest) : ds_evenInfo.saveXML()-------------");
            //trace(ds_evenInfo.saveXML());
            //trace("EventProcessApprvMgnt 화면 >> fn_callBack(searchRequest) : ds_file.saveXML()-------------");
            //trace(ds_file.saveXML());
            
            div_evntDtl.visible     = true;
            div_evntMst.visible     = false;
            div_evntTrsf.visible    = true;
            
            break;
        // 저장
        case "save" :
            gfn_alert("msg.save.success");  // 저장 되었습니다.

            fn_search();
            break;
    }
}

