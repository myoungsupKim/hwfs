//테마조회
function fn_themeSearch(){
    ds_rowSearch.setColumn(0, 'mclassCd', ds_evenInfo.getColumn(0, "mclassCd"));
    ds_rowSearch.setColumn(0, 'sclassCd', ds_evenInfo.getColumn(0, "sclassCd"));
    ds_rowSearch.setColumn(0, 'lclassCd', ds_evenInfo.getColumn(0, "lclassCd"));

    var strSvcID    = "Theme";
    var strURL      = "fs/fse/eventProcessInfoMgnt/selectTheme.xdo";
    var strInDs     = "ds_search=ds_rowSearch:A";
    var strOutDs    = "ds_cboTheme=dsThemeListOutput";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

//분류 변경시
function cbo_group_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
    fn_themeSearch();
}

//결재상태 변경시
function cbo_signEvt_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
    if (e.postvalue == "003"){
        ds_evenInfo.setColumn(0, "rtnReason", "");
        edt_rtnReason.enable = true;
    } else {
        ds_evenInfo.setColumn(0, "rtnReason", "");
        edt_rtnReason.enable = false;
    }
}

//이벤트목록 행변경시
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
    if (obj.getRowCount() == -1){
        return;
    }

    if (gfn_dsIsUpdated(ds_evenInfo) == true){
        edt_rtnReason.enable = false;
         // 해당 row의 위치를 이동하면 변경된 데이터가 사라집니다.@계속 진행 하시겠습니까?
        if (!gfn_confirm("confirm.before.moveropos")) return false;
    }

	// 조회 전 dataset 초기화
	//ds_list.clearData();
	ds_evenInfo.clearData();
	ds_progress.clearData();
	ds_inforeg.clearData();
	ds_utensil.clearData();
	ds_file.clearData();
	ds_utensilMst.clearData();
	ds_fileMst.clearData();

    ds_rowSearch.setColumn(0, 'lclassCd', obj.getColumn(ds_list.rowposition, "lclassCd"));
    ds_rowSearch.setColumn(0, 'mclassCd', obj.getColumn(ds_list.rowposition, "mclassCd"));
    ds_rowSearch.setColumn(0, 'sclassCd', obj.getColumn(ds_list.rowposition, "sclassCd"));
    ds_rowSearch.setColumn(0, 'seq', obj.getColumn(ds_list.rowposition, "seq"));

    if(obj.getColumn(e.newrow, "signStatus") == "003") {
        edt_rtnReason.enable = true;
    } else {
        edt_rtnReason.enable = false;
    }

    if(obj.getColumn(e.newrow, "requestCd") == "001") {
        var strSvcID    = "searchMaster";
        var strURL      = "fs/fse/eventProcessApprvMgnt/selectMasterInfo.xdo";
        var strInDs     = "ds_search=ds_rowSearch:A";
        var strOutDs    = "ds_evenInfo=dsEventInfo ds_progress=dsProgress ds_utensilMst=dsUtensilMst ds_fileMst=dsFileMst ds_cboTheme=dsThemeOutput";
      //var strOutDs    = "ds_evenInfo=dsEventInfo ds_progress=dsProgress ds_cboTheme=dsThemeOutput";  // 첨부파일/기기 추가 전
        var strArg      = "";
        var strCallback = "fn_callBack";
        var bAsync      = true;

        gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    } else {
        var strSvcID    = "searchRequest";
        var strURL      = "fs/fse/eventProcessApprvMgnt/selectRequestInfo.xdo";
        var strInDs     = "ds_search=ds_rowSearch:A";
        var strOutDs    = "ds_evenInfo=dsEventInfo ds_progress=dsProgress ds_inforeg=dsInforeg ds_utensil=dsUtensil  ds_file=dsFile ds_cboTheme=dsThemeOutput";
        var strArg      = "";
        var strCallback = "fn_callBack";
        var bAsync      = true;

        gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    }
}

