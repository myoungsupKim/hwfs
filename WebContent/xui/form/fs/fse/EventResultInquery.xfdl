//이벤트목록 행변경시
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
    if(obj.getRowCount() == -1) {
        return;
    }

    tab_evnt.tabindex = 0;
    ds_evenInfo.clearData();
    ds_progress.clearData();
    ds_evtFile.clearData();
    ds_inforeg.clearData();
    ds_utensil.clearData();
    ds_evtResult.clearData();
    ds_file.clearData();

    ds_search.setColumn(0, "evntId", obj.getColumn(e.newrow, "evntId"));

    var strSvcID    = "searchInfo";
    var strURL      = "fs/fse/eventResultRegMgnt/selectRequestInfo.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_evenInfo=dsEventInfo ds_progress=dsProgress ds_inforeg=dsInforeg ds_utensil=dsUtensil ds_evtResult=dsResult ds_evtFile=dsEvtFile ds_file=dsFile ds_cboTheme=dsThemeOutput";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

//탭변경시
function tab_evnt_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{
    if (tab_evnt.tabindex == 0) {
        edt_personal.visible = true;
    } else {
        edt_personal.visible = false;
    }
}
