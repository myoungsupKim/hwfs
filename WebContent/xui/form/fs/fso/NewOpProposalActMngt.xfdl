function ds_proposalPlanList_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	if(e.newrow >= 0
		&& !gfn_isNull(obj.getColumn(e.newrow,"muCd"))
		&& !gfn_isNull(obj.getColumn(e.newrow,"upjangCd"))) {
		
		// 조회조건 Param
		ds_search.setColumn(0, "muCd", 	  obj.getColumn(e.newrow,"muCd"));
		ds_search.setColumn(0, "tmCd", 	  obj.getColumn(e.newrow,"tmCd"));
		ds_search.setColumn(0, "upjangCd", 	  obj.getColumn(e.newrow,"upjangCd"));
		ds_search.setColumn(0, "year", 	  ds_search.getColumn(0, "year"));
		fn_searchDetail();
	}
}
/*--------------------------------------------------------------------------------------
* 설명	:	월별 실적 조회후 매출이익 계산
*-------------------------------------------------------------------------------------*/
function fn_setAmtMonthly(){

	// 결과값이 없을때 매출이익 클리어
	if(ds_plinvoiceList.getColumn(0, "acctClass") == "매출이익"){
		ds_plinvoiceList.clearData();
		return false;
	}

	var amtTmp= 0;
	var amtTot = 0;
	var iNum		= 0;
	
	for(var i=1; i<=12; i++){
		iNum = gfn_lPad(i, "0", 2);
		amtTmp = Number(ds_plinvoiceList.getColumn(0, "mmAmt"+iNum))
					- (Number(ds_plinvoiceList.getColumn(1, "mmAmt"+iNum))
					+ Number(ds_plinvoiceList.getColumn(2, "mmAmt"+iNum))
					+ Number(ds_plinvoiceList.getColumn(3, "mmAmt"+iNum)))
					;
		ds_plinvoiceList.setColumn(4, "mmAmt"+iNum, amtTmp);
		amtTot	= amtTot + amtTmp;
		//trace(i+":"+amtTmp+":"+amtTot);
	}
	ds_plinvoiceList.setColumn(4, "sumAmt", amtTot);
}

/*--------------------------------------------------------------------------------------
* 설명	:	월실적 조회후 매출이익 계산
*-------------------------------------------------------------------------------------*/
function fn_setAmtMonth(){

	// 결과값이 없을때 매출이익 클리어
	if(ds_accOrderPlanList.getColumn(0, "acctClass") == "매출이익"){
		ds_accOrderPlanList.clearData();
		return false;
	}
	// 신규운영 품의
	ds_accOrderPlanList.setColumn(4, "curPreAmt",
				   Number(ds_accOrderPlanList.getColumn(0, "curPreAmt"))
				- (Number(ds_accOrderPlanList.getColumn(1, "curPreAmt"))
				+ Number(ds_accOrderPlanList.getColumn(2, "curPreAmt"))
				+ Number(ds_accOrderPlanList.getColumn(3, "curPreAmt"))));

// 실적
	ds_accOrderPlanList.setColumn(4, "curAftAmt",
				   Number(ds_accOrderPlanList.getColumn(0, "curAftAmt"))
				- (Number(ds_accOrderPlanList.getColumn(1, "curAftAmt"))
				+ Number(ds_accOrderPlanList.getColumn(2, "curAftAmt"))
				+ Number(ds_accOrderPlanList.getColumn(3, "curAftAmt"))));

// 품의대비 실적
	ds_accOrderPlanList.setColumn(4, "curAmtRate",
				   Number(ds_accOrderPlanList.getColumn(0, "curAmtRate"))
				- (Number(ds_accOrderPlanList.getColumn(1, "curAmtRate"))
				+ Number(ds_accOrderPlanList.getColumn(2, "curAmtRate"))
				+ Number(ds_accOrderPlanList.getColumn(3, "curAmtRate"))));

// 신규운영 품의
	ds_accOrderPlanList.setColumn(4, "totPreAmt",
				   Number(ds_accOrderPlanList.getColumn(0, "totPreAmt"))
				- (Number(ds_accOrderPlanList.getColumn(1, "totPreAmt"))
				+ Number(ds_accOrderPlanList.getColumn(2, "totPreAmt"))
				+ Number(ds_accOrderPlanList.getColumn(3, "totPreAmt"))));

// 실적
	ds_accOrderPlanList.setColumn(4, "totAftAmt",
				   Number(ds_accOrderPlanList.getColumn(0, "totAftAmt"))
				- (Number(ds_accOrderPlanList.getColumn(1, "totAftAmt"))
				+ Number(ds_accOrderPlanList.getColumn(2, "totAftAmt"))
				+ Number(ds_accOrderPlanList.getColumn(3, "totAftAmt"))));

// 품의대비 실적
	ds_accOrderPlanList.setColumn(4, "totAmtRate",
				   Number(ds_accOrderPlanList.getColumn(0, "totAmtRate"))
				- (Number(ds_accOrderPlanList.getColumn(1, "totAmtRate"))
				+ Number(ds_accOrderPlanList.getColumn(2, "totAmtRate"))
				+ Number(ds_accOrderPlanList.getColumn(3, "totAmtRate"))));
				
}

