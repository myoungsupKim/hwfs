	fv_accOrderPlanListColInfo = gfn_getDatasetInfo(ds_accOrderPlanList);
	fv_plinvoiceListColInfo = gfn_getDatasetInfo(ds_plinvoiceList);
	
	ds_proposalPlanList.clearData();
	ds_accOrderPlanList.clearData();
	ds_plinvoiceList.clearData();
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_proposalPlanList);
    gfn_clearSortMark(grd_accOrderPlanList);
	gfn_clearSortMark(grd_plinvoiceList);
    
    // 조회조건 Param
    ds_search.setColumn(0, "srchYear", cal_searchYr.value);
    ds_search.setColumn(0, "year", cal_searchYear.value);
	
	var strSvcID    = "search";
    var strURL      = "fs/fso/newOpProposalActMngt/selectNewOpProposalActMngtList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_proposalPlanList=ds_proposalPlanList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

