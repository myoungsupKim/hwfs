************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		case "search" :
			if(ds_proposalPlanList.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_proposalPlanList, fv_proposalPlanListColInfo);
			}
			break;
			
        
        case "searchDetail" :
			if(ds_accOrderPlanList.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_accOrderPlanList, fv_accOrderPlanListColInfo);
			}
			if(ds_plinvoiceList.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_plinvoiceList, fv_plinvoiceListColInfo);
			}
			
			// 조회후 월실적 매출이익 셋팅
			 fn_setAmtMonth();
			 
			// 조회후 월별실적 매출이익 셋팅
			 fn_setAmtMonthly();
			
			break;
			
		case "save" :			
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
			break;
    }
}



