function fn_searchPrcgEmpCd() {
		
	//서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
	fv_searchPrcgEmpColInfo = gfn_getDatasetInfo(ds_searchPrcgEmp);
	ds_searchPrcgEmp.clearData();
	
    var strSvcID    = "searchPrcgEmpCd";
    var strURL      = "fs/fso/marketerActMngt/selectPrcgEmpCd.xdo";
	var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_searchPrcgEmp=ds_searchPrcgEmp";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = false;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		case "searchPrcgEmpCd" :			
			if(ds_searchPrcgEmp.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo(ds_searchPrcgEmp, fv_searchPrcgEmpColInfo);
 		    }else {
				//fn_search();
 		    }
 		    break;
		
		case "searchMarketerActMngtList" :
			if(ds_proposalPlanList.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_proposalPlanList, fv_proposalPlanListColInfo);
			}
			if(ds_marketerActMngtList.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_marketerActMngtList, fv_marketerActMngtListColInfo);
			}else{
				//계획비세팅 bind:planRateAmt  bind:actAmt/bind:planAmt*100  :   bind:planRateAmtSum  bind:actAmtSum/bind:planAmtSum*100
				for(var i=0; i<ds_marketerActMngtList.rowcount; i++){
					ds_marketerActMngtList.setColumn(i, "planRateAmt", gfn_round(Number(ds_marketerActMngtList.getColumn(i, "actAmt"))/Number(ds_marketerActMngtList.getColumn(i, "planAmt"))*100));
					ds_marketerActMngtList.setColumn(i, "planRateAmtSum", gfn_round(Number(ds_marketerActMngtList.getColumn(i, "actAmtSum"))/Number(ds_marketerActMngtList.getColumn(i, "planAmtSum"))*100));
				}
				
				var row = ds_marketerActMngtList.addRow();
		
				for(var i=1; i<5; i++){ //수주액은 제외
					ds_marketerActMngtList.setColumn(row, "acctClass" ,"매출이익");
					ds_marketerActMngtList.setColumn(row, "planAmt" ,Number(ds_marketerActMngtList.getColumn(1, "planAmt"))-Number(ds_marketerActMngtList.getColumn(2, "planAmt"))-Number(ds_marketerActMngtList.getColumn(3, "planAmt"))-Number(ds_marketerActMngtList.getColumn(4, "planAmt")));
					ds_marketerActMngtList.setColumn(row, "actAmt" ,Number(ds_marketerActMngtList.getColumn(1, "actAmt"))-Number(ds_marketerActMngtList.getColumn(2, "actAmt"))-Number(ds_marketerActMngtList.getColumn(3, "actAmt"))-Number(ds_marketerActMngtList.getColumn(4, "actAmt")));
					ds_marketerActMngtList.setColumn(row, "planRateAmt" ,Number(ds_marketerActMngtList.getColumn(1, "planRateAmt"))-Number(ds_marketerActMngtList.getColumn(2, "planRateAmt"))-Number(ds_marketerActMngtList.getColumn(3, "planRateAmt"))-Number(ds_marketerActMngtList.getColumn(4, "planRateAmt")));
					ds_marketerActMngtList.setColumn(row, "planAmtSum" ,Number(ds_marketerActMngtList.getColumn(1, "planAmtSum"))-Number(ds_marketerActMngtList.getColumn(2, "planAmtSum"))-Number(ds_marketerActMngtList.getColumn(3, "planAmtSum"))-Number(ds_marketerActMngtList.getColumn(4, "planAmtSum")));
					ds_marketerActMngtList.setColumn(row, "actAmtSum" ,Number(ds_marketerActMngtList.getColumn(1, "actAmtSum"))-Number(ds_marketerActMngtList.getColumn(2, "actAmtSum"))-Number(ds_marketerActMngtList.getColumn(3, "actAmtSum"))-Number(ds_marketerActMngtList.getColumn(4, "actAmtSum")));
					ds_marketerActMngtList.setColumn(row, "planRateAmtSum" ,Number(ds_marketerActMngtList.getColumn(1, "planRateAmtSum"))-Number(ds_marketerActMngtList.getColumn(2, "planRateAmtSum"))-Number(ds_marketerActMngtList.getColumn(3, "planRateAmtSum"))-Number(ds_marketerActMngtList.getColumn(4, "planRateAmtSum")));
				}
			}
			if(ds_planList.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_planList, fv_planListColInfo);
			}else{
				var row = ds_planList.addRow();
		
				for(var i=1; i<5; i++){ //수주액은 제외
					ds_planList.setColumn(row, "acctClass" ,"매출이익");
