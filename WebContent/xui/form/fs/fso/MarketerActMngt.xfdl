		gfn_alert("업장조건이 누락되었습니다.");
		return;
 	}
 	
//  	if(ds_search.getColumn(0, "marketerSabun") == undefined || ds_search.getColumn(0, "marketerSabun") == "" || ds_search.getColumn(0, "marketerSabun") == null){
// 		gfn_alert("담당마케터를 선택하세요.");
// 		cbo_searchPrcgEmp.setFocus();
// 		return;
//  	}
	
	// 조회시 Validation Check
	//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;
	
	//서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
	fv_proposalPlanListColInfo = gfn_getDatasetInfo(ds_proposalPlanList);
	fv_marketerActMngtListColInfo = gfn_getDatasetInfo(ds_marketerActMngtList);
	fv_planListColInfo = gfn_getDatasetInfo(ds_planList);
	fv_actListColInfo = gfn_getDatasetInfo(ds_actList);
	
	ds_proposalPlanList.clearData();
	ds_marketerActMngtList.clearData();
	ds_planList.clearData();
	ds_actList.clearData();
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_proposalPlanList);
    gfn_clearSortMark(grd_marketerActMngtList);
    gfn_clearSortMark(grd_planList);
	gfn_clearSortMark(grd_actList);
        
    // 조회조건 Param
    ds_search.setColumn(0, "year", cal_searchYear.value);
	ds_search.setColumn(0, "verCd", cbo_verCd.value);
	ds_search.setColumn(0, "accMon", cal_accMon.text);
	
	var strSvcID    = "searchMarketerActMngtList";
    var strURL      = "fs/fso/marketerActMngt/selectMarketerActMngtList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_proposalPlanList=ds_proposalPlanList ds_marketerActMngtList=ds_marketerActMngtList ds_planList=ds_planList ds_actList=ds_actList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

