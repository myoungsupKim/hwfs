/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (!gfn_dsIsUpdated(ds_payWay)) {
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	// 저장시 Validation Check	
	if (gfn_validCheck(ds_payWay, ds_payWayValid) == false)	return;
	
	if(ds_payWay.rowcount > 0)
	{
		if(gfn_parseInt(ds_payWay.getSum("salAmt")) <= 0 || 
		gfn_parseInt(ds_payWay.getSum("salAmt")) != gfn_parseInt(ds_funeralDeadDeadSheet.getSum("adjustAmt") - gfn_parseInt(ds_funeralDeadDeadSheet.getSum("salsIncludeNAmt"))))
		{
			//gfn_alert("결제 총 금액이 판매 총 금액과 일치해야 합니다.");
			//trace(gfn_parseInt(ds_payWay.getSum("salAmt")));
			gfn_alert("총 매출등록 금액이 일치 하지 않습니다.");
			return;
		}
		
		if(gfn_parseInt(ds_payWay.getSum("guestSalAmt")) <= 0 ||  
		gfn_parseInt(ds_payWay.getSum("guestSalAmt")) != gfn_parseInt(ds_funeralDeadDeadSheet.getSum("adjustAmt")))
		{
		//trace(" guestSaleAmt " + gfn_parseInt(ds_payWay.getSum("guestSalAmt")));
		//trace(" adjustAmt " + gfn_parseInt(ds_funeralDeadDeadSheet.getSum("adjustAmt")));
			gfn_alert("총 고객결제 금액이 일치 하지 않습니다.");
			return;
		}
		
		var cnt = 0;
		for(var i=0; i<ds_payWay.rowcount; i++) {
			
			if(gfn_isNull(ds_payWay.getColumn(i, "salAmt")) || (toNumber(ds_payWay.getColumn(i, "salAmt"))<=0)) {
				gfn_alert("매출등록 금액을 입력하여 주십시요.");
				ds_payWay.rowposition = i;
				return;
			}
			
			if(gfn_isNull(ds_payWay.getColumn(i, "guestSalAmt")) || (toNumber(ds_payWay.getColumn(i, "guestSalAmt"))<=0)) {
				gfn_alert("고객결제 금액을 입력하여 주십시요.");
				ds_payWay.rowposition = i;
				return;
			}
			
			for(var n=0; n<ds_payWay.rowcount; n++)
			{
				if(ds_payWay.getColumn(i, "occurDate") != ds_payWay.getColumn(n, "occurDate")) {
					cnt++;
				}
			}	
		}
		
		if(cnt > 0 )
		{
			gfn_alert("매출일자는 상이 할수 없습니다. 매출일자를 통일하여 주십시오.");
			return;
		}
	}
	
	//마감된 매출일자인지 검증
	fn_searchCloseYn();
	
//     var strSvcID    = "save";
//     var strURL      = "fs/fsf/funeralDeadDeadSheet/savePayWayList.xdo";
//     var strInDs     = "ds_payWay=ds_payWay:U";
//     var strOutDs    = "";
//     var strArg      = "";
//     var strCallback = "fn_callBack";
//     var bAsync      = true;
//     
//     gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

