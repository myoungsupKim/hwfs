{									// :DSRowPosChangeEventInfo
	if(e.newrow >= 0
		&& !gfn_isNull(obj.getColumn(e.newrow,"funeralHallCd"))
		&& !gfn_isNull(obj.getColumn(e.newrow,"guestNum"))) {
		
		// 조회조건 Param
		ds_search.setColumn(0, "funeralHallCd", ds_deadGuest.getColumn(e.newrow,"funeralHallCd"));
		ds_search.setColumn(0, "guestNum", ds_deadGuest.getColumn(e.newrow,"guestNum"));
		//edt_theDeadNm.value = ds_deadGuest.getColumn(e.newrow,"theDeadNm");
		fn_searchDtl();
	//	fn_searchPayWayList();
	}	
}

