/*----------------------------------------------------------------------------------------------
 * 설명      : 출력 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_print()
{
	//gfn_alert("출력장표 수정중입니다.");
	//return;
	if(ds_deadGuest.rowcount == 0){
		gfn_alert("선택된 항목이 없습니다.");
		return false;
	}
	
	var nRow = ds_deadGuest.rowposition;

	//레포트에 전달할 파라메터 생성
	var arrParams	= SCReport.newParams ();
	SCReport.setParams(arrParams, "guestNum", ds_deadGuest.getColumn(nRow,'guestNum'));
	//SCReport.setParams(arrParams, "guestNum", '201508121');
	SCReport.setParams(arrParams, "upjangCd", ds_deadGuest.getColumn(nRow,'funeralHallCd'));
	//--------------------------------------------------
	//방법1)서버에서 Query에 의해 처리하는 경우 예
	//(필수) 레포트명
	SCReport.setParams (arrParams, SCReport.RPT_NAME, "/hwfs/fs/fsf/FuneralDeadDeadSheet.ozr"); 
		

	//출력 옵션 설정
	//-창 타이틀 (디폴트는 "ReportViewer")
	SCReport.setParams (arrParams, SCReport.RPT_TITLE, "FuneralDeadDeadSheet");
	
	SCReport.viewReport (this, arrParams);
}


