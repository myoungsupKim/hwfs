		// 조회조건 Param
		ds_search.setColumn(0, "funeralHallCd", ds_deadGuest.getColumn(ds_deadGuest.rowposition,"funeralHallCd"));
		ds_search.setColumn(0, "guestNum", ds_deadGuest.getColumn(ds_deadGuest.rowposition,"guestNum"));
		ds_search.setColumn(0, "searchType", e.postvalue);
//		fn_searchDtl();
		fn_searchFuneralDeadDeadSheet();
//		var evinfo = new DSRowPosChangeEventInfo();
//		ds_deadGuest_onrowposchanged(ds_deadGuest, evinfo);
	}
}

