<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
    DELETE FROM MAK_DRIVING_STD 
    WHERE CAR_CLASS = #CAR_CLASS#
    AND CAR_NUMBER = #CAR_NUMBER#
    AND GUBUN = #GUBUN#
	]]></statement>
	<input default-name="ds_list">
		<col name="CAR_CLASS" size="256" type="VARCHAR" description="" /> 
		<col name="CAR_NUMBER" size="256" type="VARCHAR" description="" /> 
		<col name="GUBUN" size="256" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_list">
	</output>
</query>