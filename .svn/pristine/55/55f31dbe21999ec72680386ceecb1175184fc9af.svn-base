function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	
	fn_center(e.row);
	
	//선택 데이터 정보
	ds_condi.setColumn(ds_condi.rowposition,"TRANS_UPJANG" , ds_list.getColumn(e.row, "TRANS_UPJANG"));
	ds_condi.setColumn(ds_condi.rowposition,"NEED_DATE" , ds_list.getColumn(e.row, "NEED_DATE"));
	
	var sSvcID;			// 서비스 ID
	var sUrl;			// jsp 경로
	var sInDs;			// 입력 데이터셋
	var sOutDs;			// 출력 데이터셋
	var sArg;			// 추가 파라미터
	var sCallFunc;		// 콜백함수명
	
	fsp_clear(this);	// Action 호출 정보 초기화하기
	
	// 입력 데이터셋 셋팅
	sSvcID		= "SEARCH_DETAIL";
	sUrl		= "U_svc::" + "ma/mak/MAK00200V_T002.jsp";
	sInDs		= "ds_condi=ds_condi";
	sOutDs		= "ds_detail=ds_detail";
	sCallFunc 	= "fn_transation_callback";
	
	// 유효성 체크
	if(!fn_transation_validation(sSvcID)){return false;}
	
	// 데이터셋 초기화
	ds_detail.clearData();
	
	
	// 트랜잭션
	jsp_transaction(this, sSvcID, sUrl, sInDs, sOutDs, sArg, sCallFunc, true, false, false);
}


function fn_center(row){
	if(row == null){
		div_center.edt_Upjang.value = "";
		div_center.edt_PoDate.value =	"";
		div_center.edt_csSabun.value = "";
		div_center.mak_Amt.value = "";
	}else{
		div_center.edt_Upjang.value = ds_list.getColumn(row , "DEPT_NM");
		div_center.edt_PoDate.value =	gfn_getFormat(ds_list.getColumn(row , "NEED_DATE") , "@@@@년 @@월 @@일");
		div_center.edt_csSabun.value = ds_list.getColumn(row , "CS_SABUN_NM");
		div_center.mak_Amt.value = ds_list.getColumn(row , "SUM_AMT");
	}
}

