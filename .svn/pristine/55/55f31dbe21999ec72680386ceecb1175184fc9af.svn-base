/*----------------------------------------------------------------------------------------------
 * 설명      : Edit 관련 이벤트 실제 처리
 *---------------------------------------------------------------------------------------------*/
function fn_Trigger_edBtn(obj)
{
	switch(obj)
	{
		// 본사
		case "comp_upjangMain":
			comp_upjang.setUpjangCode("");
			edt_search_code.value = "";
			ds_condi.setColumn(0, "SEARCH_CODE", "")	
			break;
	
	
		// 업장
		case "comp_upjang":
			// 창고조건 초기화
			edt_search_code.value = "";
			ds_condi.setColumn(0, "SEARCH_CODE", "")	
			break;
			
		default:

			break;
	}
}


