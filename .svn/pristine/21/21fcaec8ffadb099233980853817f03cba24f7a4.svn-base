	SCReport.setParams (arrParams, "PARAM2", ed_Upjang.userdata);     //사업장  
	SCReport.setParams (arrParams, "PARAM3", me_NeedDate.value);      //입고 예정일
	SCReport.setParams (arrParams, "PARAM4", cbo_Otcust.value);       //공급업체
	SCReport.setParams (arrParams, "PARAM5", cob_tax.value);          //과/면세
	SCReport.setParams (arrParams, "PARAM6", cob_CenterFlag.value);   //센터경유
	SCReport.setParams (arrParams, "PARAM7", gfn_trim(v_chk01) + gfn_trim(v_chk02) + gfn_trim(v_chk03));   //창고구분
	SCReport.setParams (arrParams, "PARAM8", me_NeedDate.text);      //입고 예정일
	
	SCReport.setParams (arrParams, "zoom", 120);
	SCReport.viewReport (this, arrParams, false);
	
}
function cbo_Subinv_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	ds_Subinv.rowposition = nNewIndex;
}
// 창고 구분 선택 전체를 선택 했을 경우.
function chk_all_OnClick(obj:CheckBox, e:ClickEventInfo)
{
	if (chk_all.value == true)
	{
		chk_01.enable = false;
		chk_02.enable = false;
		chk_03.enable = false;
		chk_01.value = false;
		chk_02.value = false;
		chk_03.value = false;
	}
	else
	{
		chk_01.enable = true;
		chk_02.enable = true;
		chk_03.enable = true;
		chk_all.val = false;
	}
}
