//=========================================================================================
function fn_search()
{  
	/*김수정 요청 
	  접속자가 고객이 아니면 CC와 담당자 컬럼을 추가해 달라 20161005
	*/
	if(g_EmpNo.substr(0,2) == 'CS')	//고객이면
	{
		gd_list.formatid='CS';
	}
	else
	{
		gd_list.formatid='IN';
	}

	var v_chk01;
	var v_chk02;
	var v_chk03;	
	//필수항목 
	if (fn_ChkManNull()) return;
	
	// 초기화
	ds_Cond.clearData();
	ds_List.clearData();
	ds_Cond.addRow();	
	ds_Cond.setColumn(0, "MAIN_UPJANG",        ed_MainUpjang.userdata);   //본사 코드
	ds_Cond.setColumn(0, "UPJANG",             ed_Upjang.userdata);       //사업장	
	ds_Cond.setColumn(0, "NEED_DATE",          me_NeedDate.value);        //입고 예정일
	ds_Cond.setColumn(0, "NEED_DATET",         me_NeedDateT.value);       //입고 예정일
	ds_Cond.setColumn(0, "OTCUSTCD",           cbo_Otcust.value);         //공급업체
	ds_Cond.setColumn(0, "TAX_CODE",           cob_tax.value);            //과/면세
	ds_Cond.setColumn(0, "FLAG_CODE",          cob_CenterFlag.value);     //센터경유
	ds_Cond.setColumn(0, "CC_NM",          	   ed_CC.value);     		  //CC명
	ds_Cond.setColumn(0, "CS_NM",          	   ed_CS.value);     		  //CS명
	ds_Cond.setColumn(0, "ITEM",          	   ed_item.value);     		  //CS명
		
	if (chk_all.value == 1)
	{
		ds_Cond.setColumn(0, "chk_all",           chk_all.value);         //창고 전체
	}
	else
	{
		if (chk_01.value == 1)    //공통창고
		{
			v_chk01 = ' ';			
		}
		if (chk_02.value == 1)    //식당창고
		{
			v_chk02 = 'N';			
		}
		if (chk_03.value == 1)    //소모품창고
		{
			v_chk03 = 'Y';			
		}    
	}	
	
	ds_Cond.setColumn(0,"chk_str", gfn_trim(v_chk01) + gfn_trim(v_chk02) + gfn_trim(v_chk03));
	
	// 조회
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "FMP00150V_T001";
	strURL   = "U_svc::" + "fm/fmp/FMP00150V_T001.jsp";
	// 데이터셋 설정
	strInDatasets  = "ds_Cond=ds_Cond";
	strOutDatasets = "ds_List=ds_List";
	
	// 파라메터 설정
	strArgument  = "g_Upjang=" + wrapQuote(g_Upjang);
	strArgument += " g_EmpNo=" + wrapQuote(g_EmpNo);
	// 콜백함수 지정
	strCallbackFunc = "fn_CallBack_Search";
	// 서버 호출 
	Transaction(strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc);
}
