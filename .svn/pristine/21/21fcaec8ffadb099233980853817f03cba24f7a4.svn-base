//=========================================================================================
//	1. Function ID : fn_AlertMsg(cd)
//	2. 개       요 : 메세지 처리
//	3. 기       능 : 화면에 나타나는 메시지 코드및 텍스트 리턴
//	4. 인 수 설 명 : cd : 원하는 메시지 코드
//=========================================================================================
function fn_AlertMsg(cd)
{
	var v_Msg="";
	switch(cd)
	{
		case "000":
			v_Msg= "이(가) 저장되었습니다.";
			break;
		case "001":
			v_Msg= "버튼이 정의 되어 있지않습니다.";
			break;
		case "002":
			v_Msg="서비스 오류:시스템 관리자에게 문의하십시오";
			break;
		case "003":
			v_Msg="필수항목은 반드시 선택하시기 바랍니다.";
			break;	
		case "004":
			v_Msg="작업대상 본사를 선택하세요.";
			break;
		case "005":
			v_Msg="을(를) 입력하세요";
			break;
		case "006":
			v_Msg="삭제할 데이터가 없습니다.";
			break;
		case "007":
			v_Msg="변경된 데이터가 없습니다.";
			break;
		case "008":
			v_Msg="시스템ID가 없습니다.";
			break;			
		default:
			break;			
	}
	return v_Msg;
}
// Call Back Search
function fn_CallBack_Search(SvcID, errCode, errMsg)
{
	ta_Sql.value = fa_Sql;
	
	if (errCode == 0)
	{
		if (ds_List.rowcount < 1)
			g_Message("EE","조회된 자료가 없습니다.");
		else
			gd_list.setFocus();
	}
	else
		g_Message("EE",fn_AlertMsg("002"));
}
function fn_ChkManNull()
{
	if (ffn_ChkNull(ed_MainUpjang.userdata))
	{
		g_Message("EE", "본사" + fn_AlertMsg("005"));
		if (ed_MainUpjang.Enable) ed_MainUpjang.SetFocus();
		return true;
	}
	
	if ( ffn_ChkNull(me_NeedDate.value) || (gfn_length(me_NeedDate.value)< 8) )
	{
		g_Message("EE", "입고예정일을 정확하게 입력하세요.");
		me_NeedDate.setFocus();
		return true;
	}
	
	if ( ffn_ChkNull(me_NeedDateT.value) || (gfn_length(me_NeedDateT.value)< 8) )
	{
		g_Message("EE", "입고예정일을 정확하게 입력하세요.");
		me_NeedDateT.setFocus();
		return true;
	}
	
	if ( gfn_subStr(me_NeedDate.value,0,4)<> gfn_subStr(me_NeedDateT.value,0,4))
	{
		g_Message("EE", "동일년도를 선택 하세요.");
		me_NeedDate.setFocus();
		return true;
	}
	
	if ( gfn_subStr(me_NeedDate.value,4,2)<> gfn_subStr(me_NeedDateT.value,4,2))
	{
		g_Message("EE", "동일 월만 조회 가능합니다.");
		me_NeedDate.setFocus();
		return true;
	}	
	
	
	return false;
}
function fn_FormKeyDown(obj:Form, e:KeyEventInfo)
{
