<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>

SELECT COUNT(*) AS CNT
  FROM SCC_COMMON_CODE
 WHERE GROUP_CODE = 'MA0044'
   AND CODE = #V_DATE#
   AND CODE_NAME = 'Y'

	</statement>
	<input default-name="_none">
	</input>
	<output default-name="ds_clsCnt">
	</output>
</query>