<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>update</type>
	<description><![CDATA[행사식 - 메뉴 수정]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
UPDATE 
 	FSM_REAL_MENU_MST
SET 
	 MENU_NAME = #MENU_NAME#
	, MENU_DATE = #MENU_DATE#
	, HALL_CD = #HALL_CD#
	, MEAL = #MEAL#
	, CORNER = #CORNER#
	, SUBINV_CODE = #SUBINV_CODE#
	, SALE_PRICE = #SALE_PRICE#
	, EXPECT_MEAL_QTY = #EXPECT_MEAL_QTY#
	, GOAL_COST_RATE = #GOAL_COST_RATE#
	, REAL_MEAL_QTY = #REAL_MEAL_QTY#
	, MENU_FEATURE = #MENU_FEATURE#
	, TASTER_DESC = #TASTER_DESC#
	, MENU_PREFER = #MENU_PREFER#
	, ALT_DIET = #ALT_DIET#
	, ATTR01 = #ATTR01#
	, ATTR02 = #ATTR02#
	, ATTR03 = #ATTR03#
	, ATTR04 = #ATTR04#
	, ATTR05 = #ATTR05#
	, ATTR06 = #ATTR06#
	, ATTR07 = #ATTR07#
	, ATTR08 = #ATTR08#
	, ATTR09 = #ATTR09#
	, ATTR10 = #ATTR10#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 UPJANG = #UPJANG#
	 AND MENU_CD = #MENU_CD#
	]]></statement>
	<input default-name="ds_list">
		<col name="UPJANG" size="22" type="NUMERIC" description="" /> 
		<col name="MENU_CD" size="20" type="VARCHAR" description="" /> 
		<col name="MENU_NAME" size="50" type="VARCHAR" description="" /> 
