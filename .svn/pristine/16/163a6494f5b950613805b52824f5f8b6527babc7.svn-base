<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>update</type>
	<description><![CDATA[자랑레시피 마스터 수정]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
UPDATE 
 	FSR_PRIDE_RECIPE_MST
SET 
	DISH_QTY = #DISH_QTY#
	, RATION_METHOD = #RATION_METHOD#
	, MAN_STD = #MAN_STD#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 PRIDE_RECIPE_SEQ = #PRIDE_RECIPE_SEQ#	]]></statement>
	<input default-name="ds_master">
		<col name="DISH_QTY" size="20" type="VARCHAR" description="" /> 
		<col name="RATION_METHOD" size="20" type="VARCHAR" description="" /> 
		<col name="MAN_STD" size="20" type="VARCHAR" description="" /> 
		<col name="g_EmpNo" size="255" type="VARCHAR" description="" /> 
		<col name="PRIDE_RECIPE_SEQ" size="20" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_master">
	</output>
</query>
