function fn_search()
{
	var v_Param = "FROM_DATE=" + wrapQuote(cal_StartDate.fn_getValue());   
	v_Param += " TO_DATE=" + wrapQuote(cal_EndDate.fn_getValue());   
	
    // action 정보 초기화 
	fsp_clear(this);	
	fsp_addSearch(this, "fs/fsi:FSI00047V_S001");	
	// 서버 호출 
	fsp_callService(this, "", "", "", "ds_STD_FSI_CHG_LOG=ds_out",v_Param, "fn_After_Search");	
}

function fn_After_Search()
{
	this.st_Result.text = ds_STD_FSI_CHG_LOG.rowcount + " 건";
}

