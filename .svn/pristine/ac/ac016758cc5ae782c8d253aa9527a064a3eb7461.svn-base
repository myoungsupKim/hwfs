 * 설명      : 트랜잭션 콜백 함수
 *---------------------------------------------------------------------------------------------*/
function fn_callback(strSvcID, nErrorCode, strErrorMsg)
{
	if( nErrorCode != 0 )
	{
		return;
	}

	switch( strSvcID )
	{
		case "SEARCH":
		break;

		default:
		break;
 	}
}

