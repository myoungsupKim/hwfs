<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT  ' ' CD,
        '==조리법유형==' NM,
        '' SET1,
        'Y' PHF_DEFAULT_YN,
        0 SORT
FROM    DUAL
UNION ALL
SELECT  CODE CD,
        CODE_NAME NM,
        SET1,
        nvl(SET2,'Y') PHF_DEFAULT_YN,
        SORT_SEQ SORT
FROM    SCC_COMMON_CODE
WHERE   GROUP_CODE='FS0003'
AND     USE_YN='Y'
ORDER BY SORT	]]></statement>
	<input default-name="ds_CookClass">
	</input>
	<output default-name="ds_CookClass">
		<col name="CD" size="50" type="VARCHAR" description="" /> 
		<col name="NM" size="100" type="VARCHAR" description="" /> 
		<col name="SORT" size="22" type="NUMERIC" description="" /> 
	</output>
</query>
