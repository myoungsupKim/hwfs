/*----------------------------------------------------------------------------------------------
 * 대체금액 입력 후 EVENT (대체금액 체크/적용후잔액 계산)
 *---------------------------------------------------------------------------------------------*/
function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo){
	if(e.columnid == "applyAmt"){
		var iReceiveAmt		= 0;	//입금금액
		var iApplyAmt		= 0;	//대체금액
		var iApplyAmt2		= 0;	//미마감금액
		var iBalAmt			= 0;	//잔액
		var iApplyAmtSum	= 0;	//대체금액합계
		var sSapSuspenseNum	= "";	//가수금번호
		var nRow = ds_list.rowposition;

		//Null 입력방지
		ds_list.setColumn(nRow, "applyAmt", Number(ds_list.getColumn(nRow, "applyAmt")));
		
		iReceiveAmt		= Number(ds_list.getColumn(nRow, "receiveAmt"));
		iApplyAmt		= Number(ds_list.getColumn(nRow, "applyAmt"));
		iApplyAmt2		= Number(ds_list.getColumn(nRow, "applyAmt2"));
		sSapSuspenseNum	= ds_list.getColumn(nRow, "sapSuspenseNum");
		
		//입력 대체금액합계
		for(var i=0; i<ds_list.rowcount; i++){
			if(ds_list.getColumn(i, "sapSuspenseNum") == sSapSuspenseNum){
				iApplyAmtSum += ds_list.getColumn(i, "applyAmt");
			}
		}
		
		if(iApplyAmt < 0){
			gfn_alert("0 이상 금액을 입력하세요.");
			ds_list.setColumn(nRow, "applyAmt", ds_list.getOrgColumn(nRow, "applyAmt"));
			bCanRowChanged = false;
			return;
		}
		
		//잔액 < 대체금액
		if(iApplyAmt != 0){
			if(iReceiveAmt < (iApplyAmt + iApplyAmt2)){
				gfn_alert("대체금액이 입금금액을 초과할 수 없습니다.");
				ds_list.setColumn(nRow, "applyAmt", ds_list.getOrgColumn(nRow, "applyAmt"));
				bCanRowChanged = false;
				return;
			}
		}
		ds_list.setColumn(nRow, "balAmt" , iReceiveAmt - (iApplyAmt + iApplyAmt2));
		
		//대체금액 입력시 체크박스 체크드
		if(iApplyAmt == 0){
			ds_list.setColumn(nRow, "chk", "0");
		}else{
			ds_list.setColumn(nRow, "chk", "1");
		}
		fn_setApplyAmtSum();
	}	
	
	if(e.columnid == "chk"){
	
		var aa = 0;
		
		for(var i=0; i<ds_list.rowcount; i++){
			if(ds_list.getColumn(i, "chk") != "0"){
				aa += 1;
			}
		}
		
		//전체 선택 시 금액 설정
		if (aa == ds_list.rowcount) {
			for(var i=0; i<ds_list.rowcount; i++){
				
				iReceiveAmt		= Number(ds_list.getColumn(i, "receiveAmt"));
				iApplyAmt		= Number(ds_list.getColumn(i, "applyAmt"));
				iApplyAmt2		= Number(ds_list.getColumn(i, "applyAmt2"));
				ds_list.setColumn(i, "applyAmt",	iReceiveAmt - iApplyAmt2);
				ds_list.setColumn(i, "balAmt",	0);
			}
			fn_setApplyAmtSum();	//대체금액 합계
		}
		
		//전체 선택해제 시 금액 설정
		if (aa == 0) {
			for(var i=0; i<ds_list.rowcount; i++){
			
				iReceiveAmt		= Number(ds_list.getColumn(i, "receiveAmt"));
				iApplyAmt		= Number(ds_list.getColumn(i, "applyAmt"));
				iApplyAmt2		= Number(ds_list.getColumn(i, "applyAmt2"));
				ds_list.setColumn(i, "applyAmt",	0);
				ds_list.setColumn(nRow, "balAmt" , iReceiveAmt - (iApplyAmt + iApplyAmt2));
			}
			fn_setApplyAmtSum();	//대체금액 합계
		}	
	}
	
}

