		sReceiveDate = arrSchVal[6];
		edt_reSum.value = edt_arAmtSum.value - edt_applyAmtSum.value;
		MuSearchComponent01.setMuCode(arrSchVal[0]);
	}
	
	// 월력 초기화(해당Form, 값지정, 값변경시 실행할 callBack함수, dataset과 bind)
	cal_receiveDateFrom.fn_initMonthCal(this, gfn_getYear()+gfn_getMonth(), "", ds_search, "receiveDateFrom");		//gfn_getYear()+gfn_getMonth(gfn_addMonth(fv_sToday, -1))
