function ds_list_canrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo){
	if(bCanRowChanged == false){
		bCanRowChanged = true;
		return false;
	}
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 입금금액 클릭시 대체금액 변경 EVENT
 *---------------------------------------------------------------------------------------------*/
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo){
	var iReceiveAmt	= 0;	//입금금액
	var iApplyAmt	= 0;	//대체금액
	var iApplyAmt2	= 0;	//미마감금액

	if(e.cell == 4 || e.cell == 5){	//대체금액 Setting
		iReceiveAmt	= Number(ds_list.getColumn(ds_list.rowposition, "receiveAmt"));	//obj.getCellValue(e.row, e.cell);
		iApplyAmt	= Number(ds_list.getColumn(ds_list.rowposition, "applyAmt"));	//obj.getCellValue(e.row, e.cell+1);
		iApplyAmt2	= Number(ds_list.getColumn(ds_list.rowposition, "applyAmt2"));

		if(iReceiveAmt == (iApplyAmt + iApplyAmt2)){
			ds_list.setColumn(ds_list.rowposition, "applyAmt",	0);
		}else{
			ds_list.setColumn(ds_list.rowposition, "applyAmt",	iReceiveAmt - iApplyAmt2);
		}
	}
	if(e.cell == 0){
		fn_setApplyAmtSum();		
	}
	
	var balAmtIdx = grd_list.getBindCellIndex("body","balAmt");
	
	if(e.cell == balAmtIdx){
		var arrArg = new Array();
		var sapSuspenseNum = "";
		if(!gfn_isNull(ds_list.getColumn(ds_list.rowposition, "sapSuspenseNum"))){
			sapSuspenseNum = ds_list.getColumn(ds_list.rowposition, "sapSuspenseNum");
		}
		arrArg[0] = sapSuspenseNum

				
		var dsObj = gfn_dialog(	"SuspenseHistoryCallPop"
								,"X_SAR::SuspenseHistoryCallPop.xfdl"
								,{arrSchVal:arrArg}
								,-1		//Width
								,-1		//Height
								,""		//Style
								,-1		//Left
								,-1		//Top
								,false	//Modeless
								,true	//ShowTitle
								,"-1"	//Align
								,true);	//Resizable
	}
}

