        if (!gfn_isNull(arrSchVal[10])) {
        	if(gfn_getDiffMonth(cal_receiveDateFrom.fn_getValue(),arrSchVal[10]) < 0){
				gfn_alert("가수금 입금월의 전월로 입금처리 할 수 없습니다.\n시작일은 " + arrSchVal[10] + "보다 클수 없습니다.\n" + arrSchVal[10] + "(입금일자) < " + cal_receiveDateFrom.fn_getValue() + "(가수금 입금월)" );				
				cal_receiveDateFrom.fn_initMonthCal(this, gfn_getYear()+gfn_getMonth(), "", ds_search, "receiveDateFrom");		//gfn_getYear()+gfn_getMonth(gfn_addMonth(fv_sToday, -1))				cal_receiveDateTo.fn_initMonthCal(this, gfn_getYear()+gfn_getMonth(), "", ds_search, "receiveDateTo");	
				return;
			}
			if(gfn_getDiffMonth(cal_receiveDateTo.fn_getValue(),arrSchVal[10]) < 0){
				gfn_alert("가수금 입금월의 전월로 입금처리 할 수 없습니다.\n종료일은 " + arrSchVal[10] + "보다 클수 없습니다.\n" + arrSchVal[10] + "(입금일자) < " + cal_receiveDateFrom.fn_getValue() + "(가수금 입금월)" );
			cal_receiveDateFrom.fn_initMonthCal(this, gfn_getYear()+gfn_getMonth(), "", ds_search, "receiveDateFrom");		//gfn_getYear()+gfn_getMonth(gfn_addMonth(fv_sToday, -1))
				cal_receiveDateTo.fn_initMonthCal(this, gfn_getYear()+gfn_getMonth(), "", ds_search, "receiveDateTo");
				return;
			}
		}
		*/
		if(gfn_getDiffMonth(cal_receiveDateFrom.fn_getValue(), cal_receiveDateTo.fn_getValue()) < 0){
			gfn_alert("종료월이 시작월보다 빠릅니다.");
			//gfn_alert("msg.err.validator.date.great");	//종료일이 시작일보다 빠릅니다.
			return;
		}
	}
	
    gfn_transaction(this
