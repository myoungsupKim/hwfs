/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg){
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0){
		return;
	}
	
    switch(strSvcID){
            
		case "selectAcntCd" :

			if(ds_bankAcctNo.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_bankAcctNo, fv_objColInfo2);
			}
			
			ds_bankAcctNo.insertRow(0);
			ds_bankAcctNo.setColumn(0, "acntCd", null);
			ds_bankAcctNo.setColumn(0, "acntNm", "전체");
			cmb_bankAcctNo.index = 0;
			break;
			
        //조회
        case "search" :
			//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
			if(ds_list.getRowCount() == 0) gfn_loadDatasetInfo (ds_list, fv_objColInfo);
			fn_searchDtl();
            break;
            
		case "searchDtl" :
			//alert(ds_detail.rowcount);
			break;
    }
}

