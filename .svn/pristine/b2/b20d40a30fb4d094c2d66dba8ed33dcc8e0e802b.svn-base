	fv_objColInfo = gfn_getDatasetInfo (ds_list);
	
	if(!gfn_isNull(cal_receiveDateFrom.fn_getValue())){
		if(gfn_isNull(cal_receiveDateTo.fn_getValue())){
			cal_receiveDateTo.fn_setValue(cal_receiveDateFrom.fn_getValue());
		}		
	    //입금일자와 가수금 입금월 비교 추가 20180524 김호석(양경민 요청)
