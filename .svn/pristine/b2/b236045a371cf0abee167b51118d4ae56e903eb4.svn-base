************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		case "searchCode" :	
			
			if(ds_searchPrcgEmp.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_searchPrcgEmp, fv_searchPrcgEmpColInfo);
 		    }
 		    if(ds_prcgEmp.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_prcgEmp, fv_prcgEmpColInfo);
 		    } 		    
 		    if(ds_searchLocCd.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo(ds_searchLocCd, fv_searchLocCdColInfo);
 		    }
 		    if(ds_locCd.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo(ds_locCd, fv_locCdColInfo);
 		    }
 		    if(ds_locCd2.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo(ds_locCd2, fv_locCd2ColInfo);
 		    }
            if(ds_jobCd.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_jobCd, fv_jobCdColInfo);
 		    }
 		    //trace(ds_jobCd.saveXML());
 		    if(ds_opVendor.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_opVendor, fv_opVendorColInfo);
 		    }
 		    if(ds_prcgDept.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_prcgDept, fv_prcgDeptColInfo);
 		    }
 		    
 		    cbo_searchPrcgEmp.index = 0;
 		    cbo_searchLocCd.index = 0;
			
			if(arrRtn instanceof Object) {
				ds_search.setColumn(0, "muCd", arrRtn[0]);
				ds_search.setColumn(0, "muNm", arrRtn[1]);
				ds_search.setColumn(0, "tmCd", arrRtn[2]);
				ds_search.setColumn(0, "tmNm", arrRtn[3]);
				ds_search.setColumn(0, "objectCd", arrRtn[5]);
				ds_search.setColumn(0, "objectNm", arrRtn[6]);
				arrRtn = "";
			}else if(isFileUp=="Y") {
				isFileUp = "N";
			}
			fn_search();
            break;
            
        case "searchEmpMarketer":
            if(ds_empMarketer.rowcount>0) {
				ds_accObjectCard.setColumn(0, "prcgEmp", ds_empMarketer.getColumn(0, "prcgEmp"));
				ds_accObjectCard.setColumn(0, "prcgEmpNm", ds_empMarketer.getColumn(0, "prcgEmpNm"));
				ds_accObjectCard.setColumn(0, "prcgDept", ds_empMarketer.getColumn(0, "prcgDept"));
				ds_accObjectCard.setColumn(0, "prcgDeptNm", ds_empMarketer.getColumn(0, "prcgDeptNm"));
            }
            break;
            
        case "search":
			if(ds_objectList.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_objectList, fv_objectListColInfo);
 				
 				fv_accObjectCardColInfo= gfn_getDatasetInfo(ds_accObjectCard);
				fv_accObjectMealUpriceColInfo= gfn_getDatasetInfo(ds_accObjectMealUprice);
				fv_accObjectUpjangColInfo= gfn_getDatasetInfo(ds_accObjectUpjang);
				ds_accObjectCard.clearData();
				ds_accObjectMealUprice.clearData();
				ds_accObjectUpjang.clearData();
				ds_file.clearData();
				
				if(edt_muNm.getMuCode() == "2002") {
					div1.addrComp.clearAll();
				}else if(edt_muNm.getMuCode() == "2004") {
					div2.addrComp.clearAll();
				}else if(edt_muNm.getMuCode() == "2003") {
					div3.addrComp.clearAll();
				}
 		    }
            break;
            
        case "searchDetail" :
			if(div1.visible) {
				if(ds_accObjectCard.getRowCount() == 1)
				{
					var strFilter = "";
					var strLoc = ds_accObjectCard.getColumn(ds_accObjectCard.rowposition,"loc");
					if(strLoc != "undefined" && strLoc != "")
					{
						strFilter = "set1 == '" + strLoc  + "'";
						ds_locCd2.filter(strFilter);
					}
					else
					{
						ds_locCd2.filter(strFilter);
					}
				}
			}
			
            if(ds_accObjectCard.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_accObjectCard, fv_accObjectCardColInfo);
 		    }
 		    if(ds_accObjectMealUprice.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_accObjectMealUprice, fv_accObjectMealUpriceColInfo);
 		    }
 		    if(ds_accObjectUpjang.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
 				gfn_loadDatasetInfo (ds_accObjectUpjang, fv_accObjectUpjangColInfo);
 		    }
 		    
 		    if(div1.visible) {
				div1.cal_contractExpire.mae_cal.value = ds_accObjectCard.getColumn(0, "contractEndDate");				
			}else if(div3.visible) {
				div3.cal_contractExpire.mae_cal.value = ds_accObjectCard.getColumn(0, "contractEndDate");
			}
 		    
 		    // 상세 진행상태 생성
 		    ds_detailProgressStatus.clearData();
 		    var addRw = 0;
 		    if(ds_accObjectCard.rowcount > 0) {
				
				// 급식
				if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FS", 0) >= 0) {
					// 월력 초기화
//					div1.cal_accMon.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "accMon"), "", ds_accObjectCard, "accMon");
					// calendar 초기화
//					div1.cal_contractExpire.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "contractEndDate"), "", ds_accObjectCard, "contractEndDate");
				// 식재
				}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "식재", 0) >= 0) {
//					div2.cal_accMon.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "accMon"), "", ds_accObjectCard, "accMon");				
//					div2.cal_deliveredDate.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "deliveredDate"), "", ds_accObjectCard, "deliveredDate");
				
				// 외식
				}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "외식", 0) >= 0) {
//					div3.cal_accMon.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "accMon"), "", ds_accObjectCard, "accMon");
					// calendar 초기화
//					div3.cal_contractExpire.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "contractEndDate"), "", ds_accObjectCard, "contractEndDate");
				// FC 사업지원부
				}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FC", 0) >= 0){
//					div1.cal_accMon.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "accMon"), "", ds_accObjectCard, "accMon");	
					// calendar 초기화
//					div1.cal_contractExpire.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "contractEndDate"), "", ds_accObjectCard, "contractEndDate");
				}
				
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","관리카드 등록");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "mgmtcardRegYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "mgmtcardProgressDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","현장 설명회 참석");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "spotExplPacnYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "spotExplPacnDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","입찰 및 견적서 준비");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "bidAndEstPreparationYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "bidAndEstPreparationDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","추진보고서 작성");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "reptWriteYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "reptWriteDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","PT");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "ptYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "ptDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","수주 심의");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "accOrderconsiderYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "accOrderconsiderDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","응찰");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "bidYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "bidDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","계약검토");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "contractReviewYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "contractReviewDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","운영 품의 작성");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "opproposalWriteYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "opproposalProgressDate"));
				addRw = ds_detailProgressStatus.addRow();
				ds_detailProgressStatus.setColumn(addRw, "class","계약");
				ds_detailProgressStatus.setColumn(addRw, "status", ds_accObjectCard.getColumn(0, "contractYn"));
				ds_detailProgressStatus.setColumn(addRw, "progressDate", ds_accObjectCard.getColumn(0, "contractDate"));
 		    }
 		    
            break;
        
        case "save" :
			// 첨부파일 저장시 첨부파일 참조일련번호가 전달됨			
			gfn_alert("msg.save.success");
			cbo_searchLocCd2.index = 0;
			
			// 첨부파일 저장시 첨부파일 참조일련번호가 전달됨
	//		trace(" fv_refSeq +++++++++++++++++++>>>>>>>>>> "+fv_refSeq);
			if(!gfn_isNull(fv_refSeq)) {
				ds_accObjectCard.enableevent = false;
				ds_accObjectCard.setColumn(0,"fileId",fv_refSeq);
				ds_accObjectCard.enableevent = true;
				ds_search.setColumn(0, "refSeq", fv_refSeq);
				isFileUp = "Y";
			}else{
				ds_accObjectCard.enableevent = false;
				ds_accObjectCard.setColumn(0,"fileId","");
				ds_accObjectCard.enableevent = true;
				ds_search.setColumn(0, "refSeq", "");
				isFileUp = "N";
			}
			
			fn_searchCode();
            break;   
		case "searchLocCd2" :
			//trace(ds_locCd2.saveXML());
			break;
		case "searchLocCd22" :
			//trace(ds_searchLocCd2.saveXML());
			cbo_searchLocCd2.index = 0;
			break;
		case "change" :
			gfn_alert("msg.save.success");
			
			var nRowCnt = ds_objectList.getRowCount();
			for(var i=0 ; i<nRowCnt ; i++)
			{
				if ((ds_objectList.getColumn(i, "chk") == 1))
				{
					ds_objectList.setColumn(i, "chk", "0");
				}
			}
			
			fn_searchDetail();

			break;
    }
}


