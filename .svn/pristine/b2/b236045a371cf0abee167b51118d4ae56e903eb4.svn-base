function btn_chgPrcgemp_onclick(obj:Button,  e:ClickEventInfo)
{

 	// 권한체크(FC 사업지원부)
 	if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FC", 0) >= 0) {
		gfn_alert("조회만 가능합니다.");
 		return;
 	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "구매", 0) >= 0) {
		gfn_alert("권한이 불충분합니다.");
 		return;
 	}

	var nRowCnt = ds_objectList.getRowCount();
	var bIsSelectedRow = false;
	
	//선택한 행이 있는지 확인
	for(var i=0 ; i<nRowCnt ; i++)
	{
		if ((ds_objectList.getColumn(i, "chk") == 1))
		{
			bIsSelectedRow = true;
			continue;
		}
	}

	//선택한 행이 없을 시에 메시지 출력
	if (!bIsSelectedRow)
	{
		gfn_alert("msg.err.grid.noselect", "선택 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	fn_searchPrcgemp();
	
}

function fn_searchPrcgemp()
{
	//[[팝업창을 호출한다.
	var strId       = "PersonalSearch";
	var strURL      = "cmn::PersonalSearchPop.xfdl";
	var objArgument = {
		P_MU_CD : this.qoMuCd,
		P_TM_CD : this.qoTmCd,
		P_TM_NM : this.qoTmNm,
		P_CC_CD : this.qoCcCd,
		P_CC_NM : this.qoCcNm,
		P_ALL_MU : "FALSE"
	};
	var nWidth      = 726;
	var nHeight     = 495;
	var strStyle    = "";
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";

	// 리턴값을 배열로 받을 경우	
	var arrRtn = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);

	var bIsSelectedRow = false;
	
	if (arrRtn instanceof Object)
	{
		var nRowCnt = ds_objectList.getRowCount();
		for(var i=0 ; i<nRowCnt ; i++)
		{
			if ((ds_objectList.getColumn(i, "chk") == 1))
			{
				ds_objectList.setColumn(i, "prcgEmpNm", arrRtn.nmKor);
				ds_objectList.setColumn(i, "prcgEmp", arrRtn.sabun);
				ds_objectList.setColumn(i, "prcgDeptNm", arrRtn.ccNm);
				ds_objectList.setColumn(i, "prcgDept", arrRtn.ccCd);
				bIsSelectedRow = true;
			}
		}
	}
	//팝업창을 호출한다.]]
	
	//저장
	if (bIsSelectedRow)
	{
		// v_ds_check 미사용시 변경사항 체크
		if(!gfn_dsIsUpdated(ds_objectList)) {
			gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
			return;
		}
		
		var strSvcID    = "change";
		var strURL      = "fs/fso/objectMngtCard/changeObjectMngtCard.xdo";
		var strInDs     = "ds_objectList=ds_objectList:U";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}

}]]></Script>
