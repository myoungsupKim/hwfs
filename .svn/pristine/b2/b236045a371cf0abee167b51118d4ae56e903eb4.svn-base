				ds_locCd2.filter(strFilter);
			}
			else
			{
				ds_locCd2.filter(strFilter);
			}
		}

	}
	
	// 외식일 경우
	if(gfn_indexOf(ds_search.getColumn(0, "muNm"), "외식", 0) >= 0) {
		// 임대율
		if(e.columnid=="rentRate" && toNumber(obj.getColumn(e.row, e.columnid))>100) {
