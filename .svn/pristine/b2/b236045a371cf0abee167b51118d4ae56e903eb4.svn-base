				div3.msk_bizNo.value = "";
				div3.msk_bizNo.setSelect();
				div3.msk_bizNo.setFocus();
			}
		}
*/	
	}else if(e.columnid=="contractEndDate") {
	//	alert(e.newvalue);
	}else if(e.columnid=="loc") {

		if(div1.visible) {
			var strFilter = "";
			var strLoc = ds_accObjectCard.getColumn(ds_accObjectCard.rowposition,"loc");
			if(strLoc != "undefined" && strLoc != "")
