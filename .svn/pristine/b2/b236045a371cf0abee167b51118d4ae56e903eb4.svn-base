/*
 ******************************************************************************************
 * 함  수  명   : fn_searchCode()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 사업군,직군,현운영업체현황,담당부서,담당마케터,지역 코드 조회
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_searchCode() {
		
	//서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
	fv_searchPrcgEmpColInfo = gfn_getDatasetInfo(ds_searchPrcgEmp);
	fv_prcgEmpColInfo		= gfn_getDatasetInfo(ds_prcgEmp);
	fv_searchLocCdColInfo   = gfn_getDatasetInfo(ds_searchLocCd);
	fv_locCdColInfo			= gfn_getDatasetInfo(ds_locCd);
	fv_locCd2ColInfo		= gfn_getDatasetInfo(ds_locCd2);
	fv_jobCdColInfo         = gfn_getDatasetInfo(ds_jobCd);
	fv_opVendorColInfo		= gfn_getDatasetInfo(ds_opVendor);
	fv_prcgDeptColInfo		= gfn_getDatasetInfo(ds_prcgDept);	
	ds_searchPrcgEmp.clearData();
	ds_prcgEmp.clearData();
	ds_searchLocCd.clearData();
	ds_locCd.clearData();
	ds_locCd2.clearData();
	ds_jobCd.clearData();
	ds_opVendor.clearData();
	ds_prcgDept.clearData();
	
    var strSvcID    = "searchCode";
    var strURL      = "fs/fso/objectMngtCard/selectCode.xdo";
	var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_searchPrcgEmp=ds_searchPrcgEmp ds_prcgEmp=ds_prcgEmp ds_searchLocCd=ds_searchLocCd ds_locCd=ds_locCd "
						+"ds_jobCd=ds_jobCd ds_opVendor=ds_opVendor ds_prcgDept=ds_prcgDept "
						+"ds_jobClassCd=ds_jobClassCd ds_jobMClassCd=ds_jobMClassCd ds_jobSClassCd=ds_jobSClassCd ds_locCd2=ds_locCd2";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = false;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_searchMarketer()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 담당 마케터 조회
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_searchDetail() {
	
 	if(gfn_isNum(ds_objectList.getColumn(ds_objectList.rowposition, "fileId"))) {
 		ds_search.setColumn(0, "refSeq", ds_objectList.getColumn(ds_objectList.rowposition, "fileId"));
    }else {
 		ds_search.setColumn(0, "refSeq", "999999");
    }
	
	fv_accObjectCardColInfo= gfn_getDatasetInfo(ds_accObjectCard);
	fv_accObjectMealUpriceColInfo= gfn_getDatasetInfo(ds_accObjectMealUprice);
	fv_accObjectUpjangColInfo= gfn_getDatasetInfo(ds_accObjectUpjang);
	ds_accObjectCard.clearData();
	ds_accObjectMealUprice.clearData();
	ds_accObjectUpjang.clearData();
	ds_file.clearData();

	if(div1.visible) {
		gfn_clearSortMark(div1.grd_objectMealUprice);
		gfn_clearSortMark(div1.grd_objectUpjang);
		gfn_clearSortMark(div1.grd_detailProgressStatus);
	}else if(div2.visible) {
		gfn_clearSortMark(div2.grd_objectUpjang);
		gfn_clearSortMark(div2.grd_detailProgressStatus);
	}else{
		gfn_clearSortMark(div3.grd_objectUpjang);
		gfn_clearSortMark(div3.grd_detailProgressStatus);
	}
	
	var strSvcID    = "searchDetail";
    var strURL      = "fs/fso/objectMngtCard/selectDetail.xdo";
	var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_accObjectCard=ds_accObjectCard ds_accObjectMealUprice=ds_accObjectMealUprice ds_accObjectUpjang=ds_accObjectUpjang ds_file=ds_file";    
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

