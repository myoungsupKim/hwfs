 * 각 COMPONENT 별 EVENT 영역 (필수)
************************************************************************************************/
//MU가 변경될 때에 대한 이벤트
function fn_muItemChangedEvent(obj, evt)
{
	edt_tmNm.setTmCode("");
    ds_search.setColumn(0, "muCd", edt_muNm.getMuCode());
	ds_search.setColumn(0, "muNm", edt_muNm.getMuName());
			
	// 급식
	if(gfn_indexOf(ds_search.getColumn(0, "muNm"), "FS", 0) >= 0) {
		SCPopup.bind(this, SCPopup.StUpjang, "div1.grd_objectUpjang.upjangNm", "GRID:upjangCd", null, {includeVirtureUpjang:"Y"}, {before:"fn_upjangBeforeEvent"});
		ds_search.setColumn(0, "saupType", "C");		
		div1.addrComp.clearAll();
		div1.visible = true;
		div2.visible = false;
		div3.visible = false;
		cbo_searchLocCd2.enable = true;
		cbo_searchLocCd2.index = 0;
		cbo_searchLocCd.index = 0;
	// 식재
	}else if(gfn_indexOf(ds_search.getColumn(0, "muNm"), "식재", 0) >= 0) {
		SCPopup.bind(this, SCPopup.StUpjang, "div2.grd_objectUpjang.upjangNm", "GRID:upjangCd", null, {includeVirtureUpjang:"Y"}, {before:"fn_upjangBeforeEvent"});
		ds_search.setColumn(0, "saupType", "E");		
		div2.addrComp.clearAll();
		div1.visible = false;
		div2.visible = true;
		div3.visible = false;		
		cbo_searchLocCd2.enable = false;
		cbo_searchLocCd2.index = 0;
		cbo_searchLocCd.index = 0;
	// 외식
	}else if(gfn_indexOf(ds_search.getColumn(0, "muNm"), "외식", 0) >= 0) {
		SCPopup.bind(this, SCPopup.StUpjang, "div3.grd_objectUpjang.upjangNm", "GRID:upjangCd", null, {includeVirtureUpjang:"Y"}, {before:"fn_upjangBeforeEvent"});
		ds_search.setColumn(0, "saupType", "A");
		div3.addrComp.clearAll();
		div1.visible = false;
		div2.visible = false;
		div3.visible = true;		
		cbo_searchLocCd2.enable = false;
		cbo_searchLocCd2.index = 0;
		cbo_searchLocCd.index = 0;
	// FC 사업지원부
