		div1.visible = false;
		div2.visible = false;
		div3.visible = true;		
		ds_search.setColumn(0, "saupType", "A");
		cbo_searchLocCd2.enable = false;
		cbo_searchLocCd2.index = 0;
		cbo_searchLocCd.index = 0;
		
	// FC 사업지원부
	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FC", 0) >= 0){
		
		div1.visible = true;
		div2.visible = false;
		div3.visible = false;
		ds_search.setColumn(0, "saupType", "C");
		cbo_searchLocCd2.enable = true;
		cbo_searchLocCd2.index = 0;
		cbo_searchLocCd.index = 0;
		
	}else {
		div1.visible = false;
		div2.visible = false;
		div3.visible = false;
// [속도개선 Project 시작] 2017. 7. 21. 최범주 사용자가 FS사업부일 경우 담당마케터 콤보 셋팅 기능 추가
		ds_search.setColumn(0, "saupType", "");
