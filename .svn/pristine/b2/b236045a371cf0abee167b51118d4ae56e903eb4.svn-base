/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	//trace("g_Deptid >>> "+g_Deptid+" ||| g_DeptNm >>>"+g_DeptNm+" ||| g_EmpNo >>> "+g_EmpNo+" ||| g_Name >>> "+g_Name);
	
	SCCombo.bind(this, "ComCode", "div1.cbo_guestcpnyInchargeJobGrade", ds_jobGrade, {groupCode:"MA0023", useYn:"Y"});
