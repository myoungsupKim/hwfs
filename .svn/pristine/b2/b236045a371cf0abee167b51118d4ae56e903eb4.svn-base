/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	
	if(gfn_isNull(ds_search.getColumn(0, "muCd")) || gfn_isNull(ds_search.getColumn(0, "tmCd"))) {
		gfn_alert("사업부, 팀 은 필수 항목입니다.");
		edt_muNm.setFocus();
		return;
	}
	
	// 급식, 식재, 외식 이 아닌경우, 불가.
	if((gfn_indexOf(ds_search.getColumn(0,"muNm"), "FS", 0) < 0) 
		&& (gfn_indexOf(ds_search.getColumn(0,"muNm"), "식재", 0) < 0)
		&& (gfn_indexOf(ds_search.getColumn(0,"muNm"), "외식", 0) < 0)) {
		gfn_alert("FS/식재/외식사업부 중 하나를 선택해야 합니다.");
		edt_muNm.setFocus();
		return;
	}
	
	if (gfn_dsIsUpdated(ds_accObjectCard)||gfn_dsIsUpdated(ds_accObjectMealUprice)||gfn_dsIsUpdated(ds_accObjectUpjang)) {
 		var retValue = gfn_confirm("변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?");	// 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		if (retValue == false) return;
 	}
	
	ds_accObjectCard.clearData();
	ds_accObjectMealUprice.clearData();
	ds_accObjectUpjang.clearData();
	ds_detailProgressStatus.clearData();
	ds_file.clearData();
	
	var nRow = ds_accObjectCard.addRow();
	ds_accObjectCard.setColumn(nRow, "muCd", ds_search.getColumn(0, "muCd"));
	ds_accObjectCard.setColumn(nRow, "tmCd", ds_search.getColumn(0, "tmCd"));	
	ds_accObjectCard.setColumn(nRow, "mealUpriceA", "0");
	ds_accObjectCard.setColumn(nRow, "mealUpriceB", "0");
	ds_accObjectCard.setColumn(nRow, "mealUpriceC", "0");
	ds_accObjectCard.setColumn(nRow, "mealUpriceD", "0");
	ds_accObjectCard.setColumn(nRow, "mealQtyTot", "0");
	ds_accObjectCard.setColumn(nRow, "etcAmtE", "0");
	
	ds_accObjectCard.setColumn(nRow, "prcgDept", ds_search.getColumn(0, "deptId"));
	ds_accObjectCard.setColumn(nRow, "prcgEmp", ds_search.getColumn(0, "empNo"));
	ds_accObjectCard.setColumn(nRow, "prcgDeptNm", ds_search.getColumn(0, "deptNm"));
	ds_accObjectCard.setColumn(nRow, "prcgEmpNm", ds_search.getColumn(0, "name"));
//	ds_search.setColumn(nRow, "deptId", g_Deptid);	// 부서ID
//	ds_search.setColumn(nRow, "deptNm", g_DeptNm);	// 부서명
//	ds_search.setColumn(nRow, "empNo", g_EmpNo);	// 사번
//	ds_search.setColumn(nRow, "name", g_Name);		// 성명

	// 급식
	if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FS", 0) >= 0) {
		div1.cal_accMon.fn_initMonthCal(this, (fv_sToday).substr(0, 6), "", ds_accObjectCard, "accMon");
		div1.cal_contractExpire.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "contractEndDate"), "", ds_accObjectCard, "contractEndDate");
		ds_accObjectCard.setColumn(nRow, "addrInfo", div1.addrComp.newData());
	
		// 담당부서,마케터 사용자로 default 설정.
		fn_searchEmpMarketer();
		
	// 식재
	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "식재", 0) >= 0) {
		div2.cal_accMon.fn_initMonthCal(this, (fv_sToday).substr(0, 6), "", ds_accObjectCard, "accMon");
		div2.cal_deliveredDate.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "deliveredDate"), "", ds_accObjectCard, "deliveredDate");
		ds_accObjectCard.setColumn(nRow, "addrInfo", div2.addrComp.newData());
		
		// 담당부서,마케터 사용자로 default 설정.
		fn_searchEmpMarketer();
		
	// 외식
	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "외식", 0) >= 0) {
		div3.cal_accMon.fn_initMonthCal(this, (fv_sToday).substr(0, 6), "", ds_accObjectCard, "accMon");
		div3.cal_contractExpire.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "contractEndDate"), "", ds_accObjectCard, "contractEndDate");
		ds_accObjectCard.setColumn(nRow, "addrInfo", div3.addrComp.newData());
	
		// 담당부서,마케터 사용자로 default 설정.
		fn_searchEmpMarketer();
		
	// FC 사업지원부
	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FC", 0) >= 0){
		div1.cal_accMon.fn_initMonthCal(this, (fv_sToday).substr(0, 6), "", ds_accObjectCard, "accMon");
		div1.cal_contractExpire.fn_initMonthCal(this, ds_accObjectCard.getColumn(0, "contractEndDate"), "", ds_accObjectCard, "contractEndDate");
		ds_accObjectCard.setColumn(nRow, "addrInfo", div1.addrComp.newData());
		
		// 담당부서,마케터 사용자로 default 설정.
		fn_searchEmpMarketer();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_accObjectCard.rowposition;	
	if(ds_accObjectCard.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		ds_accObjectCard.deleteRow(nRow);
		ds_accObjectMealUprice.deleteAll();
		ds_accObjectUpjang.deleteAll();
		ds_file.deleteAll();
		ds_detailProgressStatus.clearData();

		fn_save();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
 	// 권한체크(FC 사업지원부)
 	if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FC", 0) >= 0) {
		gfn_alert("조회만 가능합니다.");
 		return;
 	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "구매", 0) >= 0) {
		gfn_alert("권한이 불충분합니다.");
 		return;
 	}
 	
 	// v_ds_check 미사용시 변경사항 체크
 	if(!gfn_dsIsUpdated(ds_accObjectCard)&&!gfn_dsIsUpdated(ds_accObjectMealUprice)
			&&!gfn_dsIsUpdated(ds_accObjectUpjang)&&!gfn_dsIsUpdated(ds_detailProgressStatus)) {
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	
 	// 필수값 체크
 	if(ds_accObjectCard.rowcount > 0) {
		
		if((div1.visible)&&(gfn_isNull(ds_accObjectCard.getColumn(0, "contractEndDate")))) {
			ds_accObjectCard.setColumn(0, "contractEndDate", div1.cal_contractExpire.mae_cal.value);
		}else if((div3.visible)&&(gfn_isNull(ds_accObjectCard.getColumn(0, "contractEndDate")))) {
			ds_accObjectCard.setColumn(0, "contractEndDate", div3.cal_contractExpire.mae_cal.value);
		}
		
// 		if((div1.visible && gfn_isNull(div1.cbo_prcgDept.text))
// 			|| (div2.visible && gfn_isNull(div2.cbo_prcgDept.text))
//  			|| (div3.visible && gfn_isNull(div3.cbo_prcgDept.text)) ) {
//  			gfn_alert("담당부서가 누락되었습니다.");
//  			return;
//  		}
 		
//  		if((div1.visible && gfn_isNull(div1.cbo_prcgEmp.text))
// 			|| (div2.visible && gfn_isNull(div2.cbo_prcgEmp.text))
//  			|| (div3.visible && gfn_isNull(div3.cbo_prcgEmp.text)) ) {
//  			gfn_alert("담당 마케터가 누락되었습니다.");
//  			return;
//  		}
		
		if(gfn_isNull(ds_accObjectCard.getColumn(0, "objectNm"))) {
			gfn_alert("물건명이 누락되었습니다.");
			return;
		}else if(gfn_isNull(ds_accObjectCard.getColumn(0, "cardType"))) {
			gfn_alert("가망/물건 구분이 누락되었습니다.");
			return;
		}else if(gfn_isNull(ds_accObjectCard.getColumn(0, "contractEndDate")) && (!div2.visible)) {
			gfn_alert("계약만료 일자가 누락되었습니다.");
			return;
		}else if(!gfn_isDate8(ds_accObjectCard.getColumn(0, "contractEndDate")) && (!div2.visible)) {	
			gfn_alert("계약만료 일자 형식이 잘못되었습니다.");
			return;
		}else if(gfn_isNull(ds_accObjectCard.getColumn(0, "loc"))) {
			gfn_alert("지역정보가 누락되었습니다.");
			return;
		}else if(gfn_isNull(ds_accObjectCard.getColumn(0, "prcgDept"))) {
			gfn_alert("담당부서가 누락되었습니다.");
			return;
		}else if(gfn_isNull(ds_accObjectCard.getColumn(0, "prcgEmp"))) {
			gfn_alert("담당 마케터가 누락되었습니다.");
			return;
		}else if(gfn_isNull(ds_accObjectCard.getColumn(0, "prcgDeptNm"))) {
			gfn_alert("담당부서명이 누락되었습니다.");
			return;
		}else if(gfn_isNull(ds_accObjectCard.getColumn(0, "prcgEmpNm"))) {
			gfn_alert("담당 마케터명이 누락되었습니다.");
			return;
		}else if(!gfn_isNull(ds_accObjectCard.getColumn(0, "accMon")) && !gfn_isDate6(ds_accObjectCard.getColumn(0, "accMon"))) {
			gfn_alert("수주 월 형식이 잘못되었습니다.");
			return;
		}
		
		// 업장 빈칸일때 저장되는것 방지
		for(var i=0; i < ds_accObjectUpjang.rowcount; i++){
			if(gfn_isNull(ds_accObjectUpjang.getColumn(i, "upjangCd"))){
				gfn_alert("업장이 누락 되었습니다.");
				return;
			}
		}
		
		// 식단가 미입력한 값이 있을때 저장 되는것 방지(급식만 해당)
		for(var i = 0; i< ds_accObjectMealUprice.rowcount; i++){
			if(gfn_isNull(ds_accObjectMealUprice.getColumn(i, "mealUpriceA")) 
				&& gfn_isNull(ds_accObjectMealUprice.getColumn(i, "mealUpriceB")) 
				&& gfn_isNull(ds_accObjectMealUprice.getColumn(i, "mealUpriceC"))
				&& gfn_isNull(ds_accObjectMealUprice.getColumn(i, "mealUpriceD"))
				&& gfn_isNull(ds_accObjectMealUprice.getColumn(i, "etcAmt")))
			{
				gfn_alert(i + 1 +"행에 식단가가 누락 되었습니다.");
				ds_accObjectMealUprice.rowposition = i;
				return;
			}
		}
		
 		// 첨부 파일 추가가 있을때만
 		
// 		trace(" !gfn_dsIsUpdated(ds_accObjectCard) >>>>>>>>>>>>>>>>>>>>>>>>>>> "+!gfn_dsIsUpdated(ds_accObjectCard)+
//			" /////  ObjAtchFile.fn_countDs(insert) >>>>>>>>>>>>>>>>>>>>>> "+ObjAtchFile.fn_countDs("insert"));
 		
		if(!gfn_dsIsUpdated(ds_accObjectCard) && (ObjAtchFile.fn_countDs("insert") > 0)) {	
			
//			trace("111111111111111111111111111111111111111111111111");
			
			var rtn = ObjAtchFile.fn_uploadFile();
			//var rtn = fn_uploadFile();
			
//			trace("@@@@@@@@@@@@@@@@@@@ rtn @@@@@@@@@@ "+rtn);
			if (rtn < 0)	return;
			
			// 상태를 update 로 변경
			ds_accObjectCard.updatecontrol = false;
			ds_accObjectCard.setRowType(0, Dataset.ROWTYPE_UPDATE);
		//	trace(" ds_accObjectCard . getRowType() ====>>> "+ds_accObjectCard.getRowType(0));
			ds_accObjectCard.updatecontrol = true;
			
		}else if(gfn_dsIsUpdated(ds_accObjectCard) && ObjAtchFile.fn_countDs("insert") > 0) {
			
//			trace("222222222222222222222222222222222222222222222222");
			
			var rtn = ObjAtchFile.fn_uploadFile();	
			//var rtn = fn_uploadFile();		
					
			if (rtn < 0)	return;
		}
 	}
 	
	// 상세 진행상태 update
	if(gfn_dsIsUpdated(ds_detailProgressStatus)){
		for(var i=0; i<ds_detailProgressStatus.rowcount; i++) {
			
			if(i==0) {
				ds_accObjectCard.setColumn(0, "mgmtcardRegYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "mgmtcardProgressDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==1) {
				ds_accObjectCard.setColumn(0, "spotExplPacnYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "spotExplPacnDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==2) {
				ds_accObjectCard.setColumn(0, "bidAndEstPreparationYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "bidAndEstPreparationDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==3) {
				ds_accObjectCard.setColumn(0, "reptWriteYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "reptWriteDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==4) {
				ds_accObjectCard.setColumn(0, "ptYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "ptDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==5) {
				ds_accObjectCard.setColumn(0, "accOrderconsiderYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "accOrderconsiderDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==6) {
				ds_accObjectCard.setColumn(0, "bidYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "bidDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==7) {
				ds_accObjectCard.setColumn(0, "contractReviewYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "contractReviewDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==8) {
				ds_accObjectCard.setColumn(0, "opproposalWriteYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "opproposalProgressDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}else if(i==9) {
				ds_accObjectCard.setColumn(0, "contractYn", ds_detailProgressStatus.getColumn(i, "status"));
				ds_accObjectCard.setColumn(0, "contractDate", ds_detailProgressStatus.getColumn(i, "progressDate"));
			}
		}
	} 
	
	//ds_accObjectCard.setColumn(ds_accObjectCard.rowposition,"loc1",ds_locCd2.getColumn())
	
	var strSvcID    = "save";
    var strURL      = "fs/fso/objectMngtCard/saveObjectMngtCard.xdo";
    var strInDs     = "ds_accObjectCard=ds_accObjectCard:A ds_file=ds_file:A "
                    + "ds_accObjectMealUprice=ds_accObjectMealUprice:U ds_accObjectUpjang=ds_accObjectUpjang:U";
    var strOutDs    = "ds_file=ds_file";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

