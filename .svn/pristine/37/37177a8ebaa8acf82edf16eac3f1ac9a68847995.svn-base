function fn_search()
{
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);
    grd_list.setCellProperty("body", 0, "background", "");		
	grd_list.setCellProperty("body", 0, "background2", "");
    
    if ( !gfn_isNull(fv_deliverNum) ) {
		var cond = fv_deliverNum.replace("[","").replace("]","");
		ds_search.setColumn(0, "deliverNum", cond);
    }
    //코드목록을 초기화한다.
    ds_list.clearData();

    var strSvcID    = "select";
    var strURL      = "ft/fts/saleOrder/selectSalesOrderRegList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		//품의 조회
        case "select" :
			fv_deliverNum = "";
			ds_search.setColumn(0, "deliverNum", "");
            break;
        
        // 저장
        case "save" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				fv_flag = "UPDATE";
				fn_search();
			}
            break; 
    }
}

