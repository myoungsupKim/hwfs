/*------------------------------------------------------------
	1. Function ID : fn_Save()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_save()
{
	var chkCnt = ds_list.findRow("chk", '1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if (chkCnt < 0) {
		gfn_alert("msg.noselect","체크박스");
		grd_list.setCellProperty("body", 1, "background", "lightpink");		
		grd_list.setCellProperty("body", 1, "background2", "lightpink");

		grd_list.setCellPos(1); // cellindex
		grd_list.setFocus();
		grd_list.showEditor(); 
		return;
	} else {
		grd_list.setCellProperty("body", 1, "background", "");		
		grd_list.setCellProperty("body", 1, "background2", "");
	}

	ds_saleOrder.clearData();
	ds_saleOrderItemMgmtList.clearData();
	
	for ( var i = 0; i < ds_list.getRowCount(); i++ )
	{
		if ( (ds_list.getColumn(i, "chk") == "1" ) ) {
			var nRow = ds_saleOrder.addRow();
			var insRow = ds_saleOrderItemMgmtList.addRow();
			if ( gfn_isNull(ds_list.getColumn(i, "saleOrderNum")) )
			{
				ds_saleOrder.setColumn(nRow, "saleOrderNum", ds_list.getColumn(i, "saleOrderNum"));
				ds_saleOrderItemMgmtList.setColumn(insRow, "saleOrderNum", ds_list.getColumn(i, "saleOrderNum"));
			}
			
			ds_saleOrder.setColumn(nRow, "deliverNum", ds_list.getColumn(i, "deliverNum"));	// 저장 후 재조회되하기 위한 key
			
			ds_saleOrder.setColumn(nRow, "saleAged", ds_list.getColumn(i, "saleAged"));
			ds_saleOrder.setColumn(nRow, "salePrcgId", ds_list.getColumn(i, "salePrcgId"));
			ds_saleOrder.setColumn(nRow, "orderDate", ds_list.getColumn(i, "orderDate"));	// 주문일자
			ds_saleOrder.setColumn(nRow, "releaseDate", ds_list.getColumn(i, "releaseDate"));	// 출고요청일자
			ds_saleOrder.setColumn(nRow, "attachDept", ds_list.getColumn(i, "attachDept"));
			ds_saleOrder.setColumn(nRow, "saleOrderType", ds_list.getColumn(i, "saleOrderType"));
			
			if ( ds_list.getColumn(i, "attachDept") != "40" ) {	// 가공식품팀
				if ( ds_list.getColumn(i, "salsplceCd") == "999999")
				{
					gfn_alert("기타거래처입니다. 실제 거래처로 변경하세요");
					return;
				}
			}
			
			ds_saleOrder.setColumn(nRow, "releaseplceCd", ds_list.getColumn(i, "salsplceCd"));			
			ds_saleOrder.setColumn(nRow, "releaseplceNm", ds_list.getColumn(i, "salsplceNm"));			

			ds_saleOrderItemMgmtList.setColumn(insRow, "deliverNum", ds_list.getColumn(i, "deliverNum"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "poOrderNum", ds_list.getColumn(i, "poOrderNum"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "itemCd", ds_list.getColumn(i, "itemCd"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "itemSeq", ds_list.getColumn(i, "itemSeq"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "itemNm", ds_list.getColumn(i, "itemNm"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "unit", ds_list.getColumn(i, "unit"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "saleOrderType", ds_list.getColumn(i, "saleOrderType"));

			ds_saleOrderItemMgmtList.setColumn(insRow, "deliverQuantity", ds_list.getColumn(i, "deliverQuantity"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "deliverWgt", ds_list.getColumn(i, "deliverWgt"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "rmaQuantity", ds_list.getColumn(i, "rmaQuantity"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "whMoveQuantity", ds_list.getColumn(i, "whMoveQuantity"));

			ds_saleOrderItemMgmtList.setColumn(insRow, "orderQuantity", ds_list.getColumn(i, "orderRemainQuantity"));
			ds_saleOrderItemMgmtList.setColumn(insRow, "orderWgt", ds_list.getColumn(i, "orderRemainWgt"));

			ds_saleOrderItemMgmtList.setColumn(insRow, "entryNum", ds_list.getColumn(i, "entryNum"));			
			ds_saleOrderItemMgmtList.setColumn(insRow, "entryItemSeq", ds_list.getColumn(i, "entryItemSeq"));			
			ds_saleOrderItemMgmtList.setColumn(insRow, "proposalSeq", ds_list.getColumn(i, "proposalSeq"));	// 주문일자
			//alert(ds_list.getColumn(i, "attachDeptSap")+"//"+ds_list.getColumn(i, "chgAttachDeptSap"));
			
			
			ds_saleOrderItemMgmtList.setColumn(insRow, "attachDeptSap", ds_list.getColumn(i, "attachDeptSap"));	// cc코드
			ds_saleOrderItemMgmtList.setColumn(insRow, "chgAttachDeptSap", ds_list.getColumn(i, "chgAttachDeptSap"));	// 부서이관시 cc코드
			
			if ( ds_list.getColumn(i, "saleOrderType") == "50")	  //부서이관일 경우
			{
				//alert(ds_list.getColumn(i, "attachDeptSap") +"//"+ ds_list.getColumn(i, "chgAttachDeptSap"));
				if ( ds_list.getColumn(i, "attachDeptSap") == ds_list.getColumn(i, "chgAttachDeptSap"))	  //CC코드가 같을시
				{
					gfn_alert("귀속부서가 같습니다. 매출처를 다른 귀속부서로 변경해주세요");
					ds_list.rowposition = i;
					grd_list.setCellPos(grd_list.getBindCellIndex("Body", "salsplceNm"));
					grd_list.setFocus();
					grd_list.showEditor(); 
					return;
				}
			}
			
		}
	}
	
	if ( !gfn_dsIsUpdated(ds_saleOrder) && !gfn_dsIsUpdated(ds_saleOrderItemMgmtList) )
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	
	
    // 저장시 Validation Check
	if (gfn_validCheck(ds_saleOrder, ds_saleOrderValid) == false)	return;
	if (gfn_validCheck(ds_saleOrderItemMgmtList, ds_saleOrderItemMgmtListValid) == false)	return;
	
	fv_deliverNum = "";
	ds_search.setColumn(0, "flag", fv_flag);

 	// 자료를 저장 하시겠습니까?
	if (!gfn_confirm("confirm.before.save")) return false;

    var strSvcID    = "save";
    var strURL      = "ft/fts/saleOrder/saveSaleOrderDtl.xdo";
    var strInDs     = "ds_search=ds_search ds_saleOrder=ds_saleOrder:A ds_saleOrderItemMgmtList=ds_saleOrderItemMgmtList:A";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

