function fn_setCombo(){
	SCCombo.bind(this, "ComCode", "cbo_custCd", null, {groupCode:"PART_TIME_CUST_CD", useYn:"Y"});	// 입력:소속협력업체
	fn_setDutyAmt(this, "cbo_partTimeSalaryCd", "ds_partTimeSalaryCd");	//입력:시급
}

