	}
}

function cbo_partTimeSalaryCd_oncloseup(obj:Combo, e:ComboCloseUpEventInfo)
{
	if(cbo_partTimeSalaryCd.index > -1){
		ds_list.setColumn(ds_list.rowposition, "partTimeSalaryCdNm", ds_partTimeSalaryCd.getColumn(cbo_partTimeSalaryCd.index, "partTimeSalaryCdNm"));
	}
}
