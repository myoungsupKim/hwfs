// 쉬트 헤더 클릭이벤트
function gd_list_OnHeadClick(obj:Grid, e:GridClickEventInfo)
{	
	if(e.cell == 0)
	{
		gfn_exportExcel(obj, this.titletext);	
	}
	
	if(obj.getCellProperty("Head" ,e.cell ,"edittype") == "checkbox")
	{
		
		// 체크가 'Y"일때 입력가능 아니며 입력불가.
		
		if(obj.getCellProperty("Head" ,e.cell ,"text") == "1")
		{	
			gd_list.setCellProperty("body", e.cell, "edittype", "none");
			obj.setCellProperty("Head" ,e.cell ,"text", "0");
			
			for(var i=0; i < ds_List.getRowCount(); i++){
 				ds_List.setColumn(i, e.cell+5, 0);
 			}//for        
		}
		else if(obj.getCellProperty("Head" ,e.cell ,"text") == "0") 
		{
			gd_list.setCellProperty("body", e.cell, "edittype", "masknumber");
			obj.setCellProperty("Head" ,e.cell ,"text", "1");			
 			
 			for(var i=0; i < ds_List.getRowCount(); i++){
 				ds_List.setColumn(i, e.cell+5, 0);
 			}//for        
		}
		
	}//end 
	
	//trace(obj.getCurFormatString());
	//trace(ds_List.saveXML());
}

