// 조회된 결과로 컬럼과 데이터셋을 생성한다.
function fn_CreateData()
{
	//ds_List.clear();
	
	// 조회년월의 마지막 일수만큼 컬럼 생성.
	//var dd = ds_List.getColumn(0, "LAST_DD_CNT");
	var strDd = yy.value+"01";
