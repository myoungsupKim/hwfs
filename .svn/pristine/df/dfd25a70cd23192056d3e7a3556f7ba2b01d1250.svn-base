	var cntDy = parseInt(lstDy)+10; // DD_AMT 앞에 가져오는 컬럼갯수
	var strNbn = 0;

	// 합계값을 1/N, 입력값은 합계에 반영한다.
	for(var i=0; i<ds_List.getRowCount(); i++){
		//for(var j=0; j<ds_List.colcount; j++){	
		
