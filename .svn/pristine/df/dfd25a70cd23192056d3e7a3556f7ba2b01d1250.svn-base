	var ym = yy.value;
	
	//재 조회시 공백으로 생성되는 컬럼삭제.
	for (var i=gd_list.getFormatColCount()-1; i>0; i--) {
		gd_list.deleteContentsCol(i);
	}
	
	//그리드 컬럼 생성 //"expr:Math.round(fn_clearNaN(getAvg('GOAL_RATE"+strDt+"')),1)"
	var nStartCell = 6;	
	var j = 3;
	for (var i=1; i<=dd; i++) {
		gd_list.appendContentsCol();
		
		gd_list.setCellProperty("Head", nStartCell, "displaytype", "checkbox"); // checkBox
		gd_list.setCellProperty("Head", nStartCell, "edittype", "checkbox"); // checkBox
		gd_list.setCellProperty("Head", nStartCell, "text", "1"); // checkBox
		
		if(i <= 9){
			//gd_list.setCellProperty("head", nStartCell, "text", gfn_subStr(ym, 0, 4)+"-"+gfn_subStr(ym, 4, 2)+"-0"+i); 			//헤드 text
			//gd_list.setCellProperty("body", nStartCell, "expr", "Math.round(DD_0"+i+")"); 		//값 바인딩
			gd_list.setCellProperty("Body", nStartCell, "text", "bind:DD_0"+i); 		//값 바인딩
			gd_list.setCellProperty("Summ", j, "text", "expr:Math.round((fn_sum('DD_0"+i+"')))"  );
		}else if(i > 9 && i <= dd){
			//gd_list.setCellProperty("head", nStartCell, "text", gfn_subStr(ym, 0, 4)+"-"+gfn_subStr(ym, 4, 2)+"-"+i); 				//헤드 text
			//gd_list.setCellProperty("body", nStartCell, "expr", "Math.round(DD_"+i+")"); 		//값 바인딩
			gd_list.setCellProperty("Body", nStartCell, "text", "bind:DD_"+i); 		//값 바인딩
			gd_list.setCellProperty("Summ", j, "text", "expr:Math.round((fn_sum('DD_"+i+"')))"  );
