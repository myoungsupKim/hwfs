	    if (-1 == -1){
	    
			ds_listInput.setColumn(ds_listInput.rowposition, "SUB_R_CMP_TYPE", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "SUB_R_CMP_TYPENM", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "SUB_R_CMP_ACCTCD", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "SUB_R_CMP_ACCTCDNM", "");			
			ds_listInput.setColumn(ds_listInput.rowposition, "SUB_R_CMP_AUTO_YN", "");	
		}
				
		for (i=0;i<ds_cbo_SUB_R_CMP_CLASS.rowcount;i++){
		//alert("d");
			trace( gfn_subStr(e.newvalue, i, 1) );
			if (gfn_subStr(e.newvalue, i, 1)== "A"){
				edt_SUB_R_CMP_ACCTCD.enable    = true;
				edtBtn_SUB_R_CMP_ACCTCD.enable = true;
				
			}
			else if (gfn_subStr(e.newvalue, i, 1)== "T"){
				edt_SUB_R_CMP_TYPE.enable         = true;
				edtBtn_SUB_R_CMP_TYPE.enable       = true;
			}
		}
	
		
	}
		
	if (e.columnid == "BIZ_CHK_YN"){
		
	
		if (-1 == -1){
		
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_REG_YN", "");
		
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_ACCTCD", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_ACCTCDNM", "");
        }
        			
		if (e.newvalue == "T"){
		
			cbo_FEE_REG_YN.enable      = false;
		}
		else if (e.newvalue == "R"){
		
			cbo_FEE_REG_YN.enable      = true;
		}
		else {
	
			cbo_FEE_REG_YN.enable      = false;
		}		
	}	
	
	if (e.columnid == "FEE_REG_YN"){
	
		if (-1 == -1){
		
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_ACCTCD", "");
			ds_listInput.setColumn(ds_listInput.rowposition, "FEE_ACCTCDNM", "");
		}
	
		if (e.newvalue == "Y"){
	
			edt_FEE_ACCTCD.enable        = true;
			edtBtn_FEE_ACCTCD.enable     = true;
		}
		else {
			edt_FEE_ACCTCD.enable        = false;
			edtBtn_FEE_ACCTCD.enable     = false;
		}		
	}		


}

