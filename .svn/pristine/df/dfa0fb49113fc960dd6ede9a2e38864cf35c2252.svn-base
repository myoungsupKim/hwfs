function WorkForm_OnUnloadCompleted(obj:Form, e:CloseEventInfo)
{
	if (gfn_dsIsUpdated(ds_listInput))
	{
	
		if (g_Confirm("02", "저장하지 않은 데이터가 있습니다.\n" +
	                        "저장하시겠습니까?"))
		{
			http.Sync = true;  
			if (!before_SaveCheck("Master") ){
				
				return false;
			}
			http.Sync = false;
		}
	}
}

function edt_DR_ACCTCD_OnKeyDown(obj:Edit, e:KeyEventInfo)
{
	fn_edt_OnKeyDown(obj,e.keycode);

	if (e.keycode == 46)
	ds_listInput.setColumn(ds_listInput.rowposition, gfn_subStr(obj.name, 4), "");	
}

function edtBtn_DR_ACCTCD_OnClick(obj:Button, e:ClickEventInfo)
{
	fn_edtBtn_OnClick(obj);

    if (gfn_length(eval("edt_" + gfn_subStr(obj.name, 7)).userdata)!= 0)
	ds_listInput.setColumn(ds_listInput.rowposition, gfn_subStr(obj.name, 7), eval("edt_" + gfn_subStr(obj.name, 7)).userdata);	
}




//유형키 중복체크##############################################################################
function fn_GetDuplicateKey(objOutDs, str_TYPE_CD)
{
	
		objOutDs.clear();
		objOutDs.addColumn("TYPE_CD","STRING",256);
		objOutDs.addColumn("CNT","INT",256);
		
		objOutDs.addRow();
		
		objOutDs.setColumn(0, "TYPE_CD", str_TYPE_CD);
		
		// action 정보 초기화
		fsp_clear(this);

		// 서버에서 조회할 정보 추가
		fsp_addSearch(this, "ma/mae:MAE10020E_S002");

		// 서버 호출
		http.Sync = true;
		fsp_callService(this, "", "", "ds_cond=" + objOutDs.name, objOutDs.name + "=ds_list");
		http.Sync = false;	
		
		return objOutDs.getColumn(0, "CNT");
	
}
//유형키 중복체크##############################################################################

