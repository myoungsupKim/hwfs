	var nRow= ds_search.addRow();
	ds_search.setColumn(nRow, "upjang", "100751");	//TEST
	ds_search.setColumn(nRow, "useYn", "Y");
	
	fn_getHelperGrade();
	
	// 월력 초기화(해당Form, 값지정, 값변경시 실행할 callBack함수, dataset과 bind)
	cal_sOccurDate.fn_initMonthCal(this, gfn_getYear()+gfn_getMonth(), "", ds_search, "sOccurDate");
	cal_eOccurDate.fn_initMonthCal(this, gfn_getYear()+gfn_getMonth(), "", ds_search, "eOccurDate");
}

