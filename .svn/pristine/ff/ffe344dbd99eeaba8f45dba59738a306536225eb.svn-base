 *---------------------------------------------------------------------------------------------*/
function fn_setEnable(obj:Grid){
	var idx = obj.currentrow;
	var rowType = ds_list.getRowType(idx);
	
	if(rowType == 1 || rowType == 4){
		cbo_mucd.enable = false;
		edt_upjang.enable = false;
		edt_upjangNm.enable = false;
		edt_empno.enable = false;
		edt_empnm.enable = false;
		cal_occurDate.enable = false;
		edt_supUpjang.enable = false;
	}else{
		cbo_mucd.enable = true;
		edt_upjang.enable = true;
		edt_upjangNm.enable = true;
		edt_empno.enable = true;
		edt_empnm.enable = true;
		cal_occurDate.enable = true;
		edt_supUpjang.enable = true;
	}
}

