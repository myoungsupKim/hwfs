/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_search()
 * 기ㅡㅡㅡ능 : 조회
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-03-25  
 ******************************************************************************************/
function fn_search() 
{
	fsp_clear(this);
	fsp_addSearch(this,"fs/fsa:FSA00031E_S001");
	fsp_addSearch(this,"fs/fsa:FSA00031E_S002");
	fsp_callService(this,"","","ds_cond=ds_cond","ds_jobListView=ds_jobListView ds_jobList=ds_jobList","","fn_callBackSearch");
}

function fn_callBackSearch(errCode, errMsg) 
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오!");
		return;
	}
	
	grd_jobList.setFocus();	
}

/******************************************************************************************
 * 함  수  명   : fn_save(obj , e)
 * 기      능   : 저장
 * 작  성  자   : 김민수
 * 작성  일자   : 2015-03-25
 ******************************************************************************************/
function fn_save() 
{
	fsp_clear(this);
    fsp_addSave(this, "fs/fsa:FSA00031E_I001", "", "fs/fsa:FSA00031E_D001", "", "", "", "");
	fsp_callService(this, "", "", "ds_jobList=ds_jobList:U", "" , "", "fn_callBackSave");
}

function fn_callBackSave(errCode, errMsg) 
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오!");
		return;
	}
	else
	{
		g_Message("02", "저장 되었습니다.");
	}
}

