/**
 * @class 업장명의 Key Down Event
 * @param obj - Event Dataset
 * @param e - Event 발생정보
 * @return 
 */
function edt_upjangNm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	// Enter 키
	if(e.keycode == 13) 
	{		
        if (gfn_isNull(obj.upjang))
        {
			edt_upjangNm.updateToDataset();
			btn_search.click();
		}
