{
	// Argument Dataset으로 받기
 	try {
 		//trace("P_DATASET Dataset :\n" + P_DATASET.saveXML());
		ds_search.setColumn(0, "schCenter", P_DATASET.getColumn(0, "schCenter")); 		
		var v_attr01 = P_DATASET.getColumn(0, "attr01");
		ds_search.setColumn(0, "attr01", (gfn_isNull(v_attr01) ? '' : v_attr01));
 	} catch(err) {
 	}
 	
 	//trace(gds_userInfo.saveXML());
 	
 	//IntroFrameType05.xfdl --> CS사원
 	//IntroFrameType06.xfdl --> 영업사원
 	//trace(gds_userInfo.getColumn(0,"introPage"));	
	
	if(gds_userInfo.getColumn(0,"introPage") == "IntroFrameType06.xfdl")
	{
		ds_search.setColumn(0,"partSalesSabun",gds_userInfo.getColumn(0,"sabun"));
	}
 	
 	//trace(ds_search.saveXML());
}

