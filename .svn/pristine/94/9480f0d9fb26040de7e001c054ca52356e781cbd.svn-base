function before_SaveCheck(str_Gubun)
{

        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
        
        f_Saved_ID = 0;
        f_NewDataCheck = false;
		
        /*
        업무구분별로 addAction 지정세팅##########################################        
        */

		
 		if (ds_listInput.rowcount == 0){

			return;
		}

	    if (str_Gubun == "Master"){

			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "MGMT_SABUN"))== 0){    
		
				g_Message("EE", "담당마케터를 선택하여 주십시요.\n" +
								"확인하여 주십시요.");        
				edt_MGMT_SABUNQuery.setFocus();
				return false;
			}	
			
			
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "CUST_ID"))== "0"){    
		
				g_Message("EE", "대상고객을 선택하여 주십시요.\n" +
								"확인하여 주십시요.");        
				edt_CUST_IDQuery.setFocus();
				return false;
			}
			
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_DATE"))== 0){    
		
				g_Message("EE", "방문일자가 선택되어 있지 않습니다.\n" +
								"확인하여 주십시요."); 						       
				me_VISIT_DATE.setFocus();
				return false;
			}    
		
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_GUBUN"))== 0){    
		
				g_Message("EE", "방문구분이 선택되어 있지 않습니다.\n" +
								"확인하여 주십시요."); 						       
				edt_VISIT_GUBUN.setFocus();
				return false;
			}   
			
		
				        
	        //입력일때 Confirm체크##################################
	        if ( 
	             (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))== "0")
	           )
	        {
				if (!g_Confirm("02")) return false;	             
	        }     
	        //입력일때 Confirm체크##################################
	             
	             
	        //수정일때 Confirm체크##################################
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))!= "0")
               )
            {	        
				if (!g_Confirm("02", "데이터를 수정하시겠습니까?")) return false;
			}
	        //수정일때 Confirm체크##################################
	        
	        
	        
			//각 구분별 대체 마감체크##############################
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))!= "0")
               )
            {			
				if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))) return false;
			}
			//각 구분별 대체 마감체크##############################	        
					        
			
			
				
						
			//MST_ID 지정#################################################################
   		    if(!fn_SaveTrxData(ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))) return false;
			
		}			

        
		//저장#############################################################
	    if (str_Gubun == "Master"){

			
			// action 정보 초기화
			fsp_clear(this);	  		
										    		        
	    
            //기존 라인 및 매출 데이터 삭제.            
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(f_Saved_ID)!= "0")
               )
            {
  
				fsp_addSingle(this, "ma/mak:MAK10021E_U001");			
            }
            else 
            {
            
			    fsp_addSingle(this, "ma/mak:MAK10021E_I001");			     
            }  	            			
			
			f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID");

			//바이너리 통신 설정
			//http.Compress = true;
			//http.CompressMethod = "ZLIB";
			//바이너리 통신 설정	
			fsp_callService(this, "", "", "ds_cond=ds_listInput:A", "",  "", "fn_AfterSaveSearch");
	    }


		return true;
}


