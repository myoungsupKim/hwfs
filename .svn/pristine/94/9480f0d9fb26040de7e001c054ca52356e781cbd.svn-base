		return false;
	}
	
	//각 구분별 대체 마감체크##############################
    if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))) return false;	
	//각 구분별 대체 마감체크##############################	        
					
	//차량운영일지체크#####################################################################
	if (!fn_GetALT_DRIVING_MGMT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))) return false;	
	//차량운영일지체크#####################################################################					       		
	
	// action 정보 초기화
	fsp_clear(this);			
	
	//기존 라인 및 매출 데이터 삭제.
	fsp_addSingle(this, "ma/mak:MAK10021E_D001");
		   
	http.Sync = true;
	fsp_callService(this, "", "", "ds_cond=ds_listInput:A",  "", "", "fn_AfterDeleteSearch");		
	http.Sync = false;		
	
	return true;
}


