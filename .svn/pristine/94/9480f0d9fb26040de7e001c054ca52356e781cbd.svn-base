	//동일고객의 기존 데이터가 존재할경우 Default Setting########################################
	fn_GetBeforeCustInfo(ds_cond, "B", edt_MGMT_SABUNQuery.userdata, edt_CUST_IDQuery.userdata);
	//동일고객의 기존 데이터가 존재할경우 Default Setting########################################
		 
	me_VISIT_DATE.setFocus();
