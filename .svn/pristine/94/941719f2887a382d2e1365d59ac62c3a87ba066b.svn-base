/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	if (gfn_isNull(cal_startTerm.fn_getValue()) || gfn_isNull(cal_endTerm.fn_getValue()))
	{
		gfn_alert("조회일자를 입력하여 주십시오!");
		return;
	}
	if (cal_startTerm.fn_getValue() > cal_endTerm.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_poOrderList);
    
    //코드목록을 초기화한다.
    ds_poOrderList.clearData();

    var strSvcID    = "selectPoOrderMstList";
    var strURL      = "ft/ftp/poOrderMst/selectPoOrderMstList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_poOrderList=ds_poOrderList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		//품의 조회
        case "selectPoOrderMstList" :
            break;
        
        // 저장
        case "save" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				//alert("품의서 번호["+fv_poProposalNum+"]가 생성되었습니다.");
				fn_search();
			}
            break;
        case "delete" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.delete.success");	// 삭제되었습니다
				//alert("품의서 번호["+fv_poProposalNum+"]가 생성되었습니다.");
				fn_search();
			}
            break;
    }
}

