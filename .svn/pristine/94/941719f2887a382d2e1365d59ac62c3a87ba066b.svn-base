	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_delete()
{
	if (ds_poOrderList.rowposition == -1) return;

	if ( ds_poOrderList.getColumn(ds_poOrderList.rowposition, "poOrderStatus") ==  "C") {
		gfn_alert("ftp.err.validator.poorder.delete");	//구매주문상태가 확정 상태는 삭제할 수 없습니다.
		return;
	}

	//2019.01.28 김명섭 PO SAP전표 체크 삭제여부 추가
	if ( ds_poOrderList.getColumn(ds_poOrderList.rowposition, "delpyn") !=  "Y") {
		gfn_alert(ds_poOrderList.getColumn(ds_poOrderList.rowposition, "delpyn"));
		return;
	}

	var poOrderNum = gfn_nvl(ds_poOrderList.getColumn(ds_poOrderList.rowposition, "poOrderNum"));
	var rtVal = gfn_confirm("P/O No["+poOrderNum+"]에 대하여 삭제 하시겠습니까?");	
	if ( !rtVal == true) return;
	
	ds_poOrderList.setColumn(ds_poOrderList.rowposition, "delYn", "Y" );
	
    var strSvcID    = "delete";
    var strURL      = "ft/ftp/poOrderMst/deletePoOrderMst.xdo";
    var strInDs     = "ds_search=ds_search ds_poOrderList=ds_poOrderList:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}

//=========================================================================================
// 확정 버튼 클릭시
//=========================================================================================
function fn_confirm(obj:Button,  e:ClickEventInfo)
{
	if ( ds_poOrderList.rowcount <= 0 ) return;
	
	//서영미 : 품의 결재 안나도 구매주문 확정 가능하도록(매입 대금 지불전표 미리 발행할수 있게) 수정 바랍니다.
	if ( ds_poOrderList.getColumn(ds_poOrderList.rowposition, "signProgressStatus") !=  "COMPLETE" ) {
		gfn_alert("ftp.err.validator.poorder.confirm");	//품의 승인상태에서 확정 가능합니다.
		return;
	}
	
	if ( ds_poOrderList.getColumn(ds_poOrderList.rowposition, "poOrderStatus") ==  "C" ) {	 //확정상태는 취소 후가능
		gfn_alert("ftp.err.validator.poorder.confirm2");	//확정상태입니다 취소 후 확정 가능합니다.
		return;
	}
	

	var poOrderNum = gfn_nvl(ds_poOrderList.getColumn(ds_poOrderList.rowposition, "poOrderNum"));
	var rtVal = gfn_confirm("ftp.confirm.validator.poorder.confirm", poOrderNum);	//P/O No["+poOrderNum+"]에 대하여 확정 하시겠습니까?
	if ( !rtVal == true) return;
	
	ds_poOrderList.setColumn(ds_poOrderList.rowposition, "poOrderStatus", "C" );
	
    var strSvcID    = "save";
    var strURL      = "ft/ftp/poOrderMst/updatePoOrderStatus.xdo";
    var strInDs     = "ds_search=ds_search ds_poOrderList=ds_poOrderList:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

