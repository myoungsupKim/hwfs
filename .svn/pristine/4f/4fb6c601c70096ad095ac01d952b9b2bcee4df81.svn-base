	var rtn = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);

	if (rtn == "")
	{
		return;
	}
	
	ds_list.setColumn(ds_list.rowposition, "chgReason", rtn);

    var strSvcID    = "save";
    var strURL      = "sc/scz/custMgmt/saveList.xdo";
    var strInDs     = "ds_list=ds_list:U ds_inchargeList=ds_inchargeList:U";
    var strOutDs    = "";
    var strArg      = "maskAuth="+SCAuth.getMaskAuth(this);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

