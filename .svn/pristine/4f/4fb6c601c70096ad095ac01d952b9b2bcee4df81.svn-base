/**
 * @class 거래처 구분(검색)이 변경될 때마다 거래처 유형을 다시 설정한다.
 * @param val - 거래처 구분 코드
 * @return 
 */
function fn_bindCustType(val)
{
	switch (val)
	{
		case "1000" : //일반(국내)
			SCCombo.bind(this, "ComCode", "cbo_s_custtype", null, {groupCode:"Z01", useYn:"Y", set1:"Y"}, SCCombo.ALL);
			break;
		case "5000" : //일반(해외)
			SCCombo.bind(this, "ComCode", "cbo_s_custtype", null, {groupCode:"Z01", useYn:"Y", set2:"Y"}, SCCombo.ALL);
			break;
		case "6000" : //개인
			SCCombo.bind(this, "ComCode", "cbo_s_custtype", null, {groupCode:"Z01", useYn:"Y", set3:"Y"}, SCCombo.ALL);
			break;
		default : //전체
			SCCombo.bind(this, "ComCode", "cbo_s_custtype", null, {groupCode:"Z01", useYn:"Y"}, SCCombo.ALL);
			break;
	}
}

/**
 * @class 담당자 그리드의 Resize시 autofittype을 제어한다.
 * @param obj - Event Object
 * @param e - Event 발생정보
 * @return 
 */
function fn_gridResize(obj, e)
{
	if (obj.position.width <= 1000)
	{
		obj.autofittype = "none";
	}
	else
	{
		obj.autofittype = "col";
	}
}

/**
 * @class 거래처 구분(검색)에 따라서 콤포넌트의 할성화 여부를 처리한다.
 * @param obj - Event Object
 * @param e - Event 발생정보
 * @return 
 */
function fn_conditionControl(val)
{
	switch (val)
	{
		case "001" : //매입처
			div_corp.edt_custAcctCd.readonly = true;
			div_corp.cbo_custPayCond.readonly = true;
			div_corp.edt_vendorAcctCd.readonly = false;
			div_corp.cbo_vendorPayCond.readonly = false;
			
			div_frgn.edt_custAcctCd.readonly = true;
			div_frgn.cbo_custPayCond.readonly = true;
			div_frgn.edt_vendorAcctCd.readonly = false;
			div_frgn.cbo_vendorPayCond.readonly = false;

			div_psnl.edt_custAcctCd.readonly = true;
			div_psnl.cbo_custPayCond.readonly = true;
			div_psnl.edt_vendorAcctCd.readonly = false;
			div_psnl.cbo_vendorPayCond.readonly = false;
			break;
		case "002" : //매출처
			div_corp.edt_custAcctCd.readonly = false;
			div_corp.cbo_custPayCond.readonly = false;
			div_corp.edt_vendorAcctCd.readonly = true;
			div_corp.cbo_vendorPayCond.readonly = true;
			
			div_frgn.edt_custAcctCd.readonly = false;
			div_frgn.cbo_custPayCond.readonly = false;
			div_frgn.edt_vendorAcctCd.readonly = true;
			div_frgn.cbo_vendorPayCond.readonly = true;

			div_psnl.edt_custAcctCd.readonly = false;
			div_psnl.cbo_custPayCond.readonly = false;
			div_psnl.edt_vendorAcctCd.readonly = true;
			div_psnl.cbo_vendorPayCond.readonly = true;
			break;
		case "003" : //매입.매출처
			div_corp.edt_custAcctCd.readonly = false;
			div_corp.cbo_custPayCond.readonly = false;
			div_corp.edt_vendorAcctCd.readonly = false;
			div_corp.cbo_vendorPayCond.readonly = false;
			
			div_frgn.edt_custAcctCd.readonly = false;
			div_frgn.cbo_custPayCond.readonly = false;
			div_frgn.edt_vendorAcctCd.readonly = false;
			div_frgn.cbo_vendorPayCond.readonly = false;

			div_psnl.edt_custAcctCd.readonly = false;
			div_psnl.cbo_custPayCond.readonly = false;
			div_psnl.edt_vendorAcctCd.readonly = false;
			div_psnl.cbo_vendorPayCond.readonly = false;
			break;
	}
}

