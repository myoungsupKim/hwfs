/**
 * @class 선택된 Row의 거래처 그룹의 값에 따라서 보여지는 Tab을 변경한다.
 * @param obj - Event Object
 * @param e - Event 발생정보
  * @return 
  */
 function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
 {
	div_corp.AddrSearchComponent00.visible = true;
	div_corp.edt_zipCode.visible = false;
	div_corp.edt_mstAddr.visible = false;
	div_corp.edt_dtlAddr.visible = false;
	div_corp.edt_etcAddr.visible = false;
	div_corp.Button72.text = "주소 직접입력";

	switch (obj.getColumn(e.newrow, "custGrpCd"))
	{
		case "1000" : //일반(국내)
			div_corp.visible = true;
			div_frgn.visible = false;
			div_psnl.visible = false;
			break;
		case "5000" : //일반(해외)
			div_corp.visible = false;
			div_frgn.visible = true;
			div_psnl.visible = false;
			break;
		case "6000" : //개인
			div_corp.visible = false;
			div_frgn.visible = false;
			div_psnl.visible = true;
			break;
	}
	
	div_corp.AddrSearchComponent00.bind();
	div_psnl.AddrSearchComponent00.bind();
	
	fn_conditionControl(obj.getColumn(e.newrow, "custgubun"));
	
	fn_searchInchargeList();
 }

