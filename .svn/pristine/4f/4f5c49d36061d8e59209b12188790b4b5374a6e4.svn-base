public class JobExecDTO extends DefaultDTO implements Serializable {

	private static final long serialVersionUID = -1L;
	
	/** Job Execution Id */
	private long jobExecutionId;
	
	/** Batch Job명 */
	private String jobName;

	/** Batch Job 설명 */
	private String jobDesc;

	/** Batch Job Sync 실행 */
	private String syncYn;

	/** Batch Job InputFile */
	private String inputFile;

	/** Batch Job OutputFile */
	private String outputFile;
	
	/** Batch Job Execution User Id */
	private String executionUserId;

	/** Batch Job Execution Status */
	private String status;

	/**
	 * jobExecutionId 얻기
	 *
	 * @return the jobExecutionId
	 */
	public long getJobExecutionId() {
		return jobExecutionId;
	}

	/**
	 * jobExecutionId 설정
	 *
	 * @param jobExecutionId the jobExecutionId to set
	 */
	public void setJobExecutionId(long jobExecutionId) {
		this.jobExecutionId = jobExecutionId;
	}

	/**
	 * jobName 얻기
	 *
	 * @return the jobName
	 */
	public String getJobName() {
		return jobName;
	}

	/**
	 * jobName 설정
	 *
	 * @param jobName the jobName to set
	 */
	public void setJobName(String jobName) {
		this.jobName = jobName;
	}

	/**
	 * jobDesc 얻기
	 *
	 * @return the jobDesc
	 */
	public String getJobDesc() {
		return jobDesc;
	}

	/**
	 * jobDesc 설정
	 *
	 * @param jobDesc the jobDesc to set
	 */
	public void setJobDesc(String jobDesc) {
		this.jobDesc = jobDesc;
	}

	/**
	 * syncYn 얻기
	 *
	 * @return the syncYn
	 */
	public String getSyncYn() {
		return syncYn;
	}

	/**
	 * syncYn 설정
	 *
	 * @param syncYn the syncYn to set
	 */
	public void setSyncYn(String syncYn) {
		this.syncYn = syncYn;
	}

	/**
	 * executionUserId 얻기
	 *
	 * @return the executionUserId
	 */
	public String getExecutionUserId() {
		return executionUserId;
	}

	/**
	 * executionUserId 설정
	 *
	 * @param executionUserId the executionUserId to set
	 */
	public void setExecutionUserId(String executionUserId) {
		this.executionUserId = executionUserId;
	}

	/**
	 * inputFile 얻기
	 *
	 * @return the inputFile
	 */
	public String getInputFile() {
		return inputFile;
	}

	/**
	 * inputFile 설정
	 *
	 * @param inputFile the inputFile to set
	 */
	public void setInputFile(String inputFile) {
		this.inputFile = inputFile;
	}

	/**
	 * outputFile 얻기
	 *
	 * @return the outputFile
	 */
	public String getOutputFile() {
		return outputFile;
	}

	/**
	 * outputFile 설정
	 *
	 * @param outputFile the outputFile to set
	 */
	public void setOutputFile(String outputFile) {
		this.outputFile = outputFile;
	}

	/**
	 * status 얻기
	 *
	 * @return the status
	 */
	public String getStatus() {
		return status;
	}

	/**
	 * status 설정
	 *
	 * @param status the status to set
	 */
	public void setStatus(String status) {
		this.status = status;
	}

}
