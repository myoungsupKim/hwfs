<query>
	<type>select</type>
	<description><![CDATA[개별표준신단 레시피 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT     SINGLE_MENU_CD
         , RECIPE_CD
         , RECIPE_SEQ
         , DISH_INFO
         , ITEM_RATE
         , '' AS RECIPE_NAME
FROM     FSM_SINGLE_MENU_RECIPE
WHERE    SINGLE_MENU_CD = #SINGLE_MENU_CD#	]]></statement>
