function comp_mainUpjang_change(obj, data)
{
	fn_Trigger_edBtn("comp_mainUpjang");
}

function tab_debt_tabpage1_msk_withdrawPossAmt_onkillfocus(obj:MaskEdit, e:KillFocusEventInfo)
{
	if(tab_debt.tabpage1.msk_initialOccurAmt.value - tab_debt.tabpage1.msk_withdrawPossAmt.value < 0 ){
		gfn_alert("최초 발생액보다 회수 가능액이 더 큽니다.");
		 tab_debt.tabpage1.msk_withdrawPossAmt.value = 0;
		 tab_debt.tabpage1.msk_withdrawUnblAmt.value = 0;
		 tab_debt.tabpage1.msk_withdrawPossAmt.setFocus();
		return false;
	}else{
		tab_debt.tabpage1.msk_withdrawUnblAmt.value = tab_debt.tabpage1.msk_initialOccurAmt.value - tab_debt.tabpage1.msk_withdrawPossAmt.value;
	}
}

function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.columnid == "dealEdate"){
		if(obj.getColumn(obj.rowposition, "dealEdate") < obj.getColumn(obj.rowposition, "dealSdate")){
			gfn_alert("거래시작일이 거래종료일보다 큽니다");
			cal_dealEdate.fn_setValue(fv_sToday);
			return false;
			//trace(fv_sToday);
		}
		
		// 거래처 선택후 최초 채권 금액 가져오기
		ds_cond.clearData();
		ds_cond.addRow();
		
		ds_cond.setColumn(0, "muCd", edt_muNm.getMuCode() );
		ds_cond.setColumn(0, "tmCd", edt_tmNm.getTmCode() );
		ds_cond.setColumn(0, "dealEdate", cal_dealEdate.fn_getValue() );
		
		if(edt_muNm.getMuCode() == "2004"){ // 식재일때
			ds_cond.setColumn(0, "mainUpjangNm", edt_mainUpjang1.getUpjangName() );
			ds_cond.setColumn(0, "upjangNm", edt_upjangNm2.getUpjangName() );
		}else{ // 식재가 아닐때
			ds_cond.setColumn(0, "upjangCd", edt_upjangNm00.getUpjangCode() );
			ds_cond.setColumn(0, "custCd", edt_custNm1.getCustCode() );
		}
		
		// 거래종료일이 없을때는 조회하지 않고 리턴
		if(gfn_isNull(ds_cond.getColumn(0, "dealEdate"))) return;
		
		var strSvcID    = "searchCustInfo";
		var strURL      = "sm/saa/accdArOccurRept/selectCustInfo.xdo";
		var strInDs     = "ds_cond=ds_cond:A";
		var strOutDs    = "ds_custInfo=rsCust";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	
	}
}]]></Script>
