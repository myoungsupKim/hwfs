function tab_debt_tabpage1_msk_initialOccurAmt_onkillfocus(obj:MaskEdit, e:KillFocusEventInfo)
{
	if(tab_debt.tabpage1.msk_initialOccurAmt.value - tab_debt.tabpage1.msk_withdrawPossAmt.value < 0 ){
		gfn_alert("최초 발생액보다 회수 가능액이 더 큽니다.");
		 tab_debt.tabpage1.msk_withdrawPossAmt.value = 0;
		 tab_debt.tabpage1.msk_withdrawUnblAmt.value = 0;
		 tab_debt.tabpage1.msk_withdrawPossAmt.setFocus();
		return false;
	}else{
		tab_debt.tabpage1.msk_withdrawUnblAmt.value = tab_debt.tabpage1.msk_initialOccurAmt.value - tab_debt.tabpage1.msk_withdrawPossAmt.value;
	}
}

