/*----------------------------------------------------------------------------------------------
 * 설명      : Edit 관련 이벤트 실제 처리
 *---------------------------------------------------------------------------------------------*/
function fn_Trigger_edBtn(obj)
{
	switch(obj)
	{
		// 사업부 변경 시
		case "comp_mu" :
			ds_search.setColumn(ds_search.rowposition, "tmCd", "");
			ds_search.setColumn(ds_search.rowposition, "tmNm", "");
			ds_search.setColumn(ds_search.rowposition, "mainUpjangCd", "");
			ds_search.setColumn(ds_search.rowposition, "mainUpjangNm", "");
			ds_search.setColumn(ds_search.rowposition, "upjangCd", "");
			ds_search.setColumn(ds_search.rowposition, "upjangNm", "");
			break;

		// 팀 변경 시
		case "comp_tm" :
			ds_search.setColumn(ds_search.rowposition, "mainUpjangCd", "");
			ds_search.setColumn(ds_search.rowposition, "mainUpjangNm", "");
			ds_search.setColumn(ds_search.rowposition, "upjangCd", "");
			ds_search.setColumn(ds_search.rowposition, "upjangNm", "");
			break;
		
		
		// 본사 변경 시
 		case "comp_mainUpjang" :
			ds_search.setColumn(ds_search.rowposition, "upjangCd", "");
			ds_search.setColumn(ds_search.rowposition, "upjangNm", "");
 			break;

		default:

			break;
	}
}

