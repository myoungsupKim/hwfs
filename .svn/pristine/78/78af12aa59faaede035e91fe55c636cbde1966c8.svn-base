function fn_dealSearch(){
	
	if(ds_list.rowcount == 0) return;
	
    var basisYymm = tab_debt.tabpage2.cal_basisYymm.fn_getValue();
    ds_search.setColumn(0, "basisSyymm", gfn_subStr(gfn_addMonth(basisYymm, -5), 0, 6));
    ds_search.setColumn(0, "basisEyymm", gfn_subStr(basisYymm, 0, 6));
    ds_search.setColumn(0, "basisyymm", gfn_getLastDate(gfn_subStr(basisYymm, 0, 6)));
    
    
    
    //trace("AccdArOccurRept 화면 >> 거래내역 조회년월 조회  :  조회년월=[" + basisYymm + "]");
    //trace("AccdArOccurRept 화면 >> 거래내역 조회년월 조회  :  조회조건(ds_search), basisSyymm=[" + ds_search.getColumn(0, "basisSyymm") + "], basisEyymm=[" + ds_search.getColumn(0, "basisEyymm") + "]");

    ds_hdrDealDesc.clearData();
    ds_dealDesc.clearData();
    
  		if(edt_muNm.getMuCode() == "2004"){ // 식재일때
  			
  			if(gfn_isNull(ds_list.getColumn(ds_list.rowposition, "upjangCd"))){
				gfn_alert("업장은 필수 입니다.");
				return;
  			}
			ds_search.setColumn(0, "mainUpjangCd", ds_list.getColumn(ds_list.rowposition, "upjangCd") );
			
			if(gfn_isNull(ds_list.getColumn(ds_list.rowposition, "custCd"))){
				ds_search.setColumn(0, "upjangCd", ds_list.getColumn(ds_list.rowposition, "upjangCd") );
			}else{
				ds_search.setColumn(0, "upjangCd", ds_list.getColumn(ds_list.rowposition, "custCd") );
			}
 	
			var strSvcID    = "searchDealDesc";
			var strURL      = "sm/saa/accdArOccurRept/selectDealDescBtn.xdo";
			var strInDs     = "ds_search=ds_search:A";
			var strOutDs    = "ds_hdrDealDesc=dsHdrDealDesc ds_dealDesc=dsDealDesc";
			var strArg      = "";
			var strCallback = "fn_callBack";
			var bAsync      = true;

			gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
			
 		}else{
			ds_search.setColumn(0, "upjangCd", ds_list.getColumn(ds_list.rowposition, "upjangCd") );
			ds_search.setColumn(0, "custCd", ds_list.getColumn(ds_list.rowposition, "custCd") );
			
 			var strSvcID    = "searchDealDescFs";
			var strURL      = "sm/saa/accdArOccurRept/selectDealDescBtnFs.xdo";
			var strInDs     = "ds_search=ds_search:A";
			var strOutDs    = "ds_hdrDealDesc=dsHdrDealDesc ds_dealDesc=dsDealDesc";
			var strArg      = "";
			var strCallback = "fn_callBack";
			var bAsync      = true;

			gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
 		}
}

