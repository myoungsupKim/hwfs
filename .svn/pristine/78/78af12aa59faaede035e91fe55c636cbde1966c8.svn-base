    var strCustCd   = edt_custNm.getCustCode();
    var strUpjangCd = edt_upjangNm.getUpjangCode();
    var strObjNm    = obj.name;
    //trace("AccdArOccurRept 화면 >> fn_codeChkEvent : objNm=[" + strObjNm + "], MuCd=[" + strMuCd + "], TmCd=[" + strTmCd + "], CustCd=[" + strCustCd + "], UpjangCd=[" + strUpjangCd + "]");
			
	if ((strObjNm == "edt_custNm") || (strObjNm == "edt_upjangNm") || (strObjNm == "edt_custNm1")) {  // 거래처, 업장
		if (edt_muNm.getMuCode() == "") {
			//trace("AccdArOccurRept 화면 >> fn_codeChkEvent : (return false)사업부코드 없음..");
			gfn_alert("먼저 사업부를 선택해 주세요.");
			edt_muNm.setFocus;
			return false;
		} else if (edt_tmNm.getTmCode() == "") {
			//trace("AccdArOccurRept 화면 >> fn_codeChkEvent : (return false)팀코드 없음..");
			gfn_alert("먼저 팀을 선택해 주세요.");
			edt_tmNm.setFocus;
			return false;
		} else {
			if(strObjNm == "edt_custNm"){
				//trace("AccdArOccurRept 화면 >> fn_codeChkEvent : (return true)팀코드 있음.. TmCd=[" + strTmCd + "]");
				obj.setQid("ScoUpjangCust");//거래처팝업에 반드시 필요
				obj.setUpjang(edt_upjangNm.getUpjangCode() );
				return true;
			}else{
				obj.setQid("ScoUpjangCust");//거래처팝업에 반드시 필요
				obj.setUpjang(edt_upjangNm00.getUpjangCode() );
				return true;
			}
		}
	} else if (strObjNm == "edt_tmNm") {  // 팀
		if (edt_muNm.getMuCode() == "") {
			//trace("AccdArOccurRept 화면 >> fn_codeChkEvent : (return false)사업부코드 없음..");
			gfn_alert("먼저 사업부를 선택해 주세요.");
			edt_muNm.setFocus;
			return false;
		} else {
			//trace("AccdArOccurRept 화면 >> fn_codeChkEvent : (return true)사업부코드 있음.. MuCd=[" + strMuCd + "]");
			return true;
		}
	} else {  
		//trace("AccdArOccurRept 화면 >> fn_codeChkEvent : (return false)선택된 컴포넌트.. strObjNm=[" + strObjNm + "]");
		return false;
	}
		//return true;
		// 사업자번호, 대표자명 저장
}

