/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
    // 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
    if (nErrorCode < 0) {
        if (strSvcID == "saveSign") { //상신 후 결재정보 저장 시 오류가 난 경우
            ds_list.setColumn(ds_list.rowposition, "signInstanceId", "");
            ds_list.setColumn(ds_list.rowposition, "signProgressStatus", "");

            //그룹웨어 연동 버튼을 설정
            SCGw.setUserButton (this, fv_option, ds_list.getColumn(ds_list.rowposition, "signProgressStatus"));
        }
        return;
    }

    switch(strSvcID) {
        // 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            if(ds_hdrDealDesc.getRowCount() > 0) {
                fn_hdrDealDesc();
            }
            if(ds_list.getRowCount() > 0){
				fn_searchInfo(ds_list);
            }
             // 사업장, 거래처 팝업 활성화 설정
			edt_mainUpjang1.enable = false;
			edt_upjangNm00.enable = false;
			edt_upjangNm2.enable = false;
			edt_custNm1.enable = false;
            break;
           
        case "searchUpjang" :
            //trace(ds_upjang.saveXML());
            if(ds_upjang.getRowCount() > 0) {
                ds_list.setColumn(ds_list.rowposition, "bizNo", ds_upjang.getColumn(ds_upjang.rowposition, "bizNo"));
                ds_list.setColumn(ds_list.rowposition, "ceoNm", ds_upjang.getColumn(ds_upjang.rowposition, "ceoNm"));
                ds_list.setColumn(ds_list.rowposition, "zipCode", ds_upjang.getColumn(ds_upjang.rowposition, "zipCode"));
                ds_list.setColumn(ds_list.rowposition, "addr", ds_upjang.getColumn(ds_upjang.rowposition, "addr"));
                ds_list.setColumn(ds_list.rowposition, "addrEnc", ds_upjang.getColumn(ds_upjang.rowposition, "addrEnc"));
            }

            break;
            case "searchSaupno" :
				if(ds_saupNo.getRowCount() > 0) {
					ds_list.setColumn(ds_list.rowposition, "bizNo", ds_saupNo.getColumn(0, "saupno"));
					ds_list.setColumn(ds_list.rowposition, "ceoNm", ds_saupNo.getColumn(0, "drname1"));
					ds_list.setColumn(ds_list.rowposition, "addrKey", ds_saupNo.getColumn(0, "addrKey"));
				}           
				break;
			
			 case "searchCustInfo" :
				if(ds_custInfo.getRowCount() > 0) {
					ds_list.setColumn(ds_list.rowposition, "initialOccurAmt", ds_custInfo.getColumn(0, "totAmt"));
				}           
				break;	
            
        case "searchInfo" :
            //trace(ds_list.saveXML());
            if(ds_hdrDealDesc.getRowCount() > 0) {
                //if(ds_list.getColumn(ds_list.rowposition, "basisYymm") != gfn_subStr(fv_sToday, 0, 6)) {
                    fn_hdrDealDesc();
                //}
            }
            fn_dealSearch();
			fn_searchDataClear();
            break;
        case "searchDealDesc" :
            //trace("AccdArOccurRept 화면 >> fn_callBack  :  ds_hdrDealDesc.saveXML()  ----------");
            //trace(ds_hdrDealDesc.saveXML());
            //trace(ds_dealDesc.saveXML());
            //tab_debt.tabindex = 1;

            if(ds_hdrDealDesc.getRowCount() > 0) {
                fn_hdrDealDesc();
            }
            fn_searchDataClear();
           break;
            
            case "searchDealDescFs" :
            
            if(ds_hdrDealDesc.getRowCount() > 0) {
                fn_hdrDealDesc();
            }
            fn_searchDataClear();
            break;
        // 저장
        case "save" :
            gfn_alert("msg.save.success");  // 저장 되었습니다.
            
            fn_search();

            break;
        // 전자결재정보 저장
        case "saveSign" :
            gfn_alert("msg.save.sign.success"); // 저장 되었습니다.

            break;
    }

    //그룹웨어 연동 버튼을 설정
    SCGw.setUserButton (this, fv_option, ds_list.getColumn(ds_list.rowposition, "signProgressStatus"));
}

