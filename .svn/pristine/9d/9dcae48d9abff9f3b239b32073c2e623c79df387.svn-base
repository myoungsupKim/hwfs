 *------------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
    // 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
    if (nErrorCode < 0) {
        return;
    }

    switch(strSvcID) {
        // 코드분류 조회
        case "search" :
            //trace("SaleColletionList 화면 >> fn_callBack  :  ds_list.saveXML()");
            //trace(ds_list.saveXML());
            
            ds_list.keystring       = "G:+muCd, +teamCd";
            grd_list.treeinitstatus = "expand,all";

            if(ds_hdrDealDesc.getRowCount() > 0) {
                fn_hdrDealDesc();
            }
            break;
    }
}


