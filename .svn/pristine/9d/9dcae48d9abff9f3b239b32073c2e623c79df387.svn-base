 *------------------------------------------------------------------------------------------------*/
function fn_search()
{
    //trace("SaleColletionList 화면 >> fn_search  :  cal_basisYymm.fn_getValue()=[" + cal_basisYymm.fn_getValue() + "], gfn_addMonth(cal_basisYymm.fn_getValue(), -11)=[" + gfn_addMonth(cal_basisYymm.fn_getValue(), -11) + "]");
    //trace("SaleColletionList 화면 >> fn_search  :  gfn_subStr(gfn_addMonth(cal_basisYymm.fn_getValue(), -11 ), 0, 6)=[" + gfn_subStr(gfn_addMonth(cal_basisYymm.fn_getValue(), -11), 0, 6) + "]");
    //trace("SaleColletionList 화면 >> fn_search  :  gfn_subStr(gfn_addMonth(cal_basisYymm.fn_getValue(), -12), 0, 6)=[" + gfn_subStr(gfn_addMonth(cal_basisYymm.fn_getValue(), -12), 0, 6) + "]");
    ds_search.setColumn(0, "basisSyymm", gfn_subStr(gfn_addMonth(cal_basisYymm.fn_getValue(), -11), 0, 6));
    ds_search.setColumn(0, "basisEyymm", gfn_subStr(cal_basisYymm.fn_getValue(), 0, 6));
    ds_search.setColumn(0, "basisByymm", gfn_getLastDate(gfn_subStr(cal_basisYymm.fn_getValue(), 0, 6)));

    // 조회시 Validation Check
    if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);

    if(edt_muNm.getMuCode() == "2004"){ // 식재일때
    
    var strSvcID    = "search";
    var strURL      = "sm/saa/saleColletionList/selectListFms.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=dsSearchOutput ds_hdrDealDesc=dsHdrDealDesc";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = false;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
    }else{			// 급식일때
		var strSvcID    = "search";
		var strURL      = "sm/saa/saleColletionList/selectList.xdo";
		var strInDs     = "ds_search=ds_search:A";
		var strOutDs    = "ds_list=dsSearchOutput ds_hdrDealDesc=dsHdrDealDesc";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = false;

		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
		
    }
    
}

