function grd_lst_OnMouseOver(obj,nPosX,nPosY,nRow,nCell,nPivotIndex)
{
	
	//----------------------------------
	// Tooltip 설명 처리부
	//----------------------------------
	// Head 부 처리
	var	srow 	= 0.0;
	var	int_srow= 0;
	var	v_seq  	= "";

	if (nRow == -1 && nCell > 3) {
		srow = toNumber((nCell-1)/3);
		int_srow = parseInt(srow);
		//alert("/"+srow+"/"+int_srow+"/");
		//alert(length(DS_head.GetColumn((int_srow-1), "rem")));
		
		if (gfn_length(DS_head.getColumn((int_srow-1), "rem"))> 8 && int_srow == srow) {
			grd_lst.tooltiptext = "☞ 일정 : "+DS_head.getColumn((int_srow-1), "rem")+" ";
			return;
		}
		else { grd_lst.tooltiptext = ""; }
	}
	else { grd_lst.tooltiptext = ""; }
	// Body 부 처리
	//grd_lst.ToolTipText = "nRow :"+nRow+"\nnCell :"+nCell+"\nnPosX :"+nPosX+"\nnPosY :"+nPosY;
	v_seq  = gfn_midString(DS_head.getColumn((nCell-1), "fday"),4,4);
	//alert(v_seq);
	
	if (nRow > -1 && nCell > 0 && gfn_length(DS_menu.getColumn(nRow, "dup_sta_"+v_seq))> 0) {
		if (nPosX > 740) {
			Div_pop.position.left = nPosX-(Div_pop.position.width);
			Div_pop.position.top  = nPosY+30;
			// 화면에 보여질 정보를 처리한다.
			Div_pop.visible = true;
		}
		else {
			Div_pop.position.left = nPosX+15;
			Div_pop.position.top  = nPosY+30;
			// 화면에 보여질 정보를 처리한다.
			Div_pop.visible = true;
		}
	}
	else {
		Div_pop.visible = false;
	}
		//alert("nRow :"+nRow);
		//alert("nCell :"+nCell);
	
} 

