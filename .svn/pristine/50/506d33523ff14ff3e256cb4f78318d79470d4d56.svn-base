/*----------------------------------------------------------------------------------------------
 * 설명      : 포커스 이동
 *---------------------------------------------------------------------------------------------*/
function common_OnKeyDown(obj, e)
{
	switch( e.keycode )
	{
		// Enter
		case 13:
			switch( obj.name )
			{
				case "ed_Class2":
					eval(gfn_replace(obj.name, "ed_", "btn_")).click();
					break;
				case "ed_Class3":
					eval(gfn_replace(obj.name, "ed_", "btn_")).click();
					break;
				case "ed_Class4":
					eval(gfn_replace(obj.name, "ed_", "btn_")).click();
					break;
				case "ed_Item":
					eval(gfn_replace(obj.name, "ed_", "btn_")).click();
					break;
				case "me_From":
					if( gfn_length(gfn_trim(me_From.value)) == 0 )	btn_Date_OnClick(eval("btn_NEED_DATE"));
					break;
				case "me_To":
					if( gfn_length(gfn_trim(me_To.value)) == 0 )	btn_Date_OnClick(eval("btn_NEED_DATE1"));

					fn_search();
					return;
					break;
				default :
					break;
			}
			var v_nComp=GetNextComponent(null);
			v_nComp.setFocus();
			break;

		// del
		case 46:
			switch( obj.name )
			{
				case "cbo_Subinv":
					cbo_Subinv.index = 0;
					break;
				case "ed_Class2":
					ed_Class2.value = "";
					ed_Class2.userdata = "";
					break;
				case "ed_Class3":
					ed_Class3.value = "";
					ed_Class3.userdata = "";
					break;
				case "ed_Class4":
					ed_Class4.value = "";
					ed_Class4.userdata = "";
					break;
				case "ed_Item":
					ed_Item.value = "";
					ed_Item.userdata = "";
					break;
				case "me_From":
					me_From.value="";
					break;
				case "me_To":
					me_To.value = "";
					break;
			}
			break;
	}
}

