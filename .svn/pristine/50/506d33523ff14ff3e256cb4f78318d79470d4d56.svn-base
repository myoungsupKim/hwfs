function fn_callback(nErrorCode, strErrorMsg, strSvcID)
{
	if( nErrorCode < 0 )
	{
		g_Message("EE", "시스템오류! 전산 담당자에게 문의 하세요.\n" + errMsg);
		ds_stock.clearData();
		return;
	}

	switch( strSvcID )
	{
		// 조회
		case "search" :
			fn_setGrid(cbo_Subinv.index, tab_Main.tabindex);
			break;

		default:
			break;
	}
}

