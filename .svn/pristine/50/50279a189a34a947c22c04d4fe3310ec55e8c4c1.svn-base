	nFRow = nCopyRow;	// 콜백 전의 Row를 돌려준다.
	nCopyRow = -1;		// Row값 초기화
	// 새양식 생성
	nARow = ds_Master.addRow();
	ds_Master.setColumn(nARow,"TABLE_TYPE",ds_Master.getColumn(nFRow,"TABLE_TYPE"));
	ds_Master.setColumn(nARow,"USE_YN","Y");
	
	ds_Detail.filter("CLASS_TYPE=='L'");
	CopyF(ds_Detail1,ds_Detail);
	ds_Detail.filter("CLASS_TYPE=='M'");
