/*
 ******************************************************************************************
 * 함  수  명   : fn_addCode(strGbn)
 * 입      력   : strGbn : 분류 (대분류:L,중분류:M,소분류:S)
 * 반      환   : 없음
 * 기      능   : 코드추가
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-18  
 ******************************************************************************************
 */
function fn_addCode(strGbn)
{
	var nRow,nARow,strCode;
	
	if (strGbn == "L")
	{
		nRow 	= ds_Master.rowposition;
		
		if (nRow < 0)
		{
			g_Message("EE", "양식을 입력해 주십시오.");
			return;
		}

		strCode = ds_Detail1.getMax("CLASS_CD");
		strCode = isNaN( strCode ) ? 1000000 : toNumber(strCode) + 1000000;

		nARow 	= ds_Detail1.addRow();

		ds_Detail1.setColumn(nARow,"TABLE_TYPE",ds_Master.getColumn(nRow,"TABLE_TYPE"));
		ds_Detail1.setColumn(nARow,"FORM_CD",ds_Master.getColumn(nRow,"FORM_CD"));
		ds_Detail1.setColumn(nARow,"CLASS_CD",gfn_lPad(strCode,"0",8));
		ds_Detail1.setColumn(nARow,"CLASS_TYPE",strGbn);
		
		grd_Detail1.setFocus();
		grd_Detail1.setCellPos(2);
	}
	else if (strGbn == "M")
	{
		nRow 	= ds_Detail1.rowposition;
		
		if (nRow < 0)
		{
			g_Message("EE", "대분류를 입력해 주십시오.");
			return;
		}
		//strCode = ds_Detail2.getMax("CLASS_CD")+ 1000;
		var strDescCode = ds_Detail2.getMax("CLASS_CD");
		strDescCode = isNaN( strDescCode ) ? "0" : strDescCode;

		strCode = toNumber(ds_Detail1.getColumn(nRow,"CLASS_CD")) + toNumber(gfn_right(strDescCode, 6)) + 1000;
		//strCode = toNumber(ds_Detail1.GetColumn(nRow,"CLASS_CD")) + toNumber(right(ds_Detail2.Max("CLASS_CD"),6)) + 1000;
		
		nARow 	= ds_Detail2.addRow();
		
		ds_Detail2.setColumn(nARow,"TABLE_TYPE",ds_Detail1.getColumn(nRow,"TABLE_TYPE"));
		ds_Detail2.setColumn(nARow,"FORM_CD",ds_Detail1.getColumn(nRow,"FORM_CD"));
		ds_Detail2.setColumn(nARow,"CLASS_CD",gfn_lPad(strCode,"0",8));
		ds_Detail2.setColumn(nARow,"CLASS_TYPE",strGbn);
		ds_Detail2.setColumn(nARow,"MNGM_FLAG","0");
		ds_Detail2.setColumn(nARow,"MNGM_CNT",0);

		grd_Detail2.setFocus();
		grd_Detail2.setCellPos(2);
	}
	else
	{
		nRow 	= ds_Detail2.rowposition;
		
		if (nRow < 0)
		{
			g_Message("EE", "중분류를 입력해 주십시오.");
			return;
		}
		
		var strDescCode = ds_Detail3.getMax("CLASS_CD");
		strDescCode = isNaN( strDescCode ) ? "0" : strDescCode;
		strCode = toNumber(ds_Detail2.getColumn(nRow,"CLASS_CD")) + toNumber(gfn_right(strDescCode,3)) + 1;
		//strCode = toNumber(ds_Detail2.GetColumn(nRow,"CLASS_CD")) + toNumber(right(ds_Detail3.Max("CLASS_CD"),3)) + 1;
		
		nARow 	= ds_Detail3.addRow();
		
		ds_Detail3.setColumn(nARow,"TABLE_TYPE",ds_Detail2.getColumn(nRow,"TABLE_TYPE"));
		ds_Detail3.setColumn(nARow,"FORM_CD",ds_Detail2.getColumn(nRow,"FORM_CD"));
		ds_Detail3.setColumn(nARow,"CLASS_CD",gfn_lPad(strCode,"0",8));
		ds_Detail3.setColumn(nARow,"CLASS_TYPE",strGbn);
		ds_Detail3.setColumn(nARow,"LRG_CLASS_CD",ds_Detail1.getColumn(ds_Detail1.rowposition,"CLASS_CD"));
		ds_Detail3.setColumn(nARow,"LRG_CLASS_NAME",ds_Detail1.getColumn(ds_Detail1.rowposition,"CLASS_NAME"));
		ds_Detail3.setColumn(nARow,"MIDDLE_CLASS_CD",ds_Detail2.getColumn(nRow,"CLASS_CD"));
		ds_Detail3.setColumn(nARow,"MIDDLE_CLASS_NAME",ds_Detail2.getColumn(nRow,"CLASS_NAME"));
		ds_Detail3.setColumn(nARow,"MINUS_YN","0");
		ds_Detail3.setColumn(nARow,"NOT_FIT_YN","0");
		
		grd_Detail3.setFocus();
		grd_Detail3.setCellPos(2);
	}
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_deleteCode(strGbn)
 * 입      력   : strGbn : 분류 (대분류:L,중분류:M,소분류:S)
 * 반      환   : 없음
 * 기      능   : 코드삭제
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-19  
 ******************************************************************************************
 */
function fn_deleteCode(strGbn)
{	
	var nRow,nCnt,strClassCD,objDS,strRowtype;
	
	if (strGbn == "L")
	{
		objDS	= ds_Detail1;
		nRow	= objDS.rowposition;
		nCnt	= ds_Detail2.rowcount;
		
		if (nCnt > 0)
		{
			g_Message("EE", "하위코드가 있습니다\n확인해 주십시오.");
			return;
		}
		

	}
	else if (strGbn == "M")
	{
		objDS	= ds_Detail2;
		nRow	= objDS.rowposition;
		nCnt	= ds_Detail3.rowcount;
		
		if (nCnt > 0)
		{
			g_Message("EE", "하위코드가 있습니다\n확인해 주십시오.");
			return;
		}
	}
	else
	{
		objDS	= ds_Detail3;
		nRow	= objDS.rowposition;
	}
	
	// 삭제확인
	if(!g_Confirm("01",""))
	{
		return false;
	}
	
	strRowtype = GetRowType(objDS,nRow);
	strClassCD = objDS.getColumn(nRow,"CLASS_CD");
	objDS.deleteRow(nRow);
	
	if (strRowtype!="insert")
	{
		var actionName, cmdName, inData, outData, otherArg, callBackFnc;
		
		actionName		= "FSC00010E_T005";
		cmdName			= "U_svc::" + "fs/fsc/FSC00010E_T005.jsp";
		inData			= "ds_input=ds_input ds_Detail=" + objDS.name + ":U";
		
		jsp_transaction(this,actionName, cmdName ,inData ,outData,otherArg,callBackFnc);
	}
}
