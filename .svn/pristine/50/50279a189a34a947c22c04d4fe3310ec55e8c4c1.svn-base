 * 함  수  명   : fn_Insert()
 * 기      능   : [입력버튼] 입력 시 처리되는 작업
 * 비      고	: 팝업사용
 ******************************************************************************************
 */
function fn_insert()
{
	if (gfn_dsIsUpdated(ds_Master)==true || gfn_dsIsUpdated(ds_Detail1)==true || gfn_dsIsUpdated(ds_Detail2)==true || gfn_dsIsUpdated(ds_Detail3)==true)
	{
		g_Message("EE", "변경된 데이터가 있습니다.\n 저장 후 입력해주십시오.");
		return;
	}
	
	if (gfn_isNull(cbo_S_TABLE_TYPE.value))
	{
		g_Message("EE", "점검표구분을 선택하세요!");
		cbo_S_TABLE_TYPE.setFocus();
		return;
	}
	
	var nARow;
	
	fnCnt = 0;
	
	nARow = ds_Master.addRow();
	ds_Master.setColumn(nARow,"TABLE_TYPE",cbo_S_TABLE_TYPE.value);
	ds_Master.setColumn(nARow,"USE_YN","N");
	
	ds_Detail.clearData();
	ds_Detail1.clearData();
	ds_Detail2.clearData();
	ds_Detail3.clearData();
	
	fn_formSetting();
	
	ed_FORM_NAME.setFocus();
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_Save()
 * 기      능   : [저장버튼] 저장 시 처리되는 작업
 ******************************************************************************************
 */
function fn_save()
{
	// 변경여부 확인
	if(gfn_dsIsUpdated(ds_Master)==false && gfn_dsIsUpdated(ds_Detail1)==false && gfn_dsIsUpdated(ds_Detail2)==false && gfn_dsIsUpdated(ds_Detail3)==false)
	{
		g_Message("EE", "변경된 데이터가 없습니다.");
		return;
	}
	
	if(!fn_SaveCheck())
	{
		return;
	}
	
	if (! g_Confirm("01", '해당 점검표를 저장하시겠습니까?!')) return;

    // 서비스셋팅
    var actionName, cmdName, inData, outData, otherArg, callBackFnc;
    
    if (GetRowType(ds_Master,ds_Master.rowposition)== "insert")
    {
		ds_in.setColumn(0,"FORM_CD",""); 
    }
    else
    {
		ds_in.clearData();
		ds_in.addRow();
		ds_in.setColumn(0,"FORM_CD",ds_Master.getColumn(ds_Master.rowposition,"FORM_CD"));
    }
    
	actionName		= "FSC00010E_T003";
	cmdName			= "U_svc::" + "fs/fsc/FSC00010E_T003.jsp";
	inData			= "ds_Master=ds_Master:U ds_Detail1=ds_Detail1:U ds_Detail2=ds_Detail2:U ds_Detail3=ds_Detail3:U ds_in=ds_in";
    outData			= "ds_in=ds_in";
    callBackFnc		= "fn_afterSave";
	
	jsp_transaction(this,actionName, cmdName ,inData ,outData,otherArg,callBackFnc);
   
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_Delete()
 * 기      능   : [삭제버튼] 삭제 시 처리되는 작업
 ******************************************************************************************
 */
function fn_delete()
{
	if(ds_list.getRowCount()==0)
	{
		g_Message("EE", "삭제할 데이터가 없습니다.");
		return;
	}
	
	if (gfn_dsIsUpdated(ds_Master)==true || gfn_dsIsUpdated(ds_Detail1)==true || gfn_dsIsUpdated(ds_Detail2)==true || gfn_dsIsUpdated(ds_Detail3)==true)
	{
		g_Message("EE", "변경된 데이터가 있습니다.\n 저장 후 삭제해십시오.");
		return;
	}
	
	if (toNumber(ds_list.getColumn(ds_list.rowposition,"CNT")) != 0)
	{
		g_Message("EE", "작성된 점검등록내역이 있습니다.\n확인해 주십시오.");
		return;
	}
	
	var strMsg,btp,nRow,nFRow;
	
	nRow 	= ds_list.rowposition;
	//nFRow 	= ds_Master.findRow("FORM_CD",ds_list.getColumn(nRow,"FORM_CD"));
	nFRow 	= ds_Master.findRowExpr("FORM_CD == '"+ds_list.getColumn(nRow,"FORM_CD")+"' && TABLE_TYPE == '"+ds_list.getColumn(nRow,"TABLE_TYPE")+"'");
	
	strMsg = "[" + ds_list.getColumn(nRow,"FORM_NAME") + "] " + "를 삭제하시겠습니까?";
	
	// 삭제확인
	if(!g_Confirm("01",strMsg))
	{
		return false;
	}
	
	// Data삭제
	ds_list.deleteRow(nRow);
	ds_Master.deleteRow(nFRow);
	ds_Master.rowposition = -1;
	ds_Detail1.clearData();
	ds_Detail2.clearData();
	ds_Detail3.clearData();
	
	//trace(ds_Master.saveXML("ds_Master", "U"));
	
	// 삭제
	var actionName, cmdName, inData, outData, otherArg, callBackFnc;
	
	actionName		= "FSC00010E_T004";
	cmdName			= "U_svc::" + "fs/fsc/FSC00010E_T004.jsp";
	inData			= "ds_Master=ds_Master:U";
	
	jsp_transaction(this,actionName, cmdName ,inData ,outData,otherArg,callBackFnc);
}

