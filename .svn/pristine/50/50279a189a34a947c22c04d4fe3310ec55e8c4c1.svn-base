 * 작성  일자   : 2008-03-18  
 ******************************************************************************************
 */
function grd_list_OnCellDblClick(obj:Grid, e:GridClickEventInfo)
{
	var strMsg = "변경된 내역이 있습니다. 저장하시겠습니까?\n(취소하신 경우 자료는 초기화됩니다.)";
	var nFRow,strClassType,objds,nARow;
	
	// 변경여부체크
	//nFRow = ds_Master.FindRow("FORM_CD",ds_list.GetColumn(nRow,"FORM_CD"));	
	nFRow = ds_Master.findRowExpr("FORM_CD=='" + ds_list.getColumn(e.row,"FORM_CD") + "'&&TABLE_TYPE=='" + ds_list.getColumn(e.row,"TABLE_TYPE") + "'");
	
	if (nFRow != ds_Master.rowposition)
	{
		if (gfn_dsIsUpdated(ds_Master)==true || gfn_dsIsUpdated(ds_Detail1)==true || gfn_dsIsUpdated(ds_Detail2)==true || gfn_dsIsUpdated(ds_Detail3)==true)
		{
			if (g_Confirm("02",strMsg))
			{
				fn_save();
				return;
			}
			else
			{
				ds_Master.reset();
				ds_Detail1.reset();
				ds_Detail2.reset();
				ds_Detail3.reset();
			}
		}
	}
	
	//var strMsg = "선택하신 양식을 수정하시겠습니까? \n수정할 경우 [확인]을 \n신규추가할 경우 [취소]를 선택하세요.";
	var strMsg = "선택하신 양식을 조회하시겠습니까? \n조회할 경우 [확인]을 \n신규추가할 경우 [취소]를 선택하세요.";
	if (g_Confirm("02", strMsg))		// 기존양식 수정
	{
		//ds_Master.Reset();
		ds_Master.rowposition = nFRow;
		fn_SearchSub(nFRow,"fn_afterSearch");
	}			
	else							// 기존양식 복사하여 새양식생성			
	{
		ds_Master.enableevent 	= false;
		grd_Detail1.enableredraw		= false;
		grd_Detail2.enableredraw		= false;
		grd_Detail3.enableredraw		= false;
		
		//ds_Master.Reset();
		
		// 2015-05-26 jshoon 수정
		// Sync 함수가 중간에 호출됨으로써 아래의 로직을 타지 않아
		// 별도의 callback함수(fn_afterCopySearch)를 만들어 호출 후 로직을 이관한다.
		//http.Sync = true;
		//fn_SearchSub(nFRow,"");
		fn_SearchSub(nFRow,"fn_afterCopySearch");
		//http.Sync = false;
	}
}

/*
