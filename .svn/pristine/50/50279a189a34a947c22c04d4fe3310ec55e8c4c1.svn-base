	}
	ds_Detail2.updatecontrol = true;
	
	ds_Detail3.updatecontrol = false;
	for (var i = 0; i < ds_Detail3.getRowCount(); i++) {
		ds_Detail3.setRowType(i, Dataset.ROWTYPE_INSERT);
	}
	ds_Detail3.updatecontrol = true;
	
	ds_Detail2.filter("gfn_subStr(CLASS_CD,0,2)==" + wrapQuote(gfn_subStr(ds_Detail1.getColumn(0,"CLASS_CD"),0,2)));
	ds_Detail3.filter("gfn_subStr(CLASS_CD,0,5)==" + wrapQuote(gfn_subStr(ds_Detail2.getColumn(0,"CLASS_CD"),0,5)));
	
	//trace(ds_Detail3.saveXML("ds_Detail3", "I"));
	
	//fnCnt = toNumber(ds_Master.getColumn(ds_list.rowposition, "CNT"));
	
	fnCnt = 0;
	fn_formSetting();
	
// 	for(var i=0; i<ds_Detail.getCountNF(); i++)
// 	{
// 		strClassType = ds_Detail.getColumn(i,"CLASS_TYPE");
// 		
// 		if (strClassType == "L")
// 		{
// 			objds = ds_Detail1;
// 		}
// 		else if (strClassType == "M")
// 		{
// 			objds = ds_Detail2;
// 		}
// 		else
// 		{
// 			objds = ds_Detail3;
// 		}
// 		
// 		nARow = objds.addRow();
// 		objds.copyRow(nARow, ds_Detail, i);
// 
// 		// 복사기능을 추가한다.
// 		// 임의로 Row상태를 insert로 처리한다. 복사 기능이므로...
// 		objds.updatecontrol = false;
// 		objds.setRowType(nARow, Dataset.ROWTYPE_INSERT);
// 		objds.updatecontrol = true;
// 	}
	
	/* FirstRow은 지원되지 않는 속성입니다.*/ ds_Detail1.rowposition = ds_Detail1.FirstRow;
	/* FirstRow은 지원되지 않는 속성입니다.*/ ds_Detail2.rowposition = ds_Detail2.FirstRow;
	/* FirstRow은 지원되지 않는 속성입니다.*/ ds_Detail3.rowposition = ds_Detail3.FirstRow;
	
	ds_Master.enableevent 	= true;
	grd_Detail1.enableredraw		= true;
	grd_Detail2.enableredraw		= true;
	grd_Detail3.enableredraw		= true;
}

