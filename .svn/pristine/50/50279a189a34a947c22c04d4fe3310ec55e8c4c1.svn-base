 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-18  
 ******************************************************************************************
 */
function fn_afterSearch(errCode, errMsg)
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오");
		return;
	}
	
	ds_Detail.filter("CLASS_TYPE=='L'");
	CopyF(ds_Detail1,ds_Detail);
	ds_Detail.filter("CLASS_TYPE=='M'");
	CopyF(ds_Detail2,ds_Detail);
	ds_Detail.filter("CLASS_TYPE=='S'");
	CopyF(ds_Detail3,ds_Detail);
	
	ds_Detail2.filter("gfn_subStr(CLASS_CD,0,2)==" + wrapQuote(gfn_subStr(ds_Detail1.getColumn(0,"CLASS_CD"),0,2)));
	ds_Detail3.filter("gfn_subStr(CLASS_CD,0,5)==" + wrapQuote(gfn_subStr(ds_Detail2.getColumn(0,"CLASS_CD"),0,5)));
	
	fnCnt = toNumber(ds_Master.getColumn(ds_Master.rowposition, "CNT"));
	
	//trace("fn_afterSearch fnCnt >>> " + ds_Master.getColumn(ds_Master.rowposition, "FORM_NAME") + " , " + fnCnt);
	
	fn_formSetting();
}

/*
