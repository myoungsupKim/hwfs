				  errMsg	= 에러메세지
 * 반      환   : 없음
 * 기      능   : 저장후 실행 함수
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-18  
 ******************************************************************************************
 */
function fn_afterSave(errCode, errMsg)
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오");
		return;
	}
	else
		g_Message("03", "해당 점검표가 저장되었습니다.");
	
	if (gfn_length(ds_Master.getColumn(ds_Master.rowposition,"FORM_CD"))==0)
	{
		ds_Master.setColumn(ds_Master.rowposition,"FORM_CD",ds_in.getColumn(0,"FORM_CD"));
		ds_Master.applyChange();
		fn_SearchSub(ds_Master.rowposition,"fn_afterSearch");
	}
}

