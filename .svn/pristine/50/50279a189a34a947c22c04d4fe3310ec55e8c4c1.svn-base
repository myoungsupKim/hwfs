function ds_Detail3_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	// -점수관리가 해제되었을때 부적합관리도 해제
	if (e.columnid == "MINUS_YN" && e.newvalue == "0")
	{
		ds_Detail3.setColumn(e.row,"NOT_FIT_YN",e.newvalue);
	}
}

