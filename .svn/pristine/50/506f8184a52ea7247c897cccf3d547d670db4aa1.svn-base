 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	//v_ds_check = "ds_list";
	
	var nRow = ds_detail.addRow();
	ds_detail.setColumn(nRow, "muCd", arrArg[0]);			//사업부코드
	ds_detail.setColumn(nRow, "muNm", arrArg[1]);			//사업부명
	ds_detail.setColumn(nRow, "upjangCd", arrArg[2]);		//업장코드
	ds_detail.setColumn(nRow, "upjangNm", arrArg[3]);		//업장명
	ds_detail.setColumn(nRow, "stdRecipeCd", arrArg[4]);	//표준레시피코드
	ds_detail.setColumn(nRow, "recipeNm", arrArg[5]);		//표준레시피명
	ds_detail.setColumn(nRow, "cookUnit", arrArg[6]);		//조리단위
	ds_detail.setColumn(nRow, "recipeNmBynm", arrArg[7]);	//레시피별명
	ds_detail.setColumn(nRow, "stand", arrArg[8]);			//규격
	ds_detail.setColumn(nRow, "materialCd", arrArg[9]);		//자재(또는 레시피)코드
	ds_detail.setColumn(nRow, "recipeApplyUnit", "G");		//레시피적용단위(Def:G)
	ds_detail.setColumn(nRow, "basisUnitCd", "G");			//기준단위(Def:G)
	ds_detail.setColumn(nRow, "semiGoodsYn", "Y");			//품목구분(반제품여부) Def:Y(레시피)
	ds_detail.setColumn(nRow, "upjangModifyAllowYn", arrArg[12]);	//업장수정허용여부
	//ds_detail.setColumn(nRow, "semiGoodsYn", "N");			//품목구분(Default 식자재)
	ds_detail.setColumn(nRow, "priceSet", "2");
	
	//식자재 검색버튼 활성
// 	btn_recipePop.visible = false;
// 	btn_pumPop.visible = true;
	
	var sdate = arrArg[10];
	var edate = arrArg[11];
	if(sdate == null || sdate == "undefined"){
		sdate = fv_sToday;
	}
	if(edate == null || edate == "undefined"){
		edate = "29991231";
	}
	
	//Calendar
	cal_sdate.fn_initMonthCal(this, sdate, "", ds_detail, "sdate");
	cal_edate.fn_initMonthCal(this, edate, "", ds_detail, "edate");
	
	//Popup
	SCPopup.bind(this, "Recipe", "edt_materialCdNm", "btn_recipePop", null, {muCd:"", upjangType:""}, {before:"fn_recipeBeforeEvent", after:"fn_recipeAfterEvent"});
	
	//if(saveGb == "U"){
		fn_search();
	//}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 레시피 팝업 before 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_recipeBeforeEvent(p){
	p.option.muCd = ds_detail.getColumn(0, "muCd");
	//p.option.upjangType = ds_detail.getColumn(0, "upjangType");
  
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 레시피 팝업 after 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_recipeAfterEvent(p, data){
	ds_detail.setColumn(0, "materialCd", data.code);
	ds_detail.setColumn(0, "poUnit", "");
	ds_detail.setColumn(0, "itemSize", "");
	ds_detail.setColumn(0, "cstAmt", data.cstAmt);
	ds_detail.setColumn(0, "cookUnitItem", data.cookUnit);
	ds_detail.setColumn(0, "offerUnitItem", data.offerUnit);
	ds_detail.setColumn(0, "offerUnitNm", data.offerUnitNm);
	
	//투입재료비 계산
	var investAmt = fn_calInvestAmtRecipe(ds_detail.getColumn(0, "needQty"), data.cookUnit, data.cstAmt);
	ds_detail.setColumn(0, "investAmt", investAmt);
	
}

