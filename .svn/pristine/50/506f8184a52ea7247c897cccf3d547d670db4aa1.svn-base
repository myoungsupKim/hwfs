{
	//식자재 검색팝업
	fn_callItemPop();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 식자재 검색 팝업 호출
 *---------------------------------------------------------------------------------------------*/
function fn_callItemPop(){
	var arrArg = new Array();
	
	var strId       = "StandardRecipeDetailedItemPop";
	var strURL      = "X_RRM::StandardRecipeDetailedItemPop.xfdl";
	var objArgument = {arrArg:arrArg};
	var nWidth      = -1;
	var nHeight     = -1;
	var strStyle    = "";	
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";
	var bResizable  = true;

	// 리턴값을 배열로 받을 경우	
	var arrRtn = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign, bResizable);
	
	if ( arrRtn instanceof Object )
	{
		ds_detail.setColumn(0, "materialCd", arrRtn.code);
		ds_detail.setColumn(0, "materialCdNm", arrRtn.codeName);
		ds_detail.setColumn(0, "poUnit", arrRtn.poUnit);
		ds_detail.setColumn(0, "cstAmt", arrRtn.cstAmt);
		ds_detail.setColumn(0, "gramPrice", arrRtn.gramPrice);
		ds_detail.setColumn(0, "itemSize", arrRtn.itemSize);
		ds_detail.setColumn(0, "sdate", arrRtn.sdate);
		ds_detail.setColumn(0, "edate", arrRtn.edate);
		ds_detail.setColumn(0, "itemUpjang", fn_upjangTypeDrUpjang());
		//ds_detail.setColumn(0, "itemUpjang", arrRtn.itemUpjang);
		
		//투입재료비 계산
		msk_investAmt.value = fn_calInvestAmt(ds_detail.getColumn(0, "needQty"), ds_detail.getColumn(0, "gramPrice"));
	}
	
	msk_needQty.setFocus();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 투입재료비 계산(식자재)
 *---------------------------------------------------------------------------------------------*/
function fn_calInvestAmt(needQty, gramPrice){
	return Math.round(Number(needQty) * Number(gramPrice), 2);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 투입재료비 계산(레시피)
 *---------------------------------------------------------------------------------------------*/
function fn_calInvestAmtRecipe(needQty, cookUnit, cstAmt){
	return Math.round(Number(needQty) / Number(cookUnit) * Number(cstAmt));
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 소요량 keyup이벤트 - 투입재료비 계산
 *---------------------------------------------------------------------------------------------*/
function msk_needQty_onkeyup(obj:MaskEdit, e:KeyEventInfo)
{
	if(ds_detail.getColumn(0, "semiGoodsYn") == "Y"){
		msk_investAmt.value = fn_calInvestAmtRecipe(obj.value, ds_detail.getColumn(0, "cookUnitItem"), ds_detail.getColumn(0, "cstAmt"));
	}else{
		msk_investAmt.value = fn_calInvestAmt(obj.value, ds_detail.getColumn(0, "gramPrice"));
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 품목종류(레시피 OR 식자재)에 따른 버튼 활성 비활성
 *---------------------------------------------------------------------------------------------*/
function fn_changePumPop(){
	if(ds_detail.getColumn(0, "semiGoodsYn") == "Y"){
		btn_pumPop.visible = false;
		btn_recipePop.visible = true;
	}else{
		btn_pumPop.visible = true;
		btn_recipePop.visible = false;
	}
}

