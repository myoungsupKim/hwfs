		fsp_callService(this, "", "", "", "ds_all_out=ds_out1","YY=" + wrapQuote(cbo_Yy.value) + " DATA_CLASS=" + wrapQuote(rdo_Data_Class.value) + " TM_CD=" + wrapQuote(edt_Tm.userdata) + " UPJANG=" + wrapQuote(edt_Upjang.userdata) + " ACCT_TYPE=" + wrapQuote(cbo_Acct_type.value) + " ACCT_LVL=" + wrapQuote(v_ACCT_LVL) + " ACCT_CD1=" + wrapQuote(edt_Acct1.userdata) + " ACCT_CD2=" + wrapQuote(edt_Acct2.userdata) + " CHK_TM_CD=" + wrapQuote(chk_TM_CD.value) + " CHK_UPJANG=" + wrapQuote(chk_UPJANG.value) + " CHK_ACCT_CD=" + wrapQuote(v_ACCT_GUBUN) + " CHK_ACCT_TYPE=" + wrapQuote(chk_ACCT_TYPE.value),"fn_afterSearch");
