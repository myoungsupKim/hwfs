	<description><![CDATA[사업장용]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
UPDATE  SCC_SCHEDULE
SET
 GROUP_CODE = #GROUP_CODE#
,REGI_SABUN = #REGI_SABUN#
,SCH_TYPE = #SCH_TYPE#
,SDATE = #SDATE#
,EDATE = #EDATE#
,ALRAM_YN = #ALRAM_YN#
,TITLE = #TITLE#
,CONTENTS = #CONTENTS#
,SCH_CLASS = #SCH_CLASS#
,UUSER = #g_EmpNo#
,UDATE = SYSDATE
WHERE   SCH_ID = #SCH_ID#
	]]></statement>
	<input default-name="ds_list">
	</input>
	<output default-name="ds_out">
	</output>
</query>
