<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[사업장그룹 콤보박스 데이터 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT GRP_SEQ AS CODE
	 , GRP_NAME AS CODE_NAME
 FROM FSA_UPJANG_GRP_MST
 WHERE USE_YN = 'Y'
ORDER BY GRP_NAME ASC	]]></statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_Upjang">
	</output>
</query>
