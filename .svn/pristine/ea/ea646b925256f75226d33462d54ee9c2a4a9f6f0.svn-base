<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM SCC_USER_ROLE
WHERE 
	 SYS_TYPE = #SYS_TYPE#
	 AND SABUN = #SABUN#
	 AND ROLE = #ROLE#	]]></statement>
	<input default-name="ds_UserRole">
		<col name="SYS_TYPE" size="50" type="VARCHAR" description="" /> 
		<col name="SABUN" size="9" type="VARCHAR" description="" /> 
		<col name="ROLE" size="50" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_UserRole">
	</output>
</query>
