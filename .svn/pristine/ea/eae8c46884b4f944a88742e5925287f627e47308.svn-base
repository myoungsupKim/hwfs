function fn_makeHeadData1()
{
	objDs 			= ds_List;
	objPivotHeadDs 	= ds_PivotHead;
	
	objPivotHeadDs.clear();
	objPivotHeadDs.addColumn("PIVOT_CODE","STRING",255);
	objPivotHeadDs.addColumn("PIVOT_TEXT","STRING",255);
	
	objDs.keystring = "S:ORDER_DATE_DISP";
	var sPreKey 	  = "";
	var sCurrCode 	  = "";
	var sCurrText 	  = "";
	var nRow;
	for ( var i = 0; i < objDs.rowcount; i++ )
	{
		sCurrText = gfn_nvl(objDs.getColumn(i,"ORDER_DATE_DISP"));
		sCurrCode = sCurrText.replace("/","").replace("(","").replace(")","");

		if ( sPreKey != sCurrText)
		{
			nRow = objPivotHeadDs.addRow();
			objPivotHeadDs.setColumn(nRow,"PIVOT_CODE",sCurrCode);	
			objPivotHeadDs.setColumn(nRow,"PIVOT_TEXT",sCurrText);
		}
		sPreKey = sCurrText;
	}
	
	return objPivotHeadDs;
}

function fn_makePivotData1(objPivotHeadDs)
{
	objDs 			= ds_List;
	objPivotDs 		= ds_Pivot;
	
	objPivotDs.clear();
	
	//고정컬럼 추가
