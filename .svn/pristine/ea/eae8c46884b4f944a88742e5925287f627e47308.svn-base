			fn_makeCrossTab2();
		}
	}else{
		g_Message("EE", "조회중 오류가 발생했습니다. 관리자에게 문의하세요." + '\n' + '(' + errMsg + ')' );
		
		fn_ClearData();
	}
	//trace(ds_List.saveXML());
