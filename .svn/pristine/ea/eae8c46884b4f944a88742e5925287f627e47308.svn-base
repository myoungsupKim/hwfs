		Contents_grd_head	+= '    <Cell col="'+(i*3+2)+'" colspan="3" style="align:center middle;" displaytype="text" text="'+objPivotHeadDs.getColumn(i,"PIVOT_TEXT")+'"/>' + '\n';
		Contents_grd_head	+= '    <Cell row="1" col="'+(i*3+2)+'" style="align:center middle;" displaytype="text" text="총인원" />' + '\n';
		Contents_grd_head	+= '    <Cell row="1" col="'+(i*3+3)+'" style="align:center middle;" displaytype="text" text="신청인원" />' + '\n';
		Contents_grd_head	+= '    <Cell row="1" col="'+(i*3+4)+'" style="align:center middle;" displaytype="text" text="율" />' + '\n';
	}

	Contents_grd_head	+= '  </Band>' + '\n';

	//
	// Grid body를 작성합니다.
	//
	Contents_grd_body	= '  <Band id="body">' + '\n';
	
	// fix column
 	Contents_grd_body	+= '    <Cell displaytype="text" style="align:right;" expr="expr:currow + 1" />' + '\n';
 	Contents_grd_body	+= '    <Cell col="1" displaytype="text" style="align:left;" text="bind:SC_NM" />' + '\n';
	// pivot column
	for ( i=0; i < nHeadCount; i++ ) {
		Contents_grd_body	+= '    <Cell col="'+(i*3+2)+'" displaytype="number" style="align:right middle;background:expr(fn_SetBgColor(DATE_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'));background2:expr(fn_SetBgColor(DATE_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'));" text="bind:TOT_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'"/>' + '\n';
		Contents_grd_body	+= '    <Cell col="'+(i*3+3)+'" displaytype="number" style="align:right middle;background:expr(fn_SetBgColor(DATE_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'));background2:expr(fn_SetBgColor(DATE_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'));" text="bind:QTY_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'"/>' + '\n';
		Contents_grd_body	+= '    <Cell col="'+(i*3+4)+'" displaytype="text" style="align:right middle;background:expr(fn_SetBgColor(DATE_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'));background2:expr(fn_SetBgColor(DATE_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'));" text="expr:fn_numFormat(TOT_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'==0?100:QTY_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'/TOT_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'*100,1)+&quot;%&quot;"/>' + '\n';
	}
 	Contents_grd_body	+= '  </Band>' + '\n';
	
	//
	// Grid Summary를 작성합니다.
	//
	Contents_grd_summ	= '  <Band id="summary">' + '\n';

	// fix column
	Contents_grd_summ	+= '    <Cell colspan="2" style="align:center middle;" displaytype="text" text="합  계"/>' + '\n';
	// pivot column
	for ( i=0; i < nHeadCount; i++ ) {
		Contents_grd_summ	+= '    <Cell col="'+(i*3+2)+'" displaytype="number" style="align:right middle;font:Gulim,9;padding:0 1 0 1;" text="expr:getSum(&quot;TOT_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'&quot;)"/>' + '\n';
		Contents_grd_summ	+= '    <Cell col="'+(i*3+3)+'" displaytype="number" style="align:right middle;font:Gulim,9;padding:0 1 0 1;" text="expr:getSum(&quot;QTY_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'&quot;)"/>' + '\n';
		Contents_grd_summ	+= '    <Cell col="'+(i*3+4)+'" displaytype="text" style="align:right middle;font:Gulim,9;padding:0 1 0 1;" text="expr:fn_numFormat(getSum(&quot;TOT_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'&quot;)==0?100:getSum(&quot;QTY_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'&quot;)/getSum(&quot;TOT_'+objPivotHeadDs.getColumn(i,"PIVOT_CODE")+'&quot;)*100,1)+&quot;%&quot;"/>' + '\n';
	}
	Contents_grd_summ	+= '  </Band>' + '\n';
	
	Contents_grd_lst	+= Contents_grd_column + Contents_grd_rows + Contents_grd_head + Contents_grd_body + Contents_grd_summ + '</Format>' + '\n' + '</Formats>';
//trace(	Contents_grd_lst );
	objGrid.enableredraw = false;	
	objGrid.formats  = Contents_grd_lst;
 	objGrid.formatid = "Default"; 	
	objGrid.nodataimage = gv_noData;	
	objGrid.binddataset = objPivotBodyDs.name;
	objGrid.enableredraw = true;	
	//objPivotBodyDs.rowposition = -1;
}

function fn_numFormat(sNum, digit) 
{
	// 소숫점 반올림 처리
	if(!gfn_isNull(digit))
	{
		if ( (typeof sNum) == "string" ) {
			sNum = parseFloat(sNum);
		}
		sNum = gfn_round(sNum, digit);
	}
	
    if ( (typeof sNum) != "string" ) {
        sNum = sNum.toString();
    }

    sNum = gfn_trim(sNum);  //string
	var ppos, sDigit, nEnd, nStart=0, sRet="";
	
	if(gfn_isNull(sNum))
	    return "0.0";
	
	if( sNum.charAt(0) == "+" || sNum.charAt(0) == "-" )
	{
		sRet += sNum.charAt(0);
		nStart = 1;
	}
	
	ppos = sNum.indexOf(".",nStart);
	
	if( ppos < 0 )
		nEnd = sNum.length;
	else
		nEnd = ppos;	
			
	// 양수 영역 , 처리
	sDigit = sNum.substr(nStart, nEnd-nStart);
	for( pos = 0 ; pos < sDigit.length ; pos ++ )
	{
		if( pos != 0 && (sDigit.length-pos)%3 == 0 )
			sRet += ",";
		sRet += sDigit.charAt(pos);
	}
	// 소숫점 자리수 더하기
	sRet += sNum.substr(nEnd);

	//자리수 채우기
	if(!gfn_isNull(digit))
	{
		if ( ppos < 0 ) {
			var sPlus   = ".";
			for ( i=digit; i>0; i-- ) {
				sPlus += "0";
			}
		} else {
			var sPlus   = "";
			var nLength = sRet.split(".")[1].length;
 			for ( i=digit-nLength; i>0; i-- ) {
 				sPlus += "0";
 			}
		}
		
		sRet += sPlus;
	}
	
	return sRet;
}]]></Script>
