	objPivotHeadDs.addColumn("PIVOT_TEXT","STRING",255);
	
	objDs.keystring = "S:ORDER_DATE_DISP";
	var sPreKey 	  = "";
	var sCurrCode 	  = "";
	var sCurrText 	  = "";
	var nRow;
	for ( var i = 0; i < objDs.rowcount; i++ )
	{
		sCurrText = gfn_nvl(objDs.getColumn(i,"ORDER_DATE_DISP"));
