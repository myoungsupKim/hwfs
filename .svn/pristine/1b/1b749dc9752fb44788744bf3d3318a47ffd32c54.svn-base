function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if (gfn_isNull(cal_fromDay.fn_getValue()) || gfn_isNull(cal_toDay.fn_getValue()))
	{
		gfn_alert("의뢰일자를 입력하여 주십시오!");
		return;
	}
	
	if (cal_fromDay.fn_getValue() > cal_toDay.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
	
	// Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list1);
    gfn_clearSortMark(grd_detailViewList);

    // Dataset을 초기화한다.
    ds_list1.clearData();
    ds_detailViewList.clearData();
    
    ds_search.setColumn(0, "specmType", ds_list.getColumn(ds_list.rowposition, "code"));
    ds_search.setColumn(0, "insptFldDtl", ds_list.getColumn(ds_list.rowposition, "testGubun"));
    
    var strSvcID    = "selectDetailList";
    var strURL      = "ls/lsm/specmTypeItemInquery/selectList1.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list1=ds_list1";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

