function Combo_onitemchanged(obj:Combo, e:ItemChangeEventInfo)	{
	trace(e.postvalue);
	switch ( obj.name ) {
		case "cbo_0301" :
			ds_model.filter("mauf == '" + e.postvalue + "' || mauf == ''");
			ds_detail.filter("detail == ''");
			cbo_0302.index = 0;
			cbo_0303.index = 0;
			break;
			
		case "cbo_0302" :
			ds_detail.filter("model == '" + e.postvalue + "' || detail == ''");
			cbo_0303.index = 0;
			break;
			
		case "cbo_0303" :
			break;
	}
}

