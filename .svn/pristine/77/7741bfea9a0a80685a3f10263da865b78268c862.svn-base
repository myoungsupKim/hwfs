                       , "X_LRM::TestResultItemSendGwPop.xfdl"			// Url
                       , {fv_Contents:arrParam}			// 배열
                       , null, null, null, null, null, false, true, -1);

    if (typeof rtnVal != "undefined" && rtnVal != null && gfn_getObjkeyLength(rtnVal) > 0) {
		fn_gwDraftPopup(rtnVal);
	}
}

/**
 * 그룹웨어 상신 버튼 클릭 시 상신 팝업을 호출한다.
 * @param obj Button
 * @param e ClickEventInfo
 * @return none
 */
function fn_gwDraftPopup(rtnVal)
{
	fv_retVal = rtnVal;
	
	//그룹웨어에 전달할 파라메터 생성
	var arrParams	= SCGw.newParams ();
	
	//(필수) 결재문서 서비스명 - 문서양식별로 설정된 서비스명 설정
	var gwServiceId = "HWRC_FC_LRM_TEST_ITEM_SEND";
    var gwTitle = "";
    if (ds_testRegList.getColumn(ds_testRegList.rowposition, "materialSendResult") == "S") {	// Q-Stop
		gwTitle = "부적합 제품 사후처리 보고";
	} else {
		gwTitle = "코드중지 제품 해재 보고";
	}
	SCGw.setParams (arrParams, SCGw.GW_SERVICE_NAME, gwServiceId);
	
	//(필수) 본문 URL : (주의) .do로 호출해야 하며 GET 방식으로 파라미터를 전달 (id라는 key는 사용 불가)
	var sBodyUrl = "/ls/lrm/testRegInquery/makeGwDraftContent.do?searchId=" + ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum")
											+ "&acceptNum=" + ds_testRegList.getColumn(ds_testRegList.rowposition, "acceptNum")
											+ "&insptPurp=" + rtnVal.insptPurp
                                            + "&insptMtod=" + rtnVal.insptMtod
                                            + "&materialSendResult=" + ds_testRegList.getColumn(ds_testRegList.rowposition, "materialSendResult")	// S:Q-Stop, P:Q-Pass
                                            + "&materialUpjangSendResult=" + ds_testRegList.getColumn(ds_testRegList.rowposition, "materialUpjangSendResult")	// 101:직영, 102:식재, 103:전체
											; 
	SCGw.setParams (arrParams, SCGw.GW_BODY_URL, sBodyUrl);

	//(필수) 본문 제목 – 문서양식 별로 설정된 업무명
	SCGw.setParams (arrParams, SCGw.GW_BODY_TITLE, gwTitle);

	//(필수)문서 방향 : 가로 또는 세로 방향 (GW_VERTICAL, GW_HORIZONTAL)
	SCGw.setParams (arrParams, SCGw.GW_PORTRAIT, SCGw.VERTICAL);
	
	fv_attachList = [];
	SCGw.setParams (arrParams, SCGw.GW_ATTACH_LIST, gfn_jsonObjectToString(fv_attachList));
	
	//(선택)Client Id : 결재 instance id와 연관을 맺기위한 업무별 ID로 unique key를 이용, 생략하면 상신시각이 됨(최대 40 byte)
	//SCGw.setParams (arrParams, SCGw.GW_CLIENT_ID, edt_id.value);

	//그룹웨어 상신 팝업을 호출한다.
	SCGw.sendDraft (this, arrParams);
}

/**
 * 그룹웨어 상신 처리 후 반환되는 Instance Id를 처리한다. (업무 테이블에 update) 
 * (중요) 이 함수는 SCGw에서 호출되므로 필히 선언되어야 한다.
 * @param instanceId
 * @return none
 */
function scgw_ondraftnotify(instanceId)
{
    ds_testRegList.setColumn(ds_testRegList.rowposition, "signInstanceId", instanceId);	 //결재InstanceId 설정
	ds_testRegList.setColumn(ds_testRegList.rowposition, "signProgressStatus", SCGw.GW_DRAFT_STATUS);	 //결재진행상태를 상신으로 설정

	ds_testRegList.updatecontrol = false;
	ds_testRegList.setRowType(ds_testRegList.rowposition, Dataset.ROWTYPE_NORMAL);
	ds_testRegList.updatecontrol = true;

	fn_testResultDraft();	//결재정보 저장
}

/**
 * 전자결재 결과 정보를 업무테이블에 저장 한다.
 * @param 
 * @return 
 */
function fn_testResultDraft()
{
	ds_search.setColumn(0, "reqNum", ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum"));
	ds_search.setColumn(0, "acceptNum", ds_testRegList.getColumn(ds_testRegList.rowposition, "acceptNum"));
    ds_search.setColumn(0, "signInstanceId", ds_testRegList.getColumn(ds_testRegList.rowposition, "signInstanceId"));
    
    ds_search.setColumn(0, "insptPurp", fv_retVal.insptPurp);
    ds_search.setColumn(0, "insptMtod", fv_retVal.insptMtod);
