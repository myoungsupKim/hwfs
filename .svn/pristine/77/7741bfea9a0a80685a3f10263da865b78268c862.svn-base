 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (ds_testRegList.rowposition == -1) {
		gfn_alert("전사구매 전송할 데이터를 선택하여 주십시오!");
		return;
	}
	
	var nRowIdx = ds_testRegList.rowposition;
	if (ds_testRegList.getColumn(nRowIdx, "itemUns") != "UP") {
		gfn_alert("불용 자재는 변경할 수 없습니다.");
		return;
	}
	if ( ds_testRegList.getColumn(nRowIdx, "transYn") == "N" ) {
		gfn_alert("전송불가 검체가 존재합니다.(공통코드:부적합분류[NOT_FIT_CLSFI] 적용)");
		return;
	}
	
	if (!gfn_confirm("결재 상신 후 자동으로 전사구매쪽으로 데이터가 전송 됩니다.\n\n진행 하시겠습니까?")) return false;
	
	// 결재완료 후 수정된 데이터 상태가 풀린다.
	// 임의의 DataSet에 저장 후 상태를 Update로 변경 한다.
	// 결재 완료 후 ds_testRegListTmp DataSet을 전송 한다.
	ds_testRegListTmp.copyData(ds_testRegList);
    ds_testRegListTmp.updatecontrol = false;
	ds_testRegListTmp.setRowType(nRowIdx, Dataset.ROWTYPE_UPDATE);
	ds_testRegListTmp.updatecontrol = true;
	
	// 결재 상신
	fn_gwDraftAction();
}

