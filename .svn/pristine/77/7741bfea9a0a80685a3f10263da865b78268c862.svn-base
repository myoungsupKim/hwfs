function fn_gwDraftAction()
{
	// 검사목적과 해지사유를 입력하는 Popup을 호출 한다.
    var arrParam = new Array();
    arrParam[0] = ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum");
