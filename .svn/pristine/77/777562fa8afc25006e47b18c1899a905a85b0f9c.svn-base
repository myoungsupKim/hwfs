	ds_cbo_item_classD41.filter("gfn_left(CLASS_CODE,6)=="+wrapQuote(gfn_left(e.postvalue,6))+"||CLASS_CODE==''");
	
	ds_cbo_item_classD41.enableevent=true;
	cbo_item_classD.enableredraw=true;
}

