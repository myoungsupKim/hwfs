// 체크박스 클릭시 색상변경
function fn_CheckboxClick(obj)
{
	obj.enableredraw=false;

	if(obj.value == 1)
	{obj.style.background.color="#FFFFBB";}
	else
	{obj.style.background.color="#dcf7f9ff";}

    obj.enableredraw=true;
}


