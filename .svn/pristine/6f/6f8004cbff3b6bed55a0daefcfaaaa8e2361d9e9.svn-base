/******** B ********/
Button
{
	background		: #f8f8f8;
	border			: 1 solid #9a9a9a;
	bordertype		: round 3 3;
	font			: bold antialias 19 Droid Sans;
	color			: #555555;
	align			: center middle;
	cursor			: hand;
	padding			: ;
	shadow			: 0 0;
}
Button:pushed
{
	background		: #f0f0f0;
	border			: 1 solid #f57f50;
	color			: #f57f50;
}
Button:disabled
{
	opacity			: 40;
}


/******** C ********/
Calendar
{
	align			: left middle;
	background		: #ffffff;
	border			: 1 solid #9a9a9a;
	bordertype		: round 3 3;
	buttonsize		: 45;
	color			: #555555;
	font			: bold antialias 17 Droid Sans;
	
	daybackground		: #ffffff;
	dayborder		: 0 none;
	daycolor		: #555555;
	dayfont			: bold antialias 27 Droid Sans;
	daysize			: 71 71;

	popupbackground		: #ffffff;
	popupborder		: 1 solid #c0c0c0;
	popupsize		: 500 580;
}
Calendar:mouseover, Calendar:selected, Calendar:focused
{
	daybackground		: #f57f50;
	dayborder		: 0 none;
