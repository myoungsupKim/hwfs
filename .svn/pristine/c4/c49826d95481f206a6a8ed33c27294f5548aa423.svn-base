************************************************************************************************/
include "lib::lib_com.xjs";


/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_sToday = gfn_today();
var fv_muCd = "";
var fv_tmCd = "";
var fv_tmNm = "";
var fv_ccCd = "";

var invoiceItemCnt = 1;		//품목정보에 사용


/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "";
	
	//Dataset 초기화
	var nRow = ds_taxShetMst.addRow();
	ds_amt.addRow();			//공급가액
	ds_tax.addRow();			//세액
	
	//부가세코드 검색
	fn_selectVatCdList();
	
	//Combo
	SCCombo.bind(this, "ComCode", "rdo_publishClass", ds_publishClass, {groupCode:"PUBLISH_CLASS", useYn:"Y", defVal:"1"}); 				//발행구분
	SCCombo.bind(this, "ComCode", "cbo_rcptBillingClass", ds_rcptBillingClass, {groupCode:"RCPT_BILLING_CLASS", useYn:"Y", defVal:"02"}); 	//영수/청구구분
	SCCombo.bind(this, "ComCode", "cbo_taxShetKind", ds_taxShetKind, {groupCode:"TAX_SHET_KIND", useYn:"Y", set1:"1", defVal:"01"}); 				//세금계산서종류
	SCCombo.bind(this, "ComCode", "cbo_taxShetClsfi", ds_taxShetClsfi, {groupCode:"TAX_SHET_CLSFI", useYn:"Y", defVal:"01"}); 				//세금계산서분류

	//Popup
	//SCPopup.bind(this, "CcCode", "edt_ccNm", "btn_cc",	null, {}, {after:"fn_popCcCdAfter"}); 					//업장(CC) 팝업
	
	//Calendar
	cal_publishDate.fn_initMonthCal(this, fv_sToday, "", ds_taxShetMst, "publishDate", "Y");						//발행일자
	cal_fiscalDate.fn_initMonthCal(this, fv_sToday, "fn_fiscalDateAfter", ds_taxShetMst, "fiscalDate", "Y");		//세금계산서일자
	
	//첨부파일
	//ObjAtchFile.setSingleFile(true);
	ObjAtchFile.setDataset("ds_file");	//DataSet연결
	ObjAtchFile.setExtention("xml");
	
	//첨부파일 버튼제어(발행구분 정발행 -> 비활성)
	btn_fileAdd.enable = false;
	
	//작성일자 오늘일자로 셋팅
	edt_writeYyyy.value = fv_sToday.substring(0, 4);
	edt_writeMm.value = fv_sToday.substring(4, 6);
	edt_writeDd.value = fv_sToday.substring(6, 8);
	
	//발행구분 값 셋팅
	ds_taxShetMst.setColumn(0, "publishClass", rdo_publishClass.value);	//1:순발행 2:역발행
	
	//파라미터 변수
	fv_muCd = g_Mu_Cd;
	fv_tmCd = g_Tm_Cd;
	fv_tmNm = g_Tm_Nm;
	fv_ccCd = g_Cc_Cd;

	
	//사업부, 부서(팀), CC 설정
	TmSearchComponent01.applyAuthLevel(MuSearchComponent01);
	UpjangSearchComponent01.setForcedRoleSet("CostCenter");
	UpjangSearchComponent01.applyAuthLevel(TmSearchComponent01);
	UpjangSearchComponent00.applyAuthLevel(TmSearchComponent01);    //업장	
	//TmSearchComponent01.setApplyAuth(false);
	//UpjangSearchComponent00.setApplyAuth(false);
	//초기값 세팅
	fn_clearSearch();	
}

/*----------------------------------------------------------------------------------------------
 * 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_clearSearch(){

	MuSearchComponent01.setMuCode(g_Mu_Cd);
	ds_taxShetMst.setColumn(0, "muCd", g_Mu_Cd);	//사업부코드
	ds_taxShetMst.setColumn(0, "tmCd", g_Tm_Cd);	//부서(팀)코드
	ds_taxShetMst.setColumn(0, "tmNm", g_Tm_Nm);	//부서(팀)명
	ds_taxShetMst.setColumn(0, "ccCd", g_Cc_Cd);	//업장(CC)코드
	ds_taxShetMst.setColumn(0, "ccNm", g_Cc_Nm);	//업장(CC)명

	if (ds_taxShetMst.getColumn(0, "muCd") == "2002" && g_Cc_Cd == g_Cc_Cd){
		ds_taxShetMst.setColumn(0, "upjangCd", g_Upjang);	//업장코드
		ds_taxShetMst.setColumn(0, "upjangNm", g_UpjangNm);	//업장명						
	}else{
		ds_taxShetMst.setColumn(0, "upjangCd", "");	//업장코드
		ds_taxShetMst.setColumn(0, "upjangNm", "");	//업장명		
	}
	
	if(!gfn_isNull(ds_taxShetMst.getColumn(0, "ccCd"))){
		ds_taxShetMst.setColumn(0, "newhghtUpjang", ds_taxShetMst.getColumn(0, "ccCd"));			//신고사업장코드
		ds_taxShetMst.setColumn(0, "newhghtUpjangNm", ds_taxShetMst.getColumn(0, "ccNm"));	//신고사업장명
		//신고사업장 정보 검색
		//fn_selectUpjangAndCustInfo("upjang");
	}
	//CC코드에 종사업장 번호 조회
	fn_searchCcSubUpjang(g_Cc_Cd);

	if(fv_muCd == "2004"){
		ds_taxShetMst.setColumn(0, "vatCd", "10");						//부가세구분선택(Default:과세매출일반)
		ds_taxShetMst.setColumn(0, "rcptBillingClass", "02");			//영수/청구 구분(Default:청구)
		comp_upjang.visible=true;
		stc_upjangTitle.text="본사";
		stc_subUpjangTitle.text="하위 업장";
		comp_mainUpjang.visible=true;
		UpjangSearchComponent00.visible=false;
		schCust.enable=false;
	}else if(fv_muCd == "2005"){	//구매 물류 추가
		ds_taxShetMst.setColumn(0, "vatCd", "10");						//부가세구분선택(Default:과세매출일반)
		ds_taxShetMst.setColumn(0, "rcptBillingClass", "02");			//영수/청구 구분(Default:청구)	
	}else{
		if(fv_muCd == "2003"){	//외식은 업장 안보이게 처리
			comp_mainUpjang.visible=false;
			UpjangSearchComponent00.visible=false;
			stc_upjangTitle.text="";
		}
		ds_taxShetMst.setColumn(0, "vatCd", "20");						//부가세구분선택(Default:과세매출일반)
		ds_taxShetMst.setColumn(0, "rcptBillingClass", "01");			//영수/청구 구분(Default:영수)
	}
	
	//한화 본사 정보 조회
	fn_searchHanwha();
	ds_taxShetMst.setColumn(0, "smsDispatchYn", "Y");		//SMS전송여부(Default:예)
	ds_taxShetMst.setColumn(0, "taxShetClsfi", "01");		//세금계산서분류(Default:일반)
}

/*----------------------------------------------------------------------------------------------
 * 설명      : CC의 팝업을 호출하기 전에 발생되는 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_upjangBeforeEvent(obj, cls)
{
	cls.setTmCd(TmSearchComponent01.getTmCode());
	return true;
}

function fn_fmsUpjangAfterEvent(obj, cls){
	if(rdo_publishClass.value == "1"){		//정발행일때만 다시 셋팅
		if((!gfn_isNull(cls.custcd)) && (cls.custcd != "0")){
			ds_taxShetMst.setColumn(0,"custCd", cls.custcd);
			ds_taxShetMst.setColumn(0,"custNm", cls.custnm);
			fn_searchCust(cls.custcd);
		}
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명      : 부서부 변경시 부서, CC 초기화 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_tmCd_changed(){
    ds_taxShetMst.setColumn(0, "ccCd",  "");
    ds_taxShetMst.setColumn(0, "ccNm",  "");
    ds_taxShetMst.setColumn(0, "upjangCd",  "");
    ds_taxShetMst.setColumn(0, "upjangNm",  "");
}

function fn_ccCd_changed(obj, cls)
{
	ds_taxShetMst.setColumn(0, "newhghtUpjang", cls.ccCd);			//신고사업장코드
	ds_taxShetMst.setColumn(0, "newhghtUpjangNm", cls.ccNm);	//신고사업장명

	if(!gfn_isNull(cls.ccCd)){
		fn_searchCcSubUpjang(cls.ccCd);
	}else{
		ds_taxShetMst.setColumn(0,"upjangCd", "");
		ds_taxShetMst.setColumn(0,"upjangNm", "");	
	}
	return true;
}
/*----------------------------------------------------------------------------------------------
 * 설명      : 업장의 팝업을 호출한 후에 발생되는 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_upjangAfterEvent(obj, cls)
{
	if(rdo_publishClass.value == "1"){		//정발행일때만 다시 셋팅
		ds_taxShetMst.setColumn(0,"custCd", "");
		ds_taxShetMst.setColumn(0,"custNm", "");
		//ds_taxShetDtl1.clearData();
		//ds_taxShetDtl2.clearData();
		//ds_taxShetDtl3.clearData();
		//ds_taxShetDtl4.clearData();
		
		if((!gfn_isNull(cls.custcd)) && (cls.custcd != "0")){
			ds_taxShetMst.setColumn(0,"custCd", cls.custcd);
			ds_taxShetMst.setColumn(0,"custNm", cls.custnm);
			fn_searchCust(cls.custcd);
		}
		
		if(ds_taxShetMst.getColumn(0, "muCd") == "2004"){
			comp_upjang.setUpjangCode("");
			comp_upjang.setUpjangName("");
		}
	}	
	
	fn_ccCd_changed(obj, cls);	//종사업장 조회
	
	if(!gfn_isNull(cls.tmCd)){
		ds_taxShetMst.setColumn(0,"tmCd", cls.tmCd);
		ds_taxShetMst.setColumn(0,"tmNm", cls.tmNm);
	}
	ds_taxShetMst.setColumn(0,"ccCd", cls.ccCd);
	ds_taxShetMst.setColumn(0,"ccNm", cls.ccNm);
	
	return true;
}
function fn_setCcCd(p){
    //FS사업부인경우 2002 : ScoUpjangCust
	if(MuSearchComponent01.getMuCode() == "2002"){
		if(gfn_isNull(ds_taxShetMst.getColumn(0,"upjangCd"))){
			alert("업장을 선택해 주세요.");
			return;
		}	
		p.setQid("ScoUpjangCust");
		p.setUpjang(ds_taxShetMst.getColumn(0,"upjangCd"));
	}else{
		p.setQid("StCust");
		p.setUpjang("");
	}
	
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 본사 선택 후 팀 사업장 호출 시 인자값 전달
 * o : object, c : name(comp id) 값(1개의 form에 upjangsearch 가 여러개일 경우 사용)
 *---------------------------------------------------------------------------------------------*/
function fn_fmsUpjangBeforeEvent(o, c)
{
	var v_mainUpjang = "";

	v_mainUpjang = gfn_transNullToEmpty(ds_taxShetMst.getColumn(ds_taxShetMst.rowposition, "upjangCd"));		// 본사코드
	if(gfn_isNull(v_mainUpjang)){
		alert("본사를 선택해 주세요.");
		return;
	}	
	c.setMainUpjang(v_mainUpjang);
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 세금계산서 일자 변경 After 함수
 *---------------------------------------------------------------------------------------------*/
function fn_fiscalDateAfter(){
	var fiscalDate = ds_taxShetMst.getColumn(0, "fiscalDate");
	edt_writeYyyy.value = fiscalDate.substr(0, 4);
	edt_writeMm.value = fiscalDate.substr(4, 2);
	edt_writeDd.value = fiscalDate.substr(6, 2);
		
	//한화 본사 정보 조회
	fn_searchHanwha();
}

