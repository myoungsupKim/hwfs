function msk_totAmt_canchange(obj:MaskEdit,  e:ChangeEventInfo)
{
	
	if(cbo_vatCd.value=="20" ) { //과세매출기타
		edt_supplyAmt.value = Math.round(Number(msk_totAmt.value) / 1.1);
		msk_vatAmt.value = Number(msk_totAmt.value) - Math.round(Number(msk_totAmt.value) / 1.1);
	} else if (cbo_vatCd.value=="43"){ //면세매출기타
		edt_supplyAmt.value = msk_totAmt.value;
		msk_vatAmt.value = 0;
	} else if (cbo_vatCd.value=="13") { //영세매출일반
		edt_supplyAmt.value = msk_totAmt.value;
		msk_vatAmt.value = 0;	
	} else {
		alert("정의된 부가세 구분이 없습니다.(부가세구분에서 과세기타/면세기타 만 선택 가능)");
		return;
	}
	
	fn_setTaxShetDtl();
	
}

function fn_setTaxShetDtl()
{
	var yyyymmdd = cal_publishDate.text;
	var mm = yyyymmdd.substring(4, 6);
	var dd = yyyymmdd.substring(6, 8);

	ds_taxShetDtl1.clearData();
	ds_taxShetDtl1.addRow();
	ds_taxShetDtl1.setColumn(0, "itemSeq", "1");
	ds_taxShetDtl1.setColumn(0, "itemSupplyDate",yyyymmdd);
	ds_taxShetDtl1.setColumn(0, "itemSupplyDateMM", mm);
	ds_taxShetDtl1.setColumn(0, "itemSupplyDateDD", dd);		
	ds_taxShetDtl1.setColumn(0, "itemSupplyAmt", edt_supplyAmt.value);
	ds_taxShetDtl1.setColumn(0, "itemVatAmt", msk_vatAmt.value);
	ds_taxShetDtl1.setColumn(0, "itemApplyAmt", msk_totAmt.value);
	
	fn_calTotalAmtAndTax();
	
	edt_itemNm1.setFocus();
}
