/*----------------------------------------------------------------------------------------------
 * 설명      : 파일 추가(역발행 시 사용)
 *---------------------------------------------------------------------------------------------*/
function btn_fileAdd_onclick(obj:Button,  e:ClickEventInfo)
{
	if(!gfn_isNull(edt_filePath.value)){
		//Dataset 초기화
		ds_taxShetMst.clearData();
		ds_taxShetDtl1.clearData();
		ds_taxShetDtl2.clearData();
		ds_taxShetDtl3.clearData();
		ds_taxShetDtl4.clearData();
		ds_taxLineItem.clearData();
		ds_amt.clearData();
		ds_tax.clearData();
		
		ds_taxShetMst.addRow();
		ds_amt.addRow();
		ds_tax.addRow();
		
		ds_taxShetMst.setColumn(0, "publishDate", fv_sToday);
		ds_taxShetMst.setColumn(0, "fiscalDate", fv_sToday);
		ds_taxShetMst.setColumn(0, "publishClass", obj.value);
		ds_taxShetMst.setColumn(0, "taxShetKind", "01");
		
		//파일 ds 초기화
		ds_file.clearData();
		
		//파라미터 조건 초기화
		fn_clearSearch();
	}
	
	
	ObjAtchFile.btn_addFile.click();
	if(!ObjAtchFile.isAtchFile()) return;
	
	var path = ds_file.getColumn(0, "orgnlPath");
	edt_filePath.value = path;
	var file = new VirtualFile(path, VirtualFile.openRead | VirtualFile.openText);
	
	var text = file.read(0, "utf-8");
	file.close();
	
	//xml데이터 Dataset에 저장(DB저장위해)
	ds_taxShetMst.setColumn(0, "taxShetXmlData", text);
	
	// Parsing
	var domDoc;
	var domPar = new DomParser;
	domDoc = domPar.parseFromString(text);
	
	invoiceItemCnt = 0;
	
	//데이터 매핑
	getChildNode(domDoc, 1);
	
	//신고사업장 & 고객코드 조회
	//fn_selectUpjangAndCustInfo("upjang");
	fn_selectUpjangAndCustInfo("cust");
	
//	ds_taxShetDtl1.clearData();
//	ds_taxShetDtl2.clearData();
//	ds_taxShetDtl3.clearData();
//	ds_taxShetDtl4.clearData();
	
	//파싱한 라인 데이터 화면의 데이터셋에 매핑
	for(var i=0; i<ds_taxLineItem.rowcount; i++){
		if(i == 0){
			ds_taxShetDtl1.addRow();
			ds_taxShetDtl1.copyRow(0, ds_taxLineItem, i);
			ds_taxShetDtl1.setColumn(0, "itemSeq", "1");
		}else if(i == 1){
			ds_taxShetDtl2.addRow();
			ds_taxShetDtl2.copyRow(0, ds_taxLineItem, i);
			ds_taxShetDtl2.setColumn(0, "itemSeq", "2");
		}else if(i == 2){
			ds_taxShetDtl3.addRow();
			ds_taxShetDtl3.copyRow(0, ds_taxLineItem, i);
			ds_taxShetDtl3.setColumn(0, "itemSeq", "3");
		}else if(i == 3){
			ds_taxShetDtl4.addRow();
			ds_taxShetDtl4.copyRow(0, ds_taxLineItem, i);
			ds_taxShetDtl4.setColumn(0, "itemSeq", "4");
		}
	}
	
	//공급가액 / 세액 계산
	fn_calTotalAmtAndTax();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : xml 파싱
 *---------------------------------------------------------------------------------------------*/
function getChildNode(pElement, nodeDepth){

	if(pElement.childNodes.length == 0){
		return "";
	}
 
 	var domElemenet, domElemenet2;	
 	
 	//라인아이템 Dataset 로우추가
	if(pElement.nodeName.trim() == "TaxInvoiceTradeLineItem"){
		ds_taxLineItem.addRow();
	}

	for(var i=0; i<pElement.childNodes.length; i++){
		domElemenet = pElement.childNodes[i];
		
		//승인정보
		if(pElement.nodeName.trim() == "TaxInvoiceDocument"){
			if(domElemenet.nodeName.trim() == "IssueID"){
				ds_taxShetMst.setColumn(0, "taxShetApprvNum", domElemenet.textContent);
				ds_taxShetMst.setColumn(0, "revrsPublishTaxShetNum", ds_taxShetMst.getColumn(0, "taxShetApprvNum"));
				//alert(ds_taxShetMst.getColumn(0, "revrsPublishTaxShetNum"));
				edt_revrsPublishTaxShetNum.value = ds_taxShetMst.getColumn(0, "revrsPublishTaxShetNum");
			}
			//역발행 세금계산서 종류
			if(domElemenet.nodeName.trim() == "TypeCode"){
				ds_taxShetMst.setColumn(0, "taxShetKind", domElemenet.textContent.substring(0,2));
				ds_taxShetMst.setColumn(0, "taxShetClsfi", domElemenet.textContent.substring(2,4));
				if(domElemenet.textContent.substring(0,2) == "01"){
					if(fv_muCd == "2004"){
						ds_taxShetMst.setColumn(0, "vatCd", "10");						
					}else{
						ds_taxShetMst.setColumn(0, "vatCd", "20");											
					}
				}else if(domElemenet.textContent.substring(0,2) == "03"){
					if(fv_muCd == "2004"){
						ds_taxShetMst.setColumn(0, "vatCd", "40");						
					}else{
						ds_taxShetMst.setColumn(0, "vatCd", "43");										
					}				
				}
			}				
			if(domElemenet.nodeName.trim() == "IssueDateTime"){
				ds_taxShetMst.setColumn(0, "publishDate", domElemenet.textContent);
				ds_taxShetMst.setColumn(0, "fiscalDate", domElemenet.textContent);
				fn_fiscalDateAfter();
			}
		}	
		
		//공급받는자
		if(pElement.nodeName.trim() == "InvoicerParty"){
			if(domElemenet.nodeName.trim() == "ID"){
				ds_taxShetMst.setColumn(0, "supplierBizNo", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "SpecifiedOrganization"){
				var subElement = domElemenet.childNodes[0];
				ds_taxShetMst.setColumn(0, "supplierSubUpjangNum", subElement.textContent);
			}
			if(domElemenet.nodeName.trim() == "NameText"){
				ds_taxShetMst.setColumn(0, "supplierBizNm", domElemenet.textContent);
			}
