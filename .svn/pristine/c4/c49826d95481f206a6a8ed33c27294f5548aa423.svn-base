			}
			if(domElemenet.nodeName.trim() == "SpecifiedAddress"){
				var subElement = domElemenet.childNodes[0];
				ds_taxShetMst.setColumn(0, "supplierAddr", subElement.textContent);
			}
			if(domElemenet.nodeName.trim() == "TypeCode"){
				ds_taxShetMst.setColumn(0, "supplierBizType", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "ClassificationCode"){
				ds_taxShetMst.setColumn(0, "supplierBizCat", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "PrimaryDefinedContact"){
				var subElement = domElemenet.childNodes[0];
				ds_taxShetMst.setColumn(0, "supplierEmail", subElement.textContent);
			}
		}
		
		//공급자
		if(pElement.nodeName.trim() == "InvoiceeParty"){
			if(domElemenet.nodeName.trim() == "ID"){
				ds_taxShetMst.setColumn(0, "rcvrBizNo", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "SpecifiedOrganization"){
				var subElement = domElemenet.childNodes[0];
				ds_taxShetMst.setColumn(0, "rcvrSubUpjangNum", subElement.textContent);
			}
			if(domElemenet.nodeName.trim() == "NameText"){
				ds_taxShetMst.setColumn(0, "rcvrBizNm", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "SpecifiedPerson"){
				var subElement = domElemenet.childNodes[0];
				ds_taxShetMst.setColumn(0, "rcvrCeonm", subElement.textContent);
			}
			if(domElemenet.nodeName.trim() == "SpecifiedAddress"){
				var subElement = domElemenet.childNodes[0];
				ds_taxShetMst.setColumn(0, "rcvrAddr", subElement.textContent);
			}
			if(domElemenet.nodeName.trim() == "TypeCode"){
				ds_taxShetMst.setColumn(0, "rcvrBizType", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "ClassificationCode"){
				ds_taxShetMst.setColumn(0, "rcvrBizCat", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "PrimaryDefinedContact"){
				var subElement = domElemenet.childNodes[0];
				ds_taxShetMst.setColumn(0, "rcvrEmail", subElement.textContent);
			}
		}
		
		//Total 공급가액 / 세액
		if(pElement.nodeName.trim() == "SpecifiedMonetarySummation"){
			if(domElemenet.nodeName.trim() == "ChargeTotalAmount"){
				ds_taxShetMst.setColumn(0, "supplyAmt", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "TaxTotalAmount"){
				ds_taxShetMst.setColumn(0, "vatAmt", domElemenet.textContent);
			}
			//면세일경우는 표시 없음
			if(ds_taxShetMst.getColumn(0, "vatCd") =="40" ||  ds_taxShetMst.getColumn(0, "vatCd") =="43"){
				ds_taxShetMst.setColumn(0, "vatAmt", "0");			
			}
		}
		
		//내역리스트
		if(pElement.nodeName.trim() == "TaxInvoiceTradeLineItem"){
			if(domElemenet.nodeName.trim() == "InvoiceAmount"){
				ds_taxLineItem.setColumn(invoiceItemCnt, "itemSupplyAmt", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "NameText"){
				ds_taxLineItem.setColumn(invoiceItemCnt, "itemNm", domElemenet.textContent);
			}
			if(domElemenet.nodeName.trim() == "PurchaseExpiryDateTime"){
				var yyyymmdd = domElemenet.textContent;
				var mm = yyyymmdd.substring(4, 6);
				var dd = yyyymmdd.substring(6, 8);
				ds_taxLineItem.setColumn(invoiceItemCnt, "itemSupplyDateMM", mm);
				ds_taxLineItem.setColumn(invoiceItemCnt, "itemSupplyDateDD", dd);
			}
			if(domElemenet.nodeName.trim() == "TotalTax"){
				var subElement = domElemenet.childNodes[0];
				ds_taxLineItem.setColumn(invoiceItemCnt, "itemVatAmt", subElement.textContent);
			}
		}
		
		if(domElemenet.childNodes.length > 0){
			//자식노드 존재시 함수 호출(재귀함수)
			getChildNode(domElemenet, nodeDepth+1);
		}
	}
	
	//내역 카운트 증가
	if(pElement.nodeName.trim() == "TaxInvoiceTradeLineItem"){
		invoiceItemCnt++;
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 공급가액 / 세액 계산 함수
 *---------------------------------------------------------------------------------------------*/
function fn_calTotalAmtAndTax(){
	//순발행일경우 라인데이터 합산
	if(rdo_publishClass.value == "1"){
		var amt1 = ds_taxShetDtl1.getColumn(0, "itemSupplyAmt") == null ? 0 : ds_taxShetDtl1.getColumn(0, "itemSupplyAmt");
		var amt2 = ds_taxShetDtl2.getColumn(0, "itemSupplyAmt") == null ? 0 : ds_taxShetDtl2.getColumn(0, "itemSupplyAmt");
		var amt3 = ds_taxShetDtl3.getColumn(0, "itemSupplyAmt") == null ? 0 : ds_taxShetDtl3.getColumn(0, "itemSupplyAmt");
		var amt4 = ds_taxShetDtl4.getColumn(0, "itemSupplyAmt") == null ? 0 : ds_taxShetDtl4.getColumn(0, "itemSupplyAmt");
		                                                                        
		var vat1 = ds_taxShetDtl1.getColumn(0, "itemVatAmt") == null ? 0 : ds_taxShetDtl1.getColumn(0, "itemVatAmt");
		var vat2 = ds_taxShetDtl2.getColumn(0, "itemVatAmt") == null ? 0 : ds_taxShetDtl2.getColumn(0, "itemVatAmt");
		var vat3 = ds_taxShetDtl3.getColumn(0, "itemVatAmt") == null ? 0 : ds_taxShetDtl3.getColumn(0, "itemVatAmt");
		var vat4 = ds_taxShetDtl4.getColumn(0, "itemVatAmt") == null ? 0 : ds_taxShetDtl4.getColumn(0, "itemVatAmt");
		                                                                      
		ds_taxShetMst.setColumn(0, "supplyAmt", Number(amt1)+Number(amt2)+Number(amt3)+Number(amt4));
		ds_taxShetMst.setColumn(0, "vatAmt", Number(vat1)+Number(vat2)+Number(vat3)+Number(vat4));
	}                                       

	var totalAmt = ds_taxShetMst.getColumn(0, "supplyAmt");
	var totalTax = ds_taxShetMst.getColumn(0, "vatAmt");
	
// 	ds_taxShetMst.setColumn(0, "supplyAmt", totalAmt);
// 	ds_taxShetMst.setColumn(0, "vatAmt", totalTax);
	ds_taxShetMst.setColumn(0, "totAmt", Number(totalAmt)+Number(totalTax));
	
	//화면 공급가액, 세액 Dataset 초기화
	ds_amt.clearData();
	ds_tax.clearData();
	ds_amt.addRow();
	ds_tax.addRow();
	
	//공급가액
	for(var i=1; i<=totalAmt.toString().length; i++){
		var rslt = 0;
		if(i == 1){
			rslt = totalAmt.toString().substring(totalAmt.toString().length-1, totalAmt.toString().length);
		}else{
			rslt = totalAmt.toString().substring(totalAmt.toString().length-i, totalAmt.toString().length-i+1);
		}
		ds_amt.setColumn(0, "amt"+i, rslt);
	}
	
	//세액
	for(var i=1; i<=totalTax.toString().length; i++){
		var rslt = 0;
		if(i == 1){
			rslt = totalTax.toString().substring(totalTax.toString().length-1, totalTax.toString().length);
		}else{
			rslt = totalTax.toString().substring(totalTax.toString().length-i, totalTax.toString().length-i+1);
		}
		ds_tax.setColumn(0, "tax"+i, rslt);
		
	}
	
}

