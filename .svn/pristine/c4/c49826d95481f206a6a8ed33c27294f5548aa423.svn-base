}

/*----------------------------------------------------------------------------------------------
 * 신고사업장 & 고객정보 조회 콜백함수
 *---------------------------------------------------------------------------------------------*/
function fn_callBackUpjangAndCust(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	if(ds_upjang != null && ds_upjang.rowcount > 0){
		if(strSvcID == "upjangSearch_upjang"){
			ds_taxShetMst.setColumn(0, "newhghtUpjang", ds_upjang.getColumn(0, "upjangCode"));
			ds_taxShetMst.setColumn(0, "newhghtUpjangNm", ds_upjang.getColumn(0, "upjangName"));
			
			if(rdo_publishClass.value == "1"){
				//var saupno = ds_upjang.getColumn(0, "saupno");
				ds_taxShetMst.setColumn(0, "supplierSubUpjangNum", ds_upjang.getColumn(0, "subUpjangNum"));
			//	ds_taxShetMst.setColumn(0, "supplierBizNo", saupno.substring(0, 10));
			//	ds_taxShetMst.setColumn(0, "supplierBizNm", ds_upjang.getColumn(0, "upjangNm"));
			//	ds_taxShetMst.setColumn(0, "supplierCeonm", ds_upjang.getColumn(0, "drname"));
			//	ds_taxShetMst.setColumn(0, "supplierAddr", ds_upjang.getColumn(0, "addr"));
			//	ds_taxShetMst.setColumn(0, "supplierBizType", ds_upjang.getColumn(0, "upTae"));
			//	ds_taxShetMst.setColumn(0, "supplierBizCat", ds_upjang.getColumn(0, "upJong"));
			}
			
			fn_selectInchargeInfo(ds_upjang.getColumn(0, "upjangCode"), "");
		}else{
			ds_taxShetMst.setColumn(0, "custCd", ds_upjang.getColumn(0, "custCd"));
			ds_taxShetMst.setColumn(0, "custNm", ds_upjang.getColumn(0, "custNm"));
			
			fn_selectInchargeInfo("", ds_upjang.getColumn(0, "custCd"));
		}
	}else{
		//신고사업장 또는 고객 정보가 없을 시 선택할 수 있게 팝업 버튼 활성화
		if(strSvcID == "upjangSearch_upjang"){
			edt_newhghtUpjang.enable = true;
			edt_newhghtUpjangNm.enable = true;
			schUpjang.enable = true;
		}else{
			edt_custCd.enable = true;
			edt_custNm.enable = true;
			schCust.enable = true;
		}
	}
}

