/*----------------------------------------------------------------------------------------------
 * 설명      : 적치분 계산을 한다.
 *---------------------------------------------------------------------------------------------*/
function fn_RssSum(){

	var extensionOccur			= 0;	//연장근 발생
	var extensionUse			= 0;	//연장근 사용
	var extensionPay			= 0;	//연장근 지급
	var extensionRss			= 0;	//연장근 적치분
	var extensionRewardOccur	= 0;	//연장근 발생
	var extensionRewardUse		= 0;	//연장근 보상 사용
	var extensionRewardPay		= 0;	//연장근 보상 지급
	var extensionRewardRss		= 0;	//연장근 보상 적치분
	var holidayOccur			= 0;	//휴일근 발생
	var holidayUse				= 0;	//휴일근 사용
	var holidayPay				= 0;	//휴일근 지급
	var holidayRss				= 0;	//휴일근 적치분
	var holidayRewardOccur		= 0;	//휴일근 보상 발생
	var holidayRewardUse		= 0;	//휴일근 보상 사용
	var holidayRewardPay		= 0;	//휴일근 보상 지급
	var holidayRewardRss		= 0;	//휴일근 보상 적치분
	var nghtOccur				= 0;	//야정근 발생
	var nghtPay					= 0;	//야정근 지급
	var closeYn					= "";	//마감여부	expr:fn_setEditType(getRowLevel(currow),closeYn);

	if(ds_list.getRowCount() > 0){
		for(var i=0; i< ds_list.getRowCount();i++){
			if(closeYn != "Y") closeYn	= ds_list.getColumn(i,"closeYn");
			extensionOccur			= Number(ds_list.getColumn(i,"extensionOccur"));
			extensionUse			= Number(ds_list.getColumn(i,"extensionUse"));
			extensionPay			= Number(ds_list.getColumn(i,"extensionPay"));
			extensionRss			= Number(ds_list.getColumn(i,"extensionRss"));
			extensionRewardOccur	= Number(ds_list.getColumn(i,"extensionRewardOccur"));
			extensionRewardUse		= Number(ds_list.getColumn(i,"extensionRewardUse"));
			extensionRewardPay		= Number(ds_list.getColumn(i,"extensionRewardPay"));
			extensionRewardRss		= Number(ds_list.getColumn(i,"extensionRewardRss"));
			holidayOccur			= Number(ds_list.getColumn(i,"holidayOccur"));
			holidayUse				= Number(ds_list.getColumn(i,"holidayUse"));
			holidayPay				= Number(ds_list.getColumn(i,"holidayPay"));
			holidayRss				= Number(ds_list.getColumn(i,"holidayRss"));
			holidayRewardOccur		= Number(ds_list.getColumn(i,"holidayRewardOccur"));
			holidayRewardUse		= Number(ds_list.getColumn(i,"holidayRewardUse"));
			holidayRewardPay		= Number(ds_list.getColumn(i,"holidayRewardPay"));
			holidayRewardRss		= Number(ds_list.getColumn(i,"holidayRewardRss"));
			nghtOccur				= Number(ds_list.getColumn(i,"nghtOccur"));
			nghtPay					= Number(ds_list.getColumn(i,"nghtPay"));
		
			//소숫점 첫째자리까지 표현(반올림)
			extensionUse			= gfn_round(extensionUse, 2);
			extensionPay			= gfn_round(extensionPay, 2);
			extensionRewardUse		= gfn_round(extensionRewardUse, 2);
			extensionRewardPay		= gfn_round(extensionRewardPay, 2);
			holidayUse				= gfn_round(holidayUse, 2);
			holidayPay				= gfn_round(holidayPay, 2);
			holidayRewardUse		= gfn_round(holidayRewardUse, 2);
			holidayRewardPay		= gfn_round(holidayRewardPay, 2);
			nghtPay					= gfn_round(nghtPay, 2);				
			
			//적치분 계산
			extensionRss			= extensionOccur - (extensionUse + extensionPay);
			extensionRewardRss		= extensionRewardOccur - (extensionRewardUse + extensionRewardPay);
			holidayRss				= holidayOccur - (holidayUse + holidayPay);
			holidayRewardRss		= holidayRewardOccur - (holidayRewardUse + holidayRewardPay);
			
			ds_list.setColumn(i, "extensionUse",		extensionUse);
			ds_list.setColumn(i, "extensionPay",		extensionPay);
			ds_list.setColumn(i, "extensionRewardUse",	extensionRewardUse);
			ds_list.setColumn(i, "extensionRewardPay",	extensionRewardPay);
			ds_list.setColumn(i, "holidayUse",			holidayUse);
			ds_list.setColumn(i, "holidayPay",			holidayPay);
			ds_list.setColumn(i, "holidayRewardUse",	holidayRewardUse);
			ds_list.setColumn(i, "holidayRewardPay",	holidayRewardPay);
			ds_list.setColumn(i, "nghtPay",				nghtPay);
			ds_list.setColumn(i, "extensionRss",		extensionRss);
			ds_list.setColumn(i, "extensionRewardRss",	extensionRewardRss);
			ds_list.setColumn(i, "holidayRss",			holidayRss);
			ds_list.setColumn(i, "holidayRewardRss",	holidayRewardRss);
		}
	}
	//sumYN = true;
/*
	var idxExtensionUse         = grd_list.getBindCellIndex("body","extensionUse");         //연장근 사용
	var idxExtensionPay         = grd_list.getBindCellIndex("body","extensionPay");         //연장근 지급
	var idxExtensionRewardUse   = grd_list.getBindCellIndex("body","extensionRewardUse");   //연장근 사용(보상)
	var idxExtensionRewardPay   = grd_list.getBindCellIndex("body","extensionRewardPay");   //연장근 지급(보상)
	var idxHolidayUse           = grd_list.getBindCellIndex("body","holidayUse");           //휴일근 사용
	var idxHolidayPay           = grd_list.getBindCellIndex("body","holidayPay");           //휴일근 지급
	var idxHolidayRewardUse     = grd_list.getBindCellIndex("body","holidayRewardUse");     //휴일근 사용(보상)
	var idxHolidayRewardPay     = grd_list.getBindCellIndex("body","holidayRewardPay");     //휴일근 지급(보상)
	var idxNghtPay              = grd_list.getBindCellIndex("body","nghtPay");              //야정근 지급
*/
	if(closeYn == "Y"){
		grd_list.readonly = true;	//setCellProperty("Body",idxExtensionUse,"")
	}else{
		grd_list.readonly = false;
	}
}

