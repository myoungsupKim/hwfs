/*----------------------------------------------------------------------------------------------
 * 설명      : 사용항목 변경시  적치분 계산을 한다.
 *---------------------------------------------------------------------------------------------*/
function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.newvalue != null){
		if(    e.columnid == "extensionPay"
			|| e.columnid == "extensionUse"
		    || e.columnid == "extensionRewardUse"
		    || e.columnid == "extensionRewardPay"
			|| e.columnid == "holidayPay"
			|| e.columnid == "holidayUse"
			|| e.columnid == "holidayRewardUse"
			|| e.columnid == "holidayRewardPay"
			|| e.columnid == "nghtPay"){
			
			//사용 변경시 log 저장을 하기 위한 컬럼
			ds_list.setColumn(e.row , "logYn" , fn_dataChangedYn(e.row, e.columnid));
			fn_RssSum();
		}
	}
}

function fn_dataChangedYn(row, col){
	if(ds_list.getColumn(row, col) == ds_list.getOrgColumn(row, col)){
		return "N";
	}else{
		return "Y";
	}
}

