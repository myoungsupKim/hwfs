<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>insert</type>
	<description><![CDATA[--레시피검색및 등록  
-레시피마스터등록]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
INSERT INTO FSM_REAL_RECIPE_MST( 
 UPJANG
, MENU_CD
, RECIPE_CD
, RECIPE_NAME
, RECIPE_CLASS
, RECIPE_TYPE
, CUISINE_TYPE
, MAIN_ITEM_CODE1
, MAIN_ITEM_CODE2
, COOK_TOOL1
, COOK_TOOL2
, MAJOR_DISH_INFO
, RECIPE_COLOR
, RECIPE_SHAPE
, RECIPE_TASTE
, RECIPE_FEEL
, RECIPE_TEMPER
, SPRING_YN
, SUMMER_YN
, AUTUMN_YN
, WINTER_YN
, RESP_NUT1
, RESP_NUT2
, USE_TOOL
, VALID_TIME
, DISPLAY_METHOD
, SALE_UOM
, SALE_PRICE
, COST_RATE
, COOK_1MAN_QTY
, COOK_TIME
, EXPECT_SALE_QTY
, REAL_SALE_QTY
, CORE_TEMPER_YN
, GOODS_FEATURE
, PHF_MGMT_YN
, PHF_MGMT1
, PHF_MGMT2
, PHF_MGMT3
, PHF_MGMT4
, PHF_MGMT5
, COOK_OPRN
, PHOTO_PATH
, PREFER
, ORIGIN_RECIPE
, CUSER
, CDATE
, UUSER
, UDATE
, SPHOTO_PATH
, PHF
, RECIPE_SEQ
 )
 (
SELECT 
 UPJANG 
, #MENU_CD#
, RECIPE_CD
, RECIPE_NAME 
, RECIPE_CLASS 
, RECIPE_TYPE
, CUISINE_TYPE 
, MAIN_ITEM_CODE1
, MAIN_ITEM_CODE2
, COOK_TOOL1
, COOK_TOOL2
, ''
, RECIPE_COLOR
, RECIPE_SHAPE
, RECIPE_TASTE 
, RECIPE_FEEL 
, RECIPE_TEMPER
, SPRING_YN
, SUMMER_YN
, AUTUMN_YN
, WINTER_YN
, RESP_NUT1
, RESP_NUT2
, USE_TOOL 
, VALID_TIME
, DISPLAY_METHOD
, SALE_UOM 
, SALE_PRICE 
, COST_RATE
, 0
, ''
, DECODE(#EXPECT_SALE_QTY#,NULL,EXPECT_SALE_QTY,#EXPECT_SALE_QTY#) 
, 0
, CORE_TEMPER_YN 
, GOODS_FEATURE 
, PHF_MGMT_YN 
, PHF_MGMT1 
, PHF_MGMT2 
, PHF_MGMT3 
, PHF_MGMT4 
, PHF_MGMT5 
, COOK_OPRN 
, PHOTO_PATH 
, PREFER 
, ''
, #g_EmpNo#
, SYSDATE
, #g_EmpNo#
, SYSDATE
, SPHOTO_PATH 
, PHF 
, RECIPE_SEQ
 FROM FSR_UPJANG_RECIPE_MST
 WHERE 
	 UPJANG = #UPJANG#
	 AND HALL_CLASS = #HALL_CLASS#
	 AND RECIPE_CD = #RECIPE_CD#
 )	]]></statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_out">
	</output>
</query>