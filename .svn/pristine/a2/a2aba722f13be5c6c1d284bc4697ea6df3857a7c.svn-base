/*----------------------------------------------------------------------------------------------
 * 설명      : 합계 계산
 *---------------------------------------------------------------------------------------------*/
function fn_calTotSum(){
	var food = msk_foodSals.value;
	var bevg = msk_bevgSals.value;
	var etc = msk_etcSals.value;
	var goods = msk_goodsSals.value;
	var svc = msk_svcSals.value;
	
	food = food == null || food == "" ? 0 : food;
	bevg = bevg == null || bevg == "" ? 0 : bevg;
	etc = etc == null || etc == "" ? 0 : etc;
	goods = goods == null || goods == "" ? 0 : goods;
	svc = svc == null || svc == "" ? 0 : svc;
	
	var totSum = food + bevg + etc + goods + svc;

	ds_detail.setColumn(ds_detail.rowposition, "totSum", totSum);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 이벤트
 *---------------------------------------------------------------------------------------------*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	var rowType = ds_detail.getRowType(ds_detail.rowposition);
	if(rowType == 2){
		alert("등록중에는 삭제하실 수 없습니다");
		return;
	}
	
	if(ds_detail.getColumn(ds_detail.rowposition, "dataClass") == "C"){
		alert("입력유형이 자동인 건은 삭제하실 수 없습니다.");
		return;
	}
	
	if(confirm("삭제하시겠습니까?")){
		ds_detail.deleteRow(ds_detail.rowposition);
		
		var strSvcID    = "delete";
		var strURL      = "sm/srm/diplomatExemption/saveList.xdo";
		var strInDs     = "ds_list=ds_detail:A";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
}
