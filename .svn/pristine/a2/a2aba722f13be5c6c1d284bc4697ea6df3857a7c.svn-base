/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			if(ds_detail.rowcount == 0){
				var nRow = ds_detail.addRow();
				ds_detail.setColumn(nRow, "sysClass", ds_search.getColumn(0, "sysClass"));
				ds_detail.setColumn(nRow, "occurDate", ds_search.getColumn(0, "occurDate"));
				ds_detail.setColumn(nRow, "ccCd", ds_search.getColumn(0, "ccCd"));
				ds_detail.setColumn(nRow, "receiptNum", ds_search.getColumn(0, "receiptNum"));
            }
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			this.close(arrArg);
			break;
		// 삭제
		case "delete" :
			gfn_alert("msg.delete.success");	//삭제 되었습니다.
			this.close(arrArg);
			break;
    }
}


