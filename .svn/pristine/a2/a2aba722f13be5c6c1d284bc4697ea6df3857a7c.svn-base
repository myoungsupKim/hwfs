var fv_sToday = gfn_today();


/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "";
	
	//검색조건을 초기화 한다.
	var nRow = ds_search.addRow();
	ds_search.setColumn(nRow, "sysClass", "");
	
	//외교관 정보를 초기화 한다.
	var dRow = ds_detail.addRow();
	
	cal_occurDate.fn_initMonthCal(this, "", "", ds_detail, "occurDate", "Y");
	
	//Combo
	SCCombo.bind(this, "ComCode", "cbo_sysClass", ds_sysClass, {groupCode:"SYS_CLASS", useYn:"Y", defVal:"11"}); 	//시스템구분
	SCCombo.bind(this, "ComCode", "cbo_taxType", ds_taxType, {groupCode:"TAX_TYPE", useYn:"Y"}); 					//세금유형
	SCCombo.bind(this, "ComCode", "cbo_dataClass", ds_dataClass, {groupCode:"DATA_CLASS", useYn:"Y", defVal:"A"});	//입력유형
	SCCombo.bind(this, "ComCode", "cbo_salClass", ds_salClass, {groupCode:"SAL_CLASS", useYn:"Y"});					//영업매출구분
	
	//Popup
	SCPopup.bind(this, "CcCode", "edt_ccNm", "btn_cc",	null, {muCd:"2003"}, {after:"fn_popCcCdAfter"}); 					//업장(CC) 팝업
	SCPopup.bind(this, "ComCode", "edt_nationnm", "btn_nationPop",	null, {groupCode:"NATION_CD", useYn:"Y"}, {after:"fn_popComCodeAfter"}); 	//국가코드 팝업
	
	var leng = arrArg.length;	//부모창에서 넘긴 파라미터(length가 0보다 크면 update 0이면 regist
	if(leng > 0){
		ds_detail.setColumn(dRow, "sysClass", arrArg[0]);
		ds_detail.setColumn(dRow, "occurDate", arrArg[1]);
		ds_detail.setColumn(dRow, "ccCd", arrArg[2]);
		ds_detail.setColumn(dRow, "receiptNum", arrArg[3]);
		
		//update일경우 키필드 비활성
		cal_occurDate.enable = false;
		cbo_sysClass.enable = false;
		edt_ccCd.enable = false;
		btn_cc.enable = false;
		edt_ccNm.enable = false;
		edt_receiptNum.enable = false;
		
		this.sleep(200);	//콤보 데이터가 다 조회된 후 상세조회하기 위해
		fn_search();
	}
	
	//입력유형은 변경불가
	//cbo_dataClass.enable = false;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 업장(CC) 팝업 After 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_popCcCdAfter(p, data){
	ds_detail.setColumn(ds_detail.rowposition, "ccCd", data.code);
	ds_detail.setColumn(ds_detail.rowposition, "ccNm", data.codeName);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 국가코드 팝업 After 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_popComCodeAfter(p, data){
	ds_detail.setColumn(ds_detail.rowposition, "nationcd", data.code);
	ds_detail.setColumn(ds_detail.rowposition, "nationnm", data.codeName);
}

