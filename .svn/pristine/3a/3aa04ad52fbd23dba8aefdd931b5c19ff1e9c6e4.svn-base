<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[--레시피검색및 등록
조리법삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>
<![CDATA[
DELETE FROM FSR_UPJANG_RECIPE_CUISINE
WHERE 
	 UPJANG = #UPJANG#
	 AND HALL_CLASS = #HALL_CLASS#
	 AND RECIPE_CD = #RECIPE_CD#	]]></statement>
	<input default-name="ds_in">
		<col name="UPJANG" size="22" type="NUMERIC" description="" /> 
		<col name="HALL_CLASS" size="20" type="VARCHAR" description="" /> 
		<col name="RECIPE_CD" size="20" type="VARCHAR" description="" /> 
		<col name="CUISINE_SEQ" size="22" type="NUMERIC" description="" /> 
	</input>
	<output default-name="ds_out">
	</output>
</query>
