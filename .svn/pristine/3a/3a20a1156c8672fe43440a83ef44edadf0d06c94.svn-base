			ds_listSuspense.setColumn(nRow, "buCd",             dsObj.getColumn(i, "buCd"));
			ds_listSuspense.setColumn(nRow, "sapSuspenseNum",   dsObj.getColumn(i, "sapSuspenseNum"));
			ds_listSuspense.setColumn(nRow, "receiveDate",      dsObj.getColumn(i, "receiveDate"));    //입금일자
