function btn_deduction_onclick(obj:Button,  e:ClickEventInfo)
{
	if(gfn_validCheck(ds_receiveData, ds_searchValid, true) == false) return;
	if(ds_listAr.rowcount < 1) return;
	
	//해당 로우의 적용금액을 들고온다.
	var receiveAmt = ds_listAr.getSum("receiveAmt");
	var applyAmt = ds_listSuspense.getSum("applyAmt");
	
	//회계팀 김현녕대리 요청으로 수수료율 4% 이상은 안내 팝업(과도한 수수료 처리 못하도록 제한) 20190624
	//채권금액&가수금금액으로 수수료율 계산 
	var t_amt = Math.round(((receiveAmt-applyAmt) *100 / receiveAmt),1);	
	//예외처리 시 사용 금융투자협회점 예외추가
	//if (g_EmpNo == "Z9952396" || g_EmpNo == "201403497" ) {
	if (g_EmpNo == "Z9952396") {
		//pass
	} else {	
		if (t_amt > 4) {
			gfn_alert("수수료율 : " + t_amt + "%\n채권금액 및 가수금금액을 확인해주세요.\n(수수료율 4% 초과로 입급처리 불가)");
			grd_list.setFocus();
			return;
		}
	}
	
	if(receiveAmt == 0){
		gfn_alert("적용금액을 입력하세요.");
		grd_list.setFocus();
		return;
	}
	
	var muCd = ds_receiveData.getColumn(0, "inqueryMuCd");        //사업부
	var upjangCd = ds_receiveData.getColumn(0, "upjangCd");       //부서
	var custCd = ds_receiveData.getColumn(0, "inqueryCustCd");    //고객사
	
	var strId       = "ReceiveMgntDeductionPop";
	var strURL      = "X_SAR::ReceiveMgntDeductionPop.xfdl";
	var objArgument = {pMuCd:muCd, pUpjangCd:upjangCd, pCustCd:custCd, pReceiveAmt:receiveAmt, pApplyAmt:applyAmt};
	var nWidth      = -1;
	var nHeight     = -1;
	var strStyle    = "";
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";
	var bResizable  = true;

	// 리턴값을 배열로 받을 경우	

	var arrRtn = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign, bResizable);
	
	//총입금처리금액셋팅
	fn_changeAmtRe();
}

