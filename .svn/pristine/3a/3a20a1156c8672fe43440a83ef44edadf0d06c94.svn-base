function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);

	//조직 Component 권한 적용
	TmSearchComponent01.applyAuthLevel(MuSearchComponent01);
	UpjangSearchComponent01.setForcedRoleSet("CostCenter");
	UpjangSearchComponent01.applyAuthLevel(TmSearchComponent01);    //CC
	UpjangSearchComponent00.applyAuthLevel(TmSearchComponent01);    //업장
		
	/************************************************************************************************
	 * 신규 - 입금처리 버튼
	************************************************************************************************/
	if(gfn_isNull(pReceiveNum)){

		// 초기값 셋팅
		fn_init();

	/************************************************************************************************
	 * 변경 - 그리드 더블 클릭
	************************************************************************************************/
	}else{
	
		// 조회조건 조회
		fn_search();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_listAr";

	if(gfn_isNull(pReceiveNum)){
	
		//입금 정보 초기화
		ds_receiveData.addRow();
		ds_receiveData.setColumn(0, "receiveNum", "");
		ds_receiveData.setColumn(0, "receiveOccurClass", "RO01");		//입금 발생 구분
		ds_receiveData.setColumn(0, "deducAmt",    "0");
		ds_receiveData.setColumn(0, "receiveAmt",  "0");
		ds_receiveData.setColumn(0, "inqueryMuCd", MuSearchComponent01.getMuCode());
		ds_receiveData.setColumn(0, "inqueryTmCd", TmSearchComponent01.getTmCode());
		ds_receiveData.setColumn(0, "inqueryTmNm", TmSearchComponent01.getTmName());
		ds_receiveData.setColumn(0, "inqueryCcCd", UpjangSearchComponent01.getUpjangCode());
		ds_receiveData.setColumn(0, "inqueryCcNm", UpjangSearchComponent01.getUpjangName());

		//선수금 정보 초기화
		ds_advanceData.addRow();
		ds_advanceData.setColumn(0, "altTypeCd",               "G05");     // 선수금 대체 유형 코드
		ds_advanceData.setColumn(0, "advanceAltOccurClass",    "TO08");    // 선수금 대체 발생 구분

		if(arrSchVal.length > 0){
			ds_receiveData.setColumn(0, "inqueryMuCd",    	arrSchVal[0]);
			ds_receiveData.setColumn(0, "inqueryMuNm",    	arrSchVal[1]);
			ds_receiveData.setColumn(0, "inqueryTmCd",    	arrSchVal[2]);
			ds_receiveData.setColumn(0, "inqueryTmNm",    	arrSchVal[3]);
			ds_receiveData.setColumn(0, "inqueryCcCd",    	arrSchVal[4]);
			ds_receiveData.setColumn(0, "inqueryCcNm",    	arrSchVal[5]);
			ds_receiveData.setColumn(0, "inqueryCustCd",  	arrSchVal[6]);
			ds_receiveData.setColumn(0, "inqueryCustNm",    arrSchVal[7]);
			ds_receiveData.setColumn(0, "inqueryStartDate", arrSchVal[8]);
			ds_receiveData.setColumn(0, "inqueryEndDate",   arrSchVal[9]);
			
			if (ds_receiveData.getColumn(0, "inqueryMuCd") == "2002"){
				if (g_Cc_Cd == arrSchVal[4]) {
					ds_receiveData.setColumn(0, "upjangCd",   g_Upjang);
					ds_receiveData.setColumn(0, "upjangNm",   g_UpjangNm);
				}			
			}
			MuSearchComponent01.setMuCode(arrSchVal[0]);
		}
		//달력 초기화
		fn_chgCalDate();
			
		cal_receiveDate.fn_initMonthCal(this, fv_taxDay, "fn_calendarFromCallBack", ds_receiveData, "receiveDate");
	}else{
	
		MuSearchComponent01.setMuCode(ds_receiveData.getColumn(0,"inqueryMuCd"));
		TmSearchComponent01.setTmCode(ds_receiveData.getColumn(0,"inqueryTmCd"));
		UpjangSearchComponent01.setUpjangCode(ds_receiveData.getColumn(0,"inqueryCcCd"));
		UpjangSearchComponent00.setUpjangCode(ds_receiveData.getColumn(0,"upjangCd"));
                
		cal_fiscalDateFr.enable=false;
		cal_fiscalDateTo.enable=false;
		edt_custCd.enable=false;
		edt_custNm.enable=false;
		edt_arNum.enable=false;
		
		MuSearchComponent01.enable		= false;
		TmSearchComponent01.enable		= false;
		UpjangSearchComponent01.enable  = false;
		UpjangSearchComponent00.enable  = false;
		
		btn_receive.visible = false;
		btn_receiveCancle.visible=true;
							
		msk_allEditAmt.enable=false;
		cbo_allEditYn.enable=false;
		//달력 초기화
		fv_taxDay = ds_receiveData.getColumn(0, "inqueryEndDate");
		cal_receiveDate.fn_initMonthCal(this, null, "fn_calendarFromCallBack", ds_receiveData, "receiveDate");
		cal_receiveDate.enable=false;
	}
	
	//신용카드입력 컴포넌트 숨기자
	fn_card_visible_set(false);
	btn_advance.enable = false;
		
	if (ds_receiveData.getColumn(0, "inqueryMuCd") == "2002"){
		UpjangSearchComponent01.visible = true;
		stc_cccdTitle.visible = true;
		stc_upjangTitle.visible = true;
		UpjangSearchComponent00.visible = true;				
	}else{
		UpjangSearchComponent01.visible = true;
		stc_cccdTitle.visible = true;
		stc_upjangTitle.visible = true;
		UpjangSearchComponent00.visible = true;			
	}	
	
	//저장 위치 셋팅
	ds_receiveData.setColumn(0, "batchId", "RECEIVE");
		
	//gfn_setSalesTypeCd(this, "arType", "ds_arType", "001");	//채권유형코드
	//gfn_setSalesTypeCd(this, "receiveType", "ds_receiveType", "002");	//입금유형코드
	SCCombo.bind(this, "ComCode", "cbo_zeroAmtYn", ds_useYn, {groupCode:"USE_YN", useYn:"Y", defVal:"N"});
	

	/*채권정보 초기화*/
	cal_fiscalDateFr.fn_initMonthCal(this, null, null, ds_receiveData, "inqueryStartDate");	
	cal_fiscalDateTo.fn_initMonthCal(this, fv_taxDay, "fn_calendarToCallBack", ds_receiveData, "inqueryEndDate");
	
	//콤보조회
	fn_setCombo();
	
	//매입사 조회
	ds_search.addRow();
	fn_selectBuycpnyCd();
}
/*
//달력 초기화
*/
function fn_calendarFromCallBack()
{
	if(gfn_isNull(ds_receiveData.getColumn(0,"inqueryCcCd"))){
		alert("업장 또는 CC를 선택해 주세요.");
		ds_receiveData.setColumn(0, "receiveDate", fv_taxDay);
		return false;
	}
	mfn_GetCcMonthClsYn("C", ds_receiveData.getColumn(0,"inqueryCcCd"), gfn_subStr(ds_receiveData.getColumn(0,"receiveDate"), 0, 6), "01");
	if (ds_common.getColumn(0, 0) == "Y")
	{
		if(gfn_isNull(pReceiveNum)){
			g_Message("EE", "해당월은 월마감이 완료되어 작업할 수 없습니다.");
			ds_receiveData.setColumn(0, "receiveDate", fv_taxDay);
//			return false; 
		}
	}
	ds_receiveData.setColumn(0, "inqueryEndDate", ds_receiveData.getColumn(0, "receiveDate"));	
}
/*달력 초기화*/
function fn_chgCalDate(){
	if (gfn_isNull(ds_receiveData.getColumn(0,"inqueryCcCd"))){
		ds_receiveData.setColumn(0, "receiveDate", fv_sToday);
		ds_receiveData.setColumn(0, "inqueryEndDate", fv_sToday);
	}else{
		mfn_GetCcMonthClsYn("C", ds_receiveData.getColumn(0,"inqueryCcCd"), gfn_subStr(gfn_addMonth(fv_sToday, -1), 0, 6), "01");

		if (ds_common.getColumn(0, 0) == "N")
		{
			fv_taxDay = gfn_getLastDate(gfn_addMonth(fv_sToday, -1));
			ds_receiveData.setColumn(0, "receiveDate", fv_taxDay);
			ds_receiveData.setColumn(0, "inqueryEndDate", fv_taxDay);
		}else{
			ds_receiveData.setColumn(0, "receiveDate", fv_sToday);
			ds_receiveData.setColumn(0, "inqueryEndDate", fv_sToday);
		}
	}
}

function fn_calendarToCallBack()
{
	//채권 조회는 입금 일자 이전으로 할것 : 2015-10-11 김동표 차장 추가 요청(김명호)

	if(ds_receiveData.getColumn(0,"receiveDate") < ds_receiveData.getColumn(0,"inqueryEndDate")){
		alert("채권은 입금 일자 이전 데이터만 조회가 가능합니다.");
		ds_receiveData.setColumn(0, "inqueryEndDate", ds_receiveData.getColumn(0, "receiveDate"));	
		return;
	}
}
/*----------------------------------------------------------------------------------------------
