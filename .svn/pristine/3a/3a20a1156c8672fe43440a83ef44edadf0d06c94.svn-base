/************************************************************************************************
 * 적용금액 입력 시 잔액 비교
************************************************************************************************/
function ds_listAr_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	//var receiveAmtIdx = grd_list.getBindCellIndex("body","receiveAmt");
	if(e.columnid == "receiveAmt"){
		if (gfn_isNull(ds_listAr.getColumn(rowIdx,"receiveAmt"))){
			ds_listAr.setColumn(rowIdx,"receiveAmt", 0);
		}
		var rowIdx = ds_listAr.rowposition;
		var arBalAmt = ds_listAr.getColumn(rowIdx,"arBalAmt");
		var receiveAmt = ds_listAr.getColumn(rowIdx,"receiveAmt");
		if(arBalAmt >= 0){
			if(arBalAmt < receiveAmt){
				gfn_alert("해당 적용금액은 잔액보다 작아야 합니다.");
				ds_listAr.setColumn(rowIdx,"receiveAmt",e.oldvalue);
			}
		}else{
			if(arBalAmt > receiveAmt || receiveAmt > 0){
				gfn_alert("해당 적용금액은 잔액보다 크거나 같고 0보다 작아야 합니다.");
				ds_listAr.setColumn(rowIdx,"receiveAmt",e.oldvalue);
			}
		}
	}
}

