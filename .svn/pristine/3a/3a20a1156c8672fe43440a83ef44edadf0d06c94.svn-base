function btn_advance_onclick(obj:Button,  e:ClickEventInfo)
{
	var arrArg = new Array();
	//조회 조건값을 다시 팝업으로 전달.
    if (gfn_validCheck(ds_receiveData, ds_searchValid, true) == false) return;
	
	arrArg[0] = ds_receiveData.getColumn(0, "inqueryStartDate");
	arrArg[1] = ds_receiveData.getColumn(0, "inqueryEndDate");
	arrArg[2] = ds_receiveData.getColumn(0, "inqueryMuCd");
	arrArg[3] = ds_receiveData.getColumn(0, "inqueryTmCd");
	arrArg[4] = ds_receiveData.getColumn(0, "inqueryCcCd");
	arrArg[5] = ds_receiveData.getColumn(0, "inqueryCcNm");
	arrArg[6] = ds_receiveData.getColumn(0, "inqueryCustCd");
	arrArg[7] = ds_receiveData.getColumn(0, "inqueryCustNm");

	var strId       = "AdvanceNumPopup";
	//입금금액이 -인 경우 별로 팝업으로 대체(선수금이 0인 건도 조회해서 선택가능해야함) 20180411 김호석 추가
	if (msk_receiveAmt.value < 0) {
		var strURL      = "X_SAR::AdvanceNumPopup3.xfdl";
	} else {
		var strURL      = "X_SAR::AdvanceNumPopup.xfdl";
	}
	var objArgument = {arrSchVal:arrArg};
	var nWidth      = -1;
	var nHeight     = -1;
	var strStyle    = "";	
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";
	var bResizable  = true;

	// 리턴값을  Dataset으로 받을 경우
 	var dsObj = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);
 
	if(dsObj != null){
		for (i=0; i< dsObj.getRowCount(); i++)
		{
			if(ds_listAdvance.findRow("advanceNum", dsObj.getColumn(i, "advanceNum"))==-1){
				var nRow = ds_listAdvance.addRow();
				ds_listAdvance.copyRow(nRow, dsObj, i);
			}
		}
	}
}

