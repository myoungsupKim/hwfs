				if(ds_listAdvance.getRowType(i) > 1){	//행 타입(1:Normal, 2:insert, 4:Update, 8:Delete)
					
					//0이 아닐때로 수정(마이너스 입금 처리 시 필요 20180406 김호석
					if((iApplyAmt > 0) || (iApplyAmt < 0)){
						
						if(gfn_isNull(sAltNum)){
							ds_listAdvance.setColumn(i, "rowStatus",	"INSERT");
							ds_listAdvance.setColumn(i, "altNum", 		gv_altNum);
