    //외상매입대체인 경우 거래처 없어도 처리 가능하도록 변경 20151214 맹수영
    
    
	// 조회시 Validation Check
	if (gfn_validCheck(ds_receiveData, ds_searchValid, true) == false) return;
		
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);
	
    var strSvcID    = "searchDetail";
    var strURL      = "sm/sar/receiveMgntPop/selectDetail.xdo";
    var strInDs     = "ds_search=ds_receiveData:A";
    var strOutDs    = "ds_listAr=ds_listAr";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 입금 마스터 정보 조회
        case "search" :
            if (ds_receiveData.rowcount > 0){
				fn_init();	//fn_setCombo();
            }
			fn_ar_search();
            break;
            
		// 입금 상세정보 조회
        case "searchDetail" :			
			if(cbo_allEditYn.value=="Y"){
				if(ds_listAr.getRowCount() > 0) {
					for(i=0; i < ds_listAr.getRowCount(); i++){
						ds_listAr.setColumn(i, "receiveAmt", ds_listAr.getColumn(i, "arBalAmt"));
					}
				}
			}
			
			if (!gfn_isNull(pReceiveNum)){
			    var receiveTypeCd = ds_receiveData.getColumn(0, "receiveTypeCd");   //입금유형
			    btn_suspense.enable = false;
			    btn_advance.enable  = false;
			    cbo_receiveType.enable = false;
			    
			    //입금유형에 따른 화면들 setting
			    fn_set_visible_component(receiveTypeCd, receiveTypeCd);
			    
			    if (receiveTypeCd == "B02" || receiveTypeCd == "B13"){
			        var sCcCd = ds_receiveData.getColumn(0,"ccCd");
			        grd_list_suspense.visible = true;
			        grd_list_advance.visible = false;
			        
			        ds_receiveData.setColumn(0, "receiveDateFrom", ds_receiveData.getColumn(0, "inqueryStartDate"));
			        ds_receiveData.setColumn(0, "receiveDateTo", ds_receiveData.getColumn(0, "inqueryEndDate"));
			        ds_receiveData.setColumn(0, "ccCd", "'" + sCcCd + "'");
			        
			        //총처리금액 셋팅하자  입금의 회수금액 - 공제금액
			        //msk_arWithdrawAmt.value = ds_receiveData.getColumn(0, "arWithdrawAmt") + ds_receiveData.getColumn(0, "deducAmt");

			        //가수금대체상세조회
			        fn_searchSuspenseAltDetail();
			    } else if(receiveTypeCd == "B10" || receiveTypeCd == "B11" || receiveTypeCd == "B19"){
			        grd_list_suspense.visible = false;
			        grd_list_advance.visible  = true;
			        
			        //가수금Grid와 동일한 위치
			        grd_list_advance.position.x      = grd_list_suspense.position.x;
					grd_list_advance.position.y      = grd_list_suspense.position.y;
					grd_list_advance.position.width  = grd_list_suspense.position.width;
					grd_list_advance.position.height = grd_list_suspense.position.height;
					
					//총처리금액 셋팅하자  입금의 회수금액
			        //msk_arWithdrawAmt.value = ds_receiveData.getColumn(0, "arWithdrawAmt");
			        
			        //선수금대체조회
			        fn_selectAdvanceAltMst();
			    } else if(receiveTypeCd == "B15"){
			        //카드결제내역은 입금수단조회
			        ds_receiveData.setColumn(0,"seq","1");     //카드결제인경우 seq는 하나밖에 없음
			        
			        //총처리금액 셋팅하자  입금의 회수금액
			        //msk_arWithdrawAmt.value = ds_receiveData.getColumn(0, "arWithdrawAmt");
			        
			        fn_card_enable_set(false);
			        fn_selectReceiveWay();
			    } else {
			        //총처리금액 셋팅하자  입금의 회수금액
			        //msk_arWithdrawAmt.value = ds_receiveData.getColumn(0, "arWithdrawAmt");
			    }
			} else {
			    var receiveTypeCd = ds_receiveData.getColumn(0, "receiveTypeCd");   //입금유형
			    cbo_receiveType.enable = true;
			    if (receiveTypeCd == "B02" || receiveTypeCd == "B13"){
			        btn_suspense.enable = true;
			        btn_advance.enable  = false;
			    } else if(receiveTypeCd == "B10" || receiveTypeCd == "B11" || receiveTypeCd == "B19"){
			        btn_suspense.enable = false;
			        btn_advance.enable  = true;
			    }
			}
			
            break;
            
        //선수금 대체 마스터
        case "selectAdvanceAltMst" :
			fn_searchAdvanceAltDetail();
            break;
            
        //가수금상세조회후 후단처리
        case "searchSuspenseAltDetail" :
            var sumSusApplyAmt=0;
            if (ds_listSuspense.rowcount > 0){
                for (var i = 0;i<ds_listSuspense.rowcount;i++){
                    var susApplyAmt = ds_listSuspense.getColumn(i, "applyAmt");
                    sumSusApplyAmt += susApplyAmt;
                }
                msk_suspenseAmt.value = sumSusApplyAmt;
                msk_arWithdrawAmtReal.value = sumSusApplyAmt;
                msk_arWithdrawAmt.value = sumSusApplyAmt + ds_receiveData.getColumn(0, "deducAmt");
            }
            break;
            
        //선수금상세조회후 후단처리
        case "searchAdvanceAltDetail" :
            var sumAdvApplyAmt=0;
            if (ds_listAdvance.rowcount > 0){
                for (var i = 0;i<ds_listAdvance.rowcount;i++){
                    var advApplyAmt = ds_listAdvance.getColumn(i, "applyAmt");
                    sumAdvApplyAmt += advApplyAmt;
                }
                msk_advanceAmt.value = sumAdvApplyAmt;
            }
            break;
            
        //카드결제 조회(입금수단)후 후단처리
        case "searchReceiveWay" :
            if (ds_view.rowcount > 0){
                msk_creditCardNum.value  = ds_view.getColumn(0,"creditCardNum");   //카드번호
                edt_apprvNum.value       = ds_view.getColumn(0,"apprvNum");        //승인번호
                msk_validYymm.value      = ds_view.getColumn(0,"validYymm");       //유효기간
                msk_cardReceiveAmt.value = ds_view.getColumn(0,"receiveAmt");      //승인금액
                cbo_receiveBankcd.value  = ds_view.getColumn(0,"receiveBankcd");   //매입사
            }
            break;
            
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			opener.fn_search();
			//close();
			form.reload();
            break;  
              
        // 취소
        case "cancle" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			opener.fn_search();
			close();
            break;    
            
        case "receiveType" : 	//유형코드 기본값 셋팅
        
            if(ds_receiveType.rowcount < 2){
				if(gfn_isNull(ds_receiveType.getColumn(0, "typeCd"))){
					
					//데이터가 없을 경우 전체조회
					ds_receiveType.clearData();
					gfn_setSalesTypeCd(this, "receiveType2", "ds_receiveType", "002");
				}
            }else{
            
				//입금유형코드 채권거래처이관(B07), 채권타부서이관(B16)제외
				ds_receiveType.filter("typeCd != 'B07' && typeCd != 'B16'");
				var nRow = 0;
				//cbo_receiveType.value= "B02";
				//var nRow  = ds_receiveType.findRow("typeCd", cbo_receiveType.value);
				//fv_receiveAcctCd = ds_receiveType.getColumn(nRow,"acctCd");
				if(gfn_isNull(pReceiveNum)){
					nRow  = ds_receiveType.findRow("typeCd", "B02");
				}else{
					nRow = ds_receiveType.findRow("typeCd", ds_receiveData.getColumn(0, "receiveTypeCd"));
					
				}
				if(nRow < 0) nRow = 0;
				cbo_receiveType.index = nRow;
				fv_receiveAcctCd = ds_receiveType.getColumn(nRow, "acctCd");
				//cbo_receiveType.index = 0;
				//alert("receiveType:"+fv_receiveAcctCd);	
            }
            break;
        
     	// 입금유형 조회
        case "receiveType2" :
			ds_receiveType.filter("typeCd != 'B07' && typeCd != 'B16'");
			
			ds_receiveType.insertRow(0);
			//ds_receiveType.setColumn(0, "typeCd", "");
			ds_receiveType.setColumn(0, "typeNm", "선택");

			var nRow = 0;
			//cbo_receiveType.value= "B02";
			//var nRow  = ds_receiveType.findRow("typeCd", cbo_receiveType.value);
			//fv_receiveAcctCd = ds_receiveType.getColumn(nRow,"acctCd");
			if(gfn_isNull(pReceiveNum)){
				nRow  = ds_receiveType.findRow("typeCd", "B02");
			}else{
				nRow = ds_receiveType.findRow("typeCd", ds_receiveData.getColumn(0, "receiveTypeCd"));
				
			}
			if(nRow < 0) nRow = 0;
			cbo_receiveType.index = nRow;
			fv_receiveAcctCd = ds_receiveType.getColumn(nRow, "acctCd");
			//alert("receiveType2:"+fv_receiveAcctCd);	
            break;
			
	    case "searchTotalAdvanceAmt" : //선수금잔액 셋팅
	        msk_totalAdvanceAmt.value = ds_totalAdvanceAmt.getColumn(0,"totalAdvanceAmt");
	        break;
	        
	    case "searchTotalSuspenseAmt" : //가수금 잔액 조회
	        msk_totalSuspenseAmt.value = ds_totalSuspenseAmt.getColumn(0,"totalSuspenseAmt");
	        break;
	        
	    case "searchBuycpnyCd" : //매입사에 대한 거래처 셋팅
	        cbo_receiveBankCd_onitemchanged();
	        break;
	        
		case "getArReceiveYn" :
			break;	   
			     
		case "getSuspenseAltYn" :
			break;	   
			     
		case "getAdvanceAltYn" :
			break;	        
    }
}

