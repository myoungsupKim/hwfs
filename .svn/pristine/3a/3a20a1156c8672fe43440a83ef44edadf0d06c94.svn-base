	//	if (ds_receiveData.getColumn(0, "inqueryMuCd") == "2002"){
			if((!gfn_isNull(cls.custcd)) && (cls.custcd != "0")){
				ds_receiveData.setColumn(0,"inqueryCustCd", cls.custcd);
				ds_receiveData.setColumn(0,"inqueryCustNm", cls.custnm);
