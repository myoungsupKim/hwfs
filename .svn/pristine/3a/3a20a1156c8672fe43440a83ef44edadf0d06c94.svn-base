 	        ds_view.setColumn(0, "receiveBankcd", cbo_receiveBankcd.value);   //매입사
 	        ds_view.setColumn(0, "receiveTypeCd", ds_receiveData.getColumn(0,"receiveTypeCd"));   //입금유형
 	        if (gfn_isNull(pReceiveNum)){
 	            ds_view.setColumn(0, "rowStatus", "INSERT");
