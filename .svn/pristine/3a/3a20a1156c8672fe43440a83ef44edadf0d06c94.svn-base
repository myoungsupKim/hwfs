/*----------------------------------------------------------------------------------------------
 * 설명      : 입금유형에 따른 화면들 setting
 *---------------------------------------------------------------------------------------------*/
function fn_set_visible_component(rReceiveType, preReceiveType){
    //B02 : 가수금 B13:어음입금
	if (rReceiveType == "B02" || rReceiveType == "B13"){
	    grd_list_suspense.visible = true;
	    grd_list_advance.visible  = false;
	    
	    btn_suspense.enable    = true;
	    btn_advance.enable     = false;
	    
	    //카드입력창 처리
	    fn_card_visible_set(false);
	    //가수금이면 공제금액 활성화 아니면 비활성화
	    if (rReceiveType == "B02"){
	        msk_deducAmt.enable  = true;
	        btn_deduction.enable = true;
	    } else if (rReceiveType == "B13"){
	        msk_deducAmt.enable  = false;
	        btn_deduction.enable = false;
	    }
	    
	    //선수금내역 클리어
	    ds_listAdvance.clearData();
	    msk_advanceAmt.value = 0;
	//B10 : 선수금(선매출) B11 :계약이행예치금 B19 :선수금(사업장)
	} else if (rReceiveType == "B10" || rReceiveType == "B11" || rReceiveType == "B19"){
	    grd_list_suspense.visible = false;
	    grd_list_advance.visible  = true;
	    
	    btn_suspense.enable    = false;
	    btn_advance.enable     = true;
	    
	    grd_list_advance.position.x      = grd_list_suspense.position.x;
	    grd_list_advance.position.y      = grd_list_suspense.position.y;
	    grd_list_advance.position.width  = grd_list_suspense.position.width;
	    grd_list_advance.position.height = grd_list_suspense.position.height;
	    
	    //공제금액처리
	    msk_deducAmt.enable  = false;
	    btn_deduction.enable = false;
	    
	    //가수금내역 클리어
	    ds_listSuspense.clearData();
	    msk_suspenseAmt.value = 0;
	    
	    //공제내역 클리어
	    ds_listDeduc.clearData();
	    ds_receiveData.setColumn(0,"deducAmt",0);
	    
	    //카드입력창 처리
	    fn_card_visible_set(false);
	} else if (rReceiveType == "B15"){
	    grd_list_suspense.visible = false;
	    grd_list_advance.visible  = false;
	    
	    msk_suspenseAmt.enable = false;
	    btn_suspense.enable    = false;
	    
	    msk_advanceAmt.enable  = false;
	    btn_advance.enable     = false;
	    
	    ds_listSuspense.clearData();
	    ds_listAdvance.clearData();
	    msk_suspenseAmt.value = 0;
	    msk_advanceAmt.value = 0;
	    //공제내역 클리어
	    ds_listDeduc.clearData();
	    ds_receiveData.setColumn(0,"deducAmt",0);
	    
	    //공제금액처리
	    msk_deducAmt.enable  = false;
	    btn_deduction.enable = false;
	    
	    //카드입력창 처리
	    fn_card_visible_set(true);
	} else {
	    if (preReceiveType == "B15"){
	        grd_list_suspense.visible = true;
	    }
	    msk_suspenseAmt.enable = false;
	    btn_suspense.enable    = false;
	    
	    msk_advanceAmt.enable  = false;
	    btn_advance.enable     = false;
	    
	    ds_listSuspense.clearData();
	    ds_listAdvance.clearData();
	    msk_suspenseAmt.value = 0;
	    msk_advanceAmt.value = 0;
	    
	    //공제내역 클리어
	    ds_listDeduc.clearData();
	    ds_receiveData.setColumn(0,"deducAmt",0);
	    
	    //공제금액처리
	    msk_deducAmt.enable  = false;
	    btn_deduction.enable = false;
	    
	    //카드입력창 처리
	    fn_card_visible_set(false);
	}
	
	fn_changeAmtRe();
}

