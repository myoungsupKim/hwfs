function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	var iArBalAmt	= 0;	//잔액
	var iReceiveAmt	= 0;	//적용금액

	if(e.cell == 3){	//적용금액 Setting
		iArBalAmt = ds_listAr.getColumn(ds_listAr.rowposition, "arBalAmt");	//obj.getCellValue(e.row, e.cell);
		iReceiveAmt = ds_listAr.getColumn(ds_listAr.rowposition, "receiveAmt");	//obj.getCellValue(e.row, e.cell+1);
		if(iArBalAmt == iReceiveAmt){
			ds_listAr.setColumn(ds_listAr.rowposition, "receiveAmt", 0);
		}else{
			ds_listAr.setColumn(ds_listAr.rowposition, "receiveAmt", iArBalAmt);
		}
	}
	fn_changeAmtRe();
}

function fn_changeAmtRe()
{
 	msk_receiveAmt.value = ds_listAr.getSum("receiveAmt");
 	
 	var receiveTypeCd = ds_receiveData.getColumn(0,"receiveTypeCd");   //입금유형
 	
 	//입금유형이 가수금(사업장),어음입금 아닌경우만 총처리금액 계산
 	if (receiveTypeCd == "B02" || receiveTypeCd == "B13"){
		msk_arWithdrawAmt.value     = Number(msk_suspenseAmt.value) + ds_receiveData.getColumn(0, "deducAmt"); //보여지는 총처리금액
		msk_arWithdrawAmtReal.value = Number(msk_suspenseAmt.value); //실제 회수할 금액
	} else {
	    msk_arWithdrawAmt.value     = ds_receiveData.getColumn(0, "receiveAmt");    //가수금(사업장), 어음입금 아닌경우는 공제금액 해당사항 아니므로 총처리금액과 회수금액 같음
	    msk_arWithdrawAmtReal.value = ds_receiveData.getColumn(0, "receiveAmt");
	}
}

function fn_changeSuspenseAmtRe()
{
 	if(ds_listSuspense.getRowCount() == 0)return;
 	msk_suspenseAmt.value = ds_listSuspense.getSum("applyAmt");
 	
 	//총처리금액 계산  가수금액총액 + 공제금액
 	msk_arWithdrawAmt.value     = ds_listSuspense.getSum("applyAmt") + ds_receiveData.getColumn(0, "deducAmt");
 	msk_arWithdrawAmtReal.value = ds_listSuspense.getSum("applyAmt");
}

function fn_changeAdvanceAmtRe()
{
 	msk_advanceAmt.value = ds_listAdvance.getSum("applyAmt");
}

function msk_deducAmt_onkillfocus(obj:MaskEdit, e:KillFocusEventInfo)
{
	fn_changeAmtRe();
}

function btn_receiveCancle_onclick(obj:Button,  e:ClickEventInfo)
{
// 	입금 취소 처리
	/*전표 생성 유무 체크*/
	var jCount = 0;
	for(i = 0; i<ds_listAr.getRowCount(); i++){
		if(ds_listAr.getColumn(i, "journalzYn") == "N"){
			jCount = jCount + 1;
		}
	}
	if(ds_listAr.getRowCount() != jCount){
		alert("발생 전표가 생성된 데이터 입니다.\n\n입금 취소가 불가능합니다.");
		return;
	} 	 	
	//카드 결제한 데이터가 입금 완료된 데이터는 취소안되게 처리
	if(!gfn_isNull(ds_view.getColumn(0, "arNum"))){
		if(ds_view.getColumn(0, "arReceiveAmt") != "0") {
			alert("입금 처리된 내역중 입금 대체 처리된 데이터가 있습니다.");
			return;
		}
	}

	if (gfn_dsIsUpdated(ds_listAr) == true) 
	{
	 	alert("입금 취소시 기존 데이터를 변경 하실수 없습니다.");
	 	form.reload();
	 	return;
	}
	if (gfn_dsIsUpdated(ds_listSuspense) == true) 
	{
	 	alert("가수금 입금 취소시 기존 데이터를 변경 하실수 없습니다.");
	 	form.reload();
	 	return;
	}	
	if(msk_receiveAmt.value != msk_arWithdrawAmt.value){
		alert("취소할 채권금액과 대체된 금액이 일치 하지 않습니다.\n\n관리자에게 문의해 주시기 바랍니다.");
		form.reload();
		return;
	}

	receiveTypeCd = ds_receiveData.getColumn(0, "receiveTypeCd");

	if (receiveTypeCd == "B02" || receiveTypeCd == "B13"){
		if (ds_listSuspense.rowcount == 0){
			alert("취소 할 가수금 내역이 없습니다");
			return
		}
	//선수금(선매출), 계약이행 예치금, 선수금(사업장)
	} else if (receiveTypeCd == "B10" || receiveTypeCd == "B11" || receiveTypeCd == "B19"){
		if (ds_listAdvance.rowcount == 0){
