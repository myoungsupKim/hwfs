/*----------------------------------------------------------------------------------------------
 * 설명      : 일괄적용금액 및 일괄적용구분 변경에 따른 ds_list 적용금액 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_change_receiveAmt_listAr(allEditYn, allAmt)
{
    if ((allAmt == "" || allAmt == "0" || allAmt == null) && allEditYn == "N"){
        msk_allEditAmt.enable = true;
        //일괄금액 아니요 이고 금액이 NULL 일때 모두 0 으로 셋팅
		for (var i = 0 ; i < ds_listAr.rowcount ; i ++){
			ds_listAr.setColumn(i, "receiveAmt", 0);
		}
    } else {
		if (allEditYn == "Y"){
			msk_allEditAmt.enable = false;
			
			//적용금액 일괄 잔액금액으로 셋팅
			for (var i = 0 ; i < ds_listAr.rowcount ; i ++){
				ds_listAr.setColumn(i, "receiveAmt", ds_listAr.getColumn(i, "arBalAmt"));
			}
		} else if (allEditYn == "N"){
			msk_allEditAmt.enable = true;
			
			//적용금액을 순차적으로 차감 일괄적용금액만큼
			for (var i = 0 ; i < ds_listAr.rowcount ; i ++){
				var arBalAmt = ds_listAr.getColumn(i, "arBalAmt");
				
				if (allAmt > arBalAmt){
					allAmt -= arBalAmt;
					ds_listAr.setColumn(i, "receiveAmt", ds_listAr.getColumn(i, "arBalAmt"));
				} else {
					ds_listAr.setColumn(i, "receiveAmt", allAmt);
					allAmt = 0;
				}
			}
		}
	}
	msk_receiveAmt.value = ds_listAr.getSum('receiveAmt');
}

