function fn_SaveTrxData(real_CUST_ID)
{
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 단체급식 일때 처리
    if(div_mak10010e.visible) {
		
		if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "CUSTNM"))== "0"){    

			g_Message("EE", "고객사명을 선택하여 주십시요.\n" +	"확인하여 주십시요.");        
			div_mak10010e.edt_CUSTNM.setFocus();
			return false;
		}
	   
		f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "CUST_ID");
		
		if (numFormat(real_CUST_ID)== "0"){
		
			ds_cond.clear();
			ds_cond.addColumn("MAK_CUST_MST_A_S","INT",256);
						
			// action 정보 초기화
			fsp_clear(this);

			// 서버에서 조회할 정보 추가
			fsp_addSearch(this, "ma/mak:MAK10010E_S002");

			// 서버 호출
			http.Sync = true;
			fsp_callService(this, "", "", "", ds_cond.name + "=ds_list","","");
			http.Sync = false;
			
			real_CUST_ID = ds_cond.getColumn(ds_cond.rowposition, "MAK_CUST_MST_A_S");
			ds_listInput.setColumn(ds_listInput.rowposition, "CUST_ID", real_CUST_ID);
						
			f_Saved_ID  = 0;
			f_NewDataCheck = true;		
		}
		return true;
	
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 식재영업 일때 처리
	}else{
		
		if (gfn_length(ds_listInput2.getColumn(ds_listInput2.rowposition, "CUSTNM"))== "0") {
			g_Message("EE", "고객사명을 선택하여 주십시요.\n" + "확인하여 주십시요.");        
			div_mak10011e.edt_CUSTNM.setFocus();
			return false;
		}
	   
		f_Saved_ID = ds_listInput2.getColumn(ds_listInput2.rowposition, "CUST_ID");
		
		if (numFormat(real_CUST_ID)== "0"){
		
			ds_cond.clear();
			ds_cond.addColumn("MAK_CUST_MST_B_S","INT",256);
						
			// action 정보 초기화
			fsp_clear(this);

			// 서버에서 조회할 정보 추가
			fsp_addSearch(this, "ma/mak:MAK10011E_S002");

			// 서버 호출
			http.Sync = true;
			fsp_callService(this, "", "", "", ds_cond.name + "=ds_list","","");
			http.Sync = false;
			
			real_CUST_ID = ds_cond.getColumn(ds_cond.rowposition, "MAK_CUST_MST_B_S");
			ds_listInput2.setColumn(ds_listInput2.rowposition, "CUST_ID", real_CUST_ID);
			
			f_Saved_ID  = 0;
			f_NewDataCheck = true;
		}

		return true;
	}
}
