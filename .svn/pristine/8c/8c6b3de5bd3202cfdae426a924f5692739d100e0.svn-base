function fn_AfterSaveSearch(errCode, errMsg) {
	
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 단체급식 일때 처리
	if(div_mak10010e.visible) {	
		if(errCode != 0) {		
			g_Message("EE");		
			return;
		}		
		fn_insert();	
		
		//alert(" f_Saved_ID ++> "+f_Saved_ID);
		
		fn_SelectedCustSearch(f_Saved_ID);
		//ds_listInput_OnColumnChanged(ds_listInput,0,"CURR_OP_VENDOR","",ds_listInput.getColumn(ds_listInput.rowposition, "CURR_OP_VENDOR"),0);		
		ds_listInput.setColumn(0,"CURR_OP_VENDOR",ds_listInput.getColumn(ds_listInput.rowposition, "CURR_OP_VENDOR"));
	
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 식재영업 일때 처리
	}else {
		
		if (errCode != 0){		
			g_Message("EE");		
			return;
		}
		fn_search();		
		http.Sync = true;
		ds_list.rowposition = ds_list.findRowExpr("CUST_ID==" + f_Saved_ID);
		http.Sync = false;
	}
}

