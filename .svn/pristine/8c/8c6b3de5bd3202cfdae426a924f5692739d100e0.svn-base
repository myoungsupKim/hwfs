		(e.columnid == "MEAL_UPRICE_C_3") ||
		(e.columnid == "MEAL_UPRICE_D_1") ||
		(e.columnid == "MEAL_UPRICE_D_2") ||
		(e.columnid == "MEAL_UPRICE_D_3") ||
		(e.columnid == "MEAL_UPRICE_E_1") ||
		(e.columnid == "MEAL_UPRICE_E_2") ||
		(e.columnid == "MEAL_UPRICE_E_3") ||
		(e.columnid == "SICKBED_CNT") ||
		(e.columnid == "SALE_A") ||
		(e.columnid == "SALE_B") ||
		(e.columnid == "SALE_C") ||
		(e.columnid == "SALE_D") ||
		(e.columnid == "EXP_PROFIT_RATIO") ||
		(e.columnid == "EXP_INVEST_AMT") ||
		(e.columnid == "EMPLOYEE_A") ||
		(e.columnid == "EMPLOYEE_B") ||
		(e.columnid == "EMPLOYEE_C") ||
		(e.columnid == "EMPLOYEE_D") ||
		(e.columnid == "EMPLOYEE_E") ||
		(e.columnid == "EMPLOYEE_F") ||
		(e.columnid == "EMPLOYEE_A_PAY") ||
		(e.columnid == "EMPLOYEE_B_PAY") ||
		(e.columnid == "EMPLOYEE_C_PAY") ||
		(e.columnid == "EMPLOYEE_D_PAY") ||
		(e.columnid == "EMPLOYEE_E_PAY") ||
		(e.columnid == "EMPLOYEE_F_PAY") ||
