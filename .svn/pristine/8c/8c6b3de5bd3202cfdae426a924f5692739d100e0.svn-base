//edtBtn Component 세팅/초기화####################################################################
function Set_edtBtn_UserData(str_Gubun)
{        
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 단체급식 일때 처리
	if(div_mak10010e.visible) {
	
		if(str_Gubun == "Copy"){

			//edtBtn Component 초기화############################  		 
			for (i=0;i<components.length;i++){							
				if((gfn_subStr(components[i].name, 0, 17)== "div_mak10010e.st_") && (gfn_subStr(components[i].userdata, 0, 10)== "strMAQuery")) {
					if(gfn_length(eval("edt_" + gfn_subStr(components[i].name, 3)).Column)!= 0) {
						eval("div_mak10010e.edt_" + gfn_subStr(components[i].name, 3)).userdata = ds_listInput.getColumn(ds_listInput.rowposition, gfn_subStr(components[i].name, 3));
					}
				}
			}
			//edtBtn Component 초기화############################
		}else if (str_Gubun == "Clear"){

			//edtBtn Component 초기화############################
			for (i=0;i<components.length;i++){
					
					
				if((gfn_subStr(components[i].name, 0, 17)== "div_mak10010e.st_") && (gfn_subStr(components[i].userdata, 0, 10)== "strMAQuery")) {
					if (gfn_length(components[i].Column)!= 0)
					eval("div_mak10010e.edt_" + gfn_subStr(components[i].name, 3)).userdata = "";
				}
			}  
			//edtBtn Component 초기화############################
		}
		
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 식재영업 일때 처리
	}else {
		
		if (str_Gubun == "Copy"){
			//edtBtn Component 초기화############################ 			
			for (i=0;i<components.length;i++) {			
				if((gfn_subStr(components[i].name, 0, 17)== "div_mak10011e.st_") && (gfn_subStr(components[i].userdata, 0, 10)== "strMAQuery")) {
					if(gfn_length(eval("div_mak10011e.edt_" + gfn_subStr(components[i].name, 3)).column)!= 0) {	
						trace( eval("div_mak10011e.edt_" + gfn_subStr(components[i].name, 3)) );
						eval("div_mak10011e.edt_" + gfn_subStr(components[i].name, 3)).userdata = ds_listInput2.getColumn(ds_listInput2.rowposition, gfn_subStr(components[i].name, 3));				
					}
				}
			}
			//edtBtn Component 초기화############################
			
		}else if (str_Gubun == "Clear"){
			//edtBtn Component 초기화############################
			for (i=0;i<components.length;i++){
				if((gfn_subStr(components[i].name, 0, 17)== "div_mak10011e.st_") && (gfn_subStr(components[i].userdata, 0, 10)== "strMAQuery")) {
					if (gfn_length(components[i].column)!= 0)
						eval("div_mak10011e.edt_" + gfn_subStr(components[i].name, 3)).userdata = "";
				}
			}  
			//edtBtn Component 초기화############################
		}
	}
}

