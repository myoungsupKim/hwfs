/*----------------------------------------------------------------------------------------------
 * 설명      : Edit 관련 이벤트 실제 처리
 *---------------------------------------------------------------------------------------------*/
function fn_Trigger_edBtn(obj)
{
	switch(obj)
	{
		// 사업부 변경 시
		case "comp_mu" :
			ds_input.setColumn(ds_input.rowposition, "TM_CD", "");
			ds_input.setColumn(ds_input.rowposition, "TM_NM", "");
			ds_input.setColumn(ds_input.rowposition, "MAIN_UPJANG", "");
			ds_input.setColumn(ds_input.rowposition, "MAIN_UPJANG_NM", "");
			ds_input.setColumn(ds_input.rowposition, "UPJANG", "");
			ds_input.setColumn(ds_input.rowposition, "UPJANG_NM", "");
			break;

		// 팀 변경 시
		case "comp_tm" :
			//ds_input.setColumn(ds_input.rowposition, "MAIN_UPJANG", "");
			//ds_input.setColumn(ds_input.rowposition, "MAIN_UPJANG_NM", "");
			ds_input.setColumn(ds_input.rowposition, "UPJANG", "");
			ds_input.setColumn(ds_input.rowposition, "UPJANG_NM", "");
			break;
		
		
		// 본사 변경 시
 		case "comp_mainUpjang" :
			ds_input.setColumn(ds_input.rowposition, "UPJANG", "");
			ds_input.setColumn(ds_input.rowposition, "UPJANG_NM", "");
 			break;

		default:

			break;
	}
}



// 사업구분 변경 시 처리 : AS_IS
function cbo_Biz_OnChanged(obj:Combo, e:ItemChangeEventInfo)
{
	// 변경여부 확인
	if(gfn_dsIsUpdated(ds_list1)==true || gfn_dsIsUpdated(ds_list2)==true || gfn_dsIsUpdated(ds_list3)==true || gfn_dsIsUpdated(ds_list4)==true || gfn_dsIsUpdated(ds_list5)==true || gfn_dsIsUpdated(ds_list6)==true)
	{
		if(g_Confirm("02","변경된 내역이 있습니다. 변경된 내역을 저장하시겠습니까?"))
		{
			fn_save();
		}
	}
	
	ds_main.clearData();
	ds_list1.clearData();
	ds_list2.clearData();
	ds_list3.clearData();
	ds_list4.clearData();
	ds_list5.clearData();
	ds_list6.clearData();
	
	// 팀정보 Clear
	ed_ScTm.value = "";
	ed_ScTm.userdata = "";
	
	if (cbo_Biz.text == "식재영업")
	{
		st_Upjang.userdata = "strQuery_FMUpjang";
		
		ed_MainUpjang.enable = true;
		btn_MainUpjang.enable = true;
		ed_Upjang.value = "";
		ed_Upjang.userdata = "";	
				
		tab_list.tab1.grd_list.setFormatColProperty(2,"size",0);	
		tab_list.tab2.grd_list.setFormatColProperty(2,"size",0);	
		tab_list.tab3.grd_list.setFormatColProperty(2,"size",0);	
		tab_list.tab4.grd_list.setFormatColProperty(2,"size",0);	
		tab_list.tab5.grd_list.setFormatColProperty(2,"size",0);	
	}	
	else
	{
		st_Upjang.userdata = "strQuery_Upjang"; 
		
		ed_MainUpjang.enable = false;
		btn_MainUpjang.enable = false;	
		ed_MainUpjang.value = "";
		ed_MainUpjang.userdata = "";
		ed_Upjang.value = "";
		ed_Upjang.userdata = "";		
		
		tab_list.tab1.grd_list.setFormatColProperty(2,"size",144);	
		tab_list.tab2.grd_list.setFormatColProperty(2,"size",144);
		tab_list.tab3.grd_list.setFormatColProperty(2,"size",144);
		tab_list.tab4.grd_list.setFormatColProperty(2,"size",144);
		tab_list.tab5.grd_list.setFormatColProperty(2,"size",144);
	}
}

