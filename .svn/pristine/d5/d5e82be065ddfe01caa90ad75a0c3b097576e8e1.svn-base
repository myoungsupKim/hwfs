	}		
	
	nRow = ds_list4.rowposition;
	
	// 해지일자 체크(현금보증)
	nFRow = ds_list4.findRowExpr("CASH_CANCEL_YN=='1'&&CASH_CANCEL_DATE==null");
	if( nFRow !=-1 )
	{
		ds_list4.rowposition = nFRow;
		g_Message("EE", "해지일자를 입력하시기 바랍니다.");
		tab_list.tabindex = 3;
		tab_list.tab4.grd_list.setFocus();
		return false;
	}		

	nRow = ds_list5.rowposition;
	
	// 해지일자 체크(질권)
	nFRow = ds_list5.findRowExpr("PLEDGER_CANCEL_YN=='1'&&PLEDGER_CANCEL_DATE==null");
	if( nFRow !=-1 )
	{
		ds_list5.rowposition = nFRow;
		g_Message("EE", "해지일자를 입력하시기 바랍니다.");
		tab_list.tabindex = 4;
		tab_list.tab5.grd_list.setFocus();
		return false;
	}	
	
	
	nRow = ds_list7.rowposition;
	
	// 해지일자 체크(예치보증금)
	nFRow = ds_list7.findRowExpr("GUARAN_CANCEL_YN=='1'&&GUARAN_CANCEL_DATE==null");
	if( nFRow !=-1 )
	{
		ds_list7.rowposition = nFRow;
		g_Message("EE", "해지일자를 입력하시기 바랍니다.");
		tab_list.tabindex = 1;
		tab_list.tab7.grd_list.setFocus();
		return false;
	}	

	
	
	
	nRow = ds_list2.rowposition;
	
	// 보증번호 만기일자 체크
	nFRow = ds_list2.findRowExpr("GUARAN_BEGIN_DATE>GUARAN_EXPIRE_DATE");
	if( nFRow !=-1 )
	{
		ds_list2.rowposition = nFRow;
		g_Message("EE", "보증보험의 만기일자를 확인하십시오.");
		tab_list.tabindex = 1;
		tab_list.tab2.grd_list.setFocus();
		return false;
	}

	nRow = ds_list5.rowposition;
	
	// 질권 만기일자 체크
	nFRow = ds_list5.findRowExpr("PLEDGER_BEGIN_DATE>PLEDGER_EXPIRE_DATE");
	if( nFRow !=-1 )
	{
		ds_list5.rowposition = nFRow;
		g_Message("EE", "질권의 만기일자를 확인하십시오.");
		tab_list.tabindex = 4;
		tab_list.tab4.grd_list.setFocus();
		return false;
	}
	
	nRow = ds_list7.rowposition;
	
	// 보증번호 만기일자 체크
	nFRow = ds_list7.findRowExpr("GUARAN_BEGIN_DATE>GUARAN_EXPIRE_DATE");
	if( nFRow !=-1 )
	{
		ds_list7.rowposition = nFRow;
		g_Message("EE", "보증보험의 만기일자를 확인하십시오.");
		tab_list.tabindex = 1;
		tab_list.tab7.grd_list.setFocus();
		return false;
	}
	
	
    
    // 저장확인
	if(!g_Confirm("02","담보정보를 저장하시겠습니까?"))
	{
		return false;
	}
    
    return true;
}

