function btn_RowInsert_OnClick(obj:Button, e:ClickEventInfo)
{
    if (ds_main.rowcount == 0) return;
    
    var dstmp = "ds_list"+(tab_list.tabindex+1); 
	var grdtmp = "tab_list.tab"+(tab_list.tabindex+1)+".grd_list";
	var row = eval(dstmp).addRow();

	if(tab_list.tabindex == 0)	SCPopup.Address.newData(ds_list1, row);
	if(tab_list.tabindex == 2)	SCPopup.Address.newData(ds_list3, row);

	eval(dstmp).setColumn(row, "UPJANG", ds_main.getColumn(ds_main.rowposition, "UPJANG"));
	
	eval(grdtmp).setFocus();
	//if (cbo_Biz.text == "식재영업") eval(grdtmp).setCellPos(3);
	if (comp_mu.cbo_mu.value == fstrFics) {
		eval(grdtmp).setCellPos(3);
	}
	else {
		fn_custSet();	
		eval(grdtmp).setCellPos(2);
	}
	
	
}

function btn_RowDelete_OnClick(obj:Button, e:ClickEventInfo)
{
	fsp_clear(this);
	
	var deleteSql = "fm/fms:FMS00500E_D00"+(tab_list.tabindex+1);
	var inData = "ds_list"+(tab_list.tabindex+1)+"=ds_list"+(tab_list.tabindex+1)+":U";
	var deleteDs = "ds_list"+(tab_list.tabindex+1);
	
	if(eval(deleteDs).rowposition == -1) 
	{
		g_Message("EE", "선택된 항목이 없습니다.");
		return;
	}
	
	if(!g_Confirm("02","선택한 항목을 삭제 하시겠습니까?")) return;
	
	eval(deleteDs).deleteRow(eval(deleteDs).rowposition);
	
	fsp_addSave(this, "", "", deleteSql, "", "", "", "");

	// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", inData, "" , "", "fn_afterSave");
	http.Sync = false;
	
	//g_Message("01", "삭제 작업이 정상적으로 처리되었습니다");	
}

function grd_list_OnCellDblClick(obj:Grid, e:GridClickEventInfo)
{
	// 변경여부 확인
	if(gfn_dsIsUpdated(ds_list1)==true || gfn_dsIsUpdated(ds_list2)==true || gfn_dsIsUpdated(ds_list3)==true || gfn_dsIsUpdated(ds_list4)==true || gfn_dsIsUpdated(ds_list5)==true || gfn_dsIsUpdated(ds_list6)==true || gfn_dsIsUpdated(ds_list7)==true)
	{
		if(g_Confirm("02","변경된 내역이 있습니다. 변경된 내역을 저장하시겠습니까?"))
		{
			fn_save();
		}
	}

	// 초기화
	ds_list1.clearData();
	ds_list2.clearData();
	ds_list3.clearData();
	ds_list4.clearData();
	ds_list5.clearData();
	ds_list6.clearData();
	ds_list7.clearData();
	
	// 조회
	var actionName, cmdName, inData, outData, otherArg, callBackFnc;
	
	inData			= "";
	outData			= "ds_list1=ds_list1 ds_list2=ds_list2 ds_list3=ds_list3 ds_list4=ds_list4 ds_list5=ds_list5 ds_list6=ds_list6 ds_list7=ds_list7";
	callBackFnc		= "";
	otherArg        = "UPJANG=" + ds_main.getColumn(e.row, "UPJANG");
	otherArg		+= " maskAuth="+SCAuth.getMaskAuth(this);

	fsp_clear(this); 
	fsp_addSearch(this, "fm/fms:FMS00500E_S003");	
	fsp_addSearch(this, "fm/fms:FMS00500E_S004");	
	fsp_addSearch(this, "fm/fms:FMS00500E_S005");	
	fsp_addSearch(this, "fm/fms:FMS00500E_S006");	
	fsp_addSearch(this, "fm/fms:FMS00500E_S007");	
	fsp_addSearch(this, "fm/fms:FMS00500E_S009");	
	fsp_addSearch(this, "fm/fms:FMS00500E_S010");	// 예치	보증금
	
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출 
    
    //if (cbo_Biz.text == "단체급식")
	if (comp_mu.cbo_mu.value != fstrFics)
    {
        ds_cust.clearData();
        
		inData			= "";
		outData			= "ds_cust=ds_cust";
		callBackFnc		= "";
		otherArg        = "UPJANG=" + ds_main.getColumn(e.row, "UPJANG");
	
		fsp_clear(this); 
		fsp_addSearch(this, "fm/fms:FMS00500E_S008");	
		
		fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출 		
    }

	
}

/*
거래처 정보 Dataset(ds_cust) Callback 함수
*/
function fn_custSet()
{
	if (ds_cust.getRowCount()==0) {
		ds_cust.addRow();
	}
}

