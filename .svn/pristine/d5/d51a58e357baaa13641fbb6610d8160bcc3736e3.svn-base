/******************************************************************************************
 * 함  수  명   : grd_guidReqList_oncelldblclick(obj:Grid, e:GridClickEventInfo)
 * 기      능   : 지도신청 선택
 * 작  성  자   : 김민수
 * 작성  일자   : 2015-03-18  
 ******************************************************************************************/
function grd_guidReqList_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	var nRow = e.row;
	
	ds_guidReqMst.clearData();
	ds_guidReqMst.addRow();
	ds_guidReqMst.updatecontrol = false;
	ds_guidReqMst.setRowType(0, Dataset.ROWTYPE_UPDATE);
	ds_guidReqMst.updatecontrol = true;
	ds_guidReqMst.copyRow(0, ds_guidReqList, nRow);
	
	var sStatusCd = ds_guidReqMst.getColumn(0, "STATUS_CD");
	// 신청, 재신청 상태일 경우 승인, 반려 버튼 활성화
	if(sStatusCd == "01" || sStatusCd == "04") 
	{
		// 지도담당자 정보 세팅
		ds_guidReqMst.setColumn(0, "GUID_PRCG_NM", gfn_nvl(g_Name,""));
		ds_guidReqMst.setColumn(0, "GUID_PRCG", gfn_nvl(g_EmpNo,""));
	
		btn_approval.enable = true;
		btn_reject.enable = true;
	}
	
	fsp_clear(this);
	fsp_addSearch(this,"fs/fsm:FSM00710E_S005"); // 점장 최근 3개점 정보
	fsp_callService(this,"","","","ds_chargeInfo=ds_chargeInfo","UPJANG=" + ds_guidReqList.getColumn(e.row, "UPJANG"),"");		
}

