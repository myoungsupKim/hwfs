/******************************************************************************************
 * 시스템구분   : 메뉴관리/메뉴지도
 * 프로그램명   : FSM00710E.XML
 * 기      능   : 메뉴지도신청
 * 작  성  자   : 김민수
 * 작성  일자   : 2015-03-18
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************/
 
//=========================================================================================
// [ PART 1 ]
// Form에서 Include 할 내용을 정의
//
//=========================================================================================

//=========================================================================================
// [ PART 2 ]
// Form에서 사용될 전역변수를 선언
//
//=========================================================================================

//=========================================================================================
// [ PART 3 ]
// Form Loading 시 작업 정의
//
//=========================================================================================
function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(this);

	// X-Framework : Action 정보 생성
	fsp_init(this);
	
	//공통코드조회
	var objCode = fn_common_code("'MA0005', 'FS0119', 'FS0120'","","","non","Y");

	// 배식방법
	var sCond = "GROUP_CODE == 'MA0005'";
	objCode.filter(sCond);
	CopyF(ds_cboRationMethod, objCode.name);

	// 메뉴지도-차수상태
	var sCond = "GROUP_CODE == 'FS0119'";
	objCode.filter(sCond);
	CopyF(ds_cboStatusCd, objCode.name);
	
	// 마감 여부
	var sCond = "GROUP_CODE == 'FS0120'";
	objCode.filter(sCond);
	CopyF(ds_cboCloseYn, objCode.name);
	
	// 조회조건 DataSet 초기화
	ds_cond.clearData();
	ds_cond.addRow();
	ds_cond.setColumn(0, "GUID_YEAR", gfn_getYear(gfn_today()));
	
	btn_approval.enable = false;
	btn_reject.enable = false;
}

//=========================================================================================
// [ PART 4 ]
// Main Process Event 정의(조회,입력,삭제,저장,출력)
//
//=========================================================================================
/******************************************************************************************
 * 함  수  명   : fn_search()
 * 기      능   : 조회
 * 작  성  자   : 김민수
 * 작성  일자   : 2015-03-18  
 ******************************************************************************************/
function fn_search() 
{
	if(gfn_isNull(ds_cond.getColumn(0, "GUID_YEAR")))
	{
		g_Message("EE", "지도년도를 입력해주세요.");
		cal_guidYear.setFocus();
		return;	
	}

	// 데이터 초기화
	ds_guidSeqList.clearData();
	ds_guidReqList.clearData();
	ds_guidReqMst.clearData();
	ds_chargeInfo.clearData();

	fsp_clear(this);
	fsp_addSearch(this,"fs/fsm:FSM00710E_S001");	
	fsp_callService(this,"","","ds_cond=ds_cond","ds_guidSeqList=ds_guidSeqList","","fn_callBackSearch");
}

function fn_callBackSearch(errCode, errMsg) 
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오!");
		return;
	}
	else
	{
		btn_approval.enable = false;
		btn_reject.enable = false;
	}
}

