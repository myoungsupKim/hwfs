<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT V.UPJANG,
       A.UPJANGNM
  FROM MAS_DUALUPJANG_MGMT V,
       SCO_UPJANG_MST_V A
  WHERE V.UPJANG   = A.UPJANG
  AND   V.SABUN    = #SABUN#
  AND   V.USE_YN   = 'Y' 
UNION 
SELECT V.UPJANG,
       A.UPJANGNM
  FROM SCC_USERINFO V,
       SCO_UPJANG_MST_V A
  WHERE V.UPJANG = A.UPJANG
  AND   V.SABUN    = #SABUN#
</statement>
	<input default-name="ds_DUALUPJANG_MGMT">
<col name="SABUN" size="20" type="VARCHAR" description="ป็น๘" /> 	
	</input>
	<output default-name="ds_DUALUPJANG_MGMT">
	</output>
</query>