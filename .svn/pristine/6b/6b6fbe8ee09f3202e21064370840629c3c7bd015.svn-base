function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			btn_appendCancel.visible = false;
			fv_isTabChange = true;
			
			if (ds_list.getRowCount() < 1) {
				tab_info.tpg_corp.AddrSearchComponent00.clearAll();
				tab_info.tpg_psnl.AddrSearchComponent00.clearAll();
			}
			
			if (ds_list.getColumn(ds_list.rowposition, "custGrp") == "1000")
			{
				tab_info.tabindex = 0;
				for (var i=0,len=tab_info.tpg_corp.components.length; i<len; i++)
				{
					tab_info.tpg_corp.components[i].readonly = true;
				}
				tab_info.tpg_corp.AddrSearchComponent00.setReadonly(true);
				tab_info.tpg_corp.uc_telNo.setReadonly(true);
				tab_info.tpg_corp.uc_faxNo.setReadonly(true);
			}
			else if (ds_list.getColumn(ds_list.rowposition, "custGrp") == "5000")
			{
				tab_info.tabindex = 2;
				for (var i=0,len=tab_info.tpg_frgn.components.length; i<len; i++)
				{
					tab_info.tpg_frgn.components[i].readonly = true;
				}
				
			}
			else if (ds_list.getColumn(ds_list.rowposition, "custGrp") == "6000")
			{
				tab_info.tabindex = 1;
				for (var i=0,len=tab_info.tpg_psnl.components.length; i<len; i++)
				{
					tab_info.tpg_psnl.components[i].readonly = true;
				}
				tab_info.tpg_psnl.AddrSearchComponent00.setReadonly(true);
			}
			
			fv_isTabChange = false;
			
            //trace(ds_list.saveXML());
            break;
        
        // 중복검사
        case "searchExistsList" :
			//trace(fv_status);
			//trace(ds_existsList.saveXML());
			break;
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			try {
				if (P_SHOW_POPUP) {
					var rtn = new Array();
					rtn["requestSysId"] = "IFC";
					rtn["requestNum"] = fv_requestNum;
					this.close(rtn);
				}
				else {
					fn_search();
				}
			}
			catch (e) {
				fn_search();
			}
            break;    
    }
}


