function btn_reRequestProc_onclick(obj:Button,  e:ClickEventInfo)
{
	var targetData = [];
	var rejectRow = ds_list.rowposition;	
	
	for (var i=0, len=ds_list.getColCount(); i<len; i++)
	{
		targetData[ds_list.getColID(i)] = ds_list.getColumn(ds_list.rowposition, ds_list.getColID(i));
	}
	
	ds_list.setColumn(ds_list.rowposition, "procResult", "1");
	ds_list.setColumn(ds_list.rowposition, "procResultNm", "부결(재요청)");
	
	var nRow = ds_list.addRow();
	
	for (var i=0, len=ds_list.getColCount(); i<len; i++)
	{
		ds_list.setColumn(nRow, ds_list.getColID(i), targetData[ds_list.getColID(i)]);
	}
	
	ds_list.setColumn(ds_list.rowposition, "procResult", "D");
	ds_list.setColumn(ds_list.rowposition, "procResultNm", "요청대기");
	ds_list.setColumn(ds_list.rowposition, "rtnReason", "");
	ds_list.setColumn(ds_list.rowposition, "rejectRow", rejectRow);
	ds_list.setColumn(ds_list.rowposition, "sapRequestYn", "Y");
	
	btn_appendCancel.visible = true;
	btn_reRequestProc.visible = false;
	fn_CorpComponentIsLock(false);
	fn_PsnlComponentIsLock(false);
	fn_FrgnComponentIsLock(false);
	
	if (tab_info.tabindex == 0)
	{
		ds_list.setColumn(nRow, "custNm", ds_list.getColumn(rejectRow, "custNm"));
		tab_info.tpg_corp.msk_bsnNo.readonly = false;
		tab_info.tpg_corp.msk_subUpjangNum.readonly = false;
	}
	
	switch (ds_list.getColumn(ds_list.rowposition, "custClass"))
	{
		case "001" :
			tab_info.tpg_corp.edt_acctCd.readonly = true;
			tab_info.tpg_corp.cbo_payCond.readonly = true;
			tab_info.tpg_corp.edt_vendorAcctCd.readonly = false;
			tab_info.tpg_corp.cbo_vendorPayCond.readonly = false;

			tab_info.tpg_psnl.edt_acctCd.readonly = true;
			tab_info.tpg_psnl.cbo_payCond.readonly = true;
			tab_info.tpg_psnl.edt_vendorAcctCd.readonly = false;
			tab_info.tpg_psnl.cbo_vendorPayCond.readonly = false;

			tab_info.tpg_frgn.edt_acctCd.readonly = true;
			tab_info.tpg_frgn.cbo_payCond.readonly = true;
			tab_info.tpg_frgn.edt_vendorAcctCd.readonly = false;
			tab_info.tpg_frgn.cbo_vendorPayCond.readonly = false;
			break;
		case "002" :
			tab_info.tpg_corp.edt_acctCd.readonly = false;
			tab_info.tpg_corp.cbo_payCond.readonly = false;
			tab_info.tpg_corp.edt_vendorAcctCd.readonly = true;
			tab_info.tpg_corp.cbo_vendorPayCond.readonly = true;

			tab_info.tpg_psnl.edt_acctCd.readonly = false;
			tab_info.tpg_psnl.cbo_payCond.readonly = false;
			tab_info.tpg_psnl.edt_vendorAcctCd.readonly = true;
			tab_info.tpg_psnl.cbo_vendorPayCond.readonly = true;

			tab_info.tpg_frgn.edt_acctCd.readonly = false;
			tab_info.tpg_frgn.cbo_payCond.readonly = false;
			tab_info.tpg_frgn.edt_vendorAcctCd.readonly = true;
			tab_info.tpg_frgn.cbo_vendorPayCond.readonly = true;
			break;
		case "003" :
			tab_info.tpg_corp.edt_acctCd.readonly = false;
			tab_info.tpg_corp.cbo_payCond.readonly = false;
			tab_info.tpg_corp.edt_vendorAcctCd.readonly = false;
			tab_info.tpg_corp.cbo_vendorPayCond.readonly = false;

			tab_info.tpg_psnl.edt_acctCd.readonly = false;
			tab_info.tpg_psnl.cbo_payCond.readonly = false;
			tab_info.tpg_psnl.edt_vendorAcctCd.readonly = false;
			tab_info.tpg_psnl.cbo_vendorPayCond.readonly = false;

			tab_info.tpg_frgn.edt_acctCd.readonly = false;
			tab_info.tpg_frgn.cbo_payCond.readonly = false;
			tab_info.tpg_frgn.edt_vendorAcctCd.readonly = false;
			tab_info.tpg_frgn.cbo_vendorPayCond.readonly = false;
			break;
	}
}

