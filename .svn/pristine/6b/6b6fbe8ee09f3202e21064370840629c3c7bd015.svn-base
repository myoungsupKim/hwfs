/**
 * @class 사업자번호에 대하여 거래처를 중복검사를 위해 해당 자료를 조회해 온다.
 */
function fn_searchExistsList(val)
{
    var strSvcID    = "searchExistsList";
    var strURL      = "sc/scz/custRequest/selectExists.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_existsList=ds_existsList ds_existsStCustList=ds_existsStCustList ds_existsSccCustRequestList=ds_existsSccCustRequestList ds_existsSccSapCustList=ds_existsSccSapCustList";
    var strArg      = "maskAuth=" + SCAuth.getMaskAuth(this) + " " + val;
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    if (arguments.length == 2)
    {
		bAsync = arguments[1];
    }
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync, false);
}

/**
 * @class ds_list의 값을 지우고 초기 설정을 한다.
 */
function fn_clearData(row)
{
	
	if (tab_info.tabindex == 0)
	{
		var doNotClearColumns = "bsnNo,subUpjangNum,muCd,custGrp,addr";
		
		for (var i=0, len=ds_list.getColCount(); i<len; i++)
		{
			if (doNotClearColumns.indexOf(ds_list.getColID(i)) < 0)
			{
				ds_list.setColumn(row, ds_list.getColID(i), "");
			}
		}
		
		ds_list.setColumn(row, "custGrp", "1000"); //일반-법인으로 설정
		ds_list.setColumn(row, "nationCd", "KR"); //국가코드
		ds_list.setColumn(row, "currencyCd", "KRW"); //통화
		ds_list.setColumn(row, "custType", "001"); //거래처 유형
		ds_list.setColumn(row, "custClass", "002"); //거래처 구분
		ds_list.setColumn(row, "elctnTaxBillYn", "Y"); //전자세금계산서 여부
		ds_list.setColumn(row, "vatType", "1"); //부가세 구분
		ds_list.setColumn(row, "inspectType", "1"); //회계심사
	}
	else if (tab_info.tabindex == 1)
	{
		var doNotClearColumns = "custType,muCd,custGrp,addr";
		
		for (var i=0, len=ds_list.getColCount(); i<len; i++)
		{
			if (doNotClearColumns.indexOf(ds_list.getColID(i)) < 0)
			{
				ds_list.setColumn(row, ds_list.getColID(i), "");
			}
		}
	}
}

function fn_setData(row)
{
	var exceptionCol = "";
	var nRow2 = ds_existsList.rowposition;
	var ds = ds_existsList;
	
//	var isReadOnly = false;
	
	if (arguments.length >= 2)
	{
		exceptionCol = arguments[1];
		
		if (arguments.length == 3)
		{
			nRow2 = arguments[2];
//			isReadOnly = true;
		}
		
		if (arguments.length == 4)
		{
			nRow2 = arguments[2];
			ds = arguments[3];
//			isReadOnly = true;
		}

	}
	
	for (var i=0, len=ds_list.getColCount(); i<len; i++)
	{
		if (!(ds_list.getColID(i) == "bsnNo" || 
		      ds_list.getColID(i) == "subUpjangNum" ||
		      ds_list.getColID(i) == "requestSysCd" ||
		      ds_list.getColID(i) == "requestNum" ||
		      ds_list.getColID(i) == "requestSabun" ||
		      ds_list.getColID(i) == "requestDate" ||
		      ds_list.getColID(i) == "requestTime" ||
		      ds_list.getColID(i) == "muCd" ||
		      ds_list.getColID(i) == "muNm" ||
		      ds_list.getColID(i) == "custTypeNm" ||
		      ds_list.getColID(i) == "custClassNm" ||
		      ds_list.getColID(i) == "detailAddr" ||
		      ds_list.getColID(i) == "custGrp" ||
		      ds_list.getColID(i) == "rejectRow" ||
		      ds_list.getColID(i) == "sapRequestYn"))
		{
			if (exceptionCol != "" && exceptionCol.indexOf(ds_list.getColID(i)) < 0)
			{
				ds_list.setColumn(row, ds_list.getColID(i), ds.getColumn(nRow2, ds_list.getColID(i)));
			}
		}
	}
	
}

