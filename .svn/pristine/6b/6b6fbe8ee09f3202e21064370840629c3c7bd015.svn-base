				}
				obj.setColumn(e.row, "sapRequestYn", "Y");
				obj.setColumn(e.row, "hlevelCustCd", "");
			}
			
			obj.setColumn(e.row, "custGrp", "6000");
			obj.setColumn(e.row, "nationCd", "KR");
			obj.setColumn(e.row, "currencyCd", "KRW");
			obj.setColumn(e.row, "custClass", "002");
			obj.setColumn(e.row, "inspectType", "1");
		}
	}
