/************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역 (필수)
************************************************************************************************/
/**
 * @class Columnu의 값이 변경된 후에 발생이벤트
 */
function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	//국내 거래처
	if (tab_info.tabindex == 0)
	{
		if (e.columnid == "bsnNo" || e.columnid == "subUpjangNum")
		{
			if (gfn_isNull(obj.getColumn(e.row, "rejectRow"))) {
				fn_corp_ds_list_oncolumnchanged(obj, e);
			}
		}
	}
	//개인 거래처
	else if (tab_info.tabindex == 1)
	{
		if (e.columnid == "idNoEnc")
		{
			fn_psnl_ds_list_oncolumnchanged(obj, e);
		}
	}
	//해외 거래처
	else if (tab_info.tabindex == 2)
	{
		if (e.columnid == "frgnCustNum")
		{
			fn_frgn_ds_list_oncolumnchanged(obj, e);
		}
	}
}

function tab_info_tpg_corp_msk_bsnNo_onkeydown(obj:MaskEdit, e:KeyEventInfo)
{
	if (!obj.readonly) {
		ds_existsList.clearData();
		tab_info.tpg_corp.msk_subUpjangNum.value = "";
	}
}

/**
 * @class 등록을 취소한다.
 */
 function btn_appendCancel_onclick(obj:Button,  e:ClickEventInfo)
{
	var rejectRow = -1;
	
	if (!gfn_isNull(ds_list.getColumn(ds_list.rowposition, "rejectRow")))
	{
		rejectRow = ds_list.getColumn(ds_list.rowposition, "rejectRow");
	}
	
	ds_list.deleteRow(ds_list.rowposition);
	ds_existsList.filter("");
	ds_existsList.clearData();
	obj.visible = false;
	fv_isTabChange = true;
	
	if (tab_info.tabindex == 0)
	{
		tab_info.tpg_corp.msk_bsnNo.readonly = true;
		tab_info.tpg_corp.msk_subUpjangNum.readonly = true;
	}
	
	if (rejectRow != -1) {
		ds_list.setColumn(rejectRow, "procResult", "B");
		ds_list.setColumn(rejectRow, "procResultNm", "부결");
		ds_list.rowposition = rejectRow;
		fn_PsnlComponentIsLock(true);
		fn_CorpComponentIsLock(true);
	}
}

