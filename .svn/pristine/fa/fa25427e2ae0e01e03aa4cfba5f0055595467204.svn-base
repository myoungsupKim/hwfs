<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
  SELECT CASE WHEN JOURNALZ_YN = 'J' THEN 'J' ELSE '' END AS GAMU FROM MAS_WELFARE WHERE WELFARE_ID = #WELFARE_ID#
  </statement>
	<input default-name="ds_cond">
		<col name="WELFARE_ID" size="" type="NUMERIC" description="" /> 
	</input>
	<output default-name="ds_list">
		<col name="GAMU" size="255" type="VARCHAR" description="" /> 
	</output>
</query>