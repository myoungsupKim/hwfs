function btn_CustCd_onclick(obj:Button,  e:ClickEventInfo)
{
		var sSql  = "SELECT A.VD_CD \n";
			sSql += "     , A.VD_NM ||' ('||SUBSTR(BIZ_REG_NO, 1, 3)||'-'||SUBSTR(BIZ_REG_NO, 4, 2)||'-'||SUBSTR(BIZ_REG_NO, 6, 5)||')' AS VD_NM \n";
			sSql += "  FROM VO_FC_VENDOR A \n";
			sSql += " WHERE 1 = 1 \n";
			sSql += "   AND A.VD_CD IN ( SELECT CUSTCD FROM FSP_PO_SPECIAL_ITEM ) \n";

		fn_sql_common_setup(sSql,
							"VD_CD",
							"VD_NM",
							"거래처코드",
							"거래처명",
							"",
							"",
							"",
							"거래처코드조회",
							"");

		ed_CustCd.userdata = gds_sql_common.getColumn(0, "ret_code");
		ed_CustCd.value = gds_sql_common.getColumn(0, "ret_name");
}

