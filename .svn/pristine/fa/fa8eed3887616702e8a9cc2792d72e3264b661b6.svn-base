 * HISTORY    :
 ******************************************************************************************
 */

/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
 ************************************************************************************************/ 
include "U_lib::lib_conversionCom.xjs";

/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
 ************************************************************************************************/ 
var fa_Special_Auth;

//특판상품코드
var strLocalQuery_SPECIAL_ITEM              = "SELECT A.ITEM_CODE \n";
	strLocalQuery_SPECIAL_ITEM			   += "     , C.ITEM_NAME || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       C.ITEM_SIZE || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       DECODE(C.TAX_CODE, '100', '과세', '210', '면세', '영세') || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       A.CONTRACT_PRICE || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       NVL(B.SALE_PRICE, A.CONTRACT_PRICE) || ' | ' || \n";
	strLocalQuery_SPECIAL_ITEM			   += "       C.MAX_ORD_QTY AS ITEM_NAME \n";
	strLocalQuery_SPECIAL_ITEM			   += "  FROM HLDC_PO_CONTRACT_SPEC A \n";
	strLocalQuery_SPECIAL_ITEM			   += "     , FSP_PO_SPECIAL_ITEM B \n";
	strLocalQuery_SPECIAL_ITEM			   += "     , HLDC_PO_ITEM_MST C \n";
	strLocalQuery_SPECIAL_ITEM			   += " WHERE A.ITEM_CODE = B.ITEM_CODE \n";
	strLocalQuery_SPECIAL_ITEM			   += "   AND A.ITEM_CODE = C.ITEM_CODE \n";
var strLocalQuery_SPECIAL_ITEMKeyField      = "A.ITEM_CODE";
var strLocalQuery_SPECIAL_ITEMValueField    = "C.ITEM_NAME";
var strLocalQuery_SPECIAL_ITEMKeyFieldNM    = "상품코드";
var strLocalQuery_SPECIAL_ITEMValueFieldNM  = "상품명|구성|과면세|매입가|판매가|묶음수량"; 
var strLocalQuery_SPECIAL_ITEMCaption       = "특판상품";
var strLocalQuery_SPECIAL_ITEMAllField      = "ITEM_CODE;ITEM_NAME";

//부서코드
var strLocalQuery_CC_CD              = "SELECT DISTINCT CC_CD, MU_NM||' '||CC_NM AS CC_NM  FROM HLDC_SC_DEPT_V  WHERE (1=1) ";
var strLocalQuery_CC_CDKeyField      = "CC_CD";
var strLocalQuery_CC_CDValueField    = "CC_NM";
var strLocalQuery_CC_CDKeyFieldNM    = "CC코드";
var strLocalQuery_CC_CDValueFieldNM  = "CC코드명";
var strLocalQuery_CC_CDCaption       = "CC코드조회";
var strLocalQuery_CC_CDAllField      = "CC_CD;CC_NM";

//사원코드
var strLocalQuery_SABUN                    = "SELECT A.SABUN \n";
	strLocalQuery_SABUN                   += "     , A.NM_KOR||' ('||B.MU_NM||' '||B.CC_NM||')' AS NM_KOR \n";
	strLocalQuery_SABUN                   += "  FROM HLDC_HR_PERSONAL A \n";
	strLocalQuery_SABUN                   += "     , HLDC_SC_DEPT_V B \n";
	strLocalQuery_SABUN                   += " WHERE A.DEPT_ID = B.DEPT_ID \n";
    strLocalQuery_SABUN                   += "   AND A.PER_DTD IS NULL \n";
var strLocalQuery_SABUNKeyField      	   = "SABUN";
var strLocalQuery_SABUNValueField    	   = "A.NM_KOR||' ('||B.MU_NM||' '||B.CC_NM||')'";
var strLocalQuery_SABUNKeyFieldNM    	   = "사원번호";
var strLocalQuery_SABUNValueFieldNM  	   = "사원명"; 
var strLocalQuery_SABUNCaption       	   = "사원번호조회";
var strLocalQuery_SABUNAllField            = "SABUN;NM_KOR";

/************************************************************************************************
 * FORM EVENT 영역 (필수)
 ************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);

	// FSP Action 정보 생성
	fsp_init(this);

	// 화면 초기화
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 초기화
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	v_ds_check = "";

	//시즌목록코드 불러오기
	fn_getSeasonList();
	cb_Season.index = 0;

	// 개인정보 보호 관련
	// 로그인한 사용자가 관리자이면 개인정보 보기
	if( SCAuth.isUserRole("특판03") )
	{
		fa_Special_Auth = "Y"
		gd_list_Detail.setFormat("Default");

		Static04.visible = true;
		cbo_gbn.visible = true;
	}
	else
	{
		fa_Special_Auth = "N";
		gd_list_Detail.setFormat("Default0");
	}

	// 주문일자
	me_PrDate.value    = fn_CurrDateWeek();
	me_PrDateT.value   = fn_CurrDateWeek();

	// 공통코드
	var v_fil_cond;
	var v_ds = fn_common_code("'022', '023', '024', '028'", "", "", "non", "Y");

	// 결제수단
	v_fil_cond = "GROUP_CODE == '022'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_Pay, v_ds.name);	

	// 할부기간
	v_fil_cond = "GROUP_CODE == '023'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_Inst, v_ds.name);	

	// 카드사
	v_fil_cond = "GROUP_CODE == '024'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_Card, v_ds.name);	

	// 특판판매구분
	v_fil_cond = "GROUP_CODE == '028'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_Sale, v_ds.name);

	// 판매구분(콤보) 설정
	ds_cbo_SALE_TYPE.copyData(ds_Sale);
	ds_cbo_SALE_TYPE.insertRow(0);
	ds_cbo_SALE_TYPE.setColumn(0, "CODE", "");
	ds_cbo_SALE_TYPE.setColumn(0, "CODE_NAME", "전체");
	cbo_SALE_TYPE.index = 0;

	// 주문자MU
	fn_sql_common("C", " SELECT MU_CD, MU_NM FROM HLDC_SC_MU WHERE (1=1)"	,"  "," ORDER BY MU_CD ", "all", cbo_PR_SABUN_MU, -1, ds_Pr_Mu);

	cbo_SALE_TYPE.index = 0;
	cbo_PR_SABUN_MU.index = 0;
	
	// 팀, 사업장 설정
	edt_tm.applyAuthLevel(cbo_mu);
	edt_upjang.applyAuthLevel(edt_tm);
	
	edt_tm.setTmCode("");
	edt_upjang.setUpjangCode("");
	
}

