}

function common_onclick(obj:Button,  e:ClickEventInfo)
{
    this.closePopup();
}

/***************************************************************************
 * 설명   : LayerPopup에서 받을 공통 함수
 ***************************************************************************/
function fn_setPopupParameter(sObjParam)
{
    if (typeof sObjParam != "undefined" && sObjParam != null) 
    {
		vParam = sObjParam;

		fn_search(vParam);
    }
}

function fn_search(vParam)
{
	 
	  
	ds_paramInfo.clearData();
	var nIdx = ds_paramInfo.addRow();
	 
	ds_paramInfo.setColumn(nIdx, "searchDt", gfn_nvl(vParam.sParam1,""));
	ds_paramInfo.setColumn(nIdx, "outletCd",      gfn_nvl(vParam.sParam2,""));
	ds_paramInfo.setColumn(nIdx, "menuCd",        gfn_nvl(vParam.sParam3,""));
	ds_paramInfo.setColumn(nIdx, "reservStatus",  gfn_nvl(vParam.sParam4 ,""));
	ds_paramInfo.setColumn(nIdx, "amCd",          gfn_nvl(vParam.sParam5 ,""));
	ds_paramInfo.setColumn(nIdx, "kindCd",        gfn_nvl(vParam.sParam6 ,""));
	ds_paramInfo.setColumn(nIdx, "maskAuth",      SCAuth.getMaskAuth(this));
	
	if (gfn_validCheck(ds_paramInfo, ds_paramInfoValid) == false)	return;	//기본자료는 꼭 입력되어야 함

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);
	ds_list.clearData(); 
	 
	var strSvcID    = "search";
	var strURL      = "rc/bms/fncDiaryListTime/selectList.xdo";
	var strInDs     = "ds_paramInfo=ds_paramInfo:A";
	var strOutDs    = "ds_list=ds_list ds_grdDt=ds_grdDt";
	var strArg      = "";
	var strCallback = "fn_callBack";
	var bAsync      = true;
	
	gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync, false);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :  
		 
			fn_setHead();
            break;
        
        // 저장
        case "save" :
