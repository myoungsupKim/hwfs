function grd_plan_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.row == -1) return;
	
	if( ds_grdPlan.getRowType(e.row) == '2'  ) {
		if( gfn_getCellColId(obj, e.cell, null ) == 'allcNm' ){
			fn_userPopup();
		}
	}
}

function ds_grdPlan_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var nRow = obj.rowposition;
	
		if( e.columnid == 'schlDate' ){
			
			if( parseInt( obj.getColumn(nRow, e.columnid)) < parseInt( gfn_today()) ){
			gfn_alert("msg.err.maxdate");	// 날짜의 입력 가능 범위를 벗어났습니다.
				obj.setColumn(nRow, e.columnid, gfn_today());
			}
		}
	
}
function grd_plan_onexpanddown(obj:Grid, e:GridMouseEventInfo)
{
	if( ds_grdPlan.getRowType(e.row) == '2'  ) {
		if( gfn_getCellColId(obj, e.cell, null ) == 'allcNm' ){
			fn_userPopup();
		}
	}
}
