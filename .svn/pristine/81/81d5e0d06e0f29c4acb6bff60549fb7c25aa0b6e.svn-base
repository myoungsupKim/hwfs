	, ATTR02 = #ATTR02#
	, ATTR03 = #ATTR03#
	, ATTR04 = #ATTR04#
	, ATTR05 = #ATTR05#
	, ATTR06 = #ATTR06#
	, ATTR07 = #ATTR07#
	, ATTR08 = #ATTR08#
	, ATTR09 = #ATTR09#
	, ATTR10 = #ATTR10#
	, CUSER = #CUSER#
	, CDATE = #CDATE#
	, UUSER = #UUSER#
	, UDATE = #UDATE#
WHERE 
	 SCENARIO_CODE = #SCENARIO_CODE#
	 AND YY = #YY#
	 AND TM_CD = #TM_CD#
	 AND UPJANG = #UPJANG#
	 AND DATA_CLASS = #DATA_CLASS#	]]></statement>
