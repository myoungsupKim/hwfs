/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{	
	// v_ds_check 사용시 변경사항 체크
	if(gfn_length(v_ds_check) == 0) 
	{	
		// v_ds_check 설정기준 변경사항 체크
		if (gfn_isDataChange() == true) 
		{		
			// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
			if (!gfn_confirm("confirm.before.movepage")) return false;
		}
	}
    
	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}


/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 업장에 대한 창고리스트 조회
        case "searchSubinv" :
            if (ds_subinv.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_subinv, fv_objColInfo);
            }
            break;
    
    
		// 목록 조회 : 매출내역
        case "search" :
            //trace(ds_list.saveXML());
            if (ds_list.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_list, fv_objColInfo);
            }
            fn_magamYn();
            break;
            
		// 목록 조회 : 보정내역
        case "search2" :
            //trace(ds_list2.saveXML());
            if (ds_list2.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_list2, fv_objColInfo);
            }
            ds_list2.addColumn("dataGubun", "STRING");            
            
            break;

		// 매출마감일자 GET
        case "searchDate" :
            //trace(ds_saleDate.saveXML());
            if (ds_saleDate.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_saleDate, fv_objColInfo);
            }
            break;


		// 보정내역 저장
        case "save" :
			trace("저장건수 : " + fv_processCnt);
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			// 보정 목록 재조회
			fn_tranList();
            break;    


		// 보정내역 삭제
        case "deleteRow" :
			trace("삭제건수 : " + fv_processCnt);
			gfn_alert("msg.delete.success");	// 삭제 되었습니다.
			// 보정 목록 재조회
			fn_tranList();
            break;    
		case "searchMagam" :
			//trace(ds_magam.saveXML());
		break;
    }
}

/************************************************************************************************
