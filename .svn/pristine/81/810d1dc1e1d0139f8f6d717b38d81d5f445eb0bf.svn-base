/*----------------------------------------------------------------------------------------------
 * 설명      : 보정내역 삭제
 *---------------------------------------------------------------------------------------------*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	// 보정 목록에서 삭제 : 선택한 행의 자재를 바로 삭제
	var nRow = ds_list2.rowposition;
	var v_status = ds_list2.getColumn(nRow, "status");
	
	if(fn_magamFlag())return;//매출마감여부
	
	if(nRow == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	// 매출등록(수정불가) 일 경우에는 취소 안됨
	if(v_status=="3") {
		sMsg = "매출등록된 보정자료는 삭제할 수 없습니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}	
	
	var sMsg = "선택한 보정자료를 삭제합니다.\n계속 진행 하시겠습니까?\n\n";
		sMsg = sMsg + " - 사업장   : " + ds_list2.getColumn(ds_list2.rowposition, "upjangNm") + "\n";
		sMsg = sMsg + " - 창고 : " + ds_list2.getColumn(ds_list2.rowposition, "subinvName") + "\n";
		sMsg = sMsg + " - 보정일자   : " + ds_list2.getColumn(ds_list2.rowposition, "transDate") + "\n";
		sMsg = sMsg + " - 매출금액   : " + gfn_numFormat(gfn_nvl(ds_list2.getColumn(ds_list2.rowposition, "saleAmount"),"0")) + "\n";		


	var rtVal = gfn_confirm(sMsg, "", "C");	// 선택된 자료를 삭제 하시겠습니까?

	if (rtVal == true) 
	{	
		// 신규생성일 경우에는 해당 ROW를 삭제
		if (ds_list2.getRowType(nRow)==Dataset.ROWTYPE_INSERT) {
			var bSucc = ds_list2.deleteRow(nRow);
			if (bSucc) gfn_alert("msg.delete.success");	// 삭제 되었습니다.
			else gfn_alert("msg.err.grid.noselect");	// 선택된 항목이 없습니다.	
			return;
		}
	
		// 삭제용 dataset Clear
		ds_deleteRow.clearData();
		
		// 삭제용 dataset SET
		ds_deleteRow.addRow();
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "trNum", ds_list2.getColumn(nRow, "trNum"));					// TR_NUM
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "upjang", ds_list2.getColumn(nRow, "upjang"));					// UPJANG
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "transDate", ds_list2.getColumn(nRow, "transDate"));					// TRANS_DATE
		//여신집계 데이타를 위해 추가 2017.08.11 김호석 
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "saleAmount", ds_list2.getColumn(nRow, "saleAmount"));					// 매출금액
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "saleVat", ds_list2.getColumn(nRow, "saleVat"));					// 부가세
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "saleAmountOld", ds_list2.getColumn(nRow, "saleAmountOld"));					// 매출금액
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "saleVatOld", ds_list2.getColumn(nRow, "saleVatOld"));					// 부가세
		//trace(ds_deleteRow.saveXML());
		
		// Transaction 바로 호출해서 삭제 처리 함 
		var strSvcID    = "deleteRow";
		var strURL      = "fm/fmf/saleModifyWork/deleteRow.xdo";
		var strInDs     = "ds_deleteRow=ds_deleteRow:A";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 보정내역 Dataset 이벤트 처리
 *---------------------------------------------------------------------------------------------*/
function ds_list2_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var v_vat = 0;
	var v_vat1 = 0;
	
    switch(e.columnid)
    {
		case "itemCode" :		// 보정구분
			if (ds_list2.getColumn(e.row, "itemCode")=="709999999996") //면세매출보정일때
			{
				ds_list2.setColumn(e.row, "saleVat", 0);
			}
			else														//과세매출보정일때
			{
				v_vat1 = fn_calcVat(e.row);
				ds_list2.setColumn(e.row, "saleVat", v_vat1); 
			}
			break;
			
		case "saleAmount" : 	// 매출가보정		
			if (ds_list2.getColumn(e.row, "itemCode")=="709999999996") return;	
			v_vat = fn_calcVat(e.row);
			ds_list2.setColumn(e.row, "saleVat", v_vat); 
			break;				
	}	
	
}


/*----------------------------------------------------------------------------------------------
 * 설명      : 조건 Dataset 이벤트 처리
 *---------------------------------------------------------------------------------------------*/
function ds_search_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
    switch(e.columnid)
    {
		case "schTdate1" :		// 매출일자_FROM
			ds_list.clearData();
			break;
			
		case "schTdate2" :		// 매출일자_TO
			ds_list.clearData();
			break;

		case "schUpjang" :		// 사업장
			ds_list.clearData();
			if(!gfn_isNull(ds_search.getColumn(0,"schUpjang")))fn_magamYn();
			ds_search.setColumn(0,"schSubinv","");
			break;
	
		case "schSubinv" :		// 창고
			ds_list.clearData();
			if(!gfn_isNull(ds_search.getColumn(0,"schUpjang")))fn_magamYn();
			break;

		case "schRdate1" :		// 보정기간_FROM
			ds_list2.clearData();
			break;
			
		case "schRdate2" :		// 보정기간_TO
			ds_list2.clearData();
			break;
			
		case "schStatus" :		// 상태
			ds_list2.clearData();
			break;
			
		case "schSdate" :
			if(!gfn_isNull(ds_search.getColumn(0,"schUpjang")))fn_magamYn();
			break;
	}	
	
}



/*----------------------------------------------------------------------------------------------
 * 설명      : 보정일자 변경시 보정 데이터 제거...
 *---------------------------------------------------------------------------------------------*/
function ds_search_cancolumnchange(obj:Dataset, e:DSColChangeEventInfo)
{
		if(e.columnid == "schSdate"){
			trace(gfn_dsIsUpdated(ds_list2));
			if(ds_list2.getRowCount() > 0 && gfn_dsIsUpdated(ds_list2) == true){
				var flag = gfn_confirm("confirm.before.search", "", "C");
				if(flag)ds_list2.clearData();
				return flag;
			}
		}
		return true;
}

function btn_arSave_onclick(obj:Button,  e:ClickEventInfo)
{
	var bCount=0;

	for(var i = 0; i<ds_list2.rowcount; i++)
	{
		if(gfn_isNull(ds_list2.getColumn(i,"arNum")))
		{
			bCount = bCount  + 1;
		}
	}
	
	if(bCount > 0)
	{
		gfn_alert("등록되지 않은 품의가 " + bCount + "건 있습니다.");
	}
	
	// v_ds_check 미사용시 변경사항 체크 : 엑셀업로드 시에는 CHECK X
 	if (gfn_dsIsUpdated(ds_list2) == false)
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}

	// 저장 Transaction
    var strSvcID    = "save";
    var strURL      = "fm/fmf/saleModifyWork/saveArList.xdo";
	var strInDs     = "ds_list2=ds_list2:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}
