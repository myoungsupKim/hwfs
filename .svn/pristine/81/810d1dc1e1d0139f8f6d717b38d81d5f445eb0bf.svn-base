/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
	
	trace(g_Name);
	
	ed_Name.value = g_Name;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "";

	//조회조건을 초기화한다.
	var nRow= ds_search.addRow();
	ds_search.setColumn(nRow, "schTdate1", gfn_getYearMonth()+"01");			// 매출기간 : 현재일자기준 01일
	ds_search.setColumn(nRow, "schTdate2", gfn_today());						// 매출기간 : 현재일자
	ds_search.setColumn(nRow, "schRdate1", gfn_today());						// 보정기간 : 현재일자
	ds_search.setColumn(nRow, "schRdate2", gfn_today());						// 보정기간 : 현재일자
	ds_search.setColumn(nRow, "schSdate", gfn_today());							// 보정일자
	ds_search.setColumn(nRow, "schStatus", "");									// 보정상태 : 전체
	
	// 일자 SET
	cal_tDate1.fn_initMonthCal(this, "", "", ds_search, "schTdate1", "Y");	
	cal_tDate2.fn_initMonthCal(this, "", "", ds_search, "schTdate2", "Y");	
	cal_rDate1.fn_initMonthCal(this, "", "", ds_search, "schRdate1", "Y");	
	cal_rDate2.fn_initMonthCal(this, "", "", ds_search, "schRdate2", "Y");	
	cal_sDate.fn_initMonthCal(this, "", "", ds_search, "schSdate", "Y");	

	// 보정번호 항목 표시 숨기기
	this.grd_list2.setFormatColProperty(11,"size",0);

}

/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// 조회시 Validation Check
	if (gfn_validCheck(ds_search, ds_searchValid1, true) == false) return;

	// 기간 CHECK
	if (cal_tDate1.text > cal_tDate2.text) {
 		gfn_alert("msg.err.validator.date.great");	// 종료일이 시작일보다 빠릅니다.
 		cal_tDate1.setFocus();
		return;
	}

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);

   	//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장 
	fv_objColInfo = gfn_getDatasetInfo (ds_list);
    
    var strSvcID    = "search";
	var strURL      = "fm/fmf/saleModifyWork/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}


