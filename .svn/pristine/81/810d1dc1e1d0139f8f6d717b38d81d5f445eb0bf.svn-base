************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 업장선택 팝업 호출 이후 창고 COMBO LIST SET
 *---------------------------------------------------------------------------------------------*/
function fn_upjangSet()
{
	// 창고 DataSet 초기화
	ds_subinv.clearData();
	
	// 선택한 업장에 창고정보 SET
	var nRow= ds_search.rowposition;
	var schUpjang = ds_search.getColumn(nRow, "schUpjang");

	if(schUpjang!="") {
		fn_searchSubinv();
		fn_saleDateGet();
	}
