//=============================================================================================================
//						경리자료 그리드 컨트롤 - 그리드 펄치기
//=============================================================================================================
function fn_grdOn(obj){
	obj.position.top = 26;
	obj.position.width = Tab_UPJANG.tab4.Div0.position.width - 20;
	obj.position.height = Tab_UPJANG.tab4.Div0.position.height - 32;
	obj.anchor = "all";	
	Tab_UPJANG.tab4.Div0.btn_Off.visible = true;
}
//=============================================================================================================
//
//						경리자료 그리드 컨트롤 - 그리드 접기
//
//=============================================================================================================
function fn_grdOff(obj){
	if(obj.name == "Grid4")v_height = (Tab_UPJANG.tab4.Div0.position.height - v_top); 
	obj.position2.top = v_top;
	obj.position2.width = Tab_UPJANG.tab4.Div0.position.width - 21;
	obj.position2.height = v_height;
	
	if(obj.name != "Grid4")obj.anchor = "left top right";	
	Tab_UPJANG.tab4.Div0.btn_Off.visible = false;
}

