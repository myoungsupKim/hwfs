function Tab_UPJANG_tab1_grid_List1_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	
	//체권대체YN 팝업
	trace(e.col);
	//선수금YN조회 팝업 (col 14)
	if(e.col == 13){
		if(cbo_mu.getMuCode() == "2003") return;
		
		if(ds_listDetail1.getColumn(e.row, "ADVANCE_YN") == "Y"){
			fn_advanceYnPop();
		}
	}	
	
	if(e.col == 14){
		if(ds_listDetail1.getColumn(e.row, "AR_R_YN") == "Y"){
			fn_arYnPop("입금대체");
		}
	}
	//체권대체YN 팝업
	if(e.col == 15){
		if(ds_listDetail1.getColumn(e.row, "AR_YN") == "Y"){
			fn_arYnPop("채권이관");
		}
	}	
	
		
	// 현금 팝업
	if(e.col == 16){
		if(ds_listDetail1.getColumn(e.row, "CASH_YN") == "Y"){
			fn_cashYnPop();
		}
	}
	
	//세금계산서YN 팝업
	if(e.col == 17){
		if(ds_listDetail1.getColumn(e.row, "BIL_YN") == "Y"){
			fn_publishYnPop(ds_listDetail1);
		}
	}
	

}

