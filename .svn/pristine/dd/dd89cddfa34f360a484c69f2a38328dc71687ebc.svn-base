	fn_DataSetAllClear();
	
	// 업장세팅 후 CC코드 검색
	if(!gfn_isNull(edt_upjangNm.getUpjangCode())){
		fsp_clear(this);
		
		fsp_addSearch(this, "ma/mag:MAG10020S_S014");
		
		ds_cond.clear();
		
		ds_cond.addColumn("UPJANG", "STRING", 256);
		
		ds_cond.addRow();
		
		ds_cond.setColumn(0, "UPJANG", edt_upjangNm.getUpjangCode() );
		
		http.Sync = true;
		fsp_callService(this, "", "", "ds_cond=ds_cond","ds_ccCode=ds_ccCode","","");
		http.Sync = false;
	}
}



