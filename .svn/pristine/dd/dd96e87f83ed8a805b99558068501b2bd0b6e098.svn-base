<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[자재중분류조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<isEqual col="BLANK" value="Y">
SELECT '' AS CLASS_CODE, '' AS CLASS_NAME FROM DUAL
UNION ALL
</isEqual>
SELECT CLASS_CODE, CLASS_NAME
FROM HLDC_PO_ITEM_CLASS_HLDC_V
WHERE CLASS_TYPE = 'S'
AND   CLASS_CODE BETWEEN 'F10' AND 'F26'
AND   CLASS_CODE LIKE #CLASS_CODE# || '%'	</statement>
	<input default-name="ds_in">
		<col name="CLASS_CODE" size="255" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_cbo_item_classS">
		<col name="CLASS_CODE" size="8" type="VARCHAR" description="" /> 
		<col name="CLASS_NAME" size="50" type="VARCHAR" description="" /> 
	</output>
</query>
