			}

			if ( ds_gdspaymList.getColumn(i, "buyPaymSettledd") ==  "" )	//매입대금일자
			{
				gfn_alert("매입대금결제일 은(는) 필수 입력 항목입니다.");
				ds_gdspaymList.rowposition = i;
				grd_gdspaymList.setCellProperty("body", grd_gdspaymList.getBindCellIndex("Body", "buyPaymSettledd"), "edittype", "date");		//수정모드로
				grd_gdspaymList.setCellPos(grd_gdspaymList.getBindCellIndex("Body", "buyPaymSettledd"));
				grd_gdspaymList.setFocus();
				grd_gdspaymList.showEditor(); 

