function btn_gwDraft_onclick(obj:Button,  e:ClickEventInfo)
{
	//v_ds_check 미사용시 변경사항 체크
 	if ( !gfn_dsIsUpdated(ds_gdspaymList)) 
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}

	var chkCnt = ds_gdspaymList.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		return;
	}
	
	var cnt = 0;
    for(var i=0; i<ds_gdspaymList.rowcount; i++){
		if(ds_gdspaymList.getColumn(i, "chk") == "1"){
			cnt++;
		}
    }

	if(cnt > 1){
		gfn_alert("결재상신은 1건씩만 가능합니다.\n현재 " + cnt + "건이 선택되었습니다.");
		return;
	}	
	
	for ( var i=0; i<ds_gdspaymList.rowcount; i++ ) 
	{
		if ( ds_gdspaymList.getRowType(i) == Dataset.ROWTYPE_UPDATE ) 
		{
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "seq"))  == ""  ) {	  //데이타 없을시
				gfn_alert("저장 후 결재 상신하세요.");
				ds_gdspaymList.rowposition = i;
				return;
			}
		
			if ( ds_gdspaymList.getColumn(i, "signProgressStatus") ==  "DRAFT" ) {
				gfn_alert("상신중입니다.");	//
				return;
			}
			if ( ds_gdspaymList.getColumn(i, "signProgressStatus") ==  "COMPLETE" ) {
				gfn_alert("승인완료 상태 입니다.");	//
				return;
			}
		
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "slipStatus"),"") ==  "P" )	//전표확정
			{
				gfn_alert("전표 확정상태 입니다.");
				return; 
			}
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "slipStatus"),"") ==  "A" )	//전표요청
			{
				gfn_alert("전표 요청상태 입니다.");
				return; 
			}
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "slipStatus"),"") ==  "V" )	//입시전표
			{
				gfn_alert("임시 전표상태 입니다.");
				return; 
			}
		}
	}	
			
	// 저장시 Validation Check
	if ( !fn_chk() ) return;

	if (!gfn_confirm("결재상신 하시겠습니까?")) return false;
	
	ds_search.setColumn(0, "fiscalFlag", "+");	 //+전표 금액만 +
	ds_search.setColumn(0, "draft", "Y" );	

    var strSvcID    = "createDraftFiscalSlip";
    var strURL      = "ft/fgp/gdspaymMst/createDmtFiscalSlipIf.xdo";
    var strInDs     = "ds_search=ds_search ds_gdspaymList=ds_gdspaymList:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

function fn_draft()
{
	//그룹웨어에 전달할 파라메터 생성
	var arrParams	= SCGw.newParams ();
	
	//(필수) 결재문서 서비스명 - 문서양식별로 설정된 서비스명 설정
	SCGw.setParams (arrParams, SCGw.GW_SERVICE_NAME, "HWRC_FC_FTP_SLIP_SIGN_REQ");
	//alert("fv_slipNums :"+fv_slipNums);
	//(필수) 본문 URL : (주의) .do로 호출해야 하며 GET 방식으로 파라미터를 전달 (id라는 key는 사용 불가)
	var sBodyUrl = "/ft/ftp/poOrderMst/makeSlipGwDraftContent.do?slipNums=" + fv_slipNums
 										  +"&svc="+application.services["svc"].url
										  ; 
	SCGw.setParams (arrParams, SCGw.GW_BODY_URL, sBodyUrl);

	//(필수) 본문 제목 – 문서양식 별로 설정된 업무명
	SCGw.setParams (arrParams, SCGw.GW_BODY_TITLE, "물품대금(내수)상신 - "+ ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "poOrderNum")+" "+ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "remarks1"));

	//(필수)문서 방향 : 가로 또는 세로 방향 (GW_VERTICAL, GW_HORIZONTAL)
	SCGw.setParams (arrParams, SCGw.GW_PORTRAIT, SCGw.VERTICAL);
	
	//(선택)첨부문서 목록 : json 형태 문자열
	//SCGw.setParams (arrParams, SCGw.GW_ATTACH_LIST, fv_attachList);

	//(선택)Client Id : 결재 instance id와 연관을 맺기위한 업무별 ID로 unique key를 이용, 생략하면 상신시각이 됨(최대 40 byte)
	//SCGw.setParams (arrParams, SCGw.GW_CLIENT_ID, edt_id.value);

	//그룹웨어 상신 팝업을 호출한다.
	SCGw.sendDraft (this, arrParams);	
	
	//상신후 그냥 닫았을때 변경된 내역이 없어서 인식 못하는 부분 수정	
	ds_gdspaymList.updatecontrol = false;
	for ( var i=0; i<ds_gdspaymList.getRowCount(); i++ ) {
		if ( gfn_nvl(ds_gdspaymList.getColumn(i, "chk"),"") ==  "1" )
		{
			bSucc = ds_gdspaymList.setRowType(i, Dataset.ROWTYPE_UPDATE);
		}
	}
	ds_gdspaymList.updatecontrol = true;	
}


