	arrParam[1] = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "splrNm");	// 실지급처명
	arrParam[2] = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "signAmt");			// 수수료금액
	arrParam[3] = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "supplyPrice");		// 공급가
	arrParam[4] = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "vatAmt");				// 부가세
	arrParam[5] = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "billPublishdd");		// 세금계산서발행일
	arrParam[6] = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "bizRegNo");		    // 사업자번호
	
    var rtnVal = gfn_dialog("ElctnTexShetInfoPop"				// dialogId
                       , "X_FSI::ElctnTexShetInfoPop.xfdl"			// Url
                       , {fv_Contents:arrParam}			// 배열
                       , null, null, null, null, null, false, true, -1);
    if (typeof rtnVal != "undefined" && rtnVal != null && gfn_getObjkeyLength(rtnVal) > 0)
    {
		//사업자번호, 총금액, 공급금액, 세액이 같으면 세금계산서 번호, 발행일 세팅
		if ( rtnVal.suId ==  ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "bizRegNo")		
		  && rtnVal.grandtotal ==  ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "signAmt")
		  && rtnVal.chargetotal ==  ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "supplyPrice")
		  && rtnVal.taxtotal ==  ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "vatAmt")
		) {
			ds_gdspaymList.setColumn(ds_gdspaymList.rowposition, "elctnTaxBillNum", rtnVal.issueId);	// 국세청 세금계산서 승인번호
			ds_gdspaymList.setColumn(ds_gdspaymList.rowposition, "billPublishdd", rtnVal.issueDt);	// 국세청 세금계산서 발행일자
		}
		else
		{
			gfn_alert("선택한 실지급처["+rtnVal.suName+"]에 해당하는 금액이 맞지 않습니다.");
			return;
		}
    }    
}

/*----------------------------------------------------------------------------------------------
 * 설명      : XML 파일 선택
 *---------------------------------------------------------------------------------------------*/
function btn_xmlFileSearch_onclick(obj:Button,  e:ClickEventInfo)
{
	var chkCnt = ds_gdspaymList.getCaseCount("chk == 1");
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt==0)
	{
		gfn_alert("msg.noselect","체크박스");
		return;
	}
	if(chkCnt>1)
	{
		gfn_alert("한건만 체크하세요.");
		return;
	}
		
	var arrFile = FileDialog0.open ( "XML FileOpen", FileDialog.LOAD );
	if (gfn_isNull(arrFile)) return;
	if (arrFile.length < 1) return;
	
	// 선택한 파일 오픈
	arrFile.open( VirtualFile.openRead | VirtualFile.openText );
	var strBuf = arrFile.read(-1, "utf-8");
	fn_actionXml(strBuf);
	arrFile.close();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : XML 파일 내역을 그리드에 배치
 *---------------------------------------------------------------------------------------------*/
function fn_actionXml(sXML)
{
	var suprBsnNo = gfn_replace(gfn_replace(SCXml.getNodeValue(sXML, "InvoicerParty/ID"), "<![CDATA[", ""), "]]]]><![CDATA[>", "");	   							// 공급자 사업자번호
	var rcvrBsnNo =gfn_replace(gfn_replace(SCXml.getNodeValue(sXML, "InvoiceeParty/ID"), "<![CDATA[", ""), "]]]]><![CDATA[>", "");	   							// 공급받는자 사업자번호
	var amt =gfn_replace(gfn_replace(SCXml.getNodeValue(sXML, "SpecifiedMonetarySummation/GrandTotalAmount"), "<![CDATA[", ""), "]]]]><![CDATA[>", "");	   		//총 금액
	var supplyAmt =gfn_replace(gfn_replace(SCXml.getNodeValue(sXML, "SpecifiedMonetarySummation/ChargeTotalAmount"), "<![CDATA[", ""), "]]]]><![CDATA[>", "");	//  공급 금액
	var vatAmt =gfn_replace(gfn_replace(SCXml.getNodeValue(sXML, "SpecifiedMonetarySummation/TaxTotalAmount"), "<![CDATA[", ""), "]]]]><![CDATA[>", "");	    // 부가세 금액

	var taxBillNum =gfn_replace(gfn_replace(SCXml.getNodeValue(sXML, "TaxInvoiceDocument/IssueID"), "<![CDATA[", ""), "]]]]><![CDATA[>", "");	   				// 국세청승인번호
	var writeDate =gfn_replace(gfn_replace(SCXml.getNodeValue(sXML, "TaxInvoiceDocument/IssueDateTime"), "<![CDATA[", ""), "]]]]><![CDATA[>", "");	   			// 작성일자
	
	var bizRegNo = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "bizRegNo");		// 매입처사업자번호
	var commAmt = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "signAmt");		// 수수료금액
	var supplyPrice = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "supplyPrice");		// 공금금액
	var vatAmt2 =  gfn_nvl(ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "vatAmt"),"0");		// 세액
	var splrNm = ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "splrNm");		// 세액

	//alert(rcvrBsnNo+"/"+amt+"/"+supplyAmt+"/"+vatAmt+"/"+taxBillNum+"/"+writeDate);
	//사업자번호, 금액, 공금금액, 세액이 같으면 세금계산서 번호, 발행일 세팅
	//alert(rcvrBsnNo +"/"+ bizRegNo +"/"+ amt +"/"+ commAmt +"/"+ supplyAmt+"/"+supplyPrice  +"/"+ vatAmt +"/"+ vatAmt2+"// taxBillNum  :"+taxBillNum  );
	//trace(rcvrBsnNo +"/"+ bizRegNo +"/"+ amt +"/"+ commAmt +"/"+ supplyAmt+"/"+supplyPrice  +"/"+ vatAmt +"/"+ vatAmt2);
//	if ( rcvrBsnNo == bizRegNo && amt == commAmt && supplyAmt==supplyPrice  && vatAmt == vatAmt2 )
	//2020.02.12 김명섭 사업자변경
	//if ( rcvrBsnNo == "1018130747" && amt == commAmt && supplyAmt==supplyPrice  && vatAmt == vatAmt2 && suprBsnNo == bizRegNo )	//실제는 한화앤드 리조트 : 1018130747
	if ( rcvrBsnNo == "7118101637" && amt == commAmt && supplyAmt==supplyPrice  && vatAmt == vatAmt2 && suprBsnNo == bizRegNo )	//실제는 푸디스트 : 7118101637
	{
		ds_gdspaymList.setColumn(ds_gdspaymList.rowposition, "elctnTaxBillNum", taxBillNum  );
		ds_gdspaymList.setColumn(ds_gdspaymList.rowposition, "billPublishdd", writeDate  );
	}
	else if ( suprBsnNo != bizRegNo )
	{
		gfn_alert("선택한 실지급처["+bizRegNo+"]와 사업자번호가 일치하지 않습니다.["+suprBsnNo+"]");
		return;
	}
	else
	{
		gfn_alert("선택한 실지급처["+splrNm+"]에 해당하는 금액이 맞지 않습니다.");
		return;
	}
}
