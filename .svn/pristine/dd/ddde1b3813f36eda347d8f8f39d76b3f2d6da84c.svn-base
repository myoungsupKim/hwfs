function fn_init()
{
	// Grid내의 input field를 자동 선택 제어 함수
	FTCommon.setGridAutoSelect (this, true);
	
	ds_search.addRow();
	
	SCCombo.bind2(this, "ComCode", "cbo_term", null, {groupCode:"물품대금조회기간", useYn:"Y", defVal:""}, SCCombo.ALL);	 			//물품대금조회기간
	SCCombo.bind2(this, "ComCode", "cbo_search", null, {groupCode:"물품대금검색조건", useYn:"Y", defVal:""}, SCCombo.ALL);	 			//물품대금검색조건
	//SCCombo.bind(this, "ComCode", "cbo_poProgressStatus", null, {groupCode:"PO_PROGRESS_STATUS", useYn:"Y", defVal:""}, SCCombo.ALL);	//PO 진행 상태
	SCCombo.bind2(this, "ComCode", "grd_gdspaymList.elctnTaxBillYn", null, {groupCode:"ELCTN_TAX_BILL_YN", useYn:"Y", defVal:""});	 //전자세금계산서
	SCCombo.bind2(this, "ComCode", "grd_gdspaymList.vatNewhghtUpjang", null, {groupCode:"VAT_NEWHGHT_UPJANG", useYn:"Y", defVal:""});	 //부가세신고업장
	SCCombo.bind2(this, "ComCode", "grd_gdspaymList.taxYn", null, {groupCode:"TAX_YN", useYn:"Y", defVal:""}, SCCombo.CHOOSE);	//과세여부
	//SCCombo.bind2(this, "ComCode", "grd_gdspaymList.vatClassCd", null, {groupCode:"VAT_CLASS_CD", useYn:"Y", defVal:"", isLast:true, lastEvent:"fn_combo2LastCallback"});	 //부가세구분코드
	SCCombo.bind2(this, "ComCode", "cbo_attachDept", null, {groupCode:"ATTACH_DEPT", useYn:"Y", defVal:""}, SCCombo.ALL);	 //귀속부서
	SCCombo.bind2(this, "ComCode", "grd_gdspaymList.vatClassCd", null, {groupCode:"TAX_CD", useYn:"Y", defVal:"", attr01:"Y", isLast:true, lastEvent:"fn_combo2LastCallback"});	 //부가세구분코드
	

	// calendar 초기화(해당Form, 값지정, 값변경시 실행할 callBack함수, dataset과 bind, 필수여부)
