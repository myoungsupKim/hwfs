			if ( ds_gdspaymList.getColumn(i, "remarks1") ==  "" )	//적요
			{
				gfn_alert("적요 은(는) 필수 입력 항목입니다.");
				ds_gdspaymList.rowposition = i;
				grd_gdspaymList.setCellProperty("body", grd_gdspaymList.getBindCellIndex("Body", "remarks1"), "edittype", "text");		//수정모드로
				grd_gdspaymList.setCellPos(grd_gdspaymList.getBindCellIndex("Body", "remarks1"));
				grd_gdspaymList.setFocus();
				grd_gdspaymList.showEditor(); 

				return false;
			}

			var snglfootlstNmYn = gfn_nvl(ds_gdspaymList.getColumn(i, "snglfootlstNmYn"), "");
			if ( snglfootlstNmYn == 0 ) 	//단발성이 아니면
			{			
				if ( gfn_nvl(ds_gdspaymList.getColumn(i, "bankAcctNo"), "") == "" )
				{
					gfn_alert("계좌번호가 없습니다. 계좌번호를 입력하세요.");
					ds_gdspaymList.rowposition = i;
					grd_gdspaymList.setCellPos(grd_gdspaymList.getBindCellIndex("Body", "bankAcctNo")); // cellindex
					grd_gdspaymList.setFocus();
					grd_gdspaymList.showEditor(); 
					return;
				}
				if ( gfn_nvl(ds_gdspaymList.getColumn(i, "bankAcctOwnr"), "") == "" )
				{
					gfn_alert("예금주가 없습니다. 예금주를 입력하세요.");
					ds_gdspaymList.rowposition = i;
					grd_gdspaymList.setCellPos(grd_gdspaymList.getBindCellIndex("Body", "bankAcctOwnr")); // cellindex
					grd_gdspaymList.setFocus();
					grd_gdspaymList.showEditor(); 
					return;
				}
				if ( gfn_nvl(ds_gdspaymList.getColumn(i, "bankCd"), "") == "" )
				{
					gfn_alert("은행코드가 없습니다. 은행코드를 입력하세요.");
