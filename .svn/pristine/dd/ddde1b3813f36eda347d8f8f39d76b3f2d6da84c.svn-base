					ds_gdspaymList.rowposition = i;
					grd_gdspaymList.setCellPos(grd_gdspaymList.getBindCellIndex("Body", "bankNm")); // cellindex
					grd_gdspaymList.setFocus();
					grd_gdspaymList.showEditor(); 
					return;
