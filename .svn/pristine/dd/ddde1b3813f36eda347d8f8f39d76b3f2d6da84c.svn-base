		}
		else if ( ds_gdspaymList.getColumn(e.row, "vatClassCd") ==  "91" || ds_gdspaymList.getColumn(e.row, "vatClassCd") ==  "92")	//매입계산서
		{
			//ds_gdspaymList.setColumn(e.row, "supplyPrice", signAmt );
