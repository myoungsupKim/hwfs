/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_gdspaymList))
 	{
		// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		var retValue = gfn_confirm("confirm.before.search");
 		if (retValue == false) return;
 	}
 	
 	if (gfn_isNull(cal_startTerm.fn_getValue()) || gfn_isNull(cal_endTerm.fn_getValue()))
	{
		gfn_alert("조회일자를 입력하여 주십시오!");
		return;
	}
	if (cal_startTerm.fn_getValue() > cal_endTerm.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
 	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_gdspaymList);
    //코드목록을 초기화한다.
    ds_gdspaymList.clearData(); 
	
	ds_search.setColumn(0,"poType", "20");	//내수조회
    var strSvcID    = "select";
    var strURL      = "ft/fgp/gdspaymMst/selectList2.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_gdspaymList=ds_gdspaymList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		//품의 조회
        case "select" :
            break;
        
        // 저장
        case "save" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				//alert("품의서 번호["+fv_poProposalNum+"]가 생성되었습니다.");
				fn_search();
			}
            break;
        case "delete" :
			if (fv_iProcessCnt > 0) {
				gfn_alert("msg.delete.success");	// 삭제되었습니다
				//alert("품의서 번호["+fv_poProposalNum+"]가 생성되었습니다.");
				fn_search();
			}
            break;
        case "createFiscalSlip":
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
			break;
        case "cancelFiscalSlip":
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
			break;
        case "createDraftFiscalSlip":
			fn_draft(); 
			break;
        case "saveDmtDraft":
			gfn_alert("상신되었습니다.");	
			fn_search();
			break;
        case "getVatClassCd":
			if ( fv_vatClassCd != "" )
			{
				ds_gdspaymList.setColumn(fv_currRow, "vatClassCd", fv_vatClassCd  );
				ds_gdspaymList.setColumn(fv_currRow, "vatClassNm", fv_vatClassNm  );
			}
			break;            
    }
    SCGw.setUserButton (this, fv_option, gfn_nvl(ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "signProgressStatus"),""));
}


/*------------------------------------------------------------
	1. Function ID : fn_Delete()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_delete()
{
	var chkCnt = ds_gdspaymList.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		return;
	} 

	for ( var i=0; i<ds_gdspaymList.rowcount; i++ ) 
	{
		if ( ds_gdspaymList.getRowType(i) == Dataset.ROWTYPE_UPDATE ) 
		{
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "signProgressStatus"),"")  ==  "DRAFT" ) {
				gfn_alert("상신중입니다.");	//
				return;
			}
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "signProgressStatus"),"")  ==  "COMPLETE" ) {
				gfn_alert("승인완료 상태 입니다.");	//
				return;
			}
			
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "slipStatus"),"") ==  "A" )	//전표요청
			{
				gfn_alert("전표 요청상태 입니다.");
				return;
			}				
			if ( ds_gdspaymList.getColumn(i, "slipStatus") ==  "V" || ds_gdspaymList.getColumn(i, "slipStatus") ==  "P" || ds_gdspaymList.getColumn(i, "slipStatus") ==  "Z" )	//전표생성,취소
			{
				gfn_alert("전표가 생성되어 있습니다. 삭제할 수 없습니다.");
				ds_gdspaymList.rowposition = i;
				return;
			}
			
			if ( typeof ds_gdspaymList.getColumn(i, "seq")  == "undefined"  ) {
				gfn_alert("ft.gds.alert.delete.no.gdspaym");	//삭제할 물품 대금이 없습니다.
				ds_gdspaymList.rowposition = i;
				return;
			}			
		}
	}

	var rtVal = gfn_confirm("confirm.before.delete");	//선택된 자료를 삭제 하시겠습니까?/
	if ( !rtVal == true) return;
	
    var strSvcID    = "delete";
    var strURL      = "ft/fgp/gdspaymMst/deleteDmtGdspaymList.xdo";
    var strInDs     = "ds_search=ds_search ds_gdspaymList=ds_gdspaymList:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}

/*------------------------------------------------------------
	1. Function ID : fn_Save()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_save()
{
	//v_ds_check 미사용시 변경사항 체크
 	if ( !gfn_dsIsUpdated(ds_gdspaymList))
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	
	var chkCnt = ds_gdspaymList.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		return;
	} 	

	if ( !fn_chk() ) return;
	
	if (!gfn_confirm("confirm.before.save")) return false;

    var strSvcID    = "save";
    var strURL      = "ft/fgp/gdspaymMst/saveList2.xdo";
    var strInDs     = "ds_search=ds_search ds_gdspaymList=ds_gdspaymList:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}


function fn_chk()
{
	for ( var i=0; i<ds_gdspaymList.rowcount; i++ ) 
	{
		//if ( ds_gdspaymList.getRowType(i) == Dataset.ROWTYPE_UPDATE ) 
		if ( ds_gdspaymList.getColumn(i, "chk") == 1  ) 
		{ 
			
			if ( ds_gdspaymList.getColumn(i, "slipStatus") ==  "P" || ds_gdspaymList.getColumn(i, "slipStatus") ==  "V" )	//전표생성
			{
				gfn_alert("전표가 생성 이후는 변경할 수 없습니다.");
				ds_gdspaymList.rowposition = i;
				return false;
			}
			if ( ds_gdspaymList.getColumn(i, "buySlipDate") ==  "" )	//매입전표일자
