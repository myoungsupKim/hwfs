 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// Grid내의 input field를 자동 선택 제어 함수
	FTCommon.setGridAutoSelect (this, false);
	
	ds_search.addRow();
	
	// 년도 세팅
	var nRow;
	for (var i = Number(gfn_getYear())-4; i <= Number(gfn_getYear()); i++) {
		nRow = ds_year.addRow();
		ds_year.setColumn(nRow, "yy", i);
		ds_year.setColumn(nRow, "yyNm", i);
	}
	ds_search.setColumn(0, "year", gfn_getYear());
	cbo_year.value = gfn_getYear();
	
	//SCCombo.bind2(this, "ComCode", "cbo_planClass", null, {groupCode:"PLAN_CLASS", useYn:"Y", defVal:""}, SCCombo.ALL);	 //계획구분
	SCCombo.bind2(this, "ComCode", "grd_list.attachDept", null, {groupCode:"ATTACH_DEPT", useYn:"Y", defVal:""}, SCCombo.CHOOSE);	//귀속부서
	SCCombo.bind2(this, "ComCode", "cbo_attachDept", null, {groupCode:"ATTACH_DEPT", useYn:"Y", defVal:"", isLast:true, lastEvent:"fn_combo2LastCallback"}, SCCombo.ALL);	 //귀속부서
	SCDataset.bind(this, "ComCode", "Dataset03", {groupCode:"DATA_SET", code:"DEF_FT_TM_CODE", useYn:"Y"}, "fn_defValue");	//영업사원
}

