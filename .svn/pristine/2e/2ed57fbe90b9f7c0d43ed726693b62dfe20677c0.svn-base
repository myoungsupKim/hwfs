/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";

/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_objColInfo;	//데이터셋 컬럼정보
var v_ds_check	= "";

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);

	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	ds_cond.setColumn(0, "upjang", 		g_Upjang);
	ds_cond.setColumn(0, "upjang_nm", 	g_UpjangNm);
	cal_yyMm.fn_initMonthCal(this, gfn_today().substr(0, 6), '', ds_cond, 'to_month', 'Y');

	fn_setGridFormats();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{
	return true;
}

/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수-공통버튼)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// 조회시 Validation Check
	if( !gfn_validCheck(ds_cond, ds_condValid, true) ) return;

	// 그리드 포맷생성
	fn_setGridFormats();

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);

	//서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
	fv_objColInfo = gfn_getDatasetInfo(ds_list);

    var strSvcID    = "search";
    var strURL      = "rc/roa/monthPartList/selectList.xdo";
    var strInDs     = "ds_cond=ds_cond";
    var strOutDs    = "ds_list=ds_list";
	var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert(){}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete(){}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save(){}

/*----------------------------------------------------------------------------------------------
 * 설명      : 출력 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_print(){}

/*----------------------------------------------------------------------------------------------
 * 설명      : 도움말 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_help(){}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if( nErrorCode < 0 )
	{
		return;
	}

    switch(strSvcID)
    {
        //조회
        case "search" :
			if( ds_list.getRowCount() == 0 )
			{
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_list, fv_objColInfo);
			}

			break;
    }
}

/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 동적그리드 생성
 *---------------------------------------------------------------------------------------------*/
function fn_setGridFormats()
{
	fn_getWorkDay();

	var sFormats  = "";
	
	var sFormats  = "";
		sFormats += "<Formats> \n";
		sFormats += "<Format id='default'> \n";
		sFormats += "	<Columns> \n";
		sFormats += "		<Column size='30' band='left'/> \n";
		sFormats += "		<Column size='70' band='left'/> \n";
		sFormats += "		<Column size='70' band='left'/> \n";
		sFormats += "		<Column size='110' band='left'/> \n";
		sFormats += "		<Column size='70' band='left'/> \n";
		sFormats += "		<Column size='70' band='left'/> \n";
		sFormats += "		<Column size='70' band='left'/> \n";
		sFormats += "		<Column size='70' band='left'/> \n";

	for( var i = 0; i < ds_workDay.rowcount; i++ )
	{
		sFormats += "    <Column size='70' /> \n";
		sFormats += "    <Column size='70' /> \n";
		sFormats += "    <Column size='70' /> \n";
		sFormats += "    <Column size='70' /> \n";
		sFormats += "    <Column size='70' /> \n";
		
	}

		sFormats +=	"	</Columns> \n";
		sFormats += "	<Rows> \n";
		sFormats += "		<Row size='20' band='head' /> \n";
		sFormats += "		<Row size='20' band='head' /> \n";
		sFormats += "		<Row size='25' /> \n";
		sFormats += "		<Row size='25' band='summ'/> \n";
		sFormats += "	</Rows> \n";
		sFormats += "	<Band id='head'> \n";
		sFormats += "		<Cell rowspan='2' class='head_Excel'/> \n";
		sFormats += "		<Cell col='1' rowspan='2' text='이름'/> \n";
		
		sFormats += "		<Cell col='2' rowspan='2' text='홀/주방'/> \n";
		sFormats += "		<Cell col='3' rowspan='2' text='시급'/> \n";
		sFormats += "		<Cell col='4' rowspan='2' text='업무'/> \n";
		
		sFormats += "		<Cell col='5' colspan='3' text='합계'/> \n";

	var nHeadNum1 = 8;
	var nHeadNum2 = 8;
	for( var i = 0; i < ds_workDay.rowcount; i++ )
	{
		var mm = ds_workDay.getColumn(i, "mm");
		var dd = ds_workDay.getColumn(i, "dd");
		sFormats += "    <Cell col='" + nHeadNum1++ +"' colspan='2' text='" + mm + "월 "+ dd + "일" + "' /> \n";

		nHeadNum1++;

		sFormats += "    <Cell col='" + nHeadNum1++ +"' rowspan='2' text='근무&#13;&#10;시간'/> \n";
		sFormats += "    <Cell col='" + nHeadNum1++ +"' rowspan='2' text='O/T&#13;&#10;시간'/> \n";
		sFormats += "    <Cell col='" + nHeadNum1++ +"' rowspan='2' text='야간&#13;&#10;시간'/> \n";
	}
	
		sFormats += "    <Cell row='1' col='5' text='근무시간'/> \n";
		sFormats += "    <Cell row='1' col='6' text='O/T시간'/> \n";
		sFormats += "    <Cell row='1' col='7' text='야간시간'/> \n";

	for( var i = 0; i < ds_workDay.rowcount; i++ )
	{
		sFormats += "    <Cell row='1' col='" + nHeadNum2++ + "' text='출근'/> \n";
		sFormats += "    <Cell row='1' col='" + nHeadNum2++ + "' text='퇴근'/> \n";

		nHeadNum2 = nHeadNum2 + 3;
	}
		sFormats += "	</Band> \n";
		sFormats += "	<Band id='body' > \n";
		sFormats += "		<Cell text='bind:seq' suppress='1' style='align:center;background:#FFFFFFFF;background2:#FFFFFFFF;'/> \n";
		//sFormats += "		<Cell expr='expr:currow+1' suppress='1' style='align:center;background:#FFFFFFFF;background2:#FFFFFFFF;'/> \n";
		sFormats += "		<Cell col='1' text='bind:partTimeEmpNm' suppress='1' style='align:center;background:#FFFFFFFF;background2:#FFFFFFFF;'/> \n"; 
		sFormats += "		<Cell col='2' text='bind:dutyPlaceNm' style='align:center;background:#FFFFFFFF;background2:#FFFFFFFF;'/> \n";
		sFormats += "		<Cell col='3' text='bind:partTimeSalaryCdNm' style='align:center;background:#FFFFFFFF;background2:#FFFFFFFF;'/> \n";
		sFormats += "		<Cell col='4' text='bind:dutyDetailNm' style='align:center;background:#FFFFFFFF;background2:#FFFFFFFF;'/> \n";
		sFormats += "		<Cell col='5' text='bind:workTimeSum' /> \n";
		sFormats += "		<Cell col='6' text='bind:otTimeSum' /> \n";
		sFormats += "		<Cell col='7' text='bind:nightTimeSum' /> \n";

	var nBodyNum = 8;
	for( var i = 0; i < ds_workDay.rowcount; i++ )
	{
		sFormats += "		<Cell col='" + nBodyNum++ + "' mask='expr:dutyStartTime" + i + "==&apos;&apos;?&quot;&quot;:&apos;##:##&apos;' text='bind:dutyStartTime" + i + "'/> \n";
		sFormats += "		<Cell col='" + nBodyNum++ + "' mask='expr:dutyEndTime" + i + "==&apos;&apos;?&quot;&quot;:&apos;##:##&apos;' text='bind:dutyEndTime" + i + "'/> \n";
		sFormats += "		<Cell col='" + nBodyNum++ + "' expr='expr:dutyStartTime" + i + "==&apos;&apos;?&quot;&quot;:workTime" + i + "'/> \n";
		sFormats += "		<Cell col='" + nBodyNum++ + "' expr='expr:dutyStartTime" + i + "==&apos;&apos;?&quot;&quot;:otTime" + i + "'/> \n";
		sFormats += "		<Cell col='" + nBodyNum++ + "' expr='expr:dutyStartTime" + i + "==&apos;&apos;?&quot;&quot;:nightTime" + i + "'/> \n";
	}
	
		sFormats += "	</Band> \n";
		sFormats += "	<Band id='summary'> \n";
		sFormats += "		<Cell style='align:center;' taborder='undefined'/> \n";
		sFormats += "		<Cell col='1' style='align:center;' text='합계' taborder='undefined'/> \n";
		sFormats += "		<Cell col='2'/> \n";
		sFormats += "		<Cell col='3'/> \n";
		sFormats += "		<Cell col='4'/> \n";
		sFormats += "		<Cell col='5' displaytype='number' expr='expr:getSum(&quot;workTimeSum&quot;)'/> \n";
		sFormats += "		<Cell col='6' displaytype='number' expr='expr:getSum(&quot;otTimeSum&quot;)'/> \n";
		sFormats += "		<Cell col='7' displaytype='number' expr='expr:getSum(&quot;nightTimeSum&quot;)'/> \n";
		
	var nSumNum = 8;
	for( var i = 0; i < ds_workDay.rowcount; i++ )
	{
		sFormats += "		<Cell col='" + nSumNum++ + "'/> \n";
		sFormats += "		<Cell col='" + nSumNum++ + "'/> \n";
		
		sFormats += "		<Cell col='" + nSumNum++ + "' displaytype='number' expr='expr:getSum(&quot;workTime" + i + "&quot;)'/> \n";
		sFormats += "		<Cell col='" + nSumNum++ + "' displaytype='number' expr='expr:getSum(&quot;otTime" + i + "&quot;)'/> \n";
		sFormats += "		<Cell col='" + nSumNum++ + "' displaytype='number' expr='expr:getSum(&quot;nightTime" + i + "&quot;)'/> \n";
		
	}
		

		sFormats += "	</Band> \n";
		sFormats += "</Format> \n";
		sFormats += "</Formats> \n";

		trace(sFormats);
	grd_list.formats = sFormats;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 조회한 월의 전월 26일 ~ 조회월 25일 까지의 일자를 조회한다.
 *---------------------------------------------------------------------------------------------*/
function fn_getWorkDay()
{
	var frMonth = gfn_addMonth(ds_cond.getColumn(0, "to_month") + "01", -1);

	ds_cond.setColumn(0, "fr_month", frMonth.substr(0, 6));
	
    var strSvcID    = "search";
    var strURL      = "rc/roa/monthPartList/selectWorkDayList.xdo";
    var strInDs     = "ds_cond=ds_cond";
    var strOutDs    = "ds_workDay=ds_workDay";
	var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = false;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

