{
	background			: #bf5871 URL('theme://images/new/btn_WF_searchCm.png') left middle;
	border				: 1 solid #99465a, 0 none, 0 none, 0 none;
	color				: #ffffff;
	padding				: 0 0 0 26;
}
Button.btn_WF_searchCm:disabled
{
	background			: #e3b9c3 URL('theme://images/new/btn_WF_searchCmD.png') left middle;
	border				: 0 none;
	color				: #f4e3e7;
}
