Button.btn_WF_searchCm, Button.btn_WF_searchCm:focused
{
	align				: left middle;
	background			: #e56b88 URL('theme://images/new/btn_WF_searchCm.png') left middle;
	border				: 0 none;
	bordertype			: round 3 3;
	color				: #ffffff;
	font				: Gulim,9,bold;
	padding				: 1 0 0 26;
	shadow				: 0 0;
}
