<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT  TO_CHAR(SYSDATE,'YYYYMMDD') YMD,
        TO_CHAR(SYSDATE,'YYYYMMDDHH24MISS') YMD_TM,
        TO_CHAR(SYSDATE,'d') WEEK_CD,
        TO_CHAR(LAST_DAY(SYSDATE),'YYYYMMDD') LAST_YMD,
        CASE    
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '1' THEN 'ÀÏ'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '2' THEN '¿ù'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '3' THEN 'È­'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '4' THEN '¼ö'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '5' THEN '¸ñ'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '6' THEN '±Ý'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '7' THEN 'Åä'
        END WEEK_KOR,
        CASE    
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '1' THEN 'ìí'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '2' THEN 'êÅ'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '3' THEN 'ûý'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '4' THEN 'â©'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '5' THEN 'ÙÊ'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '6' THEN 'ÐÝ'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '7' THEN '÷Ï'
        END WEEK_CHA,                
        CASE    
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '1' THEN 'Sun'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '2' THEN 'Mon'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '3' THEN 'Tue'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '4' THEN 'Wed'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '5' THEN 'Thu'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '6' THEN 'Fri'
                WHEN    TO_CHAR(TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'), 'YYYYMMDD') ,'d') = '7' THEN 'Sat'
        END WEEK_ENG                
FROM    DUAL	]]></statement>
	<input default-name="DS_IN">
	</input>
	<output default-name="gds_currdate">
		<col name="YMD" size="8" type="VARCHAR" description="" /> 
		<col name="YMD_TM" size="14" type="VARCHAR" description="" /> 
		<col name="WEEK_CD" size="1" type="VARCHAR" description="" /> 
		<col name="WEEK_KOR" size="2" type="CHAR" description="" /> 
		<col name="WEEK_CHA" size="2" type="CHAR" description="" /> 
		<col name="WEEK_ENG" size="3" type="CHAR" description="" /> 
	</output>
</query>
