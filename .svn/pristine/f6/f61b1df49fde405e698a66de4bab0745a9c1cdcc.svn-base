	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT CLASS_CODE,CLASS_NAME 
FROM HLDC_PO_ITEM_CLASS_HLDC_V
WHERE (1=1)
AND CLASS_CODE LIKE #MCODE#  || '%'
AND CLASS_TYPE = 'D'
ORDER BY CLASS_CODE	]]></statement>
