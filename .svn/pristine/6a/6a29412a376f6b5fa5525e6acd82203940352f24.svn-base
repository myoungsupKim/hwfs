/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			if (ds_list.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_list, fv_objColInfo);
            } else {
				
				//첫번째줄 수정가능여부에 따라 설정 추가
				if(ds_list.getRowType(0) == "1"){
					cal_occurDate.enable=false;
					cbo_giftKindCdDtl.readonly=true;
					cbo_sysClass.readonly=true;
					edt_receiptNum.readonly=true;
					btn_ccCdDtl.enable=false;
					
					if ((ds_list.getColumn(0, "giftBillingYn") == "Y") || (ds_list.getColumn(0, "receiveAmt") != "0") || (ds_list.getColumn(0, "journalzYn") == "J")){
							msk_giftAmt.enable=false;
							msk_useAmt.enable=false;
							edt_giftNum.enable=false;
							msk_giftAmt.enable=false;
							msk_useAmt.enable=false;
							edt_giftNum.enable=false;
						
					}else{
						cal_occurDate.enable=true;
						cbo_giftKindCdDtl.readonly=false;
						cbo_sysClass.readonly=false;
						edt_receiptNum.readonly=false;
						btn_ccCdDtl.enable=true;		
						msk_giftAmt.enable=true;
						msk_useAmt.enable=true;
						edt_giftNum.enable=true;
					}
				}	
			}
			
            //trace(ds_list.saveXML());
            break;
            
        //등록 유무 확인
        /*
        case "searchGiftNum" :
			if(gfn_isNull(ds_giftNumList.getColumn(0, "giftNum"))){
			
			}else{
				alert("이미 등록된 상품권 입니다.");
				edt_giftNum.value = "";
			}
			break;
        */
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break;    
          
        //상품권 종류
        case "giftType" :
			break;
    }
}


