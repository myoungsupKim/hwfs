function cbo_giftKindCdDtl_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(obj.value == 760 && edt_giftNum.text.length != 13){
		alert("통합상품권은 13자리 입니다.");
		edt_giftNum.value = null;
	}
	msk_cmisRate.value = ds_gifttype.getColumn(obj.index, "cmisRate");
	ds_list.setColumn(ds_list.rowposition, "giftKindNm", cbo_giftKindCdDtl.text);
	ds_list.setColumn(ds_list.rowposition, "giftKindCd", cbo_giftKindCdDtl.value);
	ds_list.setColumn(ds_list.rowposition, "arTypeCd", ds_gifttype.getColumn(cbo_giftKindCdDtl.index, "arTypeCd"));
	
	fn_changeAmt();
}

//수수료에 따라서 금액 변경
function fn_changeAmt()
{
	if(gfn_isNull(msk_giftAmt.value) || gfn_isNull(msk_cmisRate.value)){
		msk_commAmt.value = 0;
		return;
	}
	msk_commAmt.value = Math.round(msk_giftAmt.value * msk_cmisRate.value / 1.1 / 100);
	msk_commVatAmt.value = Math.round((msk_giftAmt.value * msk_cmisRate.value / 100) - msk_commAmt.value);
}

function msk_giftAmt_onkillfocus(obj:MaskEdit, e:KillFocusEventInfo)
{
	fn_changeAmt();
}

function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(ds_list.getRowType(e.row) == "1"){
		cal_occurDate.enable=false;
		cbo_giftKindCdDtl.readonly=true;
		cbo_sysClass.readonly=true;
		edt_receiptNum.readonly=true;
		btn_ccCdDtl.enable=false;
		
		if((ds_list.getColumn(e.row, "giftBillingYn") == "Y") || (ds_list.getColumn(e.row, "receiveAmt") != "0")){
			alert("상품권이 청구된 데이터또는 채권이 대체된 경우는 수정/삭제가 불가능 합니다.");
			msk_giftAmt.enable=false;
			msk_useAmt.enable=false;
			edt_giftNum.enable=false;
		//전표처리된 데이터는 수정 불가 20200331 김호석
		} else if((ds_list.getColumn(ds_list.rowposition, "journalzYn") == "J")){ 
			alert("전표처리된 데이터는 수정/삭제가 불가능 합니다.");
			msk_giftAmt.enable=false;
			msk_useAmt.enable=false;
			edt_giftNum.enable=false;
		}else{
			msk_giftAmt.enable=true;
			msk_useAmt.enable=true;
			edt_giftNum.enable=true;			
		}
		
	}else{
		cal_occurDate.enable=true;
		cbo_giftKindCdDtl.readonly=false;
		cbo_sysClass.readonly=false;
		edt_receiptNum.readonly=false;
		btn_ccCdDtl.enable=true;		
		msk_giftAmt.enable=true;
		msk_useAmt.enable=true;
		edt_giftNum.enable=true;
	}
}

