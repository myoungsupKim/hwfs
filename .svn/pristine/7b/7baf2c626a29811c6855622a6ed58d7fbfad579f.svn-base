	ds_cond.addRow();	
	ds_cond.setColumn(ds_cond.rowposition, "TRX_DATE",    me_Date.value);
       					
	fn_DataSetAllClear();

    http.Sync = true;
    
	//Timeout 설정처리###########################################
	var t_timeout;
