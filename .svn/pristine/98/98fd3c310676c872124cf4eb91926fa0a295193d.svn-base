function fn_cateringChanged(obj:Combo, e:ItemChangeEventInfo)
{
	// 타이틀 변경
  	if(cbo_catering.index==2){
		sta_title.text="기물 승인";
	}else {
		sta_title.text="지원 배정";
	}
		
	// 변경사항 체크
 	if (gfn_isDataChange() == true) 
 	{		
 		var retValue = gfn_confirm("confirm.before.search");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		if (retValue == false) {
			return;
 		}else{
 			ds_grdCeleList.reset();				//행사 요청				
			ds_grdCookHumanList.reset();		//조리인력					
			ds_grdUtensilList.reset();			// 기기/기물 요청			
			ds_grdCookGuidList.reset();			// 조리지도
 		}
 	}

	fn_viewControl(obj.value);
}

