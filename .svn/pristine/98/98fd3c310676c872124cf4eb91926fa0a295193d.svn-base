function ds_grdCeleList_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{

	var nRow = obj.rowposition;
	
	if( e.columnid == '' ){	
		return ;
	}
	
	if( e.columnid == 'noMan' ) {
	
		var nRowCheckCnt = fn_rowCheck( obj  , 'col' );	
		if( fn_rowCheck > 0  ) {			
			obj.setColumn(nRow,  'noMan',  nRowCheckCnt);				
			return ;
		}
	}else if( e.columnid == 'allcStatus' ) {	

		if( fn_rowCheck( obj  , 'col' ) > 0  && obj.getOrgColumn(nRow, 'allcStatus') != '003' ) {			
			obj.setColumn(nRow,  'allcStatus',  obj.getOrgColumn(nRow, 'allcStatus'));				
			return ;
		}
	}else if( e.columnid == 'dutyTime' && gfn_length(obj.getColumn(nRow, 'dutyTime')) > 0  ) {			
		obj.setColumn(nRow,'dutyTime' ,gfn_rPad( obj.getColumn(nRow, 'dutyTime') ,'0', 4 ))
	}
	
	
	
	if( 	 e.columnid == 'celeNm' || e.columnid == 'celeTermSdate' || e.columnid == 'celeTermEdate'
		  || e.columnid == 'dutyTime' || e.columnid == 'supHumanStrengthCd' || e.columnid == 'noMan' 
		  || e.columnid == 'guestType' || e.columnid == 'spclfact' || e.columnid == 'caterMenu'
		  || e.columnid == 'caterUprice' || e.columnid == 'prcgNm' || e.columnid == 'prcgTelno1'
		  || e.columnid == 'prcgTelno2' || e.columnid == 'prcgTelno3' || e.columnid == 'chfsml'
		  || e.columnid == 'allcStatus'
	  ){
						
		fn_rowCopy(obj,  e.columnid);

	}
		
	if( e.columnid == 'allcStatus' && obj.getColumn(nRow,'allcStatus') == '003'  ){	
		grd_celeList.setCellPos(21);		
		gfn_alert("msg.noinput", "반려사유");	// 반려사유를 입력해주세요					
		return ;
	}
	
	
}

