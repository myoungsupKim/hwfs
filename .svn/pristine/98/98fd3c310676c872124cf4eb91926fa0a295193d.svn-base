/*----------------------------------------------------------------------------------------------
 * 설명      : 저장시 배정자 유효성 검사
 *---------------------------------------------------------------------------------------------*/

function fn_allcCheck(ds_list, gb){

	if( gb == 3){
		for (var i = 0; i < ds_list.getRowCount(); i++) 
		{
			if( ds_list.getRowType(i) == 4  || ds_list.getRowType(i) == 2 ){
				if(  ds_list.getColumn(i,'firstSignStatus') == '003' ){
					if(  ds_list.getColumn(i,'firstRtnReason') == ''){					
						gfn_alert("msg.noinput","반려사유");	// 반려사유를 입력해주세요.		

						return true;
					}   
				} else if( ds_list.getColumn(i,'firstSignStatus') == '002' ){
					if(  ds_list.getColumn(i,'firstSignStatus') == ''){
						gfn_alert("msg.noinput", "결재자");	// ("결재자를 입력해주세요		
						return true;
					}   
				}	
			}
		}
		
	} else {
		
		for (var i = 0; i < ds_list.getRowCount(); i++) 
		{
			if( ds_list.getRowType(i) == 4  || ds_list.getRowType(i) == 2 ){
				if( ds_list.getColumn(i,'allcStatus') == '003' ){
					if(  ds_list.getColumn(i,'allcReason') == ''){
						gfn_alert("msg.noinput","반려사유");	// 반려사유를 입력해주세요.				
						return true;
					}   
				} else if( ds_list.getColumn(i,'allcStatus') == '002' ){
					if(  ds_list.getColumn(i,'allcSabun') == ''){
						gfn_alert("msg.noinput", "배정자");	// 배정자를 입력해주세요.
						return true;
					}   
				}	
			}
		}
	}
	return false;

}

