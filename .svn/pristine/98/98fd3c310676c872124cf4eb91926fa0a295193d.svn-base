	if(  cbo_catering.value == '001'){	
		ds_list = ds_grdCeleList;				//행사 요청	
	} else if( cbo_catering.value == '002'){
		ds_list = ds_grdCookHumanList;			//조리인력		
	} else if( cbo_catering.value == '003' ){
		ds_list = ds_grdUtensilList;			// 기기/기물 요청
	} else if( cbo_catering.value == '004'){
		ds_list = ds_grdCookGuidList;			// 조리지도
	}
	

	var nRow = ds_list.rowposition;
	var nSelRow;

	if(ds_list.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{
		if(cbo_catering.value == "001"){
			nSelRow = ds_grdCeleList.rowposition;
			// - 버튼 클릭시 rowcount 체크
			if(fn_datasetF2(ds_grdCeleList,"noMan", nSelRow))return;
		}else if(cbo_catering.value == "002"){
			nSelRow = ds_grdCookHumanList.rowposition;
			if(fn_datasetF2(ds_grdCookHumanList,"noMan", nSelRow))return;
		}else if(cbo_catering.value == "004"){
			nSelRow = ds_grdCookGuidList.rowposition;
			if(fn_datasetF2(ds_grdCookGuidList,"noMan", nSelRow))return;
		}
		ds_list.deleteRow(nRow);
	}
	
	 
	 if( ds_list.getColumn(nRow, 'allcSeq' )  == '1'  ) {
		return ;
	 }
	 

	 if(  cbo_catering.value == '001'){	
		if( ds_list.getColumn(nRow, "allcYn") == 1){ 	//행사 요청				
			return ;
		}
	} else if( cbo_catering.value == '002'){
		if( ds_list.getColumn(nRow, "allcYn") == 1){ 	//조리인력		
			return ;
		}
	} else if( cbo_catering.value == '003' ){
		if( ds_list.getColumn(nRow, "apprvYn") == 1){ 	// 기기/기물 요청
			return ;
		}
	} else if( cbo_catering.value == '004'){
		if( ds_list.getColumn(nRow, "allcYn") == 1){ 	// 조리지도
			return ;
		}
	}

	
}

