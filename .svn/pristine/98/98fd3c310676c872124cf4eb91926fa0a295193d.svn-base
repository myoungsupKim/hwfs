/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	

	var ds_list = null;
	var grd_list = null;
	var nRow = 0;
	var nSelRow = 0;
	if(  cbo_catering.value == '001'){	
	
		ds_list = ds_grdCeleList;		//행사 요청	
		grd_list = grd_celeList;
		nSelRow = ds_list.rowposition;		
		
		
		if( fn_rowCheck(ds_list ) > 0 ){
			return ;
		}
		
		
		// 배정인원 초과시 경고 메시지
		if(fn_datasetF(ds_grdCeleList,"noMan", nSelRow))return;
		
		
		nRow = ds_list.addRow();			
		ds_list.copyRow(nRow,ds_list,nSelRow);
		
		ds_list.setColumn(nRow, "allcSeq", nRow + 1);
		ds_list.setColumn(nRow, "allcSabun", "");
		ds_list.setColumn(nRow, "allcNm", "");
		ds_list.setColumn(nRow, "allcReason", "");
		//ds_list.setColumn(nRow, "allcStatus", "001");
		ds_list.setColumn(nRow, "newYn", "Y");		
		ds_list.setColumn(nRow, "caterClass", "001");		
		

		ds_grdCeleList.updateSortGroup("S:-celeTermSdate-requestUpjangCd-requestId-seq+allcSeq");
		
	} else if( cbo_catering.value == '002'){
		ds_list = ds_grdCookHumanList;	//조리인력
		grd_list = grd_cookHumanList;
		nSelRow = ds_list.rowposition;
		
		if( fn_rowCheck(ds_list ) > 0  ){
			return ;
		}
		
		// 배정인원 초과시 경고 메시지
		if(fn_datasetF(ds_grdCookHumanList,"noMan", nSelRow))return;
				
		nRow = ds_list.addRow(nSelRow);			
		ds_list.copyRow(nRow,ds_list,nSelRow);
		
		ds_list.setColumn(nRow, "allcSeq", nRow + 1);
		ds_list.setColumn(nRow, "allcSabun", "");
		ds_list.setColumn(nRow, "allcNm", "");
		ds_list.setColumn(nRow, "allcReason", "");
		//ds_list.setColumn(nRow, "allcStatus", "001");
		ds_list.setColumn(nRow, "newYn", "Y");			

		ds_list.updateSortGroup("S:-guidTermSdate-requestUpjangCd-requestId-seq+allcSeq");
		
	} else if( cbo_catering.value == '003' ){
		return ;
		
	} else if( cbo_catering.value == '004'){
		ds_list = ds_grdCookGuidList;								// 조리지도
		grd_list = grd_cookGuidList;
		nSelRow = ds_list.rowposition;
		
		if( fn_rowCheck(ds_list ,'row') > 0  ){
			return ;
		}
		
		// 배정인원 초과시 경고 메시지
		if(fn_datasetF(ds_grdCookGuidList,"noMan", nSelRow))return;
		
		nRow = ds_list.addRow(nSelRow);			
		ds_list.copyRow(nRow,ds_list,nSelRow);
		
		
		ds_list.setColumn(nRow, "allcSeq", nRow + 1);
		ds_list.setColumn(nRow, "allcSabun", "");
		ds_list.setColumn(nRow, "allcNm", "");
		ds_list.setColumn(nRow, "allcReason", "");
		//ds_list.setColumn(nRow, "allcStatus", "001");
		ds_list.setColumn(nRow, "newYn", "Y");		

		ds_list.updateSortGroup("S:-guidTermSdate-requestUpjangCd-requestId-seq+allcSeq");
		
	}
	
		
	// Grid 입력 cell 지정
	grd_list.setFocus();
	grd_list.setCellPos(1);
}

/*-----------------------------------------------------------------------------------
 *설명    : 배정자 + 버튼 클릭시 배정인원 초과 체크
 *----------------------------------------------------------------------------------*/
function fn_datasetF(obj,col, row){
		obj.filter("requestId =='"+obj.getColumn(row,"requestId")+"'");
		var rowCnt = obj.getRowCount();
		obj.filter("");
		if(rowCnt == obj.getColumn(row,col)){
			gfn_alert("인원보다 많은 행을 추가 할 수 없습니다.");
			return true;
		}
		return false;
}

/*-----------------------------------------------------------------------------------
 *설명    : 배정자 - 버튼 클릭시 rowcount 체크
 *----------------------------------------------------------------------------------*/
function fn_datasetF2(obj,col, row){
		obj.filter("requestId =='"+obj.getColumn(row,"requestId")+"'");
		var rowCnt = obj.getRowCount();
		obj.filter("");
		if(rowCnt == 1){
			gfn_alert("더이상 삭제하실 수 없습니다.");
			return true;
		}
	return false;
}

