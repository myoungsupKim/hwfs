					 && nSeq == ds_list.getColumn(i, 'seq') ){
				
					if( nRow != i && nAllcSabun == ds_list.getColumn(i, 'allcSabun') ){
						//gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
						gfn_alert("msg.err.overlap.date", "배정자");	//배정자가 중복 입력되었습니다.
						
						nAllcSabun = '';
						nAllcNm =  '';
						
						return ;
					}
				}else{
					break;
				}
			}
			
			ds_list.setColumn(nRow, 'allcSabun', nAllcSabun);
			ds_list.setColumn(nRow, 'allcNm', nAllcNm);
			
		}else{
			ds_list.setColumn(nRow, 'firstSignPrcgEmp', nAllcSabun);
			ds_list.setColumn(nRow, 'firstSignPrcgEmpNm', nAllcNm);
					
		}
		if(cbo_catering.value == "001"){
			ds_grdCeleList.setColumn(ds_grdCeleList.rowposition, "allcStatus", "002");
		}else if(cbo_catering.value == "002"){
			ds_grdCookHumanList.setColumn(ds_grdCookHumanList.rowposition, "allcStatus", "002");
		}else if(cbo_catering.value == "004"){
			ds_grdCookGuidList.setColumn(ds_grdCookGuidList.rowposition, "allcStatus", "002");
		}
	}

	//팝업창을 호출한다.]]
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 배정 행 추가
 *---------------------------------------------------------------------------------------------*/
function fn_rowCopy(ds_list, nColumnid){

	var nRow = ds_list.rowposition;
	var nRequestUpjangCd = ds_list.getColumn(nRow, 'requestUpjangCd');
	var nRequestId 		 = ds_list.getColumn(nRow, 'requestId');
	var nSeq 			 = ds_list.getColumn(nRow, 'seq');	
	var nChangeValue 	 = ds_list.getColumn(nRow, nColumnid);	
	var nStartRow 		 = ds_list.findRowExpr("requestUpjangCd == '"+nRequestUpjangCd+"'&& requestId == '"+nRequestId+"' && seq == '"+ nSeq+"' ");
	
	for ( var i = nStartRow ; i < ds_list.getRowCount() ; i++){		  
		if ( nRequestUpjangCd == ds_list.getColumn(i, 'requestUpjangCd') 
		  && nRequestId == ds_list.getColumn(i, 'requestId') 
		  && nSeq == ds_list.getColumn(i, 'seq') ){
			ds_list.setColumn(i, nColumnid, nChangeValue );
		}else{
			break;
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 배정자 행 추가 체크
 *---------------------------------------------------------------------------------------------*/
function fn_rowCheck(ds_list , nGubun){
	var nRow = ds_list.rowposition;
	var nNoMan = parseInt(ds_list.getColumn(nRow, "noMan"));
		
	var nRequestUpjangCd = ds_list.getColumn(nRow, 'requestUpjangCd');
	var nRequestId 		 = ds_list.getColumn(nRow, 'requestId');
	var nSeq 			 = ds_list.getColumn(nRow, 'seq');	
	var nCnt = 0
	var nStartRow 		 = ds_list.findRowExpr("requestUpjangCd == '"+nRequestUpjangCd+"'&& requestId == '"+nRequestId+"' && seq == '"+ nSeq+"' ");
	
	for ( var i = nStartRow ; i < ds_list.getRowCount() ; i++){		  
		if (    nRequestUpjangCd == ds_list.getColumn(i, 'requestUpjangCd') 
			 && nRequestId == ds_list.getColumn(i, 'requestId') 
			 && nSeq == ds_list.getColumn(i, 'seq') 
			 && ds_list.getColumn(i, 'allcStatus') != '003'  ){
				nCnt++;				
		}else{
			break;
		}
	}
		
	if( nGubun == 'col') {
		if( nCnt  > nNoMan ){
			//gfn_alert("배정 인원("+nCnt+"명)보다 미만입니다.", "");	// 선택된 항목이 없습니다.
			gfn_alert("msg.err.mindata.date", "배정 인원("+nCnt+"명)");	// 배정 인원("+nCnt+"명)보다 미만입니다.
			return nCnt;
		}
	}else{
		if( nCnt + 1 > nNoMan ){
			//gfn_alert("인원("+ (nNoMan) +"명)을 초과하였습니다.", "");	// 선택된 항목이 없습니다.
			gfn_alert("msg.err.maxdata.date", "인원("+ (nNoMan) +"명)");	// 인원("+ (nNoMan) +"명)을 초과하였습니다.
			
			return nNoMan;
		}
	}

	return 0;
}

