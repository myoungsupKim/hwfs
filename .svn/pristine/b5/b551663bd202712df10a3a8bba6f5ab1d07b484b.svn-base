function fn_save()
{
	if (!gfn_dsIsUpdated(ds_reservMaster)) 
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	// 저장시 Validation Check	
	//if (gfn_validCheck(grd_list, ds_listValid) == false)	return;
	for(var i=0; i<ds_reservMaster.rowcount; i++) {
		if(gfn_isNull(ds_reservMaster.getColumn(i, "reservNm"))||gfn_isNull(ds_reservMaster.getColumn(i, "reservDate"))) {
			gfn_alert("예약자명,예약일은 필수입력값입니다.");
			ds_reservMaster.rowposition = i;
			return;
		}
	}
	
    var strSvcID    = "save";
    var strURL      = "fs/fsf/funeralDeadReg/saveReservMaster.xdo";
    var strInDs     = "ds_reservMaster=ds_reservMaster:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

