************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		case "searchFuneralDeadRegInfo" :
            if(ds_funeralDeadInfo.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_funeralDeadInfo, fv_funeralDeadColInfo);
			}
			
			// enable 제어
 			for(var i=1; i<=15; i++) {
 				
 				if(i!=4) {
					if(i<10) {
						if(gfn_isNull(ds_funeralDeadInfo.getColumn(0, "theDeadNm0"+i))) {
						//	eval("tab_funeralDeadReg.tabpage1.div0"+i+".div0"+i).style.background.color = "whitesmoke";							
				//			eval("tab_funeralDeadReg.tabpage1.div0"+i+".edt_funeralParlor0"+i).enable= false;							
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".edt_theDeadNm0"+i).enable     = false;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".edt_borneOutDate0"+i).enable = false;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".edt_amt0"+i).enable          = false;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".chk_chng0"+i).enable         = false;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".chk_chng0"+i).style.color = "#777777ff";							
						}else {
						//	eval("tab_funeralDeadReg.tabpage1.div0"+i+".div0"+i).style.background.color = "yellow";
 				//			eval("tab_funeralDeadReg.tabpage1.div0"+i+".edt_funeralParlor0"+i).enable= true;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".edt_theDeadNm0"+i).enable     = true;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".edt_borneOutDate0"+i).enable = true;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".edt_amt0"+i).enable          = true;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".chk_chng0"+i).enable         = true;
 							eval("tab_funeralDeadReg.tabpage1.div0"+i+".chk_chng0"+i).style.color = "blue";							
						}
					}else{
						if(gfn_isNull(ds_funeralDeadInfo.getColumn(0, "theDeadNm"+i))) {
						//	eval("tab_funeralDeadReg.tabpage1.div0"+i+".div"+i).style.background.color = "whitesmoke";							
 				//			eval("tab_funeralDeadReg.tabpage1.div"+i+".edt_funeralParlor"+i).enable= false;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".edt_theDeadNm"+i).enable     = false;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".edt_borneOutDate"+i).enable = false;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".edt_amt"+i).enable          = false;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".chk_chng"+i).enable         = false;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".chk_chng"+i).style.color = "#777777ff";							
						}else{
						//	eval("tab_funeralDeadReg.tabpage1.div0"+i+".div"+i).style.background.color = "yellow";
 				//			eval("tab_funeralDeadReg.tabpage1.div"+i+".edt_funeralParlor"+i).enable= true;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".edt_theDeadNm"+i).enable     = true;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".edt_borneOutDate"+i).enable = true;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".edt_amt"+i).enable          = true;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".chk_chng"+i).enable         = true;
 							eval("tab_funeralDeadReg.tabpage1.div"+i+".chk_chng"+i).style.color = "blue";							
						}
					}
 				}
 			}
			tab_funeralDeadReg.tabpage1.setFocus();
			fn_searchReservMaster();
            break;
            
        case "searchReservMaster" :
            if(ds_reservMaster.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_reservMaster, fv_reservMasterColInfo);
			}
            break;
            
        case "searchFuneralParlorHistory" :
            if(ds_funeralParlorHistory.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_funeralParlorHistory, fv_funeralParlorHistory);
			}
            break;    
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break;
    }
}


