*/    
    }
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_callPopup()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 호실변경 POPUP 호출 이벤트
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_callPopup(obj:CheckBox, e:MouseEventInfo)
{
	
	obj.value = true;
	
	var curHallCd = ds_search.getColumn(0, "upjangCd");		// 장례식장
	var curParlor  = ds_funeralDeadInfo.getColumn(0,"funeralParlor"+String(obj.name).substr(8, 2));	// 빈소
	var theDeadNm  = ds_funeralDeadInfo.getColumn(0,"theDeadNm"+String(obj.name).substr(8, 2));		// 고인명
	var guestNum  = ds_funeralDeadInfo.getColumn(0,"guestNum"+String(obj.name).substr(8, 2));		// 고객번호
	var borneOutDt = ds_funeralDeadInfo.getColumn(0,"borneOutDate"+String(obj.name).substr(8, 2));	// 안치일(YYYY-MM-DD)
	var usingRoom  = "";	// 현재 이용중인 호실
	for(var i=1; i<=15; i++) {
 				
		if(i!=4) {
			if(i<10) {
				if(!gfn_isNull(ds_funeralDeadInfo.getColumn(0, "theDeadNm0"+i))) {
				
					usingRoom += ",0"+i;
					
				}
			}else{
				if(!gfn_isNull(ds_funeralDeadInfo.getColumn(0, "theDeadNm"+i))) {
				
					usingRoom += ","+i;
					
				}
			}
		}
	}
	usingRoom = usingRoom.substr(1, usingRoom.length-1);
	
	//alert("장례식장curHallCd: "+curHallCd+" curParlor: "+curParlor+" guestNum: "+guestNum+" borneOutDt: "+borneOutDt+" usingRoom: "+usingRoom);
	
	var strId       = "FuneralRoomNumberChgpop";
	var strURL      = "X_FSF::FuneralRoomNumberChgpop.xfdl";
	var objArgument = {argHallCd:curHallCd,argParlor:curParlor,argGuestNum:guestNum,argOutDt:borneOutDt,argUsingRoom:usingRoom};
	var nWidth      = 646;
	var nHeight     = 400;
	var strStyle    = "";
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";

	// 리턴값을 배열로 받을 경우
	var arrRtn = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);
	fn_search();
// 	if(!gfn_isNull(arrRtn)) {
// 		
// 	}
}
