function fn_search()
{
	var strSvcID    = "";
	var strURL      = "";
	var strInDs     = "";
	var strOutDs    = "";
	var strArg      = "";
	var strCallback = "";
	var bAsync      = true;
	
	// 조회조건 Param
	ds_search.setColumn(0, "funeralHallCd", ds_search.getColumn(0, "upjangCd"));
	if(tab_funeralDeadReg.tabindex == 0) {
	
		// v_ds_check 미사용시 변경사항 체크
// 		if (gfn_dsIsUpdated(ds_funeralDeadInfo)||gfn_dsIsUpdated(ds_reservMaster))
// 		{
// 			var retValue = gfn_confirm("confirm.before.search");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
// 			if (retValue == false) return;
// 		}
		
		// 조회시 Validation Check
		//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

		// Grid sort 표시 초기화(필수)
		gfn_clearSortMark(tab_funeralDeadReg.tabpage1.div_reservMaster.grd_reservMaster);
		
		//서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
		fv_funeralDeadColInfo = gfn_getDatasetInfo(ds_funeralDeadInfo);				
		ds_funeralDeadInfo.clearData();		
		
		strSvcID    = "searchFuneralDeadRegInfo";
		strURL      = "fs/fsf/funeralDeadReg/selectFuneralDeadRegInfo.xdo";
		strInDs     = "ds_search=ds_search:A";
		strOutDs    = "ds_funeralDeadInfo=ds_funeralDeadInfo";
		strArg      = "";
		strCallback = "fn_callBack";
		bAsync      = true;
    }else{
    
		// Grid sort 표시 초기화(필수)
		gfn_clearSortMark(tab_funeralDeadReg.tabpage2.grd_funeralParlorHistory);
		//서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
		fv_funeralParlorHistory = gfn_getDatasetInfo(ds_funeralParlorHistory);
		
	//	trace(ds_search.saveXML());
		
		strSvcID    = "searchFuneralParlorHistory";
		strURL      = "fs/fsf/funeralDeadReg/selectFuneralParlorHistory.xdo";
		strInDs     = "ds_search=ds_search:A";
		strOutDs    = "ds_funeralParlorHistory=ds_funeralParlorHistory";
		strArg      = "";
		strCallback = "fn_callBack";
		bAsync      = true;
    }
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

function fn_insert(){
	var addRw = ds_reservMaster.addRow();
	ds_reservMaster.setColumn(addRw, "funeralHallCd", ds_search.getColumn(0, "upjangCd"));
	ds_reservMaster.setColumn(addRw, "reservDate", fv_sToday);	// 초기값
}

function fn_delete(){
	if (this.ds_reservMaster.rowposition < 0) {
		gfn_alert("msg.err.grid.noselect");			// 선택된 항목이 없습니다.
	}else {
		var confirm = gfn_confirm("confirm.before.delete");			// 선택된 자료를 삭제 하시겠습니까?
		// 삭제를 선택 하면
		if(confirm)	{
			ds_reservMaster.deleteRow(ds_reservMaster.rowposition);
		}
	}
}

