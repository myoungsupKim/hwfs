function fn_callMenu(obj, e:MouseEventInfo)
{	
	var arrParam = new Array();
 	var deadNm    = "";
	var bornOutDt = "";		
 	if(String(obj.name).substr(0, 13)=="edt_theDeadNm") {
		deadNm    = ds_funeralDeadInfo.getColumn(0, String(obj.name).substr(4, 15));
		bornOutDt = gfn_dateToStr(ds_funeralDeadInfo.getColumn(0, "borneOutDate"+String(obj.name).substr(13, 2)));
		arrParam[0] = deadNm;
		arrParam[1] = bornOutDt; 
		gfn_openMenu("2201",arrParam);	// 고객등록
/*
		gfn_openMenuForm( "FuneralPatronMstReg.xfdl"		// pgm_id, 
					  ,"X_FSF"				// path,
					  ,arrParam				// v_call_param,
					  ,"고객등록");		// v_pgm_text
*/  
    }else if(String(obj.name).substr(0, 7)=="edt_amt") {
		deadNm    = ds_funeralDeadInfo.getColumn(0, "theDeadNm"+String(obj.name).substr(7, 2));
		bornOutDt = gfn_dateToStr(ds_funeralDeadInfo.getColumn(0, "borneOutDate"+String(obj.name).substr(7, 2)));
		casketDt  = gfn_dateToStr(ds_funeralDeadInfo.getColumn(0, "casketDate"+String(obj.name).substr(7, 2)));
		arrParam[0] = deadNm;
		arrParam[1] = bornOutDt;	// 발인일
		arrParam[2] = casketDt;		// 입관일
		gfn_openMenu("2207",arrParam);		// 고인별판매품목출고
