	this.close(" ");
}

function btn_PlusJajea_OnClick(obj:Button, e:ClickEventInfo)
{
	/*
	if(toNumber(gfn_nvl(msk_OpPrice.value,"0"))<=0)
	{
		g_Message("EE","운영단가를 입력하세요.");
		msk_OpPrice.setFocus();
		return;
	}
	*/
	//if(toNumber(gfn_nvl(msk_OpRate.value,"0"))<=0 && toNumber(gfn_nvl(msk_OpPrice.value,"0"))<=0)
	if(toNumber(gfn_nvl(msk_OpRate.value,"0")) == 0 && toNumber(gfn_nvl(msk_OpPrice.value,"0")) == 0 )
	{
		g_Message("EE","운영단가 또는 운영율중에 최소한 하나는 입력하세요.");
		msk_OpPrice.setFocus();
		return;
	}
	if(toNumber(gfn_nvl(msk_CeilVal.value,"0"))<=0)
	{
		g_Message("EE","올림수를 입력하세요.");
		msk_CeilVal.setFocus();
		return;
	}
	
	var fRow = OBJ_DS.findRow("ITEM_CODE", ds_ListItem.getColumn(ds_ListItem.rowposition,"ITEM_CODE"));
	
	if ( fRow > -1 ) 
	{
		g_Message("EE", "이미 등록된 상품입니다.");
		return;
	}
	
	var aRow = OBJ_DS.addRow();
	OBJ_DS.setColumn(aRow, "UPJANG", ds_ListItem.getColumn(ds_ListItem.rowposition,"UPJANG"));
	OBJ_DS.setColumn(aRow, "SDATE", ds_ListItem.getColumn(ds_ListItem.rowposition,"SDATE"));
	OBJ_DS.setColumn(aRow, "EDATE", ds_ListItem.getColumn(ds_ListItem.rowposition,"EDATE"));
	OBJ_DS.setColumn(aRow, "CLASS_NAME2", ds_ListItem.getColumn(ds_ListItem.rowposition,"CLASS_NAME2"));
	OBJ_DS.setColumn(aRow, "ITEM_CODE", ds_ListItem.getColumn(ds_ListItem.rowposition,"ITEM_CODE"));
	OBJ_DS.setColumn(aRow, "ITEM_NAME", ds_ListItem.getColumn(ds_ListItem.rowposition,"ITEM_NAME"));
	OBJ_DS.setColumn(aRow, "ITEM_SIZE", ds_ListItem.getColumn(ds_ListItem.rowposition,"ITEM_SIZE"));
	OBJ_DS.setColumn(aRow, "PO_UOM", ds_ListItem.getColumn(ds_ListItem.rowposition,"PO_UOM"));
	OBJ_DS.setColumn(aRow, "SALE_PRICE", ds_ListItem.getColumn(ds_ListItem.rowposition,"SALE_PRICE"));
	OBJ_DS.setColumn(aRow, "OP_RATE", msk_OpRate.value);
	OBJ_DS.setColumn(aRow, "OP_PRICE", msk_OpPrice.value);
	
	if ( gfn_isNull(msk_OpRate.value) ) {
		if (numFormat(ds_ListItem.getColumn(ds_ListItem.rowposition,"SALE_PRICE"))== "0") {
			OBJ_DS.setColumn(aRow, "OP_RATE", 100 * msk_OpPrice.value);
		} else {
			if (numFormat(msk_OpPrice.value)== "0") {
				OBJ_DS.setColumn(aRow, "OP_RATE", 0);
			} else {
				OBJ_DS.setColumn(aRow, "OP_RATE", 100 * ((toNumber(msk_OpPrice.value) / toNumber(ds_ListItem.getColumn(ds_ListItem.rowposition,"SALE_PRICE"))) - 1));
			}
		}
	}
	
	OBJ_DS.setColumn(aRow, "CEIL_VAL", msk_CeilVal.value);
	
}

