function fn_summary()
{
		me_MEAL_CNT_SUM.value = toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "MEAL_CNT_A")) + 
							toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "MEAL_CNT_B")) + 
							toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "MEAL_CNT_C")) + 
							toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "MEAL_CNT_D")) + 
							toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "MEAL_CNT_E"));
	
	
		me_SALE_SUM.value =     toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "SALE_A")) + 
								toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "SALE_B")) + 
								toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "SALE_C")) + 
								toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "SALE_D"));

	
		me_EMPLOYEE_SUM.value = toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_A")) + 
								toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_B")) + 
								toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_C")) + 
								toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_D")) +
								toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_E")) +
								toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_F"));
	
	
		me_EMPLOYEE_A_PAY_SUM.value =     toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_A")) * 
								          toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_A_PAY"));
        
        me_EMPLOYEE_B_PAY_SUM.value =     toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_B")) * 
								          toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_B_PAY"));
								          													
        me_EMPLOYEE_C_PAY_SUM.value =     toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_C")) * 
								          toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_C_PAY"));

									
        me_EMPLOYEE_D_PAY_SUM.value =     toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_D")) * 
								          toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_D_PAY"));

									
        me_EMPLOYEE_E_PAY_SUM.value =     toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_E")) * 
								          toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_E_PAY"));

									
        me_EMPLOYEE_F_PAY_SUM.value =     toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_F")) * 
								          toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "EMPLOYEE_F_PAY"));

									
									
									
									
		me_EMPLOYEE_PAY_SUM.value =     toNumber(me_EMPLOYEE_A_PAY_SUM.value) + 
										toNumber(me_EMPLOYEE_B_PAY_SUM.value) + 
										toNumber(me_EMPLOYEE_C_PAY_SUM.value) + 
										toNumber(me_EMPLOYEE_D_PAY_SUM.value) + 
										toNumber(me_EMPLOYEE_E_PAY_SUM.value) + 
										toNumber(me_EMPLOYEE_F_PAY_SUM.value);
}

