function before_SaveCheck(str_Gubun)
{

        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
        
        f_Saved_ID = 0;
        f_NewDataCheck = false;
        
      
        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
		
	
 		if (ds_listInput.rowcount == 0){

			return;
		}

	    if (str_Gubun == "Master"){

			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "CUSTNM"))== "0")
			{
			
				g_Message("EE", "고객사명을 지정하여 주십시요.");
				edt_CUSTNM.setFocus();
				return false;
			}
			
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "MGMT_DEPT_ID"))== "0")
			{
			
				g_Message("EE", "담당부서를 지정하여 주십시요.");
				edt_MGMT_DEPT_ID.setFocus();
				return false;
			}			
		
	        //입력일때 Confirm체크##################################
	        if ( 
	             (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "CUST_ID"))== "0")
	           )
	        {
				if (!g_Confirm("02")) return false;	             
	        }     
	        //입력일때 Confirm체크##################################
	             
	             
	        //수정일때 Confirm체크##################################
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "CUST_ID"))!= "0")
               )
            {	        
				if (!g_Confirm("02", "데이터를 수정하시겠습니까?")) return false;
			}
	        //수정일때 Confirm체크##################################
	        
						
			//CUST_ID 지정#################################################################
			if(!fn_SaveTrxData(ds_listInput.getColumn(ds_listInput.rowposition, "CUST_ID"))) return false;



		}			

        
		//저장#############################################################
	    if (str_Gubun == "Master"){

			// action 정보 초기화
			fsp_clear(this);	  		
										    		        
	    
            //기존 라인 및 매출 데이터 삭제.            
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(f_Saved_ID)!= "0")
               )
            {
  
			    fsp_addSingle(this, "ma/mak:MAK10010E_U001");		
			    fsp_addSingle(this, "ma/mak:MAK10010E_I002");					
            }
            else if (
					 (ds_listInput.rowcount > 0) &&
					 (numFormat(f_Saved_ID)== "0")
				    )
            {
  
			    fsp_addSingle(this, "ma/mak:MAK10010E_I001");
			    fsp_addSingle(this, "ma/mak:MAK10010E_I002");						
            }
            				    
            				    
			
			f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "CUST_ID");
			fsp_callService(this, "", "", "ds_cond=ds_listInput:A", "",  "", "fn_AfterSaveSearch()");
	    }
	   

		return true;
}


