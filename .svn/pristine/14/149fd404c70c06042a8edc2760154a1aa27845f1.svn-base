    <Script type="xscript4.0"><![CDATA[/*
  화 면 명 : 클레임요청
  파 일 명 : ClaimReq.xfdl
  기    능 : 클레임요청

    수정일    수정자                  수정내용
  ----------   -------  --------------------------------------------------
  2015.05.06	최영준	최초 생성
  2017.02.10	kihoon	[CH201702_00230] 클레임 요청시간(클라이언트->서버) 변경
*/

include "lib::lib_com.xjs";


function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

function fn_init() {
	var fv_today = gfn_today();
	var sFromDt = "";
	if ( fv_today.substr(0, 4) == "2018" ) {
		sFromDt = "20180201";
	} else {
		sFromDt = fv_today.substr(0, 4) + "0101";
	}
	cal_date1.fn_initMonthCal(this, sFromDt, "", ds_search, "schDate1", 'Y');
	cal_date2.fn_initMonthCal(this, fv_today, "", ds_search, "schDate2", 'Y');
	//fn_drawChart();
	
	fn_search();
}

function fn_search() {

	if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

    var strSvcID    = "search";
    var strURL      = "fm/fmc/orderClaimCnt/chartSearch.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}


function fn_callBack(strSvcID, nErrorCode, strErrorMsg) {
	if (nErrorCode < 0) {
		return;
	}
	
    switch ( strSvcID ) {
        case "search" :
            //trace(ds_list.saveXML());
            if ( ds_list.rowcount == 0 ) {
				gfn_alert("조회된 데이터가 없습니다.");
            } else {
				fn_setChartData();
            }
            break;
    }
}

function fn_setChartData() {
	//fds_chart1.keystring = "G:+cls1Cd";
	fds_chart2.filterstr.current = "cls1Cd == 'G'";
	fds_chart3.filterstr.current = "cls1Cd == 'L'";
	fds_chart4.filterstr.current = "cls1Cd == 'E'";
	
	fn_drawChart();
}

function fn_drawChart() {
	var colorCd1 = ["c74118", "93ba1c", "3879b5"];
	var strChartXML1 = '<chart';
		strChartXML1 += ' caption="Claim 유형별 발생비율"';
		strChartXML1 += ' baseFont="굴림"';
		strChartXML1 += ' baseFontSize="12"';
		strChartXML1 += ' labelFontSize="10"';
		strChartXML1 += ' startingangle="90"'; 
		strChartXML1 += ' showlabels="1"';
		strChartXML1 += ' enablesmartlabels="1"';
		strChartXML1 += ' showpercentvalues="1"';
		strChartXML1 += ' showpercentintooltip="0"';
		strChartXML1 += ' plottooltext="$datavalue"';
		strChartXML1 += ' > \n';
		strChartXML1 += '    <set label="기타" value="'+ds_list.getCaseSum("cls1Cd == 'E'", "cnt")+'" color="c74118"  /> \n';
		strChartXML1 += '    <set label="배송" value="'+ds_list.getCaseSum("cls1Cd == 'L'", "cnt")+'" color="93ba1c" /> \n';
		strChartXML1 += '    <set label="상품" value="'+ds_list.getCaseSum("cls1Cd == 'G'", "cnt")+'" issliced="1" color="3879b5" /> \n';
	/*
	for ( var i=0; i<fds_chart1.rowcount; i++ ) {
		strChartXML1 += '    <set label="'+fds_chart1.getColumn(i, "cls2Nm")+'" value="'+fds_chart1.getColumn(i, "cnt")+'" color="'+colorCd1[i]+'"  /> \n';
	}
	*/
		strChartXML1 += '</chart> \n';
	
	
	
	var nCnt = 0;
	var colorCd2 = ["90b81a", "ebb923", "a9cfeb", "096609", "c74114", "f09d3e", "3d7cb8", "fcf14c", "f299a3", "bba9cf" ];
	var strChartXML2 = '<chart';
		strChartXML2 += ' caption="상품Claim 유형별 발생비율"';
		strChartXML2 += ' baseFont="굴림"';
		strChartXML2 += ' baseFontSize="12"';
		strChartXML2 += ' labelFontSize="10"';
		strChartXML2 += ' startingangle="90"'; 
		strChartXML2 += ' showlabels="1"';
		strChartXML2 += ' enablesmartlabels="1"';
		strChartXML2 += ' showpercentvalues="1"';
		strChartXML2 += ' showpercentintooltip="0"';
		strChartXML2 += ' plottooltext="$datavalue"';
		strChartXML2 += ' > \n';
	
	ds_listChart2.clearData();
	for ( var i=0; i<fds_chart2.rowcount; i++ ) {
		if ( fds_chart2.getRowLevel(i) == 1 ) {
			var nRow = ds_listChart2.addRow();
			ds_listChart2.setColumn(nRow, "cls2Nm", fds_chart2.getColumn(i-1, "cls2Nm"));
			ds_listChart2.setColumn(nRow, "cnt", fds_chart2.getColumn(i, "cnt"));	
		}
	}
	
	ds_listChart2.keystring = "S:+cnt";
	for ( var i=ds_listChart2.rowcount-7; i<ds_listChart2.rowcount; i++ ) {
		if ( i == ds_listChart2.rowcount-1 ) {
			strChartXML2 += '    <set label="'+ds_listChart2.getColumn(i , "cls2Nm")+'" issliced="1" value="'+ds_listChart2.getColumn(i, "cnt")+'" color="'+colorCd2[nCnt]+'"  /> \n';
		} else {
			strChartXML2 += '    <set label="'+ds_listChart2.getColumn(i , "cls2Nm")+'" value="'+ds_listChart2.getColumn(i, "cnt")+'" color="'+colorCd2[nCnt]+'"  /> \n';
		}
		nCnt++;
	}
		strChartXML2 += '</chart> \n';
		
	
	nCnt = 0;	
	var colorCd3 = ["269999", "d66565", "945994", "608733", "ada50e", "269ad4", "b3494d", "3a7ab5", "f09e41", "cf512b", "267826", "b4d6f0", "f0c135", "96bd24"];
	var strChartXML3 = '<chart';
		strChartXML3 += ' caption="배송Claim 유형별 발생비율"';
		strChartXML3 += ' baseFont="굴림"';
		strChartXML3 += ' baseFontSize="12"';
		strChartXML3 += ' labelFontSize="10"';
		strChartXML3 += ' startingangle="90"'; 
		strChartXML3 += ' showlabels="1"';
		strChartXML3 += ' enablesmartlabels="1"';
		strChartXML3 += ' showpercentvalues="1"';
		strChartXML3 += ' showpercentintooltip="0"';
		strChartXML3 += ' plottooltext="$datavalue"';
		strChartXML3 += ' > \n';
		
	for ( var i=fds_chart3.rowcount-7; i<fds_chart3.rowcount; i++ ) {
			if ( i == fds_chart3.rowcount-1 ) {
			strChartXML3 += '    <set label="'+fds_chart3.getColumn(i, "cls2Nm2")+'" issliced="1" value="'+fds_chart3.getColumn(i, "cnt")+'" color="'+colorCd3[nCnt]+'"  /> \n';
			nCnt++;
		} else {
			strChartXML3 += '    <set label="'+fds_chart3.getColumn(i, "cls2Nm2")+'" value="'+fds_chart3.getColumn(i, "cnt")+'" color="'+colorCd3[nCnt]+'"  /> \n';
			nCnt++;
		}
	}
		strChartXML3 += '</chart> \n';
		
	var colorCd4 = ["d66565", "945994", "269ad4", "f0c135", "96bd24", "3a7ab5"];
	var strChartXML4 = '<chart';
		strChartXML4 += ' caption="기타Claim 유형별 발생비율"';
		strChartXML4 += ' baseFont="굴림"';
		strChartXML4 += ' baseFontSize="12"';
		strChartXML4 += ' labelFontSize="10"';
		strChartXML4 += ' startingangle="90"'; 
		strChartXML4 += ' showlabels="1"';
		strChartXML4 += ' enablesmartlabels="1"';
		strChartXML4 += ' showpercentvalues="1"';
		strChartXML4 += ' showpercentintooltip="0"';
		strChartXML4 += ' plottooltext="$datavalue"';
		strChartXML4 += ' > \n';
		
	var nCartStd = (fds_chart4.rowcount == 6 ? 6 : 7);
	for ( var i=fds_chart4.rowcount-nCartStd; i<fds_chart4.rowcount; i++ ) {
		if ( i == fds_chart4.rowcount-1 ) {
			strChartXML4 += '    <set label="'+fds_chart4.getColumn(i, "cls2Nm2")+'" issliced="1" value="'+fds_chart4.getColumn(i, "cnt")+'" color="'+colorCd4[i]+'"  /> \n';
		} else {
			strChartXML4 += '    <set label="'+fds_chart4.getColumn(i, "cls2Nm2")+'" value="'+fds_chart4.getColumn(i, "cnt")+'" color="'+colorCd4[i]+'"  /> \n';
		}
		
	}
		strChartXML4 += '</chart> \n';
		
		
		
	for ( var i=1; i<5; i++ ) {
		this.components["atx_chart"+i].StopPlay();
		this.components["atx_chart"+i].Movie = gv_chartPath+"Pie2D.swf";
		this.components["atx_chart"+i].SetVariable('dataXML', eval("strChartXML"+i));
		this.components["atx_chart"+i].Rewind();
		this.components["atx_chart"+i].Play();
	}
}



