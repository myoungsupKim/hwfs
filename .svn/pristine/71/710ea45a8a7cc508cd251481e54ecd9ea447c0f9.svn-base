      <OZQUERY NAME="SQL_M" INCLUDE="" MASTERSET="" DBINFOID="hldc" SCRIPT="false" MAXROW="0" HIDDEN="false" LOADSFIELDINFODYNAMICALLY="false" SDMTYPE="0" SIGN="false" HIDEDESIGNTIME="false" INSERT_ROW_QUERY="" DELETE_ROW_QUERY="" UPDATE_ROW_QUERY="" PREPARED="false">SELECT A.NEED_DATE
         , TO_CHAR(A.RC_UPJANG) AS UPJANG, B.UPJANGNM_DISP, A.SUBINV_CODE
         , DECODE('#OZParam.PARAM5#',NULL,B.CTPRT_PRICEGB||B.CTPRT_FLAG,'#OZParam.PARAM7#') AS CTPRT_GB
         , DECODE('#OZParam.PARAM5#',NULL,B.CTPRT_TYPE,'#OZParam.PARAM13#') AS CTPRT_TYPE
         , NVL(PO_GETREGION_FUN(A.SHIP_ID,A.NEED_DATE),' ') AS REGION_NAME
  FROM HWFS_FMS_UPJANG B
         , (SELECT A.NEED_DATE, A.RC_UPJANG, A.SUBINV_CODE, B.SHIP_ID
               FROM PO_PO A, PO_SUBINVENTORY B
            WHERE A.SUBINV_CODE = B.SUBINV_CODE
                AND A.NEED_DATE BETWEEN '#OZParam.PARAM1#' AND '#OZParam.PARAM2#'
                AND A.RC_UPJANG LIKE NVL('#OZParam.PARAM5#','%')
                AND A.SUBINV_CODE LIKE NVL('#OZParam.PARAM3#','S10%')
                AND A.CENTER_CODE = NVL('#OZParam.PARAM4#',A.CENTER_CODE)
                AND A.LINE_STATUS &gt;= '008'
            GROUP BY A.NEED_DATE, A.RC_UPJANG, A.SUBINV_CODE, B.SHIP_ID) A
 WHERE A.RC_UPJANG = B.UPJANG
   AND B.CTPRT_FLAG &lt;&gt; DECODE('#OZParam.PARAM5#',NULL,'N','PEK')
 ORDER BY A.NEED_DATE, NVL(PO_GETREGION_FUN(A.SHIP_ID,A.NEED_DATE),' '), A.RC_UPJANG, A.SUBINV_CODE


