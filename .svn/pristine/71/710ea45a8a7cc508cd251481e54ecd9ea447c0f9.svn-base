    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', B2.DRNAME, ST_UPJANG_HIST_FUN(B0.UPJANG,A.NEED_DATE,'5')) AS DRNAME_L"
    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', B2.ADDR2, ST_UPJANG_HIST_FUN(B0.UPJANG,A.NEED_DATE,'7')) AS ADDR_L"
    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', 'http://203.233.82.50/hwfs/upload/fm/stamp/'||B2.STAMP_IMG, 'http://203.233.82.173/report/image/stamp_1.gif') AS STAMP_IMG"
    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', B1.UPJANGNM_DISP2||'/'||D2.SUBINV_NAME_DISP2, D.SUBINV_NAME) AS UPJANGNM_R"
    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', SUBSTR(B1.SAUPNO,1,3) || '-' || SUBSTR(B1.SAUPNO,4,2) || '-' || SUBSTR(B1.SAUPNO,6,5), SUBSTR(F.SAUPNO,1,3) || '-' || SUBSTR(F.SAUPNO,4,2) || '-' || SUBSTR(F.SAUPNO,6,5)) AS SAUPNO_R"
    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', B1.DRNAME, F.DRNAME1) AS DRNAME_R"
    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', B1.ADDR2, F.ADDR) AS ADDR_R"
    + "     , A.ITEM_CODE, A.ITEM_NAME"
    + "     , A.PO_UOM || '(' || A.ITEM_SIZE || ')' ITEM_SIZE"
    + "     , E.KEEPING_TYPE"
    + "     , A.TRANS_QTY"
    + "     , ROUND(A.SALE_PRICE / (1 + (NVL(DECODE(A.TAX_CODE, '100', D1.FM_SALE_RATE, D1.FM_NOSALE_RATE),0)/100))) AS SALE_PRICE"
    + "     , 0 AS GREEN_AMT"
    + "     , ROUND(A.TRANS_QTY * ROUND(A.SALE_PRICE / (1 + (NVL(DECODE(A.TAX_CODE, '100', D1.FM_SALE_RATE, D1.FM_NOSALE_RATE),0)/100)))) * DECODE(A.TAX_CODE,100,1,0) TAX_AMOUNT"
    + "     , ROUND(A.TRANS_QTY * ROUND(A.SALE_PRICE / (1 + (NVL(DECODE(A.TAX_CODE, '100', D1.FM_SALE_RATE, D1.FM_NOSALE_RATE),0)/100)))) * DECODE(A.TAX_CODE,100,0,1) NOTAX_AMOUNT"
    + "     , ROUND(A.TRANS_QTY * ROUND(A.SALE_PRICE / (1 + (NVL(DECODE(A.TAX_CODE, '100', D1.FM_SALE_RATE, D1.FM_NOSALE_RATE),0)/100)))) SALE_AMOUNT"
    + "     , ROUND(ROUND(A.TRANS_QTY * ROUND(A.SALE_PRICE / (1 + (NVL(DECODE(A.TAX_CODE, '100', D1.FM_SALE_RATE, D1.FM_NOSALE_RATE),0)/100)))) * DECODE(A.TAX_CODE,100,0.1,0)) SALE_VAT"
    + "     , ROUND(ROUND(A.TRANS_QTY * ROUND(A.SALE_PRICE / (1 + (NVL(DECODE(A.TAX_CODE, '100', D1.FM_SALE_RATE, D1.FM_NOSALE_RATE),0)/100)))) * DECODE(A.TAX_CODE,100,1.1,1)) SALE_TOTAL"
    + "     , A.NEED_DATE||' '||A.SUBINV_CODE AS KEY_FIELD"
//    + "  --   , G.ORIGIN_INFO||' '||G.PART_NAME||' '||G.ITEM_GRADE||' '||G.IDNUMBER||' '||G.BLNUMBER||' '||G.CONF_NUM AS ITEM_INFO"
    + "     , (SELECT Z.ORIGIN_INFO||' '||Z.PART_NAME||' '||Z.ITEM_GRADE||' '||Z.IDNUMBER||' '||Z.BLNUMBER||' '||Z.CONF_NUM|| ' / 수입유통 식별번호: ' ||Z.IMPORT_NUM FROM PO_PO_HIST Z WHERE Z.PO_ID = A.PO_ID) AS ITEM_INFO"
    + "     , '" + #SQL_M.REGION_NAME# + "' AS REGION_NAME"
    + "  FROM (SELECT A.*, (A.CENTER_DELI_QTY + A.DIRECT_DELI_QTY) AS TRANS_QTY"
    + "         , ROUND((A.CENTER_DELI_QTY + A.DIRECT_DELI_QTY) * A.SALE_PRICE) AS SALE_AMOUNT"
    + "          FROM PO_PO A"
    + "         WHERE A.NEED_DATE = '" + #SQL_M.NEED_DATE# + "'"
    + "           AND A.SUBINV_CODE = '" + #SQL_M.SUBINV_CODE# + "'"
    + "           AND A.RC_UPJANG = '" + #SQL_M.UPJANG# + "'"
    + "           AND A.LINE_STATUS &gt; '009'"
    + "           AND A.DELIVERED_QTY + A.UNDELIVERED_QTY &gt; 0"
    + "         UNION ALL /*이상 배송수량*/"
    + "        SELECT A.*, -1 * A.UNDELIVERED_QTY AS TRANS_QTY"
    + "         , ROUND(-1 * A.UNDELIVERED_QTY * A.SALE_PRICE) AS SALE_AMOUNT"
    + "          FROM PO_PO A"
    + "         WHERE A.NEED_DATE = '" + #SQL_M.NEED_DATE# + "'"
    + "           AND A.SUBINV_CODE = '" + #SQL_M.SUBINV_CODE# + "'"
    + "           AND A.RC_UPJANG = '" + #SQL_M.UPJANG# + "'"
    + "           AND A.LINE_STATUS &gt; '009'"
    + "           AND A.UNDELIVERED_QTY &gt; 0"
    + "         UNION ALL /*이상 감량수량, 이하 반품수량*/"
    + "        SELECT A.*, -1 * A.RETURNED_QTY AS TRANS_QTY"
    + "         , ROUND(-1 * A.RETURNED_QTY * A.SALE_PRICE) AS SALE_AMOUNT"
    + "          FROM PO_PO A"
    + "         WHERE A.NEED_DATE = '" + #SQL_M.NEED_DATE# + "'"
    + "           AND A.SUBINV_CODE = '" + #SQL_M.SUBINV_CODE# + "'"
    + "           AND A.RC_UPJANG = '" + #SQL_M.UPJANG# + "'"
    + "           AND A.LINE_STATUS &gt; '009'"
    + "           AND A.RETURNED_QTY &gt; 0) A"
    + "     , ST_UPJANG B, ST_UPJANG B0, HWFS_FMS_UPJANG B1, HWFS_FMS_UPJANG B2"
    + "     , PO_SUBINVENTORY D, FM_SALE_RATE D1, HWFS_FMS_SUBINVENTORY D2"
    + "     , PO_ITEM_MST E"
    + "     , ST_CUST F"
    + "     , PO_ITEM_HIST G"
    + " WHERE A.RC_UPJANG = B.UPJANG AND B.VAT_UPJANG = B0.UPJANG AND B.UPJANG = B1.UPJANG AND B1.MAIN_UPJANG = B2.UPJANG"
    + "   AND A.SUBINV_CODE = D.SUBINV_CODE AND A.SUBINV_CODE = D2.SUBINV_CODE"
    + "   AND A.SUBINV_CODE = D1.SUBINV_CODE(+)"
    + "   AND SUBSTR(A.ITEM_CODE,1,6)||'00' = D1.ITEM_CLASS(+)"
    + "   AND A.NEED_DATE BETWEEN D1.SDATE(+) AND D1.EDATE(+)"
    + "   AND D1.USE_YN(+) = 'Y'"
    + "   AND A.ITEM_CODE = E.ITEM_CODE"
    + "   AND B.CUSTCD = F.CUSTCD"
    + "   AND A.CENTER_CODE = G.CENTER_CODE(+)"
    + "   AND A.CUSTCD = G.CUSTCD(+)"
    + "   AND A.ITEM_CODE = G.ITEM_CODE(+)"
    + " ORDER BY A.NEED_DATE, A.RC_UPJANG, A.SUBINV_CODE, A.ITEM_CODE, A.TRANS_QTY DESC";
}

