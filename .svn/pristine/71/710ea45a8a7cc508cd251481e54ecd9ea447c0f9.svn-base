    + "     , '" + #SQL_M.REGION_NAME# + "' AS REGION_NAME"
    + "  FROM (SELECT A.NEED_DATE, A.RC_UPJANG, A.SUBINV_CODE, A.CUSTCD, A.CENTER_CODE"
    + "             , A.ITEM_CODE, A.ITEM_NAME, A.ITEM_SIZE, A.PO_UOM, A.TAX_CODE"
    + "             /*발주/배송중인 경우 배송량, 검수이상인 경우 최종 실입고량*/"
    + "             , DECODE(SIGN(TO_NUMBER(A.LINE_STATUS) - 9), 1, (A.DELIVERED_QTY - A.RETURNED_QTY), (A.CENTER_DELI_QTY - A.DIRECT_DELI_QTY)) AS TRANS_QTY"
    + "             , A.SALE_PRICE, A.PO_ID"
    + "          FROM PO_PO A"
    + "         WHERE A.NEED_DATE = '" + #SQL_M.NEED_DATE# + "'"
    + "           AND A.SUBINV_CODE = '" + #SQL_M.SUBINV_CODE# + "'"
    + "           AND A.RC_UPJANG = '" + #SQL_M.UPJANG# + "'"
    + "           AND ((A.LINE_STATUS BETWEEN '008' AND '009') OR (A.LINE_STATUS &gt; '009' AND (A.DELIVERED_QTY - A.RETURNED_QTY) &gt; 0))"
    + "         UNION ALL /*이하 보정자재*/"
    + "        SELECT A.TRANS_DATE AS NEED_DATE, A.TRANS_UPJANG AS RC_UPJANG, A.SUBINV_CODE, A.CUSTCD, A.CENTER_CODE"
    + "             , A.ITEM_CODE, A.ITEM_NAME, A.ITEM_SIZE, A.TRANS_UOM AS PO_UOM, A.TAX_CODE"
    + "             , A.TRANS_QTY"
    + "             , A.SALE_PRICE, A.SOURCE_ID AS PO_ID"
    + "          FROM PO_TRANSACTION A"  --SOURCE_ID &gt;&gt; PO_ID
    + "         WHERE A.TRANS_DATE = '" + #SQL_M.NEED_DATE# + "'"
    + "           AND A.SUBINV_CODE = '" + #SQL_M.SUBINV_CODE# + "'"
    + "           AND A.TRANS_UPJANG = '" + #SQL_M.UPJANG# + "'"
    + "           AND A.TRANS_TYPE = 'I001'"
    + "           AND A.SOURCE_TYPE = 'PO'"
    + "           AND A.ITEM_CODE IN ('709999999996','709999999997')) A"
    + "     , ST_UPJANG B, ST_UPJANG B0, HWFS_FMS_UPJANG B1, HWFS_FMS_UPJANG B2"
    + "     , PO_SUBINVENTORY D, HWFS_FMS_SUBINVENTORY D2"
    + "     , PO_ITEM_MST E"
    + "     , ST_CUST F"
    + "     , PO_ITEM_HIST G"
    + " WHERE A.RC_UPJANG = B.UPJANG AND B.VAT_UPJANG = B0.UPJANG AND B.UPJANG = B1.UPJANG AND B1.MAIN_UPJANG = B2.UPJANG"
    + "   AND A.SUBINV_CODE = D.SUBINV_CODE AND A.SUBINV_CODE = D2.SUBINV_CODE"
    + "   AND A.ITEM_CODE = E.ITEM_CODE"
    + "   AND B.CUSTCD = F.CUSTCD"
    + "   AND A.CENTER_CODE = G.CENTER_CODE(+)"
    + "   AND A.CUSTCD = G.CUSTCD(+)"
    + "   AND A.ITEM_CODE = G.ITEM_CODE(+)"
    + " ORDER BY A.NEED_DATE, A.RC_UPJANG, A.SUBINV_CODE, A.ITEM_CODE, A.TRANS_QTY DESC";
}
