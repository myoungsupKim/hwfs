    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', B1.UPJANGNM_DISP2||'/'||D2.SUBINV_NAME_DISP2, D.SUBINV_NAME) AS UPJANGNM_R"
    + "     , DECODE('" + #SQL_M.CTPRT_TYPE# + "', '1', SUBSTR(B1.SAUPNO,1,3) || '-' || SUBSTR(B1.SAUPNO,4,2) || '-' || SUBSTR(B1.SAUPNO,6,5), SUBSTR(F.SAUPNO,1,3) || '-' || SUBSTR(F.SAUPNO,4,2) || '-' || SUBSTR(F.SAUPNO,6,5)) AS SAUPNO_R"
