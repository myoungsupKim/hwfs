if (#SQL_M.CTPRT_GB# == "수량P")
{
sql = "SELECT SUBSTR(A.NEED_DATE,1,4)||'년'||SUBSTR(A.NEED_DATE,5,2)||'월'||SUBSTR(A.NEED_DATE,7,2)||'일' AS TRANS_DATE"
