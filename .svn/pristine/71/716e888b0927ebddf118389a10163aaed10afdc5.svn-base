}
Button.btn_WFSA_BgOpen1:disabled
{
	background			: URL('theme://images/new/btn_WFSA_BgOpen1D.png') right bottom;
	border				: 0 none;
}
Button.btn_WFSA_BgClose1, Button.btn_WFSA_BgClose1:selected
{
	background			: URL('theme://images/new/btn_WFSA_BgClose1N.png') right bottom;
	border				: 0 none;
