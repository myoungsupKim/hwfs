Button.btn_WF_searchCm, Button.btn_WF_searchCm:focused
{
	align				: left middle;
	background			: #ea5424 URL('theme://images/new/btn_WF_searchCm.png') left middle;
	border				: 0 none;
	bordertype			: round 3 3;
	color				: #ffffff;
	font				: Gulim,9,bold;
	padding				: 1 0 0 26;
	shadow				: 0 0;
}
Button.btn_WF_searchCm:mouseover, Button.btn_WF_searchCm:selected
{
	background			: #ff8963 URL('theme://images/new/btn_WF_searchCm.png') left middle;
	border				: 0 none;
	color				: #ffffff;
}
Button.btn_WF_searchCm:pushed
{
	background			: #e27958 URL('theme://images/new/btn_WF_searchCm.png') left middle;
	border				: 1 solid #b56146, 0 none, 0 none, 0 none;
	color				: #ffffff;
	padding				: 0 0 0 26;
}
Button.btn_WF_searchCm:disabled
{
	background			: #e2c7b7 URL('theme://images/new/btn_WF_searchCmD.png') left middle;
	border				: 0 none;
	color				: #f3e9e2;
}
