/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            if (!gfn_isNull(pCouponProcNum)){
                ds_search.setColumn(0,"muCd", ds_mstData.getColumn(0,"muCd"));
                //쿠폰처리번호 존재시 쿠폰소멸내역 조회
                fn_termination_search();
            }
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			close();
            break;
        // 취소
        case "cancle" :
			gfn_alert("msg.action.success");	// 취소 처리 되었습니다.
			close();
            break;
            
        case "acctType" :
			ds_acctType.filter("typeCd == 'G01' || typeCd == 'G03'");
			break;
    }
}


