/*----------------------------------------------------------------------------------------------
 * 설명      : 소멸대상추가 버튼 클릭시 체크콤보인 데이타들 소멸쿠폰영역에 추가
 *---------------------------------------------------------------------------------------------*/
function btn_add_onclick(obj:Button,  e:ClickEventInfo)
{
	if (ds_list.rowcount == 0){
	    alert("소멸대상쿠폰 내역이 없습니다");
	    return;
	}
	
	//소멸대상이 있으면 지우고 시작하자(쿠폰상태로 판매인지,소멸인지 구분하여 서버에서 처리할것임)
	if (ds_termination_list.rowcount > 0){
		ds_termination_list.clearData();
	}
	
	var cnt=0;
	var sumCouponAmt=0;
	var sumSupplyAmt=0;
	var sumVatAmt=0;
	
	for (var i=0;i < ds_list.rowcount;i++){
	    if (ds_list.getColumn(i, "chk") != "0"){
	        var nRow = ds_termination_list.addRow();
	        ds_termination_list.setColumn(nRow, "saleDate", ds_list.getColumn(i, "saleDate"));
	        ds_termination_list.setColumn(nRow, "couponNum", ds_list.getColumn(i, "couponNum"));
	        ds_termination_list.setColumn(nRow, "couponProcNum", ds_list.getColumn(i, "couponProcNum"));
	        ds_termination_list.setColumn(nRow, "couponKindCd", ds_list.getColumn(i, "couponKindCd"));
	        ds_termination_list.setColumn(nRow, "couponKindNm", ds_list.getColumn(i, "couponKindNm"));
	        ds_termination_list.setColumn(nRow, "muCd", ds_list.getColumn(i, "muCd"));
	        ds_termination_list.setColumn(nRow, "ccCd", ds_list.getColumn(i, "ccCd"));
	        ds_termination_list.setColumn(nRow, "acctCd", ds_list.getColumn(i, "acctCd"));
	        ds_termination_list.setColumn(nRow, "couponAmt", ds_list.getColumn(i, "couponAmt"));
	        ds_termination_list.setColumn(nRow, "supplyAmt", ds_list.getColumn(i, "supplyAmt"));
	        ds_termination_list.setColumn(nRow, "vatAmt", ds_list.getColumn(i, "vatAmt"));
	        ds_termination_list.setColumn(nRow, "couponStatus", ds_list.getColumn(i, "couponStatus"));
	        ds_termination_list.setColumn(nRow, "advanceNum", ds_list.getColumn(i, "advanceNum"));
	        ds_termination_list.setColumn(nRow, "vatCd", ds_list.getColumn(i, "vatCd"));
	        ds_termination_list.setColumn(nRow, "custCd", ds_list.getColumn(i, "custCd"));
	        ds_termination_list.setColumn(nRow, "custNm", ds_list.getColumn(i, "custNm"));
	        
	        cnt += 1;
	        sumCouponAmt += ds_list.getColumn(i, "couponAmt");
	        sumSupplyAmt += ds_list.getColumn(i, "supplyAmt");
	        sumVatAmt    += ds_list.getColumn(i, "vatAmt");
	    }
	}
	
	//건수,쿠폰합계,공급가액합계,부가세합계 셋팅
	msk_cnt.value       = cnt;
	msk_couponAmt.value = sumCouponAmt;
	msk_supplyAmt.value = sumSupplyAmt;
	msk_vatAmt.value    = sumVatAmt;
	
	if (ds_mstData.rowcount == 0){
	    ds_mstData.clearData();
	    ds_mstData.addRow();
	}
	ds_mstData.setColumn(0,"couponAmt", sumCouponAmt);
	ds_mstData.setColumn(0,"supplyAmt", sumSupplyAmt);
	ds_mstData.setColumn(0,"vatAmt"   , sumVatAmt);
}

