function fn_upjangChange(obj, data)
{
	if(gfn_isNull(edt_upjangNm.getUpjangCode() )){
		return;
	}
	
	ds_cond.clear();
	
	ds_cond.addColumn("UPJANG","STRING",256);	
				
	ds_cond.addRow();
	
	ds_cond.setColumn(0, "UPJANG", edt_upjangNm.getUpjangCode() );
	fsp_clear(this);
	fsp_addSearch(this, "ma/mag:MAG10053S_S001");
	
    http.Sync = true;
	fsp_callService(this, "", "", "ds_cond=ds_cond","ds_cbo_Loc=ds_list","","fn_searchSubJob()");
	http.Sync = false;	
}

function fn_tmChange(obj, data){

	edt_upjangNm.setUpjangCode("");
	
	if(gfn_isNull(edt_tmNm.getTmCode() )){
		return;
	}
	
	ds_cond.clear();
	
	ds_cond.addColumn("TM_CD","STRING",256);	
				
	ds_cond.addRow();
	
	ds_cond.setColumn(0, "TM_CD", edt_tmNm.getTmCode() );
	fsp_clear(this);
	fsp_addSearch(this, "ma/mag:MAG10053S_S001");
	
    http.Sync = true;
	fsp_callService(this, "", "", "ds_cond=ds_cond","ds_cbo_Loc=ds_list","","fn_searchSubJob()");
	http.Sync = false;
	
}

function fn_searchSubJob(){
	
	fsp_clear(this);
	fsp_addSearch(this, "ma/mag:MAG10053S_S002");
	http.Sync = true;
	fsp_callService(this, "", "", "ds_cond=ds_cond","ds_cbo_SubJob=ds_list","","fn_callBackSubJob()");
	http.Sync = false;
}

