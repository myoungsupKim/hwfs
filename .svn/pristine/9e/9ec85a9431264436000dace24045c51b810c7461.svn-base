<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[표준식단코드 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT     UPJANG,
           MENU_CD,
           MENU_NAME,
           MENU_CLASS
FROM       FMM_STD_MENU
WHERE      MENU_CD=#MENU_CD#
AND        UPJANG=#g_Upjang#
	]]></statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_Menu">
	</output>
</query>
