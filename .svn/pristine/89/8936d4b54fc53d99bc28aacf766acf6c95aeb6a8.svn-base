	<statement name="fsaleInsert"><![CDATA[INSERT INTO APP_PO_CONTRACT_FSALE
	(
		            CENTER_CODE	         ,
		            UPJANG	             ,
		            ITEM_CODE	         ,
		            CONTRACT_START	     ,
		            CONTRACT_END	     ,
		            CUSTCD	             ,
		            AP_UNITPRICE_UPJANG	 ,
		            CENTER_FLAG	         ,
		            UNIT_PRICE           ,
		            MARGIN_PRICE         ,
		            SALE_PRICE	         ,
		            OP_RATE              ,
		            OP_PRICE             ,
		            OP_EDATE             ,
		            OP_RATE2             ,
		            OP_PRICE2            ,
		            OP_EDATE2            ,
		            USE_YN	             ,
		            CREATE_DATE	         ,
		            CREATE_BY	         ,
		            UPDATE_DATE	         ,
		            UPDATE_BY	         ,
		            VD_SN	             ,
		            APPLY_SD	         ,
		            UPJANG_GRP	         ,
		            DUTY_YN	             ,
		            PB_YN	             ,
		            MANUF_YN	         ,
		            ECO_ITEM_YN	         ,
		            NPB_YN		         ,
		            TOT_AMT 	         ,
		            BOX_QTY 	         ,
		            RTN_YN 	         	 ,
		            MD_NAME 	         ,
		            D_DAYS 	             ,
		            D_TIMES        	     ,
		            PROC_GB	             ,
		            PROC_YN	
	)
	VALUES (
			        :centerCode,
			        :upjang,
			        :itemCode,
			        :contractStart,
			        :contractEnd,
			        :custcd,
			        :apUnitpriceUpjang,
			        :centerFlag,
			        :unitPrice,
			        :marginPrice,
			        :salePrice,
			        :opRate,
			        :opPrice,
			        :opEdate,
			        :opRate2,
			        :opPrice2,
			        :opEdate2,
			        :useYn,
			        :createDate,
			        :createBy,
			        :updateDate,
			        :updateBy,
			        :vdSn,
			        :applySd,
			        :upjangGrp,
			        :dutyYn,
			        :pbYn,
		            :manufYn,
		            :ecoItemYn,
		            :npbYn,
		            :totAmt,
		            :boxQty,
		            :rtnYn,
		            :mdName,
		            :dDays,
		            :dTimes,
			        :procGb,
			        :procYn
	)
	ON
		DUPLICATE KEY
	UPDATE
		            CENTER_CODE	         = :centerCode,       
		            UPJANG	             = :upjang,           
		            ITEM_CODE	         = :itemCode,         
		            CONTRACT_START	     = :contractStart,    
		            CONTRACT_END	     = :contractEnd,      
		            CUSTCD	             = :custcd,           
		            AP_UNITPRICE_UPJANG	 = :apUnitpriceUpjang,
		            CENTER_FLAG	         = :centerFlag,       
		            UNIT_PRICE	   	     = :unitPrice,       
		            MARGIN_PRICE	   	 = :marginPrice,       
		            SALE_PRICE	         = :salePrice,    
		            OP_RATE              = :opRate,
                    OP_PRICE             = :opPrice,
                    OP_EDATE             = :opEdate,
		            OP_RATE2             = :opRate2,
                    OP_PRICE2            = :opPrice2,
                    OP_EDATE2            = :opEdate2,
		            USE_YN	             = :useYn,            
		            CREATE_DATE	         = :createDate,       
		            CREATE_BY	         = :createBy,         
		            UPDATE_DATE	         = :updateDate,       
		            UPDATE_BY	         = :updateBy,         
		            VD_SN	             = :vdSn,             
		            APPLY_SD	         = :applySd,             
		            UPJANG_GRP	         = :upjangGrp,             
		            DUTY_YN	             = :dutyYn,           
		            PB_YN	             = :pbYn,             
		            MANUF_YN	         = :manufYn,
		            ECO_ITEM_YN	         = :ecoItemYn,
		            NPB_YN	             = :npbYn,
		            TOT_AMT 	         = :totAmt,
		            BOX_QTY 	         = :boxQty,
		            RTN_YN 	         	 = :rtnYn,
		            MD_NAME 	         = :mdName,
		            D_DAYS 	             = :dDays,
		            D_TIMES        	     = :dTimes,
		            PROC_GB	             = :procGb,           
		            PROC_YN	             = :procYn]]></statement>
