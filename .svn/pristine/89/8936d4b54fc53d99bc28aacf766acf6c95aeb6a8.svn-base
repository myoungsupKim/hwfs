					ITEM_CLASS4   	 =  :itemClass4        ,
					ITEM_NAME     	 =  :itemName          ,
					ITEM_SIZE     	 =  :itemSize          ,
					PO_UOM        	 =  :poUom             ,
					KG_CONVERT_RATE	 =  :kgConvertRate     ,
					MIN_ORD_QTY   	 =  :minOrdQty         ,
					MAX_ORD_QTY   	 =  :maxOrdQty         ,
					MULTIPLIER_QTY	 =  :multiplierQty     ,
					D_DAYS        	 =  :dDays             ,
					D_TIMES       	 =  :dTimes            ,
					TAX_CODE      	 =  :taxCode           ,
					ORIGIN_TYPE   	 =  :originType        ,
					ORIGIN_NAT    	 =  :originNat         ,
					ORIGIN_DTL    	 =  :originDtl         ,
					POINT_FLAG    	 =  :pointFlag         ,
					KEEPING_TYPE  	 =  :keepingType       ,
					CONSUM_YN     	 =  :consumYn          ,
					SPICE_YN	     =  :spiceYn           ,
					CORN_YN	         =  :cornYn            ,
					UNUSE_YN      	 =  :unuseYn           ,
					STP_YN	         =  :stpYn             ,
					USE_YN        	 =  :useYn             ,
					CREATE_DATE   	 =  :createDate        ,
					CREATE_BY     	 =  :createBy          ,
					UPDATE_DATE   	 =  :updateDate        ,
					UPDATE_BY     	 =  :updateBy          ,
					ITEM_TYPE     	 =  :itemType          ,
					IMG_YN        	 =  :imgYn             ,
					IMG_PATH      	 =  :imgPath           ,
					KEEP_TYPE     	 =  :keepType          ,
					WIN_ITEM_CD   	 =  :winItemCd         ,
					EXPR_DATE   	 =  :exprDate          ,
					PROC_GB  	     =  :procGb            ,
					PROC_YN	         =  :procYn             
]]></statement>
