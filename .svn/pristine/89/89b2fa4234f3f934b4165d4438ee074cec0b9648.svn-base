/*----------------------------------------------------------------------------------------------
 * 설명      : 메뉴 리스트의 데이터를 출력 리스트에 추가
 *---------------------------------------------------------------------------------------------*/
function grd_list_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	var nRow = ds_printList.addRow();
	ds_printList.setColumn(nRow, "menuCd", ds_list.getColumn(ds_list.rowposition,"menuCd"));
	ds_printList.setColumn(nRow, "menuNm", ds_list.getColumn(ds_list.rowposition,"menuNm"));
	ds_printList.setColumn(nRow, "applyPrice", ds_list.getColumn(ds_list.rowposition,"applyPrice"));
	ds_printList.setColumn(nRow, "barCd", ds_list.getColumn(ds_list.rowposition,"barCd"));
	ds_printList.setColumn(nRow, "cuser", g_Name);
	ds_printList.setColumn(nRow, "outletCd", gfn_nvl(edt_upjang.getUpjangCode(),""));
	
	fn_getTotList();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 출력 리스트의 데이터를 삭제
 *---------------------------------------------------------------------------------------------*/
function grd_printList_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	ds_printList.deleteRow(ds_printList.rowposition);
	
	fn_getTotList();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 출력 리스트의 총 데이터 수 출력
 *---------------------------------------------------------------------------------------------*/
function fn_getTotList()
{
	st_tot.text = ds_printList.rowcount;
}


