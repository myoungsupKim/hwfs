/*----------------------------------------------------------------------------------------------
 * 설명      : 오즈레포트 실행
 *---------------------------------------------------------------------------------------------*/
function fn_runOZ()
{
	//레포트에 전달할 파라메터 생성
	var arrParams	= SCReport.newParams ();
	SCReport.setParams (arrParams, "cuser", g_Name);
	
	//방법1)서버에서 Query에 의해 처리하는 경우 예
	//(필수) 레포트명
	SCReport.setParams (arrParams, SCReport.RPT_NAME, "/hwfs/rc/sle/SaleBarList.ozr");

	//출력 옵션 설정
	//-창 타이틀 (디폴트는 "ReportViewer")
	SCReport.setParams (arrParams, SCReport.RPT_TITLE, "바코드 라벨 출력");

	//Report 호출
	SCReport.viewReport(this, arrParams, false);
}

