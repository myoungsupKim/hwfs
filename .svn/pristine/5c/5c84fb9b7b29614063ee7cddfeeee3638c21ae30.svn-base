			f_Saved_ID  = 0;
			f_NewDataCheck = true;
		}
	
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 식재영업 일때 처리
	}else{
	
		if (gfn_length(ds_listInput2.getColumn(ds_listInput2.rowposition, "MGMT_SABUN"))== 0) {
			g_Message("EE", "담당마케터를 선택하여 주십시요.\n" + "확인하여 주십시요.");   
			div_mak10021e.edt_MGMT_SABUNQuery.setFocus();
			return false;
		}		
		
		if (numFormat(ds_listInput2.getColumn(ds_listInput2.rowposition, "CUST_ID"))== "0") { 
			g_Message("EE", "대상고객을 선택하여 주십시요.\n" + "확인하여 주십시요.");      
			div_mak10021e.edt_CUST_IDQuery.setFocus();
			return false;
		}
		
		if (gfn_length(ds_listInput2.getColumn(ds_listInput2.rowposition, "VISIT_DATE"))== 0) { 
			g_Message("EE", "방문일자가 선택되어 있지 않습니다.\n" + "확인하여 주십시요.");						       
			div_mak10021e.me_VISIT_DATE.setFocus();
			return false;
		}    

		if (gfn_length(ds_listInput2.getColumn(ds_listInput2.rowposition, "VISIT_GUBUN"))== 0) { 
			g_Message("EE", "방문구분이 선택되어 있지 않습니다.\n" + "확인하여 주십시요.");						       
			div_mak10021e.edt_VISIT_GUBUN.setFocus();
			return false;
		}    

		f_Saved_ID = ds_listInput2.getColumn(ds_listInput2.rowposition, "VISIT_ID");
		
		if(numFormat(real_VISIT_ID)== "0") {
		
			ds_cond.clear();
			ds_cond.addColumn("MAK_VISIT_MGMT_B_S","INT",256);
						
			// action 정보 초기화
			fsp_clear(this);
