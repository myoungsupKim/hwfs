function fn_AfterSaveSearch(errCode, errMsg)
{
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 단체급식 일때 처리
	if(div_mak10020e.visible) {
		
		//바이너리 통신 설정
		//http.Compress = false;
		//http.CompressMethod = "";
		//바이너리 통신 설정
		if(errCode != 0) {		
			g_Message("EE");		
			return;
		}
		
		fn_search();
		
		http.Sync = true;
		ds_list.rowposition = ds_list.findRowExpr("VISIT_ID==" + f_Saved_ID);
		http.Sync = false;
	
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 식재영업 일때 처리
	}else{
	
		//바이너리 통신 설정
		//http.Compress = false;
		//http.CompressMethod = "";
		//바이너리 통신 설정
		if (errCode != 0) {			
			g_Message("EE");		
			return;
		}			

		fn_search();	
		
		http.Sync = true;
		ds_list.rowposition = ds_list.findRowExpr("VISIT_ID==" + f_Saved_ID);
		http.Sync = false;
	}
}

