//삭제처리###########################################################################
function before_DeleteCheck(str_GUBUN)
{
	// 해당 마케터가 아닌 사람은 삭제를 못하게 처리 : 2015.11.02 추가
	if(edt_MGMT_SABUNQuery.userdata != g_EmpNo){
		gfn_alert("담당 마케터가 아니므로 삭제를 하실 수 없습니다.");
		return false;
	}

	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 단체급식 일때 처리
	if(div_mak10020e.visible) {
	
		if((ds_listInput.rowcount == 0) || (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))== "0") || (!g_Confirm("01"))) {
			return false;
		}
		
		//각 구분별 대체 마감체크##############################
		if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))) return false;	
		//각 구분별 대체 마감체크##############################	        
		
		//차량운영일지체크#####################################################################
		if (!fn_GetALT_DRIVING_MGMT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "VISIT_ID"))) return false;	
		//차량운영일지체크#####################################################################				       		
		
		// action 정보 초기화
		fsp_clear(this);	
		//기존 라인 및 매출 데이터 삭제.
		fsp_addSingle(this, "ma/mak:MAK10020E_D001");
				   
		http.Sync = true;
		fsp_callService(this, "", "", "ds_cond=ds_listInput:A",  "", "", "fn_AfterDeleteSearch()");
		http.Sync = false;
	
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 식재영업 일때 처리
	}else{
	
		if((ds_listInput2.rowcount == 0) || (numFormat(ds_listInput2.getColumn(ds_listInput2.rowposition, "VISIT_ID"))== "0") || (!g_Confirm("01"))) {
			return false;
		}
		
		//각 구분별 대체 마감체크##############################
		if (!fn_GetALT_CLOSE(ds_cond, ds_listInput2.getColumn(ds_listInput2.rowposition, "VISIT_ID"))) return false;	
		//각 구분별 대체 마감체크##############################	        
						
		//차량운영일지체크#####################################################################
		if (!fn_GetALT_DRIVING_MGMT_CLOSE(ds_cond, ds_listInput2.getColumn(ds_listInput2.rowposition, "VISIT_ID"))) return false;	
		//차량운영일지체크#####################################################################					       		
		
		// action 정보 초기화
		fsp_clear(this);		
		//기존 라인 및 매출 데이터 삭제.
		fsp_addSingle(this, "ma/mak:MAK10021E_D001");
			   
		http.Sync = true;
		fsp_callService(this, "", "", "ds_cond=ds_listInput2:A",  "", "", "fn_AfterDeleteSearch");		
		http.Sync = false;
	}
	
	return true;
}


