// Div 내부일때 처리(edtBtn OnClick 가져오기) ################################################################
function fn_own_edtBtn_OnClick(obj, str_Where)
{
	if(!gfn_isNull(str_Where)) {
		var strType = str_Where.toString().toUpperCase();
		if(strType == "[OBJECT CLICKEVENTINFO]") str_Where = "";
	}else {
		str_Where = "";
	}
	
    //권한에 따른 업장 팝업 내역 설정######################################################
    //fn_sql_common_setup 함수 호출시 첫번째 파라메터인 SQL을 fn_MakeUpjangSQL() 지정한다.
    var strQuery_SqlTitle,
		strQuery_NewSqlData;
    
    //@@@@@@@@@@@@@@@@@@@@@@@@@@ 단체급식 일때 처리
    if(div_mak10020e.visible) {
		strQuery_SqlTitle    =  eval("div_mak10020e.st_" + gfn_subStr(obj.name, 7)).userdata;
		strQuery_NewSqlData  =  eval(eval("div_mak10020e.st_" + gfn_subStr(obj.name, 7)).userdata);
           
		if((strQuery_SqlTitle == "strMAQuery_Upjang") || (strQuery_SqlTitle == "strQueryLocal_Upjang")) {
			strQuery_NewSqlData = fn_MakeUpjangSQL();
		}
		//권한에 따른 업장 팝업 내역 설정######################################################
		//fn_sql_common_setup 함수 호출시 첫번째 파라메터인 SQL을 fn_MakeUpjangSQL() 지정한다.
		fn_sql_common_setup(strQuery_NewSqlData,
							eval(eval("div_mak10020e.st_" + gfn_subStr(obj.name, 7)).userdata + "KeyField"),
							eval(eval("div_mak10020e.st_" + gfn_subStr(obj.name, 7)).userdata + "ValueField"),
							eval(eval("div_mak10020e.st_" + gfn_subStr(obj.name, 7)).userdata + "KeyFieldNM"),
							eval(eval("div_mak10020e.st_" + gfn_subStr(obj.name, 7)).userdata + "ValueFieldNM"),
							"",
							"",
							str_Where,
							eval(eval("div_mak10020e.st_" + gfn_subStr(obj.name, 7)).userdata + "Caption"),
							eval(eval("div_mak10020e.st_" + gfn_subStr(obj.name, 7)).userdata + "AllField") );	
		
		if (!gfn_isNull(gds_sql_common.getColumn(0, "ret_code"))){
		
			eval("div_mak10020e.edt_" + gfn_subStr(obj.name, 7)).userdata = gds_sql_common.getColumn(0, "ret_code");
			eval("div_mak10020e.edt_" + gfn_subStr(obj.name, 7)).value     = gds_sql_common.getColumn(0, "ret_name");	
		}
		
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 식재영업 일때 처리	
	}else {
		strQuery_SqlTitle    =  eval("div_mak10021e.st_" + gfn_subStr(obj.name, 7)).userdata;
		strQuery_NewSqlData  =  eval(eval("div_mak10021e.st_" + gfn_subStr(obj.name, 7)).userdata);
		
		if((strQuery_SqlTitle == "strMAQuery_Upjang") || (strQuery_SqlTitle == "strQueryLocal_Upjang")) {
			strQuery_NewSqlData = fn_MakeUpjangSQL();
		}
		//권한에 따른 업장 팝업 내역 설정######################################################
		//fn_sql_common_setup 함수 호출시 첫번째 파라메터인 SQL을 fn_MakeUpjangSQL() 지정한다.
