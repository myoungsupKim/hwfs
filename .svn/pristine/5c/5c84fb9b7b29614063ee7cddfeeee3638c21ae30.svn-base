	fsp_addSearch(this, "ma/mak:MAK10150P_S002");
	f_AfterCallServerCheck = true;
	
	// 단체급식일때 GUBUN -> A로 파라미터 전송, 식재일때는 B로 파라미터 전송
	if(div_mak10020e.visible){
		// 서버 호출 
		http.Sync = true;
		fsp_callService(this, "", "", "", objOutDs.name + "=ds_list","GUBUN=" + "A" + " VISIT_ID=" + real_MST_ID, "");	
		http.Sync = false;
	}else{
		// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", "", objOutDs.name + "=ds_list","GUBUN=" + "B" + " VISIT_ID=" + real_MST_ID, "");	
	http.Sync = false;
	}
	
