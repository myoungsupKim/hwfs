function edt_CUST_IDQuery_OnKeyDown(obj:Edit, e:KeyEventInfo)
{   
    //@@@@@@@@@@@@@@@@@@@@@@@@@@ 단체급식 일때 처리
	if(div_mak10020e.visible) {
	
		if (e.keycode == 13) {		
			if (gfn_length(gfn_trim(edt_CUST_IDQuery.value))== 0){			
					g_Message("EE", "대상고객을 입력하여 주십시요.");
					edt_CUST_IDQuery.setFocus();
					return false;		
			}
		}
		
		if (e.keycode == 13){			
			edtBtn_CUST_IDQuery_OnClick();
			fn_DataSetAllClear();	
		}else if (e.keycode == 46){		
			obj.userdata = "";
			obj.value     = "";			
			
			fn_DataSetAllClear();	
			//eval(obj.id + "_OnChanged(" + obj.id + ", " + "" + ")");
		}
	
	//@@@@@@@@@@@@@@@@@@@@@@@@@@ 식재영업 일때 처리
	}else{
	
		fn_edt_OnKeyDown(obj,e.keycode);	
		fn_DataSetAllClear();
	}
}

