}

/******************************************************************************************
 * 함ㅡ수ㅡ명 : function btn_addItem_onclick(obj:Button,  e:ClickEventInfo)
 * 기ㅡㅡㅡ능 : 자재추가
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-04-01
 ******************************************************************************************/
function btn_addItem_onclick(obj:Button,  e:ClickEventInfo)
{
	// 자재 선택 체크
	if(ds_itemList.rowposition < 0) 
	{
		g_Message("EE", "추가 하실 자재를 선택하세요.");
		return;
	}
	
	// 자재 선택 체크
	if(gfn_length(gfn_trim(cbo_recipe.value)) == 0 || gfn_trim(cbo_recipe.value) == "0")
	{
		g_Message("EE", "레시피를 선택하세요.");
		cbo_recipe.setFocus();
		return;
	}
	
	// 1인 소요량 입력 체크
	if( gfn_length(gfn_trim(me_Qty.value)) == 0 || gfn_trim(me_Qty.value) == null ) 
	{
		g_Message("EE", "1인 소요량을 입력하세요.");
		me_Qty.setFocus();
		return;
	}
	
	if( toNumber(me_Qty.value) <= 0 ) 
	{
		g_Message("EE", "1인 소요량은 0보다 큰값을 입력하세요.");
		me_Qty.setFocus();
		return;
	}

	if(opener.ds_menuItemList.findRowExpr("RECIPE_CD=='" + gfn_trim(cbo_recipe.value) + "' && ITEM_CODE=='" + gfn_trim(ds_itemList.getColumn(ds_itemList.rowposition, "ITEM_CODE")) + "'") != -1)
	{
		g_Message("EE","선택하신 레시피에 이미 자재가\n등록되어 있습니다.");
		return;
	}

	ds_Data.clearData();
	var nRow = ds_Data.addRow();
	ds_Data.setColumn(nRow, "UPJANG",       UPJANG                                                          ); // 업장코드
	ds_Data.setColumn(nRow, "MENU_CD",      MENU_CD                                                         ); // 식단코드
	ds_Data.setColumn(nRow, "RECIPE_CD",    cbo_recipe.value                                                ); // 레시피코드
	ds_Data.setColumn(nRow, "ITEM_CODE",    ds_itemList.getColumn(ds_itemList.rowposition, "ITEM_CODE")     ); // 자재코드
	ds_Data.setColumn(nRow, "ITEM_SEQ",     0                                                               ); // 자재순번
	ds_Data.setColumn(nRow, "NEED_QTY",     me_Qty.value                                                    ); // 1인소요량
	ds_Data.setColumn(nRow, "EDIBLE_QTY",   0                                                               ); // 가식부량 : SQL내 계산 => (1인 소요량 * (100 - 폐기율))
	ds_Data.setColumn(nRow, "ORDER_YN",     "N"                                                             ); // 발주여부
	ds_Data.setColumn(nRow, "PR_ID",        -1                                                              ); // PR_ID
	ds_Data.setColumn(nRow, "PR_QTY",       0                                                               ); // 구매신청수량
	ds_Data.setColumn(nRow, "NEED_DATE",    null                                                            ); // 입고 희망일자 : 제공일(MENU_DATE) - FSI_ITEM_NUT.D_DAY
	ds_Data.setColumn(nRow, "UNIT_PRICE",   ds_itemList.getColumn(ds_itemList.rowposition, "CONTRACT_PRICE")); // 매입단가
	ds_Data.setColumn(nRow, "MARGIN_RATE",  ds_itemList.getColumn(ds_itemList.rowposition, "MARGIN_RATE")   ); // 출하율
	ds_Data.setColumn(nRow, "MARGIN_PRICE", ds_itemList.getColumn(ds_itemList.rowposition, "ITEM_PRICE")    ); // 출하율 적용단가
	ds_Data.setColumn(nRow, "MENU_DATE",    MENU_DATE                                                       ); // 제공일
	ds_Data.setColumn(nRow, "HALL_CD",      HALL_CD                                                         ); // 식당코드
	ds_Data.setColumn(nRow, "MEAL",         MEAL                                                            ); // 끼니코드
	ds_Data.setColumn(nRow, "CORNER",       CORNER                                                          ); // 코너코드
		
	fsp_clear(this);
	fsp_addSave(this, "fs/fsm:FSM00060P_I001", "", "", "", "", -1, "");
	fsp_callService(this, "", "", "ds_Data=ds_Data:A", "ds_Data=ds_Data", "", "fn_callbackSave");		// 서비스 호출
}

function fn_callbackSave(errCode, errMsg)
{	
	if( errCode != 0 )
	{
		g_Message("EE","자재대체 중 오류가 발생했습니다.\n에러메세지:" + errMsg);
		return;
	}
	else
	{
		g_Message("02", "자재가 정상적으로 추가 되었습니다.");
		opener.fn_search();
	}
}

/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_color(nRow)
 * 기ㅡㅡㅡ능 : 자재명에 칼라 적용 (구매신청완료된 자재는 순번필드에 표현함)
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-04-29
 ******************************************************************************************/
function fn_color(nRow)
{
	// 금지 자재
	var sMenuDate = gfn_trim(ds_cond.getColumn(0, "MENU_DATE"));
	
	if( sMenuDate >= ds_itemList.getColumn(nRow,"RESTRICT_START") &&
		sMenuDate <= ds_itemList.getColumn(nRow,"RESTRICT_END") )
	{
		if(sMenuDate >= ds_itemList.getColumn(nRow,"CNL_START") &&
		   sMenuDate <= ds_itemList.getColumn(nRow,"CNL_END") )
		{
			return "default";
		}
		else
		{
			return "#FFE49C";
		}
	}
	return "default";
}

/******************************************************************************************
 * 함ㅡ수ㅡ명 : cbo_feature_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
 * 기ㅡㅡㅡ능 : 자재특성 필터기능
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-04-29
 ******************************************************************************************/
function cbo_feature_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	switch(e.postindex)
	{
		case 0 :
			ds_itemList.filter("");
			break;
		default :
			ds_itemList.filter("FEATURE_ICON == '" + e.postvalue + "'");
			break;	
	}
}



