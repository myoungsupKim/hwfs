/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_search()
 * 기ㅡㅡㅡ능 : 자재조회
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-04-01
 ******************************************************************************************/
var fa_JDATE;
function fn_search()
{
	//자재명, 분류중에서 반드시 선택해야함 전체검색 방지 20180518 김호석
	if (gfn_isNull(ds_cond.getColumn(0,"ITEM_NAME")) && gfn_isNull(ds_cond.getColumn(0,"BIG_CODE")))  {
		g_Message("EE", "자재명 또는 분류는 반드시 선택해야 합니다.");
		return false;	
	}
	
	if(!gfn_isNull(MEAL) && !gfn_isNull(CORNER))
	{
		var arg	 = "sMENU_DATE="+ wrapQuote(MENU_DATE)
				+= " sUPJANG="  + wrapQuote(UPJANG)
				+= " sHALL_CD=" + wrapQuote(HALL_CD)
				+= " sMEAL="    + wrapQuote(MEAL)
				+= " sCORNER="  + wrapQuote(CORNER);
		jsp_transaction(this, "FSM00060P_T001", "U_svc::" + "fs/fsm/FSM00060P_T001.jsp", "", "", arg, "", false, false, true);
		if (gfn_length(fa_JDATE)<= 0)
		{
			g_Message("EE", "입고예정일 정보를 확인할 수 없습니다!");
			exit;
		}
	}
	
	ds_itemList.clearData();
	fsp_clear(this);
	fsp_addSearch(this,"fs/fsm:FSM00060P_S002");
	fsp_callService(this,"","","ds_Input=ds_cond","ds_itemList=ds_ListItem","","fn_callbackSearch","",false);
}

function fn_callbackSearch(errCode, errMsg)
{
	if( errCode != 0 )
	{
		g_Message("EE", errMsg);
		return;
	} 
	else 
	{
		edt_itemName.setFocus();
	}
}

