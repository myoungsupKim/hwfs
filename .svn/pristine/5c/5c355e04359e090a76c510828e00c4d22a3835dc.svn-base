function fn_init()
{
	gfn_clearSortMark(grd_cal);
	ds_cal.clearData();
	 
	//날자  
 	edt_monthly.mae_cal.value = gfn_subStr(fv_sToday,0,6);
 	
}
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	
	ds_paramInfo.clearData();
	var nIdx = ds_paramInfo.addRow();
	ds_paramInfo.setColumn(nIdx, "monthly", gfn_nvl(edt_monthly.mae_cal.value+'01',""));
	ds_paramInfo.setColumn(nIdx, "amCd",  gfn_nvl(edt_serAm.uCode ,""));
	ds_paramInfo.setColumn(nIdx, "maskAuth", SCAuth.getMaskAuth(this));
	
	if (gfn_validCheck(ds_paramInfo, ds_paramInfoValid) == false)	return;	//기본자료는 꼭 입력되어야 함

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_cal);
	ds_cal.clearData();
	
    var strSvcID    = "search";
	var strURL      = "rc/bms/proMonthlyList/selectList.xdo";
	var strInDs     = "ds_paramInfo=ds_paramInfo:A";
	var strOutDs    = "ds_startWeekEndDay=ds_startWeekEndDay ds_dayData=ds_dayData";
	var strArg      = "";
	var strCallback = "fn_callBack";
	var bAsync      = true;
	 
	gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

