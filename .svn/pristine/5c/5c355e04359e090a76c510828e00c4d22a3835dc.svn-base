/*----------------------------------------------------------------------------------------------
 * 설명      : AM검색후 삭제 하는 경우
 *---------------------------------------------------------------------------------------------*/ 
function edt_serAm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) btn_teamCd_onclick();
	if ((e.keycode == 46) || (e.keycode == 8)) 
	{
		edt_serAm.uCode = "";   //코드
		edt_serAm.value = "";    //명	
		ds_cal.clearData();
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명      : AM 선택 버튼을 누른 경우
 *---------------------------------------------------------------------------------------------*/
function btn_searchAm_onclick(obj:Button,  e:ClickEventInfo)
{
	var rtnVal = gfn_dialog("CodePop"				// dialogId
					   , "X_BMS::AmPop.xfdl"			// Url
					   , null			// 배열
					   , null, null, null, null, null, false, true, -1);
 
	if (typeof rtnVal != "undefined" && rtnVal != null) 
	{
		if (rtnVal[0] == "OK")
		{	
			edt_serAm.uCode = rtnVal[1];
			edt_serAm.value = rtnVal[2]; 
			ds_cal.clearData();
		}
	}
}
  


function grd_saleChDallyRprt_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if ( e.cell == 8 ) {
		var temp = grd_cal.getCurFormatString();
		grd_cal.enableredraw = false;
		grd_cal.deleteContentsRow("head", 0);
		gfn_exportExcel(obj);
		grd_cal.formats = "<formats>"+temp+"</formats>";
		grd_cal.enableredraw = true;
	}
}
