<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[사용횟수
:사업장별 제공횟수]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT C.JOB_CD
       ,C.JOB_NAME
       ,C.SUB_JOB_CD
       ,C.SUB_JOB_NAME
       ,A.UPJANG
       ,C.UPJANGNM
       ,B.RECIPE_CD
       ,B.RECIPE_NAME
       ,COUNT(*) AS CNT
<isNotNull col="UPJANG">
       ,FSR_MULTI_RECIPE_NAME_FUN(A.UPJANG,B.RECIPE_CD,#FROM_MENU_DATE#,#TO_MENU_DATE#) AS MULTI_NAME
</isNotNull>
<isNull col="UPJANG">
       ,'' AS MULTI_NAME
</isNull>
  FROM FSM_REAL_MENU_MST A
       ,FSM_REAL_RECIPE_MST B
       ,MAC_UPJANG_V C
<isNotNull col="HALL_CLASS">
       ,FSA_HALL_MST D
</isNotNull>
<isNotNull col="FROM_DT">
       ,FSR_STD_RECIPE_MST F
</isNotNull>
 WHERE A.UPJANG = B.UPJANG
   AND A.MENU_CD = B.MENU_CD
   AND A.UPJANG = C.UPJANG
<isNotNull col="JOB_CD">
   AND C.JOB_CD = #JOB_CD#
</isNotNull>
<isNotNull col="SUB_JOB">
   AND C.SUB_JOB_CD = #SUB_JOB#
</isNotNull>
<isNotNull col="UPJANG">
   AND A.UPJANG = #UPJANG#
</isNotNull>
<isNotNull col="HALL_CLASS">
   AND A.UPJANG = D.UPJANG
   AND A.HALL_CD = D.HALL_CD
   AND D.HALL_CLASS = #HALL_CLASS#
</isNotNull>
<isNotNull col="MEAL">
   AND A.MEAL = #MEAL#
</isNotNull>
<isNotNull col="CORNER">
   AND A.CORNER = #CORNER#
</isNotNull>
<isNotNull col="RECIPE_CLASS">
   AND B.RECIPE_CLASS = #RECIPE_CLASS#
</isNotNull>
<isNotNull col="RECIPE_TYPE">
   AND B.RECIPE_TYPE = #RECIPE_TYPE#
</isNotNull>
<isNotNull col="CUISINE_TYPE">
   AND B.CUISINE_TYPE = #CUISINE_TYPE#
</isNotNull>
<isNotNull col="RECIPE_NAME">
   AND B.RECIPE_NAME LIKE '%' || #RECIPE_NAME# || '%'
</isNotNull>
<isNotNull col="FROM_MENU_DATE">
   AND A.MENU_DATE BETWEEN #FROM_MENU_DATE# AND #TO_MENU_DATE#
</isNotNull>
<isNotNull col="FROM_DT">
   AND B.RECIPE_CD = F.RECIPE_CD
   AND TO_CHAR(F.CDATE,'YYYYMMDD') BETWEEN #FROM_DT# AND #TO_DT#
</isNotNull>
GROUP BY C.JOB_CD,C.JOB_NAME,C.SUB_JOB_CD,C.SUB_JOB_NAME,A.UPJANG,C.UPJANGNM,B.RECIPE_CD,B.RECIPE_NAME	</statement>
	<input default-name="ds_input">
	</input>
	<output default-name="ds_list2">
	</output>
</query>
