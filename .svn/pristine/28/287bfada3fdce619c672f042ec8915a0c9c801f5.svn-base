<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>insert</type>
	<description><![CDATA[담보정보관리(보증보험)-입력]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
INSERT INTO FMS_SECURE_GUARANTEE( 
	 GUARAN_ID
   , UPJANG
   , CUSTNO
   , GUARAN_AMT
   , GUARAN_BEGIN_DATE
   , GUARAN_EXPIRE_DATE
   , GUARAN_CANCEL_YN
   , GUARAN_CANCEL_DATE
   , GUARAN_NAME
   , GUARAN_NOTE
   , CUSER
   , CDATE
   , UUSER
   , UDATE
 ) VALUES ( 
	  FMS_SECURE_GUARANTEE_S.NEXTVAL
    , #UPJANG#
    , #CUSTNO#
    , #GUARAN_AMT#
    , #GUARAN_BEGIN_DATE#
    , #GUARAN_EXPIRE_DATE#
    , DECODE(#GUARAN_CANCEL_YN#,'1','Y','N')
    , #GUARAN_CANCEL_DATE#
    , #GUARAN_NAME#
    , #GUARAN_NOTE#
    , #g_EmpNo#
	, SYSDATE
	, #g_EmpNo#
	, SYSDATE
 )	]]></statement>
	<input default-name="ds_list2">
	</input>
	<output default-name="ds_list">
	</output>
</query>
