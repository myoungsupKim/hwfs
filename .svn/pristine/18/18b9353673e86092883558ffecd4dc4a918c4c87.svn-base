	<statement>
<![CDATA[
DELETE
  FROM FMM_UPJANG_RECIPE_CUISINE
 WHERE UPJANG = #UPJANG#
   AND SUBINV_CLASS = #SUBINV_CLASS#
   AND RECIPE_CD = #RECIPE_CD#
   AND CUISINE_SEQ = #CUISINE_SEQ# 	]]></statement>
	<input default-name="ds_CUISINE_LIST">
	</input>
	<output default-name="ds_CUISINE_LIST">
	</output>
</query>
