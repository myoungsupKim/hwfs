/*----------------------------------------------------------------------------------------------
 * 엔지니어링 계산
 *---------------------------------------------------------------------------------------------*/
function fn_engineering()
{
	if( ds_list != null && ds_list.rowcount > 0 )
	{
		var menuCnt = 0;			// 메뉴수
		var totQty = 0;				// 총판매량
		var menuMix = 0;			// 메뉴믹스
		var margin = 0;				// 품목마진
		var upjangGongMargin = 0;	// 총공헌마진
		var totCstAmt = 0;			// 총원가
		var totSalAmt = 0;			// 매출액
		var upjangSalAmt = 0;		// 총매출액
		var avgGongMargin = 0;		// 평균공헌마진
		var gongMarginStr = "";		// 공헌마진분류1
		var gongMarginInt = 0;		// 공헌마진분류2
		var menuMixStr = "";		// 메뉴믹스분류1
		var menuMixInt = 0;			// 메뉴믹스분류2
		var menuMixRate = 0;		// 메뉴믹스비율
		var result = "";			// 메뉴판정
		var salAmtRate = 0;			// 매출액(%)
		var salHighRank = 0;		// 판매량 상위(%)
		var gongMarginRate = 0;		// 공헌마진(%)
		var menuMixComp = 0;		// 메뉴믹스와비교(%)

		salHighRank = Number(msk_salHighRank.value) / 100;
		//menuCnt = ds_list.rowcount - 1;
		menuCnt = ds_list.rowcount;
		totQty = ds_list.getSum("salQuantity");

		for( var i = 0; i < ds_list.rowcount; i++ )
		{
			var netPrice = ds_list.getColumn(i, "netPrice");
			var cstAmt = ds_list.getColumn(i, "cstAmt");
			var salQuantity = ds_list.getColumn(i, "salQuantity");

			margin				= netPrice - cstAmt; // 품목마진
			totCstAmt			= cstAmt * salQuantity; // 총원가
			//totSalAmt			= netPrice * salQuantity; // 총매출
			totSalAmt			= ds_list.getColumn(i, "netAmt"); // 총매출
			upjangSalAmt		= ds_list.getSum("netPrice * salQuantity");
			gongMargin			= (netPrice - cstAmt) * salQuantity;
			upjangGongMargin	= ds_list.getSum("(netPrice - cstAmt) * salQuantity");
			avgGongMargin		= Math.round(upjangGongMargin / totQty);
			menuMixRate			= Math.round(100 / menuCnt * salHighRank, 2);

			// 품목마진
			ds_list.setColumn(i, "margin", Math.round(margin));

			// 총원가
			ds_list.setColumn(i, "totCstAmt", Math.round(totCstAmt));

			// 원가율
			ds_list.setColumn(i, "totCstRate", Math.round((cstAmt / netPrice) * 100, 1));

			// 총매출
			ds_list.setColumn(i, "totSalAmt", totSalAmt);

			//판매율
			menuMix = Math.round(salQuantity / totQty * 100, 2);
			ds_list.setColumn(i, "menuMix", menuMix);

			// 공헌마진분류
			if( avgGongMargin >= margin )
			{
				gongMarginStr = "저";
				gongMarginInt = 2;
			}
			else
			{
				gongMarginStr = "고";
				gongMarginInt = 4;
			}
			ds_list.setColumn(i, "gongMarginStr", gongMarginStr);
			ds_list.setColumn(i, "gongMarginInt", gongMarginInt);

			//메뉴믹스분류
			if(menuMixRate >= menuMix){
				menuMixStr = "저";
				menuMixInt = 1;
			}else{
				menuMixStr = "고";
				menuMixInt = 5;
			}
			ds_list.setColumn(i, "menuMixStr", menuMixStr);
			ds_list.setColumn(i, "menuMixInt", menuMixInt);

			//메뉴판정
			if( (gongMarginInt+menuMixInt) == 3 ){
				result = "Dog";
			}else if( (gongMarginInt+menuMixInt) == 5 ){
				result = "Puzzle";
			}else if( (gongMarginInt+menuMixInt) == 7 ){
				result = "Horse";
			}else if( (gongMarginInt+menuMixInt) == 9 ){
				result = "Star";
			}
			ds_list.setColumn(i, "result", result);

			//매출액(%)
			salAmtRate = Math.round(totSalAmt / upjangSalAmt * 100, 1);
			ds_list.setColumn(i, "salAmtRate", salAmtRate);

			// 메뉴믹스와비교(%)
			menuMixComp = Math.round((1 / menuCnt) * (salHighRank * 100), 2);
			ds_list.setColumn(i, "menuMixComp", menuMixComp);

			// 공헌마진(%)
			gongMarginRate = Math.round((gongMargin / upjangGongMargin) * 100, 1);
			ds_list.setColumn(i, "gongMarginRate", gongMarginRate);
			ds_list.setColumn(i, "avgGongMargin",  Math.round(upjangGongMargin / totQty));
			ds_list.setColumn(i, "gongMargin", Math.round(gongMargin));
		}
	}

	// 매출액(%) 기준으로 정렬
	ds_list.updateSortGroup("S:-salAmtRate");

	//  매출누계 abc분석 추가

	var salAmtAcc = 0; // 매출누계
	for( var n = 0; n < ds_list_f.rowcount; n++ )
	{
		salAmtAcc += ds_list_f.getColumn(n, "salAmtRate");
		
		ds_list_f.setColumn(n, "salAmtAcc", salAmtAcc);
		
		if( salAmtAcc <= 70 )
			ds_list_f.setColumn(n, "abc", "A");
 		else if( salAmtAcc > 70 && salAmtAcc <= 90 )
 			ds_list_f.setColumn(n, "abc", "B");
 		else
 			ds_list_f.setColumn(n, "abc", "C");
	}

	grd_list.binddataset = "ds_list_f";
}

