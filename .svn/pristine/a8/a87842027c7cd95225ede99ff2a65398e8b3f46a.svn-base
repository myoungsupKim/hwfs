 * 분석표 버튼 클릭 이벤트
 *---------------------------------------------------------------------------------------------*/
function btn_analyze_onclick(obj:Button,  e:ClickEventInfo)
{
	var objRight = btn_analyze.position.right;
	var objBottom = btn_analyze.position.bottom;

	var nWidth = div_analyze.position.width;
	var nHeight = div_analyze.position.height;
	
	var nRight = objRight;
	var nTop = objBottom;
	var nLeft = nRight - nWidth;
	var nBottom = nTop + nHeight
	
	div_analyze.position.left = nLeft;
	div_analyze.position.top = nTop;
	div_analyze.position.right = nRight;
	div_analyze.position.bottom = nBottom;
	
	if(div_analyze.visible){
		div_analyze.visible = false;
	}else{
		div_analyze.visible = true;
	}
}

