function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "";
	
//	ds_list.keystring = "G:upjangCd,S:upjangCd";

	//검색조건을 초기화 한다.
	var nRow = ds_search.addRow();
	//ds_search.setColumn(nRow, "muCd", "2003");
	ds_search.setColumn(nRow, "salHighRank", "70");

	// 업장유형
	SCCombo.bind(this, "ComCode", "cbo_schClassCd", ds_sclassCd, {groupCode:"SCLASS_CD", useYn:"Y", set1:"Y"}, SCCombo.ALL);
	SCDataset.bind(this, "ComCode", "ds_sclassCd_g", {groupCode:"SCLASS_CD", useYn:"Y", set1:"Y"});

	// Calendar(검색조건 바인드)
	cal_occurDateFr.fn_initMonthCal(this, gfn_addMonth(fv_sToday, -1), "", ds_search, "occurDateFr", "Y");
	cal_occurDateTo.fn_initMonthCal(this, fv_sToday, "", ds_search, "occurDateTo", "Y");
	cal_btDate.fn_initMonthCal(this, gfn_today(), "", ds_search, "schBtDate", "Y");
	
//	schMuCd.setMuCode(ds_search.getColumn(0, "muCd"));
//	schMuCd.cbo_mu.readonly = true;

	schTeam.applyAuthLevel(schMuCd);
	schUpjang.applyAuthLevel(schTeam);	
	
	//사용자 버튼 위치 조정
	//role : src(조회권한), ins(입력권한), del(삭제권한), sav(저장권한), prt(출력권한), exl(엑셀권한)
	//ischange : true (공통버튼 무조건 숨김), false (공통버튼 권한에 따라 숨김)
	SCAuth.setUserButton(this, {buttons : [{id:"btn_analyze", role:"src", ischange:false}]});
}

