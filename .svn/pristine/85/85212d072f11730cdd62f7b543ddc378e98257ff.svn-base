/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";
include "lib::LimsUtil.xjs";

/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/

var argArgs;

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
    // Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
    gfn_formOnLoad(this);		// GRID에는 공통 이벤트 제외
 
	edt_detailNm.value="";
	
	argArgs = this.getOwnerFrame().fv_Contents;
	 
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	
	grd_codeList.setFocus(); 
	fn_search();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{

}
/*----------------------------------------------------------------------------------------------
 * 설명      : 코드를 검색 한다.
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	gfn_clearSortMark(grd_codeList);
    ds_codeList.clearData();
	
	var strSvcID    = "search";
    var strURL      = "sc/scc/codeMgmt/selectCodeList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_codeList=ds_codeList";
    var strArg      = "groupCode=" + argArgs[0] 
				    + " codeName=" + edt_detailNm.value 
				    + " maskAuth=" + SCAuth.getMaskAuth(this) 
				    + " useYn=" + "Y";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_codeList.saveXML());
            if (argArgs[1] =="ALL")
            {
				ds_codeList.insertRow(0);
				ds_codeList.setColumn(0,"code","");
				ds_codeList.setColumn(0,"codeName","전체");
            }
            
            //grd_codeList.setCellPos(grd_codeList.getBindCellIndex("Body", "tableCd"));
			//grd_codeList.showEditor(true);
			grd_codeList.setFocus();
            
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;    
    }
}


/***************************************************************************
 * 설명   : LayerPopup에서 받을 공통 함수
 ***************************************************************************/
/*

*/
function btn_confirm_onclick(obj:Button,  e:ClickEventInfo)
{
	var retVal = new Array();
	
	
	if (ds_codeList.getRowCount() >= 1)
    {
		 fn_Ok();
		 return;
    } else
    {
		retVal[0] = "CANCEL"
    }
    
}

function btn_close_OnClick(obj:Button,  e:ClickEventInfo)
{
	var retVal = new Array();
    retVal[0] = "CANCEL";
    close(retVal);
}

function btn_search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_search();
}
 

function edt_detailNm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	 
	if(e.keycode =="13")
	{
		fn_search();
	}
}

function grd_list_OnCellDblClick(obj:Grid, e:GridClickEventInfo)
{
	var retVal = new Array();
	if (ds_codeList.getRowCount() >= 1)  fn_Ok();
     
}

function grd_list_OnKeyDown(obj:Grid, e:KeyEventInfo)
{
	if(e.keycode =="13") fn_Ok();
}

function fn_Ok()
{
	var retVal = new Array();
	if (ds_codeList.getRowCount() >= 1)
	{
		retVal[0] = "OK";
		retVal[1] = ds_codeList.getColumn(ds_codeList.rowposition, "code");
		retVal[2] = ds_codeList.getColumn(ds_codeList.rowposition, "codeName");
		retVal[3] = ds_codeList.getColumn(ds_codeList.rowposition, "set1");
		retVal[4] = ds_codeList.getColumn(ds_codeList.rowposition, "set2");
		retVal[5] = ds_codeList.getColumn(ds_codeList.rowposition, "set3");
		close(retVal);
	} 

}

 ]]></Script>
