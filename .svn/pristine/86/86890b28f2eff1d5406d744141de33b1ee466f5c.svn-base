function fn_cbo_cuisineType_OnChanged(obj, sPostValue)
{
    var cond = "";
    if ( cbo_recipeType.value  > "00" ) {
        cond = " RECIPE_TYPE == '" + cbo_recipeType.value + "'";
    }

    if ( sPostValue > "00" ) {
        var cond2 = "CUISINE_TYPE == '" + sPostValue + "'";
        if ( gfn_length( cond )> 0 ) {
            cond = cond + " && " + cond2;
        } else {
            cond = cond2;
        }
    }

    if ( gfn_length( cond )> 0 ) {
        ds_recipeList.filter(cond);
    } else {
        ds_recipeList.filter("");
    }    
}

