	else {

		ds_itemIn.ClearData();
		for ( i=0; i < ds_recipeList.rowcount; i++) {
			ds_itemIn.AddRow();
			ds_itemIn.SetColumn(i,"UPJANG",ds_recipeList.GetColumn(i,"UPJANG"));
