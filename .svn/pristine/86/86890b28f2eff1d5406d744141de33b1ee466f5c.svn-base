    for( var i = 0; i < ds_FM0007Cd.getRowCount(); i++ ) {
        var nRow = ds_FS0001Cd.findRow("CODE", ds_FM0007Cd.getColumn(i, "SET2"));
        
		var objComp = new CheckBox();
		objComp.init("chk_"+F_CHK_CNT, leftPos, 5, leftPos+width, 26);
		this.addChild(objComp.name, objComp);
		objComp.show();
		objComp.text  		= ds_FS0001Cd.getColumn(nRow, "CODE_NAME");
		objComp.truevalue 	= ds_FS0001Cd.getColumn(nRow, "CODE");
		objComp.enable 		= true;
		
        F_CHK_CNT++;
        leftPos += width;
    }    
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_changeItemClass
 * 입      력   : 
 * 반      환   : 없음
 * 기      능   : 분류 변경 
 * 작  성  자   : 조은영
 * 작성  일자   :
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_changeItemClass( dsObj, classType, strCode ) {
	//trace(  "classType==="+classType+":::strCode==="+strCode);
    ds_itemClassList.filter("");
    var len = gfn_length( strCode );
    for ( var i = 0; i < ds_itemClassList.getRowCount(); i++ ) {
        if ( ds_itemClassList.getColumn(i, "CLASS_TYPE") == classType
            && gfn_subStr(ds_itemClassList.getColumn(i, "CODE"), 0, len)== strCode ) {
            var nRow = dsObj.addRow();
            dsObj.copyRow(nRow, ds_itemClassList, i);
        }
    }
    
    dsObj.insertRow(0);
    dsObj.setColumn(0, "CODE", "");
}
