 * 함  수  명   : fn_afterSave()
 * 기      능   : [저장버튼] 저장 시 처리되는 작업 - 콜백 
 ******************************************************************************************
 */
function fn_afterSearchDup() 
{

	var Inx;
	var ret ="";
	
	for( i=0; i < ds_recipeList.rowcount; i++) {
		if (ds_recipeList.getColumn(i,"CHK") == 1) {
			Inx = ds_itemOut.findRow("RECIPE_CD",ds_recipeList.getColumn(i,"RECIPE_CD"));
			if (Inx >= 0) {
	
				ds_recipeList.setColumn(i,"FLAG","S");
				ret += " [" + ds_recipeList.getColumn(i,"RECIPE_NAME") + "] " ;
			}
		}
	}
	
	if (gfn_length(ret)>0) {
		g_Message("EE",	"선택하신 상품이 기존 " + ret+ "레시피에 이미 존재하여 변경할 수 없습니다.\n그 외 레시피만 일괄변경됩니다.");
	}
}


