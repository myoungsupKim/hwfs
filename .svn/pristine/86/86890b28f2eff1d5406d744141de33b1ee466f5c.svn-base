/*
 ******************************************************************************************
 * 함  수  명   : grd_list_OnCellDblClick
 * 입      력   : 
 * 반      환   : 없음
 * 기      능   : 변경전 / 후 상품 설정 
 * 작  성  자   : 조은영
 * 작성  일자   :
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function grd_list_OnCellDblClick(obj:Grid, e:GridClickEventInfo)
{

	var dsObj = eval(obj.binddataset);
	var flag = obj.userdata + "_";
	
	// 현재 조회한 것과 같을 경우 return 
	// if ( ds_cond.getColumn(0, flag + "ITEM_CODE") ==  dsObj.getColumn(nRow, "ITEM_CODE") ) {
        // return;
    // }
        
	ds_cond.setColumn(0, flag + "ITEM_CODE", dsObj.getColumn(e.row, "ITEM_CODE"));
	ds_cond.setColumn(0, flag + "ITEM_NAME", dsObj.getColumn(e.row, "ITEM_NAME"));
	ds_cond.setColumn(0, flag + "ITEM_SIZE", dsObj.getColumn(e.row, "ITEM_SIZE"));
	ds_cond.setColumn(0, flag + "PO_UOM", dsObj.getColumn(e.row, "PO_UOM"));
	ds_cond.setColumn(0, flag + "KG_CONVERT_RATE", dsObj.getColumn(e.row, "KG_CONVERT_RATE"));
	ds_cond.setColumn(0, flag + "G_ITEM_PRICE", dsObj.getColumn(e.row, "G_UNIT_PRICE"));
	ds_cond.setColumn(0, flag + "ITEM_PRICE", dsObj.getColumn(e.row, "UNIT_PRICE"));

	eval("ed_item" + obj.userdata).value 
        = "[ " + dsObj.getColumn(e.row, "ITEM_CODE") + " ] "
            + dsObj.getColumn(e.row, "ITEM_NAME"); 

    // 레시피 정보 조회 
    if ( obj.userdata == "B") {
        ds_cond.setColumn(0, "ITEM_CODE", dsObj.getColumn(e.row, "ITEM_CODE"));
        grd_list.binddataset = "";
        gfn_clearGrid(grd_list);
        btn_searchHist.enable = true;
        
        // 레시피 정보 조회 
        fsp_clear(this);
        fsp_addSearch(this, "fm/fmm:" + F_SQL_PREX + "_S001");
            
        fsp_callService(this, "", ""
            , "ds_cond=ds_cond ds_recipeClass=ds_recipeClass"
            , "ds_recipeList=ds_recipeList"
            , ""
            , "fn_afterSearch");		// 서비스 호출 
    }
}
