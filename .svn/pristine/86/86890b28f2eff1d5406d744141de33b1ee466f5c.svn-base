 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function ed_needQty_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
    if( nChar != 13 ) {
        return;
    }
    
    var p = toNumber( obj.value) / 100;
	for ( var i = 0; i < ds_recipeList.getRowCount(); i++ ) {
        if ( ds_recipeList.getColumn(i, "CHK") == "0" ) {
            continue;
        }
        
        ds_recipeList.setColumn(i, "NEED_QTY", Math.round( toNumber(ds_recipeList.getColumn(i, "NEED_QTY")) * p, 0) );
	}
}

