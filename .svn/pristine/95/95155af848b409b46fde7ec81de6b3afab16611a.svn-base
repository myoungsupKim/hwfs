/*----------------------------------------------------------------------------------------------
 * 설명      : 계산...............
 *---------------------------------------------------------------------------------------------*/
function fn_calculation()
{
	var num = fn_fromToGap() + 1;
	var sNum;
	if(gfn_length(num) == 1) sNum = '0'+num;
	else 					 sNum = num;
	
	var sumMachu = "";
	for(var i=0; i<num-1; i++)
	{
		if(i == 0)
		{
			if(gfn_length(num) == 1)	sumMachu += "MACHU_AMT_0" + (i+2)
			else						sumMachu += "MACHU_AMT_" + (i+2)
		}
		else
		{
			if(gfn_length(num) == 1)	sumMachu += "+MACHU_AMT_0" + (i+2)
			else						sumMachu += "+MACHU_AMT_" + (i+2)
		}
	}

	var totalDay = 0;
	for(var i=1; i<num; i++)
	{
		totalDay += parseInt(gfn_getLastDate(addMonth(cal_to.fn_getValue()+'01',-i)).substr(6))
	}	
	
	//grid_Slip.setCellProperty('body',15,'text','expr:(('+sumMachu+')/((GICHO_AMT_02+GICHO_AMT_'+sNum+')/2))');  // 매출채권회수일
	grid_Slip.setCellProperty('body',15,'text','expr:'+totalDay+'/(('+sumMachu+')/((GICHO_AMT_02+GICHO_AMT_'+sNum+')/2))'); // 매출채권회수일
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 시작일, 종료일 개월 수
 *---------------------------------------------------------------------------------------------*/
function fn_fromToGap()
{
	var num = 0;
	var toDate = new Array();
	var fromDate = new Array();
	toDate[0] = cal_to.fn_getValue().substr(0 ,4);
	toDate[1] = cal_to.fn_getValue().substr(4 ,2);
	fromDate[0] = cal_from.fn_getValue().substr(0 ,4);
	fromDate[1] = cal_from.fn_getValue().substr(4 ,2);
	
	if(toDate[0] - fromDate[0] != 0) num = (toDate[0] - fromDate[0]) * 12
	
	num = num + parseInt(toDate[1] - fromDate[1]);
	
	return num;
}

