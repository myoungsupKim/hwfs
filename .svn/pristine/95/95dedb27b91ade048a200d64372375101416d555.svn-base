 	MAP_MAKEUP_STD1
SET 
	 STD_NAME = #STD_NAME#
	, STD_DESC = #STD_DESC#
	, INC_DECR_STD = #INC_DECR_STD#
	, STD_RATE = #STD_RATE#
	, ATTR01 = #ATTR01#
	, ATTR02 = #ATTR02#
	, ATTR03 = #ATTR03#
	, ATTR04 = #ATTR04#
	, ATTR05 = #ATTR05#
	, ATTR06 = #ATTR06#
	, ATTR07 = #ATTR07#
	, ATTR08 = #ATTR08#
	, ATTR09 = #ATTR09#
	, ATTR10 = #ATTR10#
	, CUSER = #CUSER#
	, CDATE = #CDATE#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 YY = #YY#
	 AND STD_CD = #STD_CD#	]]></statement>
