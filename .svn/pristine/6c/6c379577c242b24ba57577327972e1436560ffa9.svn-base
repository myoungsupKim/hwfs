/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/ 
/*----------------------------------------------------------------------------------------------
 * 설명      :  화면 로드시 최초 공통 코드를 보여준다.
 *---------------------------------------------------------------------------------------------*/
function fn_searchGrp()
{ 
     
 
    //POS번호 검색
    ds_posNum.clearData();
	
    var strSvcID    = "selectListPosNumAll";
    var strURL      = "rc/sle/posMgmt/selectListPosNumAll.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_posNum=ds_posNum";
    var strArg      = "outletCd= "  
					+ " maskAuth=" + SCAuth.getMaskAuth(this);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
 
}
 

/*----------------------------------------------------------------------------------------------
 * 설명      : 검색 한다.
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{  
	 
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_posFnctSet); 
    ds_posFnctSet.clearData();
	
	 
    var strSvcID    = "search";
    var strURL      = "rc/sle/posFnctSet/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_posFnctSet=ds_posFnctSet";
    var strArg      = "bcCd="          + "ALL"
					+ " outletCd="     + gfn_nvl(edt_upjang.getUpjangCode(),"")
					+ " posNum="       + (gfn_nvl(cbo_searchPosNum.value,"")=="00"?"":gfn_nvl(cbo_searchPosNum.value,""))
					+ " fnctClass="    + gfn_nvl(cbo_searchFnctId.value,"")
					+ " fnctCd="       + gfn_nvl(cbo_searchFnctCd.value,"")
					+ " useYn="        + (chk_serchUseYn.value?1:0)
					+ " maskAuth="     + SCAuth.getMaskAuth(this);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	
}
  

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	
	//업장 선택은
 	
	 
	var nRow = ds_posFnctSet.addRow();	
 
	ds_posFnctSet.setColumn(nRow , "bcCd", "ALL");
	ds_posFnctSet.setColumn(nRow , "outletCd", (gfn_nvl(edt_upjang.getUpjangCode(),"")==""?"":edt_upjang.getUpjangCode()));
	ds_posFnctSet.setColumn(nRow , "outletNm", (gfn_nvl(edt_upjang.getUpjangCode(),"")==""?"":edt_upjang.getUpjangName()));
	ds_posFnctSet.setColumn(nRow , "useYn","1");
	btn_outletNm.enable=true;
	
	ds_posNum.filter("outletCd==" + edt_upjang.getUpjangCode());
	ds_posNumList.clearData();
	ds_posNumList.copyData(ds_posNum,true); 
	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	 
	var nRow = ds_posFnctSet.rowposition;
	if(ds_posFnctSet.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		
		var bIsProcRow = false;
		if (ds_posFnctSet.getRowType(nRow) == Dataset.ROWTYPE_INSERT)
		{
			ds_posFnctSet.deleteRow(nRow);
		} 
		else
		{
			ds_posFnctSet.setColumn(nRow, "useYn", 0);
			bIsProcRow = true;
		}
		
		if (!bIsProcRow) return;

		sMsg = gfn_getMessage("msg.action.success", "");
		sMsg = sMsg += "\r\r처리된 결과를 저장하시겠습니까?";
		
		//실제 저장처리를 수행할 지를 알려준다.
		if (gfn_confirm(sMsg, "", "C"))
		{
			fn_save();
		}
		else
		{
			sMsg = "변경된 자료는 나중에 저장하시기 바랍니다.";
			gfn_alert(sMsg, "", "A");
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 처리
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{

 	if (!gfn_dsIsUpdated(ds_posFnctSet))
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	// 저장시 Validation Check
	if (gfn_validCheck(ds_posFnctSet, ds_posFnctSetValid) == false)	return;
	 
    var strSvcID    = "save";
    var strURL      = "rc/sle/posFnctSet/saveData.xdo";
    var strInDs     = "ds_posFnctSet=ds_posFnctSet:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 출력 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_print()
{
	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 도움말 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_help()
{
	
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		case "search":
			comboPosNumSet(); 
			break;
		case "selectCodeList":
			
			break;
		case "selectListPosNumAll":
			ds_posNum.filter("outletCd==" + edt_upjang.getUpjangCode());
			ds_searchposNumList.clearData();
			ds_searchposNumList.copyData(ds_posNum,true); 
			cbo_searchPosNum.index=0;
 			break;
		case "selectCodeListSub":
			break;
		case "searchList":
			
			break;
		 
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			btn_outletNm.enable=false;
			fn_search();
            break;    
    }
}


/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/


/************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역 (필수)
************************************************************************************************/

function ds_posFnctSet_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	//신규 추가행에 대해서만 처리한다.
	
	if (obj.getRowType(obj.rowposition) == Dataset.ROWTYPE_INSERT)
	{
		edt_outletNm.enable=true; 
		cbo_posNum.enable=true;
		cbo_fnctClass.enable=true;
		cbo_fnctCd.enable=true;
		if ((e.columnid == "bcCd" ) || (e.columnid == "outletCd") || (e.columnid == "posNum") || (e.columnid == "fnctClass") || (e.columnid == "fnctCd") )
		{
			 
			var iFindRowIdx = obj.findRowExpr( "bcCd == '" + gfn_nvl(obj.getColumn(e.row, "bcCd"),"") + "'"
								 + " && outletTypeCd == '" + gfn_nvl(obj.getColumn(e.row, "outletCd"),"")  + "'"
								 + " && picId == '"        + gfn_nvl(obj.getColumn(e.row, "posNum"),"")  + "'"
								 + " && class == '"        + gfn_nvl(obj.getColumn(e.row, "fnctClass"),"")  + "'"
								 + " && picPlace == '"     + gfn_nvl(obj.getColumn(e.row, "fnctCd"),"") +"'");

			
			var sMsg = "";
			
			//trace("....." + obj.getColumn(e.row, "fnctId"));
			//자기 자신을 제외하고 검색된 행이 존재할 시에는 중복 메시지를 표시한다.
			if (iFindRowIdx != e.row && iFindRowIdx != -1)
			{
				sMsg = obj.getColumn(e.row, e.columnid);
				gfn_alert("msg.err.duplicatedata", sMsg); //msg.err.duplicatedata   =  입력하신 $1 은(는) 이미 등록된 자료입니다.
				 
			 	obj.setColumn(e.row, e.columnid, "");
			 	grd_posFnctPic.setCellPos(grd_posFnctPic.getBindCellIndex("Body", e.columnid));
			 	grd_posFnctPic.setFocus();
				return;	
			}
		} 
	}
	 
} 

 
 

function edt_outletNm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) btn_outletNm_onclick();
	if ((e.keycode == 46) || (e.keycode == 8)) 
	{
		edt_outletNm.outletCd = "";
		edt_outletNm.value    = "";
	}	
}
//업장(입력)
function btn_outletNm_onclick(obj:Button,  e:ClickEventInfo)
{
	
	var arrParam = new Array();
	arrParam[0] ="";		//ALL,CHOOSE,""  => 3가지 사용 가능
	arrParam[1] ="";		//지점
	arrParam[2] = gfn_nvl(cbo_mu.getMuCode(),"");  //mu
	arrParam[3] = gfn_nvl(edt_tm.getTmCode(),"");  //tm
	var rtnVal = gfn_dialog("CodePop"					// dialogId
					   , "X_SLE::OutletInfoPop.xfdl"	// Url
					   , {fv_Contents:arrParam}			// 배열
					   , null, null, null, null, null, false, true, -1);
	//trace(nRowIdx + "===" + rtnVal);
	if (typeof rtnVal != "undefined" && rtnVal != null) 
	{
		if (rtnVal[0] == "OK")
		{
			edt_outletNm.outletCd = rtnVal[1];
			edt_outletNm.value    = rtnVal[2];
			ds_posNum.filter("outletCd==" + rtnVal[1]);
			ds_posNumList.clearData();
			ds_posNumList.copyData(ds_posNum,true); 
			
			ds_posFnctSet.setColumn(ds_posFnctSet.rowposition,"outletCd",rtnVal[1]);
			ds_posFnctSet.setColumn(ds_posFnctSet.rowposition,"outletNm",rtnVal[2]);
			cbo_posNum.index=0;
			ds_posFnctSet.setColumn(ds_posFnctSet.rowposition,"posNum",cbo_posNum.value);
			ds_posFnctSet.setColumn(ds_posFnctSet.rowposition,"posNumNm",cbo_posNum.text);
			 
		}
	}

}

 

function grd_posFnctSet_onselectchanged(obj:Grid, e:GridSelectEventInfo)
{
	comboPosNumSet();
	 
}

function comboPosNumSet()
{
	var nRow = ds_posFnctSet.rowposition;
	if(ds_posFnctSet.rowposition == -1) 
	{
		return;
	}
	var outCd = ds_posFnctSet.getColumn(nRow,"outletCd");
	 
	ds_posNum.filter("outletCd==" + outCd);
	ds_posNumList.clearData();
	ds_posNumList.copyData(ds_posNum,true); 
}

 
function cbo_posNum_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	ds_posFnctSet.setColumn(ds_posFnctSet.rowposition,"posNum",cbo_posNum.value);
	ds_posFnctSet.setColumn(ds_posFnctSet.rowposition,"posNumNm",cbo_posNum.text);
}
 
 
//사업부 변경되는 경우
function fn_muChange(obj, event)
{
	edt_tm.setTmCode("");
	edt_upjang.setUpjangCode("");
	ds_posFnctSet.clearData();
}
//팀(운영부서) 변경되는 경우
function fn_tmChange(obj, data)
{
	edt_upjang.setUpjangCode("");
	ds_posFnctSet.clearData();
}
function fn_upjangChange(obj,data)
{
	 
	ds_posFnctSet.clearData();
	ds_posNum.filter("outletCd==" + edt_upjang.getUpjangCode());
	ds_searchposNumList.clearData();
	ds_searchposNumList.copyData(ds_posNum,true); 
	 
} 
 

function cbo_searchFnctId_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	
	var ds = eval("ds_hwfsBindingCombo_cbo_searchFnctId");
	var row = ds.findRowExpr("code=='"+obj.value+"'");
	var grpCd = ds.getColumn(row, "set1");
	if (gfn_nvl(grpCd,"") == "")
	{
		 
		SCCombo.bind(this, "ComCode", "cbo_searchFnctCd",      		    null, {groupCode: "SLE_0012", code:"00",useYn:"Y",defVal:""},SCCombo.CHOOSE);	 
	}
	else
	{
		SCCombo.bind(this, "ComCode", "cbo_searchFnctCd",      		    null, {groupCode: "" + grpCd + "", useYn:"Y",defVal:""},SCCombo.CHOOSE);	 
	}
}

function cbo_fnctClass_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	ds_posFnctSet.setColumn(ds_posFnctSet.rowposition, "fnctClassNm", obj.text );
	
	var ds = eval("ds_hwfsBindingCombo_cbo_fnctClass");
	var row = ds.findRowExpr("code=='"+obj.value+"'");
	var grpCd = ds.getColumn(row, "set1");
	if (gfn_nvl(grpCd,"") == "")
	{
		SCCombo.bind(this, "ComCode", "cbo_fnctCd", null, {groupCode: "SLE_0012", code:"00",useYn:"Y",defVal:"firstrow"});	 
	}
	else
	{
		SCCombo.bind(this, "ComCode", "cbo_fnctCd", null, {groupCode: "" + grpCd + "", useYn:"Y",defVal:"firstrow"});	 
	}
	
	
}

function cbo_fnctCd_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	ds_posFnctSet.setColumn(ds_posFnctSet.rowposition, "fnctCdNm", obj.text );
}
 
function ds_posFnctSet_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	//하위 콤보 박스를 세팅해주어야 한다.
	
	var tKey = obj.getColumn(obj.rowposition,"fnctCd");
	var tGrp = obj.getColumn(obj.rowposition,"fnctClass");
	
	var ds = eval("ds_hwfsBindingCombo_cbo_fnctClass");
	var row = ds.findRowExpr("code=='"+ tGrp +"'");
	var grpCd = ds.getColumn(row, "set1");
	 
	if (gfn_nvl(grpCd,"") == "")
	{
		SCCombo.bind(this, "ComCode", "cbo_fnctCd",    null, {groupCode: "SLE_0012", code:"00",useYn:"Y",defVal:"00"});	 
	}
	else
	{
		SCCombo.bind(this, "ComCode", "cbo_fnctCd", null, {groupCode: "" + grpCd + "", useYn:"Y",defVal:"" + tKey + ""});	 
	}
	
	if (obj.getRowType(obj.rowposition) == Dataset.ROWTYPE_INSERT)
	{
		 
		 edt_outletNm.enable=true;
		cbo_posNum.enable=true;
		cbo_fnctClass.enable=true;
		cbo_fnctCd.enable=true;
	}
	else
	{
	 
		edt_outletNm.enable=false;
		btn_outletNm.enable=false;
		cbo_posNum.enable=false;
		cbo_fnctClass.enable=false;
		cbo_fnctCd.enable=false;
	}
	
}


 
]]></Script>
