 *---------------------------------------------------------------------------------------------*/
function fn_callRecipeBatchChange()
{
	var strId       = "RecipeBatchChange";
	var strURL      = "X_RRM::RecipeBatchChange.xfdl";
	var objArgument =  {  P_materialCd : ds_list.getColumn(ds_list.rowposition, "materialCd")
						, P_upjangCd : ds_search.getColumn(0, "upjangCd")
						, P_upjangNm : ds_search.getColumn(0, "upjangNm")
						, P_openType : "POP"
					   };
	var nWidth      = -1;
	var nHeight     = -1;
	var strStyle    = "";	
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";
	var bResizable  = true;

	// 리턴값을 배열로 받을 경우	
	var arrRtn = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign, bResizable);

	if( arrRtn instanceof Object ){}

	fn_search();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 잔여일수에 따른 그리드 색상변경
 *---------------------------------------------------------------------------------------------*/
function fn_setGridColor(remainDay){
	var color = "";

	if(gfn_isNull(remainDay)){
		color = "red";
	}else{
		if(remainDay <= 14 && remainDay >= 0){
			color = "gold";
		}
	}
	
	return color;
}

