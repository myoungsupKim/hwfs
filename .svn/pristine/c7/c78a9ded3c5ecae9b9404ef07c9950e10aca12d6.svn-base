/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}

    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			for( var i = ds_list.rowcount; i >= 0; i-- )
			{
				var preItemPrice = ds_list.getColumn(i, "preItemPrice");
				var itemPrice = ds_list.getColumn(i, "itemPrice");
			
				if( (preItemPrice == 0) || (preItemPrice == itemPrice) )
					ds_list.deleteRow(i);
			}

			for( var n = 0; n < ds_list.rowcount; n++ )
			{
				strRemark = "";
				if( ds_list.getColumn(n,"shpMap") == "N" )
				{
					strRemark = "단가계약 안됨";
					ds_list.setColumn(n,"prRemark",strRemark);
					ds_list.setColumn(n,"prYn" , "A");
					ds_list.setColumn(n,"prQty", 0);
				}
				else if (ds_list.getColumn(n,"mtgUns") == "N")
				{
					strRemark = "불용자재";
					ds_list.setColumn(n,"prRemark",strRemark);
					ds_list.setColumn(n,"prYn" , "B");
					ds_list.setColumn(n,"prQty", 0);
				}

				else if (ds_list.getColumn(n,"mtgStp") == "N")
				{
					strRemark = "중지자재";
					ds_list.setColumn(n,"prRemark",strRemark);
					ds_list.setColumn(n,"prYn" , "B");
					ds_list.setColumn(n,"prQty", 0);
				}

				else if (ds_list.getColumn(n,"reqWkd") == "N")
				{
					strRemark = "거래처 요일 제한";
					ds_list.setColumn(n,"prRemark",strRemark);
					ds_list.setColumn(n,"prYn" , "A");
					ds_list.setColumn(n,"prQty", 0);
				}

				else if(gfn_length(ds_list.getColumn(n,"restrictPeriod")) > 0)
				{
					strRemark = "금지자재(" + ds_list.getColumn(n,"restrictPeriod") + ", " + ds_list.getColumn(n,"restrictReason") + ")";
					ds_list.setColumn(n,"prRemark",strRemark);
					ds_list.setColumn(n,"prYn" ,"C");
					ds_list.setColumn(n,"prQty",0);
				}
				else if(ds_list.getColumn(n,"dDayYn") ==  "N")
				{
					strRemark = "D-" + ds_list.getColumn(n,"dDays") + "일" + ds_list.getColumn(n,"dTimes") + "시 제한";
					ds_list.setColumn(n,"prRemark",strRemark);
					ds_list.setColumn(n,"prYn" , "A");
					ds_list.setColumn(n,"prQty", 0);
				}
				else
				{
					ds_list.setColumn(n,"prYn" , "Y");
				}
			}

			ds_list.applyChange();
			grd_list.binddataset = ds_list;


		break; 
    }
}


