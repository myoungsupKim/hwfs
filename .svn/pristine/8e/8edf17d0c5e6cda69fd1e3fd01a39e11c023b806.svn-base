/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";


/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_processStatus = "N";

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	SCCombo.bind(this, "ComCode", "cbo_userAuthLevel", null, {groupCode:"SC0003", useYn:"Y", set2:"외부", defVal:"21"});
	SCCombo.bind(this, "ComCode", "cbo_mainType", null, {groupCode:"MAIN_TYPE", useYn:"Y", set2:"외부", defVal:"INTRO_DEF"}); //메인화면 유형

	ds_search.addRow();
	ds_search.setColumn(0, "sabun", P_SABUN);
	ds_search.setColumn(0, "requestDate", P_REQUEST_DATE);
	ds_search.setColumn(0, "requestCd", P_REQUEST_CD);
	ds_search.setColumn(0, "userId", P_USER_ID);
	ds_search.setColumn(0, "requestClass", P_REQUEST_CLASS);
	ds_search.setColumn(0, "userAuthLevel", P_USER_AUTH_LEVEL);
	ds_search.setColumn(0, "mainType", P_MAIN_TYPE);
	ds_search.setColumn(0, "simplePoYn", P_SIMPLE_PO_YN);

	if (P_REQUEST_CLASS == "식재영업")
	{
		sta_label.text = "사업장";
		edt_upjangnm.value = P_UPJANGNM;
	}
	else
	{
		sta_label.text = "공급업체";
		edt_upjangnm.value = P_OTCUSTNM;
	}
	
	edt_userId.value = P_USER_ID;
	edt_sabun.value = P_SABUN;
	edt_userName.value = P_USER_NAME;
	edt_reqUserName.value = P_REQ_USER_NAME;
	edt_requestName.value = P_REQUEST_NAME;
	edt_requestDate.value = P_REQUEST_DATE;
	txt_remark.value = P_REMARK;
	cbo_mainType.value = P_MAIN_TYPE;
	cbo_userAuthLevel.value = P_USER_AUTH_LEVEL;
	chk_simplePoYn.value = P_SIMPLE_PO_YN;
	
	//trace("P_SIMPLE_PO_YN : " +P_SIMPLE_PO_YN);
	
	trace(ds_search.saveXML());

	fn_search();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{	
	if (gfn_dsIsUpdated(ds_userAuthList) == true) 
	{
		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
        if (!gfn_confirm("confirm.before.movepage")) return;
    }
    
	// 조회프로그램에도 Return true; 기술해 주세요!
	this.close();
}


