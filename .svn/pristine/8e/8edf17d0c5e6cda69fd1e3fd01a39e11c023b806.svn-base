/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			cbo_mainType.value = P_MAIN_TYPE;
			cbo_userAuthLevel.value = P_USER_AUTH_LEVEL;
			
            break;
        
        // 저장
        case "save" :
			if (fv_processStatus == "Y")
			{
				var rtn = {STATUS:"S", STATUS_NM:"완료"};
				gfn_alert("sc.scu.useridreq.006");	// 성공
				
				this.close(rtn);
			}
			else
			{
				gfn_alert("sc.scu.useridreq.007");	// 오류
			}
            break;    
    }
}


