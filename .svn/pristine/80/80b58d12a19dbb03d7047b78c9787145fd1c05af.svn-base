<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT V.SABUN,
       (SELECT NM_KOR FROM SCO_FS_HR_PERSONAL_V WHERE SABUN = V.SABUN) SABUNNM,
   	   V.UPJANG,
	   A.UPJANGNM,
	   V.USE_YN
 FROM MAS_DUALUPJANG_MGMT V
    , SCO_UPJANG_MST_V A
    , HLDC_SC_DEPT_V B
 WHERE	V.UPJANG = A.UPJANG
   AND A.DEPT_ID = B.DEPT_ID
  <isNotNull col="TM_CD">
 AND        B.TM_CD = #TM_CD#
  </isNotNull>	

  <isNotNull col="SABUN">
 AND        V.SABUN = #SABUN#
  </isNotNull>	  
</statement>
	<input default-name="ds_cond">
		<col name="TM_CD" size="255" type="VARCHAR" description="ÆÀ" />
		<col name="SABUN" size="20" type="VARCHAR" description="»ç¹ø" /> 

	</input>
	<output default-name="ds_list">
	</output>
</query>