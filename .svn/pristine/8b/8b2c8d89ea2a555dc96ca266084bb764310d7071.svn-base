<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[가맹점 여신(당월입금액)]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT SUM(C.AMOUNT) AS AMOUNT
  FROM HLDC_FM_RECEIPT C
 WHERE C.RECEIPT_DATE  BETWEEN SUBSTR(#NEED_DATE#,1,6)||'01' AND SUBSTR(#NEED_DATE#,1,6)||'31'
   AND C.DCGUBUN       = '2'
   AND C.ACCTCD        IN ('1110411', '2110441') --외상,선수만.. 잡이익은 제외
   AND C.UPJANG = #UPJANG#
	]]></statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_list">
	</output>
</query>
