/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";
include "lib::LimsUtil.xjs";

/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_iProcessCnt = 0;  	// 저장건수를 저장한다.(Controller Class에서만 할당함.)
var curDate = gfn_today();
var bIsTester = true;
var codeList = "DECISION_STATUS,DECISION,LAST_DECISION";

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
    // Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
    gfn_formOnLoad(this);

    // 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 화면의 Combo 처리
//     SCCombo.bind(this, "ComCode", "cbo_decisionStatus", null, {groupCode:"DECISION_STATUS", useYn:"Y"}, SCCombo.ALL);
// 
//     SCCombo.bind(this, "ComCode", "grd_specmList.decision", ds_decision, {groupCode:"DECISION", useYn:"Y"});
//     SCCombo.bind(this, "ComCode", "grd_specmList.lastDecision", ds_lastDecision, {groupCode:"LAST_DECISION", useYn:"Y"});

	// 사용 코드를 한번에 가져와서 Dataset에 순차적으로 binding 시킨다.
	LimsUtil.selectCodeBatchList(this, null, codeList);

    cal_fromDay.fn_initMonthCal(this, gfn_addDate(curDate, -7), "", ds_search, "fromDay", "Y");
    cal_toDay.fn_initMonthCal(this, curDate, "", ds_search, "toDay", "Y");
    
    cal_fromDayTest.fn_initMonthCal(this, "", "", ds_search, "fromDayTest");
    cal_toDayTest.fn_initMonthCal(this, "", "", ds_search, "toDayTest");
    
    grd_testArticleList.readonly = true;
    div_cmnBtn.btn_save.enable = false;
    
	fn_selectInsptUser();	// 검사항목리스트에 보여질 검사자 조회 후 DataSet Setting
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 공통코드 binding 조회 후 callback function
 *---------------------------------------------------------------------------------------------*/
function fn_codeBatchCallback()
{
	ds_decisionStatus.insertRow(0);
	ds_decisionStatus.setColumn(0, "code", "");
	ds_decisionStatus.setColumn(0, "codeName", "전체");
	
	cbo_decisionStatus.index = 0;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{
	// v_ds_check 미사용시 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
	if (gfn_dsIsUpdated(ds_specmList) || gfn_dsIsUpdated(ds_testArticleList))
	{
		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
        if (!gfn_confirm("confirm.before.movepage")) return false;
    }

	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}

/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	fn_doSearch();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장버튼 클릭
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (!gfn_dsIsUpdated(ds_testArticleList))
	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return;
	}
	
	for (var i = 0; i < ds_testArticleList.rowcount; i++)
	{
		// 최종판정이 있는데 판정이 없으면 메세지 출력
		if (ds_testArticleList.getRowType(i) == Dataset.ROWTYPE_UPDATE) {
			if (gfn_isNull(ds_testArticleList.getColumn(i, "lastDecision")))
			{
				gfn_alert("최종판정항목에 입력안된 시험항목이 있습니다.\n최종판정항목을 입력하여 주십시오!");
				return;
			}
			if (gfn_isNull(ds_testArticleList.getColumn(i, "result")))
			{
				gfn_alert("결과항목에 입력안된 시험항목이 있습니다.\n결과항목을 입력하여 주십시오!");
				return;
			}
		}
	}
	
	// 자료를 저장 하시겠습니까?
	if (!gfn_confirm("confirm.before.save")) return false;

    var strSvcID    = "saveList";
    var strURL      = "ls/ltm/testResultSpecmArticleReg/saveList.xdo";
    var strInDs     = "ds_search=ds_search:A ds_testArticleList=ds_testArticleList:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0)
	{
		return;
	}

    switch(strSvcID)
    {
		// 조회
        case "selectList" :
            break;
        case "selectInsptUserList" :	// 담당자
			ds_userList.insertRow(0);
			ds_userList.setColumn(0, "sabun", "");
			ds_userList.setColumn(0, "userNm", "전체");
			cbo_userList.index = 0;
			
			// 1. 검사자 권한이 있으면 시험담당자를 자신으로 세팅 한다.
			// 2. 그리드에서 수정이 가능하도록 입력란을 풀어준다.
			// 3. 저장 버튼을 비 활성화 시킨다.
			//trace(gfn_getUserId() + " , " + gds_limsAuthLevels.findRow('authCd', '101'));
			//trace(gds_limsAuthLevels.saveXML());
			if ( gds_limsAuthLevels.findRow('authCd', '101') != -1 ) {
				cbo_userList.value = gfn_getUserId();
				grd_testArticleList.readonly = false;
				div_cmnBtn.btn_save.enable = true;
			}
            break;
        // 시험항목조회
        case "selectTestArticleList" :
            break;
        // 저장
        case "saveList" :
			if (fv_iProcessCnt > 0) gfn_alert("msg.save.success");	// 저장 되었습니다.
			
			fn_searchDetail();
            break;
    }
}

/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 시험담당자 조회
 *---------------------------------------------------------------------------------------------*/
function fn_selectInsptUser()
{
    ds_search.setColumn(0, "authCd", "101");	// 담당자권한 코드중 검사자

    var strSvcID    = "selectInsptUserList";
    var strURL      = "ls/lcm/userMgmt/selectUserList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_userList=ds_userList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync, false);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 조회
 *---------------------------------------------------------------------------------------------*/
function fn_doSearch()
{
	if (gfn_isNull(cal_fromDay.fn_getValue()) || gfn_isNull(cal_toDay.fn_getValue()))
	{
		gfn_alert("접수일자를 입력하여 주십시오!");
		return;
	}
	if (cal_fromDay.fn_getValue() > cal_toDay.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
	if (cal_fromDayTest.fn_getValue() > cal_toDayTest.fn_getValue())
	{
		gfn_alert("시험 시작일이 종료일보다 큽니다.");
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_specmList);
    gfn_clearSortMark(grd_testArticleList);

    // Dataset을 초기화한다.
    ds_specmList.clearData();
    ds_testArticleList.clearData();

    var strSvcID    = "selectList";
    var strURL      = "ls/ltm/testResultSpecmArticleReg/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_specmList=ds_specmList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체목록 그리드 클릭
 *---------------------------------------------------------------------------------------------*/
function grd_specmList_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.oldrow == e.row) return;
	fn_searchDetail();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 클릭 후 시험항목 조회
 *---------------------------------------------------------------------------------------------*/
function fn_searchDetail()
{
	// Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_testArticleList);

    // Dataset을 초기화한다.
    ds_testArticleList.clearData();
    
    ds_search.setColumn(0, "specmCd", ds_specmList.getColumn(ds_specmList.rowposition, "specmCd"));

    var strSvcID    = "selectList";
    var strURL      = "ls/ltm/testResultSpecmArticleReg/selectDetailList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_testArticleList=ds_testArticleList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

