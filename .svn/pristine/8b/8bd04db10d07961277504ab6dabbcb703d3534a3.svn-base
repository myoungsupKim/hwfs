/*----------------------------------------------------------------------------------------------
 * 설명      : 신랑,신부 이름이 입력되면 해당 내역을 MST에 예약 명으로 넣는다
 *---------------------------------------------------------------------------------------------*/
function ds_function_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{ 

	
	if (e.columnid == "radioYn")
	{
		var uesValue = obj.getColumn(e.row,"radioYn");
		obj.setColumn(e.row,"radioYn",(uesValue == "true" ? 1:0));
	}
	if (e.columnid == "edgeYn")
	{
		var uesValue = obj.getColumn(e.row,"edgeYn");
		obj.setColumn(e.row,"edgeYn",(uesValue == "true" ? 1:0));
	}
	if (e.columnid == "societyYn")
	{
		var uesValue = obj.getColumn(e.row,"societyYn");
		obj.setColumn(e.row,"societyYn",(uesValue == "true" ? 1:0));
	}
	if (e.columnid == "seatgstYn")
	{
		var uesValue = obj.getColumn(e.row,"seatgstYn");
		obj.setColumn(e.row,"seatgstYn",(uesValue == "true" ? 1:0));
	}
	 
	if (e.columnid == "consultingYn")
	{
		var uesValue = obj.getColumn(e.row,"consultingYn");
		obj.setColumn(e.row,"consultingYn",(uesValue == "true" ? 1:0));
	}
	if (e.columnid == "intdYn")
	{
		var uesValue = obj.getColumn(e.row,"intdYn");
		obj.setColumn(e.row,"intdYn",(uesValue == "true" ? 1:0));
	}
	if (e.columnid == "secndYn")
	{
		var uesValue = obj.getColumn(e.row,"secndYn");
		obj.setColumn(e.row,"secndYn",(uesValue == "true" ? 1:0));
	}
	if (e.columnid == "threeYn")
	{
		var uesValue = obj.getColumn(e.row,"threeYn");
		obj.setColumn(e.row,"threeYn",(uesValue == "true" ? 1:0));
	}
	
	
	
	if (e.columnid == "husb") 
	{
		var sRow  = ds_function.rowposition;
		var husb  = ds_function.getColumn(sRow,"husb");
		var nRow  = ds_reservMst.rowposition;
		ds_reservMst.setColumn(nRow, "rsvnNm",husb );			//예약자는 신랑으로 한다.
	}
	if ((e.columnid == "husb") || (e.columnid == "bride"))
	{
		var sRow  = ds_function.rowposition;
		var husb  = ds_function.getColumn(sRow,"husb");
		var bride = ds_function.getColumn(sRow,"bride");
		var nRow  = ds_reservMst.rowposition;
		ds_reservMst.setColumn(nRow, "reservNm",husb + "/" + bride);			//행사종류 웨딩(2020)
	
