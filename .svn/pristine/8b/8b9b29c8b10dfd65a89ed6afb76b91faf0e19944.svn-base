/*
 ******************************************************************************************
 * 함  수  명   : fn_afterSearch()
 * 입      력   : errCode 	= 에러코드
				  errMsg	= 에러메세지
 * 반      환   : 없음
 * 기      능   : 사업장 조회 후 실행 함수
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-05-06    
 ******************************************************************************************
 */
function fn_SearchData()
{
	// 조회조건체크
	var nRow = ds_main.rowposition;
	
	if (nRow < 0)
	{
		g_Message("EE", "사업장 정보가 없습니다.");
		return;
	}
	
	// 초기화
	ds_update.clearData();
	
	// 조회
	var actionName, cmdName, inData, outData, otherArg, callBackFnc;
	
	outData			= "ds_update=ds_update";
	callBackFnc		= "fn_afterSearchData";
	
	otherArg		= "UPJANG=" + wrapQuote(ds_main.getColumn(nRow,"UPJANG"));
	otherArg		+= " g_PersInfoMask=" + g_PersInfoMask;	
	fsp_clear(this); 
    fsp_addSearch(this, "fm/fms:FMS00103E_S001");									// 조회 
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc, false, false, true);		// 서비스 호출 
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_afterSearchData()
 * 입      력   : errCode 	= 에러코드
				  errMsg	= 에러메세지
 * 반      환   : 없음
 * 기      능   : 가상계좌정보조회 후 실행 함수
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-05-06  
 ******************************************************************************************
 */
function fn_afterSearchData(errCode, errMsg)
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오");
		return;
	}
	
	var nRow = ds_main.rowposition;
	var acntcd = ds_update.getColumn(0,"ACNTCD");
	if(gfn_length(acntcd) == 14){
		acntcd = gfn_getFormat(acntcd,"@@@-@@@@@@-@@-@@@");
	}
	ds_main.setColumn(nRow,"BANK_CD",ds_update.getColumn(0,"BANK_CD"));		// 거래은행
	ds_main.setColumn(nRow,"BANKNM",ds_update.getColumn(0,"BANKNM"));		// 거래은행명
	ds_main.setColumn(nRow,"ACNTCD",						acntcd);		// 계좌정보
	ds_main.setColumn(nRow,"BANK_DRNM",ds_update.getColumn(0,"BANK_DRNM"));	// 예금주
}

