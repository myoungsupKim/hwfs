	if(ds_list.rowcount == 0){
		gfn_alert("msg.search.nodata");	// 조건에 해당하는 자료가 없습니다.
		return false;
	}

	//전표발행여부 체크
	for(var i=0; i<ds_list.rowcount; i++){
	//alert(ds_list.getColumn(i, "journalzYn"));
		if(ds_list.getColumn(i, "taxSendYn") == "Y" && Number(ds_list.getColumn(i, "applyAmt")) > 0){
			//gfn_alert("이미 세금계산서가 발행된 선수금이관정보는 이관처리/취소할 수 없습니다.");
			//ds_list.rowposition = i;
			//return false;
		}
	}
	if(arg == "PROC"){	//이관처리

		// v_ds_check 미사용시 변경사항 체크
		if(gfn_dsIsUpdated(ds_list) == false){
			gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
			return false;
		}else{
			if(gfn_isNull(cal_fiscalDateFromSch.text)){
				gfn_alert("msg.noinput", "이관일자");	// $1 을(를) 입력해 주십시요.
				cal_fiscalDateFromSch.setFocus();
				return false;
			}
			if(bCreateMode){
				if (gfn_isNull(edt_custCd.value)){
					gfn_alert("msg.noselect", "From고객사");	// $1 을(를) 선택해 주십시요.
					//edt_custCode.setFocus();
					return false;
				}
			}
			if (gfn_isNull(MuSearchComponent01.getMuCode())){
				gfn_alert("msg.noselect", "사업부");	// $1 을(를) 선택해 주십시요.
				return false;
			}
			if (gfn_isNull(TmSearchComponent01.getTmCode())){
				gfn_alert("msg.noselect", "팀");	// $1 을(를) 선택해 주십시요.
				return false;
			}
			if (gfn_isNull(UpjangSearchComponent01.getUpjangCode())){
				gfn_alert("msg.noselect", "CC");	// $1 을(를) 선택해 주십시요.
				return false;
			}
			if(gfn_isNull(cal_transAltDate.text)){
				gfn_alert("msg.noselect", "일자");	// $1 을(를) 선택해 주십시요.
				cal_transAltDate.setFocus();
				return false;
			}
			if(gfn_isNull(cmb_transTypeCd.value)){
				gfn_alert("msg.noselect", "이관유형");	// $1 을(를) 선택해 주십시요.
				cmb_transTypeCd.setFocus();
				return false;
			}
			if(cmb_transTypeCd.value == "G06"){
				if(gfn_isNull(edt_transCustCd.value)){
					gfn_alert("msg.noselect", "To고객사");	// $1 을(를) 선택해 주십시요.
					//edt_transCustCd.setFocus();
					return false;
				}
			}else if(cmb_transTypeCd.value == "G07"){
				if(gfn_isNull(edt_ccCd.value)){
					gfn_alert("msg.noselect", "업장명(CC코드)");	// $1 을(를) 선택해 주십시요.
					edt_ccCd.setFocus();
					return false;
				}
			}
			if(gfn_isNull(edt_remark.value)){
				gfn_alert("msg.noinput", "적요");	// $1 을(를) 입력해 주십시요.
				edt_remark.setFocus();
				return false;
			}
		}

		//재이관 여부 체크
		for(var i=0; i<ds_list.rowcount; i++){
			if(ds_list.getRowType(i) > 1){	//행 타입(1:Normal, 2:insert, 4:Update, 8:Delete)
				if(!gfn_isNull(ds_list.getColumn(i, "mngmvAdvanceNum"))){
					if(ds_list.getColumn(i, "applyAmt") > 0){
						//재이관 제한 해제 2018.03.22 김호석(문재현대리, 이재웅대리  확인)
						//gfn_alert("선수금번호 ["+ds_list.getColumn(i, 'advanceNum')+"]는\n이관처리/취소할 수 없습니다.(재이관불가)");
						//ds_list.rowposition = i;
						//return false;
					}
				}
			}
		}
	}
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 선수금이관/취소 Setting
 *---------------------------------------------------------------------------------------------*/
function fn_setAdvanceAltInfo(arg){

	var iApplyAmt 	= 0;
	var sAltNum		= "";
	var sRowStatus	= "";
	
	//이관
	if(arg == "PROC"){

		//신규/수정 구분
		if(gfn_isNull(gv_altNum)) sRowStatus = "INSERT";
		else sRowStatus = "UPDATE";
		
		//선수금대체마스터 세팅
		ds_search.setColumn(0, "rowStatus",	sRowStatus);
		ds_search.setColumn(0, "altAmt",	edt_applyAmtSum.value);
		ds_search.setColumn(0, "typeGrpCd",	"007");
		
		for(var i=0; i<ds_list.rowcount; i++){
			iApplyAmt	= ds_list.getColumn(i, "applyAmt");
			sAltNum		= ds_list.getColumn(i, "altNum");
			
			ds_search.setColumn(0, "altDate",   cal_transAltDate.text);
			
			if(ds_list.getRowType(i) > 1){	//행 타입(1:Normal, 2:insert, 4:Update, 8:Delete)
			
				if(iApplyAmt > 0){
				
					if(gfn_isNull(sAltNum)){
						ds_list.setColumn(i, "rowStatus",	"INSERT");
						ds_list.setColumn(i, "altNum", 		gv_altNum);
					}
					
					ds_list.setColumn(i, "altDate",			cal_transAltDate.text);	// 대체 일자
					ds_list.setColumn(i, "transAltDate",	cal_transAltDate.text);	// 이관일자
					ds_list.setColumn(i, "mngmvAdvanceNum",	ds_list.getColumn(i, "advanceNum"));	// 이관 선수금 번호: 거래처 또는 업장 이관시 이전 선수금 번호 저장
					ds_list.setColumn(i, "remark",			edt_remark.value);			// 비고
					ds_list.setColumn(i, "altTypeCd",    	cmb_transTypeCd.value);     // 선수금 대체 유형 코드
					ds_list.setColumn(i, "transYn",			"Y");						// 이관여부
					
					// 거래처이관
					if(cmb_transTypeCd.value == "G06"){
						ds_list.setColumn(i, "advanceOccurClass",       "AO03");    // 선수금 발생 구분
						ds_list.setColumn(i, "advanceAltOccurClass",    "TO04");    // 선수금 대체 발생 구분
						
						if (ds_search.getColumn(0, "inqueryMuCd") == "2004"){	//식재 인경우는 업장으로 처리 
							ds_list.setColumn(i, "transCustCd",      		ds_search.getColumn(0, "transCustCd"));	// 분할거래처코드
							ds_list.setColumn(i, "transCustNm",      		ds_search.getColumn(0, "transCustNm"));	// 분할거래처명
							ds_list.setColumn(i, "transCcCd",        		ds_search.getColumn(0, "transCcCd"));	// 분할CC코드
							ds_list.setColumn(i, "transCcNm",        		ds_search.getColumn(0, "transCcNm"));	// 분할CC명				
							ds_list.setColumn(i, "transUpjangCd",        	ds_search.getColumn(0, "upjangCdTo"));	// 분할CC코드
							ds_list.setColumn(i, "transUpjangNm",        	ds_search.getColumn(0, "upjangNmTo"));	// 분할CC명									
						}else{					
							ds_list.setColumn(i, "transCustCd",      		ds_search.getColumn(0, "transCustCd"));	// 분할거래처코드
							ds_list.setColumn(i, "transCustNm",      		ds_search.getColumn(0, "transCustNm"));	// 분할거래처명
							ds_list.setColumn(i, "transCcCd",        		ds_list.getColumn(i, "ccCd"));			// 분할CC코드
							ds_list.setColumn(i, "transCcNm",        		ds_list.getColumn(i, "ccNm"));			// 분할CC명
						}
						
					// 타부서이관
					}else if(cmb_transTypeCd.value == "G07"){
						ds_list.setColumn(i, "advanceOccurClass",       "AO04");
						ds_list.setColumn(i, "advanceAltOccurClass",    "TO05");
						ds_list.setColumn(i, "transCustCd",      		ds_list.getColumn(i, "custCd"));		// 분할거래처코드
						ds_list.setColumn(i, "transCustNm",      		ds_list.getColumn(i, "custNm"));		// 분할거래처명			
						ds_list.setColumn(i, "transCcCd",        		ds_search.getColumn(0, "transCcCd"));	// 분할CC코드
						ds_list.setColumn(i, "transCcNm",        		ds_search.getColumn(0, "transCcNm"));	// 분할CC명
						//업장코드 추가 20160603 처리자 : 맹수영
						ds_list.setColumn(i, "transUpjangCd",        	ds_search.getColumn(0, "upjangCdTo"));	// 분할CC코드
						ds_list.setColumn(i, "transUpjangNm",        	ds_search.getColumn(0, "upjangNmTo"));	// 분할CC명
					//계약이행예치금
					}else if(cmb_transTypeCd.value == "G08"){
						ds_list.setColumn(i, "advanceOccurClass",       "AO10");
						ds_list.setColumn(i, "advanceAltOccurClass",    "TO12");
						ds_list.setColumn(i, "transCustCd",      		ds_list.getColumn(0, "custCd"));	// 분할거래처코드
						ds_list.setColumn(i, "transCustNm",      		ds_list.getColumn(0, "custNm"));	// 분할거래처명
						ds_list.setColumn(i, "transCcCd",        		ds_list.getColumn(0, "ccCd"));	// 분할CC코드
						ds_list.setColumn(i, "transCcNm",        		ds_list.getColumn(0, "ccNm"));	// 분할CC명				
						ds_list.setColumn(i, "transUpjangCd",        	ds_list.getColumn(0, "upjangCd"));	// 분할CC코드
						ds_list.setColumn(i, "transUpjangNm",        	ds_list.getColumn(0, "upjangNm"));	// 분할CC명	
						ds_list.setColumn(i, "advanceTypeCd",        	"F07");	// 선수금유형	
						ds_list.setColumn(i, "acctCd",        		    "21401030");	// 계정
					//계정이관(선매출) 20190416 김호석추가
					}else if(cmb_transTypeCd.value == "G09"){
						ds_list.setColumn(i, "advanceOccurClass",       "AO12");
						ds_list.setColumn(i, "advanceAltOccurClass",    "TO15");
						ds_list.setColumn(i, "transCustCd",      		ds_list.getColumn(0, "custCd"));	// 분할거래처코드
						ds_list.setColumn(i, "transCustNm",      		ds_list.getColumn(0, "custNm"));	// 분할거래처명
						ds_list.setColumn(i, "transCcCd",        		ds_list.getColumn(0, "ccCd"));	// 분할CC코드
						ds_list.setColumn(i, "transCcNm",        		ds_list.getColumn(0, "ccNm"));	// 분할CC명				
						ds_list.setColumn(i, "transUpjangCd",        	ds_list.getColumn(0, "upjangCd"));	// 분할CC코드
						ds_list.setColumn(i, "transUpjangNm",        	ds_list.getColumn(0, "upjangNm"));	// 분할CC명	
						ds_list.setColumn(i, "advanceTypeCd",        	"F08");	// 선수금유형
						ds_list.setColumn(i, "acctCd",        		    "21401070");	// 계정
					}
				}else{
					if(!gfn_isNull(sAltNum)){
						ds_list.setColumn(i, "rowStatus",	"DELETE");	//수정모드에서 적용금액이 0 이면 단건 이관취소
					}
				}
			}
		}
		
		//신규일 경우 조회조건데이터를 복사한다.
		if(sRowStatus == "INSERT"){
			ds_search.setColumn(0, "muCd", 			ds_search.getColumn(0,"inqueryMuCd"));
			ds_search.setColumn(0, "tmCd", 			ds_search.getColumn(0,"inqueryTmCd"));
			ds_search.setColumn(0, "ccCd", 			ds_search.getColumn(0,"inqueryCcCd"));
			//ds_search.setColumn(0, "upjangCd", 		ds_search.getColumn(0,"inqueryUpjangCd"));
			ds_search.setColumn(0, "custCd", 		ds_search.getColumn(0,"inqueryCustCd"));
			ds_search.setColumn(0, "custNm", 		ds_search.getColumn(0,"inqueryCustNm"));
			//ds_search.setColumn(0, "transTypeCd",	ds_search.getColumn(0,"inqueryAdvanceTypeCd"));
			//ds_search.setColumn(0, "advanceNum", 	ds_search.getColumn(0,"inqueryAdvanceNum"));
        }

		ds_search.setColumn(0, "altTypeCd",		cmb_transTypeCd.value);

		// 거래처이관
		if(cmb_transTypeCd.value == "G06"){
			ds_search.setColumn(0, "advanceAltOccurClass",	"TO04");

		// 타부서이관
		}else if(cmb_transTypeCd.value == "G07"){
			ds_search.setColumn(0, "advanceAltOccurClass",	"TO05");
			
		//계정이관(계약이행예치금)
		}else if(cmb_transTypeCd.value == "G08"){
			ds_search.setColumn(0, "advanceAltOccurClass",	"TO12");
		//계정이관(선수금)
		}else if(cmb_transTypeCd.value == "G09"){
			ds_search.setColumn(0, "advanceAltOccurClass",	"TO15");
		}
		
	//이관취소
	}else if(arg == "CANCEL"){
		ds_search.setColumn(0, "recdStatus", "D");
		for(var i=0; i<ds_list.rowcount; i++){
			ds_list.setColumn(i, "rowStatus",  "CANCEL");
			ds_list.setColumn(i, "transYn",    "Y");
			//if(!gfn_isNull(ds_list.getColumn(i, "altNum"))){
			//	//ds_list.setColumn(i, "recdStatus", "D");
			//	ds_list.setColumn(i, "rowStatus",  "CANCEL");	//전체 이관취소
			//	ds_list.setColumn(i, "transYn",    "Y");   			
			//}
		}
	}
   
}

/*----------------------------------------------------------------------------------------------
