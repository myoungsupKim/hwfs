/*----------------------------------------------------------------------------------------------
 * 잔액 클릭시 적용금액 변경 EVENT
 *---------------------------------------------------------------------------------------------*/
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo){
	var iOccurAmt	= 0;	//잔액
	var iApplyAmt	= 0;	//적용금액

	//식권 환불관련 데이터가 있을 경우
	if(fv_update == false) return;
	
	if((e.cell == 5 && grd_list.formatid == "insert") || (e.cell == 3 && grd_list.formatid == "update")){	//적용금액 Setting
		iOccurAmt = ds_list.getColumn(ds_list.rowposition, "occurAmt");	//obj.getCellValue(e.row, e.cell);
		iApplyAmt = ds_list.getColumn(ds_list.rowposition, "applyAmt");	//obj.getCellValue(e.row, e.cell+1);
		
		if(iApplyAmt == iOccurAmt){
			ds_list.setColumn(ds_list.rowposition, "applyAmt", 0);
		}else{
			ds_list.setColumn(ds_list.rowposition, "applyAmt", iOccurAmt);
		}
	}
}

