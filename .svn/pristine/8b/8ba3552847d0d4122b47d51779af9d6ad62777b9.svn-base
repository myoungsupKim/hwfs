 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg){
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0){
		return;
	}
	
    switch(strSvcID){

        //조회
        case "search" :
        
			//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
			if(ds_list.getRowCount() == 0) gfn_loadDatasetInfo (ds_list, fv_objColInfo);
			fn_formSetInit(bCreateMode);
			
			//급식용 사업부 세팅
			muChk = ds_search.getColumn(0, "inqueryMuCd");
			
			//조회 후 그리드 속성 세팅
			fn_setCellProperty(grd_list);
						
            break;

        //조회조건 조회
        case "selectAdvanceAltMst" :

			var inqueryStartDate	= ds_search.getColumn(0, "inqueryStartDate");
			var inqueryEndDate		= ds_search.getColumn(0, "inqueryEndDate");
			var fiscalDate			= ds_search.getColumn(0, "fiscalDate");

			cal_fiscalDateFromSch.fn_initMonthCal(this, inqueryStartDate, null, ds_search, "inqueryStartDate", "Y");
			cal_fiscalDateToSch.fn_initMonthCal(this, inqueryEndDate, null, ds_search, "inqueryEndDate");
			cal_transAltDate.fn_initMonthCal(this, fiscalDate, null, ds_search, "fiscalDate", "Y");
			
			var orgAltTypeCd = ds_search.getColumn(0, "transTypeCd");
			/*
			if(cmb_transTypeCd.value == "G06"){
				edt_ccCd.value = "";
				edt_ccNm.value = "";
			}else if(cmb_transTypeCd.value == "G07"){
				edt_transCustCd.value = "";
				edt_transCustNm.value = "";
