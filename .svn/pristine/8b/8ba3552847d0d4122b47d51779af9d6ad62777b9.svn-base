function fn_setApplyAmtSum(){
	var iApplyAmtSum = 0;
	for(var i=0; i<ds_list.rowcount; i++){
		iApplyAmtSum += Number(ds_list.getColumn(i, "applyAmt"));
	}
	edt_applyAmtSum.value = iApplyAmtSum;
}

