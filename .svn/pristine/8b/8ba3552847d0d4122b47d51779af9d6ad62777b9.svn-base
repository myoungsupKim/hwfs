function fn_upjangAfterEvent(obj, cls)
{
	if(!gfn_isNull(cls.ccCd)){
		ds_search.setColumn(0,"inqueryMuCd", 	cls.muCd);
		ds_search.setColumn(0,"inqueryTmCd", 	cls.tmCd);
		ds_search.setColumn(0,"inqueryCcCd", 	cls.ccCd);
		ds_search.setColumn(0,"inqueryCcNm", 	cls.ccNm);
		ds_search.setColumn(0,"inqueryCustCd",  "");
		ds_search.setColumn(0,"inqueryCustNm",  "");
		
		if (ds_search.getColumn(0, "inqueryMuCd") == "2002"){
			if((!gfn_isNull(cls.custcd)) && (cls.custcd != "0")){
				ds_search.setColumn(0,"inqueryCustCd", cls.custcd);
				ds_search.setColumn(0,"inqueryCustNm", cls.custnm);
			}
		}
	}else{
		ds_search.setColumn(0,"inqueryCustCd",  "");
		ds_search.setColumn(0,"inqueryCustNm",  "");
	}
	//fn_setCombo();
	fn_chgCalDate();
	
	return true;
}
