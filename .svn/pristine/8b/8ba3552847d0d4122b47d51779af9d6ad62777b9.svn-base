function fn_chkTicketYn(){

	var tCnt = 0;
	for(var i=0; i<ds_list.rowcount; i++){
		if(ds_list.getColumn(i, "advanceTypeCd") == "F05" || ds_list.getColumn(i, "advanceTypeCd") == "F10"){ //f05선수  f10외상
			if(Number(ds_list.getColumn(i, "applyAmt")) > 0) tCnt++;
		}
	}
	
	if(tCnt > 0){
		gfn_alert("식권판매(선수/외상)인 선수금정보는 이관처리/취소 처리할 수 없습니다.");
		return false;
	}else{
		return true;
	}
}

