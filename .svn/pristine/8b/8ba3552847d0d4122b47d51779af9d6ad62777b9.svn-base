//조회 후 그리드 속성 세팅
function fn_setCellProperty(obj){

	var cnt = 0;
	
	//수정모드
	if(!gfn_isNull(gv_altNum)){
		for(var i=0; i<ds_list.rowcount; i++){
			if(ds_list.getColumn(i, "advanceTypeCd") == "F05" || ds_list.getColumn(i, "advanceTypeCd") == "F10"){ //f05선수  f10외상
				if(Number(ds_list.getColumn(i, "applyAmt")) > 0) cnt++;
			}
		}
		
		//식권 환불관련 데이터가 있을 경우
		if(cnt > 0){
			cal_transAltDate.enable	= false;
			cmb_transTypeCd.enable	= false;
			edt_remark.enable		= false;
			obj.setCellProperty("body", 4, "edittype", "none");
			obj.setCellProperty("body", 4, "editdisplay", "none");
			//obj.setFormatColProperty(4, "edittype", "none");
			//obj.setFormatColProperty(4, "editdisplay", "none");
			fv_update = false;
		}else{
			edt_remark.enable		= true;
		}
	}
}

