<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[확정 여부 조회 SQL]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
		SELECT COUNT(1) AS CONFIRM_YN   												
		  FROM MAP_DAY_LOG_MGMT2												
		 WHERE YY = SUBSTR(#INPUT_YY#, 1, 4)
		   AND MM = SUBSTR(#INPUT_YY#, 5, 2) 	
		   AND TM_CD = #TM_CD#  
		   AND SABUN = #SABUN#
		   AND DAY_CLASS = #DAY_CLASS#
	</statement>
	<input default-name="ds_Cond">
	</input>
	<output default-name="ds_confirm_YN">
		<col name="CONFIRM_YN" size="256" type="VARCHAR" description="" />  
	</output>
</query>