// [ PART 3 ]
// Form Loading 시 작업 정의
//
//=========================================================================================
function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(this);

	// X-Framework : Action 정보 생성
	fsp_init(this);
	
	// 초기화
	fn_init();
}

/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_init()
 * 기ㅡㅡㅡ능 : 초기화
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-04-07
 ******************************************************************************************/
function fn_init() 
{
	// 조회조건 DataSet 초기화
	ds_bCond.clearData();
	ds_bCond.addRow();
	ds_aCond.clearData();
	ds_aCond.addRow();
	ds_recipeCond.clearData();
	ds_recipeCond.addRow();

	// 자재 분류코드 조회
	fsp_clear(this);
	fsp_addSearch(this, "fs/fsi:FSI90010E_S002");	// 자재 대분류	
	fsp_addSearch(this, "fs/fsi:FSI90010E_S003");	// 자재 중분류
	fsp_addSearch(this, "fs/fsi:FSI90010E_S004");	// 자재 소분류
	fsp_callService(this, "", "", "", "ds_bCboItemClass2=ds_cboItemClass2 ds_bCboItemClass3=ds_cboItemClass3 ds_bCboItemClass4=ds_cboItemClass4", "BLANK=Y", "fn_callbackInit");

	// 공통코드 조회
	v_ds = fn_common_code("'FS0001','FS0002','FS0003'","","","blk","Y");

	//FS0001 : RECIPE_CLASS
	v_fil_cond = "GROUP_CODE == 'FS0001' && CODE < 'K'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_chkRecipeClass, v_ds.name);		
	
	//분류 2줄로 생성 20200805
	v_fil_cond2 = "GROUP_CODE == 'FS0001' && CODE >= 'K'";
	v_ds.filter(v_fil_cond2);
	CopyF(ds_chkRecipeClass2, v_ds.name);	

	//FS0002 : RECIPE_TYPE
	v_fil_cond = "GROUP_CODE == 'FS0002'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_recipeType, v_ds.name);	

	//FS0003 : CUISINE_TYPE
	v_fil_cond = "GROUP_CODE == 'FS0003'";
	v_ds.filter(v_fil_cond);
	CopyF(ds_cuisineType, v_ds.name);	
		
	// 레시피분류 체크박스 생성
	fn_makeChkRecipeClass();
	fn_makeChkRecipeClass2(); //분류 2줄로 생성 20200805
		
	// 센터 콤보 조회
	fn_sql_common("C", strQuery_ScCenter,""," ORDER BY CENTER_CODE", "all", cbo_center, -1, ds_center);
	cbo_center.index = 0;
	
	cal_dutyDate.fn_initMonthCal(this, gfn_today(), "", ds_aCond, "DUTY_DATE", "");
			
	// 레시피분류 체크박스
	//SCCheckbox.bind(this, 'ComCode', 'div_recipeClass', {groupCode:"FS0001", useYn:"Y"}, {appearance:{usingDb:true},append:true,apendText:'전체'}, {onclick:"fn_initForm"});
	
	ds_aCond.setColumn(0, "ZERO_PRICE", 1);
}

function fn_initForm() 
{
	// 조회조건 DataSet 초기화
	ds_aCond.clearData();
	ds_aCond.addRow();
	ds_bCond.clearData();
	ds_bCond.addRow();

	// 데이터 초기화
	ds_bItemList.clearData();
	ds_aItemList.clearData();
	ds_sourceItemList.clearData();
	ds_targetItemList.clearData();
	ds_recipeList.clearData();
	
	// 분류코드 필터링 초기화
	ds_bCboItemClass3.filter("");
	ds_bCboItemClass4.filter("");
	ds_aCboItemClass3.filter("");
	ds_aCboItemClass4.filter("");
}

function fn_callbackInit(errCode, errMsg)
{
	if( errCode != 0 ) {
		g_Message("EE", errMsg);
		return;
	}
	else
	{
		// 변경후 대,중,소 분류로 분류코드 복사
		Copy(ds_aCboItemClass2, "ds_bCboItemClass2");
		Copy(ds_aCboItemClass3, "ds_bCboItemClass3");
		Copy(ds_aCboItemClass4, "ds_bCboItemClass4");
		
		edt_bItemNm.setFocus();
	}
}

function fn_makeChkRecipeClass()
{
	var iWidth = 118;
	var iUnit = 93;
	var sCode = "";
	var sName = "";
	for(var i=0; i < ds_chkRecipeClass.getRowCount(); i++)
	{
		sCode = ds_chkRecipeClass.getColumn(i, "CODE");
		sName = ds_chkRecipeClass.getColumn(i, "CODE_NAME");
		
		if(gfn_isNull(sCode)) continue;
		
		var chkObj = new CheckBox("chk_recipeClass" + i, iWidth + 2, 34, iWidth + iUnit, 54);
		chkObj.text = sName;
		chkObj.userdata = sCode;
		this.addChild("chk_recipeClass" + i, chkObj);
		chkObj.onclick.setHandler(fn_chkClick);
		chkObj.style.padding.left = 5;
		chkObj.style.background.color = "#EEEEEE";
		chkObj.show();
		iWidth = iWidth + iUnit;
	}
}

//분류 2줄로 생성 20200805
function fn_makeChkRecipeClass2()
{
	var iWidth = 118;
	var iUnit = 93;
	var sCode = "";
	var sName = "";
	for(var i=0; i < ds_chkRecipeClass2.getRowCount(); i++)
	{
		sCode = ds_chkRecipeClass2.getColumn(i, "CODE");
		sName = ds_chkRecipeClass2.getColumn(i, "CODE_NAME");
		
		if(gfn_isNull(sCode)) continue;
		
		var chkObj = new CheckBox("chk_recipeClass2" + i, iWidth + 2, 55, iWidth + iUnit, 75);
		chkObj.text = sName;
		chkObj.userdata = sCode;
		this.addChild("chk_recipeClass2" + i, chkObj);
		chkObj.onclick.setHandler(fn_chkClick);
		chkObj.style.padding.left = 5;
		chkObj.style.background.color = "#EEEEEE";
		chkObj.show();
		iWidth = iWidth + iUnit;
	}
}

function fn_chkClick(obj)
{
	if(obj.value) 
	{
		obj.style.background.color = "pink";
		obj.style.font = "Gulim,9";
		obj.style.color = "#555555";
	}
	else
	{
		obj.style.background.color = "#EEEEEE";
		obj.style.font = "Gulim,9";
		obj.style.color = "";
	}
}

function fn_getRecipeClass()
{
	var sRecipeClass = "";

	var sCode = "";
	var sName = "";
	var chkObj;
	
	for(var i=0; i < ds_chkRecipeClass.getRowCount(); i++)
	{
		sCode = ds_chkRecipeClass.getColumn(i, "CODE");
		sName = ds_chkRecipeClass.getColumn(i, "CODE_NAME");
		
		if(gfn_isNull(sCode)) continue;

		chkObj = eval("chk_recipeClass" + i);
		if(chkObj.value == 1)
		{
			sRecipeClass = sRecipeClass + chkObj.userdata + ',';
		}
	}
	
	for(var i=0; i < ds_chkRecipeClass2.getRowCount(); i++)
	{
		sCode = ds_chkRecipeClass2.getColumn(i, "CODE");
		sName = ds_chkRecipeClass2.getColumn(i, "CODE_NAME");
		
		if(gfn_isNull(sCode)) continue;

		chkObj = eval("chk_recipeClass2" + i);
		if(chkObj.value == 1)
		{
			sRecipeClass = sRecipeClass + chkObj.userdata + ',';
		}
	}
	
	
	

	return sRecipeClass;
}

