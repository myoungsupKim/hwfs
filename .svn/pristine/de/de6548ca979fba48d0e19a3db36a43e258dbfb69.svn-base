/*------------------------------------------------------------
	1. Function    : fn_Common_OnKeyDown(obj,nChar,bShift,bCtrl,bAlt,LLParam,HLParam)
	2. Description : Common Enter Key Event
	3. Arguments   : 
	4. History     : 2008. 03. 06 - 최초작성
*------------------------------------------------------------*/
function fn_Common_OnKeyDown(obj, e)
{
	switch(obj.name) {
		case "ed_UpjangNm" :
			if( e.keycode == "13" ) {
				if( gfn_length(gfn_trim(ed_UpjangNm.value))== 0 ) {
					btn_Upjang_OnClick();
				} else {
					cbo_Subinv.setFocus();
					cbo_Subinv.dropdown();
				}
			} else if( e.keycode == "46" ) {
				ds_Input.setColumn(0, "UPJANG",   "");
				ds_Input.setColumn(0, "UPJANGNM", "");
				ds_Subinv.clearData();
			}
			break;
		case "cbo_Subinv" :
			if( e.keycode == "13" ) {
				cbo_Meal.setFocus();
				cbo_Meal.dropdown();
			}
			break;
		case "cbo_Meal" :
			if( e.keycode == "13" ) {
				cbo_Corner.setFocus();
				cbo_Corner.dropdown();
			}
			break;
		case "cbo_Corner" :
			if( e.keycode == "13" ) me_MenuDate.setFocus();
			break;
		case "me_MenuDate" :
			if( e.keycode == "13" ) {
				chk_SPICES.setFocus();
			}
			break;
		default :
			break;
	}
}
