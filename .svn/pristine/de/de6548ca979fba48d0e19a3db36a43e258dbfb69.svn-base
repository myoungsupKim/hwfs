/*
 ******************************************************************************************
 * 함  수  명   : fn_Print()
 * 기      능   : [출력버튼] 출력 시 처리되는 작업
 ******************************************************************************************
 */
function fn_print()
{
	if( ds_List.getRowCount() == 0 ) {
		g_Message("EE", "출력 내역이 존재하지 않습니다.\n조회 후 출력해 주세요");
	} else {
		//레포트에 전달할 파라메터 생성
		var arrParams	= SCReport.newParams ();
		
		//--------------------------------------------------
		//방법1)서버에서 Query에 의해 처리하는 경우 예
		//(필수) 레포트명
		SCReport.setParams (arrParams, SCReport.RPT_NAME, "/FICS/FMP00420V_R01.ozr");

		//출력 옵션 설정
		//-창 타이틀 (디폴트는 "ReportViewer")
		SCReport.setParams (arrParams, SCReport.RPT_TITLE, "전처리계획표");
		
		//레포트에 맞게 전달되는 개발자 정의 파라미터
		SCReport.setParams (arrParams, "PARAM1", gfn_trim(ds_Input.getColumn(0, "UPJANG")));
		SCReport.setParams (arrParams, "PARAM2", ds_Input.getColumn(0, "SUBINV_CODE"));	
		SCReport.setParams (arrParams, "PARAM3", ds_Input.getColumn(0, "MEAL"));
		SCReport.setParams (arrParams, "PARAM4", ds_Input.getColumn(0, "CORNER"));
		SCReport.setParams (arrParams, "PARAM5", ds_Input.getColumn(0, "MENU_DATE"));
		SCReport.setParams (arrParams, "PARAM6", ds_Input.getColumn(0, "CHK_SPICES"));
		SCReport.setParams (arrParams, "PARAM7", ds_Input.getColumn(0, "SUBINV_NAME_DISP"));
		
		SCReport.setParams (arrParams, "zoom", 120);
		//--------------------------------------------------
		SCReport.viewReport (this, arrParams, false);	
	}
}
