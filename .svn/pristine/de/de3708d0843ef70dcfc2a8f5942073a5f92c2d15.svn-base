	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	ds_search.addRow();
	
	SCCombo.bind2(this, "ComCode", "cbo_term", null, {groupCode:"물품대금조회기간", useYn:"Y", defVal:""}, SCCombo.ALL);	 			//물품대금조회기간
