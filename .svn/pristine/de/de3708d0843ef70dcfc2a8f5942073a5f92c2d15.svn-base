function ds_gdspaymList_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var taxYn = gfn_nvl(ds_gdspaymList.getColumn(e.row, "taxYn"));	//과세여부
	var supplyPrice = parseInt(gfn_nvl(ds_gdspaymList.getColumn(e.row, "supplyPrice")));	//공급가액
	if ( taxYn == "Y" )
	{
		ds_gdspaymList.setColumn(e.row,"vatAmt", supplyPrice/10);	//부가세 10%
		ds_gdspaymList.setColumn(e.row,"signAmt", supplyPrice+(supplyPrice/10));	//부가세 10%
	}
	else
	{
		ds_gdspaymList.setColumn(e.row,"vatAmt", 0);	//부가세 10%
		ds_gdspaymList.setColumn(e.row,"signAmt", supplyPrice);	//부가세 10%
	}
	
	if(e.columnid == "taxYn" || e.columnid == "elctnTaxBillYn" ) 
	{
		fv_currRow = e.row;
		var taxYn = gfn_nvl(ds_gdspaymList.getColumn(fv_currRow, "taxYn"));
		var elctnTaxBillYn = gfn_nvl(ds_gdspaymList.getColumn(fv_currRow, "elctnTaxBillYn"));
		
		fn_getVatClassCd(  taxYn, elctnTaxBillYn );	  //부과세구분코드 조회
	}	
	
	
}

/*----------------------------------------------------------------------------------------------
 * 설명      :부과세구분코드 조회 
 *---------------------------------------------------------------------------------------------*/
function fn_getVatClassCd(taxYn, elctnTaxBillYn)
{
//alert(taxYn+"//"+elctnTaxBillYn);
	ds_search.setColumn(0, "groupCode", "TAX_CD");	 //과세코드
	ds_search.setColumn(0, "flag", "100");	 //물품대금(내수)
	ds_search.setColumn(0, "taxYn", taxYn);
	ds_search.setColumn(0, "elctnTaxBillYn", elctnTaxBillYn);
    var strSvcID    = "getVatClassCd";
    var strURL      = "ft/fgp/gdspaymMst/getVatClassCd.xdo";
    var strInDs     = "ds_search=ds_search";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);	
}

function grd_gdspaymList_onexpandup(obj:Grid, e:GridMouseEventInfo)
{
	if(e.row <= -1) return;
    var sColumn = gfn_getCellColId(obj, e.col);

	//Expand 클릭시
	if (sColumn == "vatClassCd")
	{
		var arrParam = new Array();
		arrParam[0] = gfn_nvl(ds_gdspaymList.getColumn(e.row, "vatClassNm") , "");	// 전송할 파라미터 1  
		var  arrRtn = gfn_dialog("TaxTypePop"				// dialogId
						   , "X_FSI::TaxTypePop.xfdl"			// 과세유형 팝업 호출 
						   , {fv_Contents:arrParam}			// 배열
						   , null, null, null, null, null, false, true, -1);
		if ( arrRtn instanceof Object )
		{
			ds_gdspaymList.setColumn(e.row, "vatClassCd", arrRtn["taxType"]  );
			ds_gdspaymList.setColumn(e.row, "vatClassNm", arrRtn["taxTypeNm"]  );	
		}
	}	
