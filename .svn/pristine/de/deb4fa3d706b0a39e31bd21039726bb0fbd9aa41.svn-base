<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[표준레시피삭제시 조리법일괄삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM FMM_STD_RECIPE_CUISINE
WHERE  UPJANG=#UPJANG#
AND	 RECIPE_CD = #RECIPE_CD#	]]></statement>
	<input default-name="ds_Recipe">
		<col name="RECIPE_CD" size="20" type="VARCHAR" description="" /> 
		<col name="UPJANG" size="255" type="NUMERIC" description="" /> 
	</input>
	<output default-name="ds_Recipe">
	</output>
</query>
