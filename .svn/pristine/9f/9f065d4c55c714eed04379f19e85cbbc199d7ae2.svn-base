		ds_reservMst.setColumn(i,"reservNum","");
		ds_reservMst.setColumn(i,"reservStatus","1");
		
	}
	ds_reservMst.updatecontrol = true;
	
	//연회룸
	ds_banquetRoom.updatecontrol = false;
	for(var i = 0; i < ds_banquetRoom.rowcount; i++)
	{ 
		var bSucc = ds_banquetRoom.setRowType(i, Dataset.ROWTYPE_INSERT);
		ds_banquetRoom.setColumn(i,"reservNum","");
		ds_banquetRoom.setColumn(i,"reservStatus","1");
	}
	ds_banquetRoom.updatecontrol = true;
	
	//메뉴
	ds_goods.updatecontrol = false;
	for(var i = 0; i < ds_goods.rowcount; i++)
	{ 
		var bSucc = ds_goods.setRowType(i, Dataset.ROWTYPE_INSERT);
		ds_goods.setColumn(i,"reservNum","");
	}
	ds_goods.updatecontrol = true;
	
	//객실
	ds_room.updatecontrol = false;
	for(var i = 0; i < ds_room.rowcount; i++)
	{ 
		var bSucc = ds_room.setRowType(i, Dataset.ROWTYPE_INSERT);
		ds_room.setColumn(i,"reservNum","");
	}
	ds_room.updatecontrol = true;
	//기타
	ds_other.updatecontrol = false;
	for(var i = 0; i < ds_other.rowcount; i++)
	{ 
		var bSucc = ds_other.setRowType(i, Dataset.ROWTYPE_INSERT);
		ds_other.setColumn(i,"reservNum","");
	}
	ds_other.updatecontrol = true;
	//외부
	ds_outside.updatecontrol = false;
	for(var i = 0; i < ds_outside.rowcount; i++)
	{ 
		var bSucc = ds_outside.setRowType(i, Dataset.ROWTYPE_INSERT);
		ds_outside.setColumn(i,"reservNum","");
	}
	ds_outside.updatecontrol = true;
	//웨딩
	ds_function.updatecontrol = false;
	for(var i = 0; i < ds_function.rowcount; i++)
	{ 
		var bSucc = ds_function.setRowType(i, Dataset.ROWTYPE_INSERT);
		ds_function.setColumn(i,"reservNum","");
	}
	ds_function.updatecontrol = true;
	 
}


/*----------------------------------------------------------------------------------------------
 * 설명      : ds_reservMst 일자,시간이 변경되면 하위에 사용되는 일자 시간을 모두 변경 한다.
 *---------------------------------------------------------------------------------------------*/ 
function ds_reservMst_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var nRow = obj.rowposition;
	//예약기간을 검색 한다. 
	if ((e.columnid == "strDate") || (e.columnid == "endDate"))
	{
		var strDate = ds_reservMst.getColumn(nRow, "strDate");
		var endDate = ds_reservMst.getColumn(nRow, "endDate");
		if (strDate > endDate)
		{
			gfn_alert("msg.err.validator.comparemin", "시작일^종료일");	// $1 이(가) $2 보다 큽니다.
			ds_reservMst.setColumn(nRow, "endDate",strDate);
		}
	}
	//예약시간
	if ((e.columnid == "strTime") || (e.columnid == "endTime"))
	{
		var strTime = ds_reservMst.getColumn(nRow, "strTime");
		var endTime = ds_reservMst.getColumn(nRow, "endTime");
		 
		if (strTime > endTime)
		{
			gfn_alert("msg.err.validator.comparemin", "시작시간^종료시간");	// $1 이(가) $2 보다 큽니다.
			ds_reservMst.setColumn(nRow, "endTime",strTime);
			cbo_endTime.value=strTime;
		}
	}
	if ((e.columnid == "strDate") || (e.columnid == "endDate") || (e.columnid == "strTime") || (e.columnid == "endTime"))
	{
		var strDate = ds_reservMst.getColumn(nRow, "strDate");
		var endDate = ds_reservMst.getColumn(nRow, "endDate");
		var strTime = ds_reservMst.getColumn(nRow, "strTime");
		var endTime = ds_reservMst.getColumn(nRow, "endTime");
		
		//하위는 모두 해당 일자와 시간으로 변경한다.
		//banquet room
		for(var i = 0; i < ds_banquetRoom.rowcount; i++)
		{ 
			ds_banquetRoom.setColumn(i , "strDate", strDate);
			ds_banquetRoom.setColumn(i , "endDate", endDate);
			ds_banquetRoom.setColumn(i , "strTime", strTime); 
			ds_banquetRoom.setColumn(i , "endTime", endTime);
		}	
		//menu
		for(var i = 0; i < ds_goods.rowcount; i++)
		{ 
			ds_goods.setColumn(i , "strDate", strDate);
			ds_goods.setColumn(i , "endDate", endDate);
			ds_goods.setColumn(i , "strTime", strTime); 
			ds_goods.setColumn(i , "endTime", endTime);
		}	
		
		//room
		for(var i = 0; i < ds_room.rowcount; i++)
		{ 
			ds_room.setColumn(i , "arrivDate", strDate);
			ds_room.setColumn(i , "chkotDate", endDate);
			var dd = gfn_getDiffDay(arrDt, chkDt);
			ds_room.setColumn(i , "lodgeDd", dd);
		}	
		//other
		for(var i = 0; i < ds_other.rowcount; i++)
		{ 
			ds_other.setColumn(i , "strDate", strDate);
			ds_other.setColumn(i , "endDate", endDate);
			ds_other.setColumn(i , "strTime", strTime); 
			ds_other.setColumn(i , "endTime", endTime);
		}
		//outside
		for(var i = 0; i < ds_outside.rowcount; i++)
		{ 
			ds_outside.setColumn(i , "strDate", strDate);
			ds_outside.setColumn(i , "endDate", endDate);
			ds_outside.setColumn(i , "strTime", strTime); 
			ds_outside.setColumn(i , "endTime", endTime);
		}
		
	}
	
	
}

