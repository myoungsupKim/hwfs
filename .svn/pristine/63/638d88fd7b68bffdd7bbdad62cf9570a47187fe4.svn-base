<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM MAC_RPT_SET
WHERE  RPT_ID = #RPT_ID#
	 AND LINE_NO = #LINE_NO#	]]></statement>
	<input default-name="ds_din">
		<col name="RPT_ID" size="20" type="VARCHAR" description="" /> 
		<col name="LINE_NO" size="22" type="NUMERIC" description="" /> 
	</input>
	<output default-name="ds_dout">
	</output>
</query>
