function grd_listAdvance_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	var iOccurAmt	= 0;	//잔액
	var iApplyAmt	= 0;	//적용금액

	if(e.cell == 3){	//적용금액 Setting
		iOccurAmt = ds_listAdvance.getColumn(ds_listAdvance.rowposition, "occurAmt");	//obj.getCellValue(e.row, e.cell);
		iApplyAmt = ds_listAdvance.getColumn(ds_listAdvance.rowposition, "applyAmt");	//obj.getCellValue(e.row, e.cell+1);
		
		if(iApplyAmt == iOccurAmt){
			ds_listAdvance.setColumn(ds_listAdvance.rowposition, "applyAmt", 0);
		}else{
			ds_listAdvance.setColumn(ds_listAdvance.rowposition, "applyAmt", iOccurAmt);
		}
	}
}

function ds_listAdvance_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.columnid == "applyAmt"){
		var iOccurAmt		= 0;	//잔액
		var iApplyAmt		= 0;	//적용금액
		var iBalAmt			= 0;	//적용후잔액
		var iOccurAmtOrg	= 0;	//잔액합계		
		var iApplyAmtOrg	= 0;	//적용금액합계
		var iApplyAmtSum	= 0;	//입력 적용금액합계
		var sAdvanceNum		= "";	//선수금번호
		var nRow = ds_listAdvance.rowposition;
		
		iOccurAmt		= ds_listAdvance.getColumn(nRow, "occurAmt");
		iApplyAmt		= ds_listAdvance.getColumn(nRow, "applyAmt");
		iOccurAmtOrg	= ds_listAdvance.getColumn(nRow, "occurAmtOrg");
		iApplyAmtOrg	= ds_listAdvance.getColumn(nRow, "applyAmtOrg");
		sAdvanceNum		= ds_listAdvance.getColumn(nRow, "advanceNum");
		
		//입력 적용금액합계
		for(var i=0; i<ds_listAdvance.rowcount; i++){
			if(ds_listAdvance.getColumn(i, "advanceNum") == sAdvanceNum){
				iApplyAmtSum += ds_listAdvance.getColumn(i, "applyAmt");
			}
		}
		
		//잔액 < 적용금액
		if(iOccurAmt < iApplyAmt){
			gfn_alert("적용금액이 잔액을 초과할 수 없습니다.");
			ds_listAdvance.setColumn(nRow, "applyAmt", ds_listAdvance.getOrgColumn(nRow, "applyAmt"));
			bCanRowChanged = false;
		}
		fn_setApplyAmtSum();
	}	
}
