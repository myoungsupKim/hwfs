/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// 조회시 Validation Check
	if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

    gfn_clearSortMark(grd_list);

    var strSvcID    = "search";
    var strURL      = "sm/sar/receiveMgntPop/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_receiveData=ds_receiveData";
    var strArg      = "receiveNum=" + pReceiveNum;
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	
	var nRow = ds_list.addRow();	
	
	ds_list.setColumn(nRow, "GUBUN", 'AD');
	ds_list.setColumn(nRow, "INPUT_USER", gfn_getUserId());
	
	// Grid 입력 cell 지정
	grd_list.setFocus();
	grd_list.setCellPos(1);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_list.rowposition;
	
	if(ds_list.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		ds_list.deleteRow(nRow);
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if(msk_receiveAmt.value==0){
		alert("상계처리될 채권에 대한 대체금액을 입력해 주세요.");
		return;
	}
	if(msk_applyAmt.value==0){
		alert("상계처리 할 선수금에 대한 대체금액을 입력해 주세요.");
		return;
	}
	if(msk_receiveAmt.value != msk_applyAmt.value){
		alert("상계처리 대상 금액과 처리 금액이 다릅니다.");
		return;
	}

	// 선수금 대체번호가 없으면 신규
	if(gfn_isNull(ds_advanceData.getColumn(0, "altNum"))){
		ds_advanceData.setColumn(0, "rowStatus",	"INSERT");
	}else{
		ds_advanceData.setColumn(0, "rowStatus",	"UPDATE");	
	}
	ds_advanceData.setColumn(0, "altDate", ds_receiveData.getColumn(0, "receiveDate"));
	ds_advanceData.setColumn(0, "muCd",	ds_receiveData.getColumn(0, "muCd"));
	ds_advanceData.setColumn(0, "ccCd",	ds_receiveData.getColumn(0, "ccCd"));
	ds_advanceData.setColumn(0, "remark",	ds_receiveData.getColumn(0, "remark"));
	ds_advanceData.setColumn(0, "custCd",	ds_receiveData.getColumn(0, "custCd"));
//	ds_advanceData.setColumn(0, "custNm",	ds_receiveData.getColumn(0, "custNm"));
	ds_advanceData.setColumn(0, "remark",	ds_receiveData.getColumn(0, "remark"));
	
	//채권회수금액 셋팅
	ds_receiveData.setColumn(0, "arWithdrawAmt",	ds_receiveData.getColumn(0, "receiveAmt"));
	
    for(var i=0; i<ds_listAdvance.rowcount; i++){
		
	//선수금대체
		iApplyAmt	= ds_listAdvance.getColumn(i, "applyAmt");
		sAltNum		= ds_listAdvance.getColumn(i, "altNum");

		if(ds_listAdvance.getRowType(i) > 1){	//행 타입(1:Normal, 2:insert, 4:Update, 8:Delete)
		
			if(iApplyAmt > 0){
				
				if(gfn_isNull(sAltNum)){
					ds_listAdvance.setColumn(i, "rowStatus",	"INSERT");
					ds_listAdvance.setColumn(i, "altNum", 		gv_altNum);
				}else{
					ds_listAdvance.setColumn(i, "altNum", 		gv_altNum);
					ds_listAdvance.setColumn(i, "rowStatus",	"UPDATE");
				}
				//입력 데이터 세팅
				ds_listAdvance.setColumn(i, "altDate",				ds_receiveData.getColumn(0, "receiveDate"));					// 대체일자
				ds_listAdvance.setColumn(i, "remark",				edt_remark.value);					// 적요
			}else{
				if(!gfn_isNull(sAltNum)){
					ds_listAdvance.setColumn(i, "rowStatus",	"DELETE");	//수정모드에서 적용금액이 0 이면 단건 대체취소
				}
			}
		}
    }
	// 저장시 Validation Check	
	if (gfn_validCheck(ds_receiveData, ds_receiveDataValid) == false)	return;
	
    var strSvcID    = "save";
    var strURL      = "sm/sar/arDebitOffsetClearPop/saveList.xdo";
    var strInDs     = "ds_listAr=ds_listAr:U ds_listAdvance=ds_listAdvance:U ds_receiveData=ds_receiveData ds_advanceData=ds_advanceData";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

