/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            fn_ar_search();
            break;
            
		// 입금 상세정보 조회
        case "searchDetail" :
			fn_changeAmtRe();
			fn_selectAdvanceAltMst();
            break;
		//선수금 대체 마스터
        case "selectAdvanceAltMst" :
			fn_searchAdvanceAltDetail();
            break;
		//선수금 대체 DETAIL
        case "searchAdvanceAltDetail" :
              fn_setApplyAmtSum();               
            break;
        // 저장
        case "save" :
			gfn_alert("상계 처리가 완료 되었습니다.");	// 저장 되었습니다.
			opener.fn_search();
			close();
            break;    
            
        // 취소
        case "cancle" :
			gfn_alert("상계 취소 처리가 완료 되었습니다.");	// 저장 되었습니다.
			opener.fn_search();
			close();
            break;    
    }
}


