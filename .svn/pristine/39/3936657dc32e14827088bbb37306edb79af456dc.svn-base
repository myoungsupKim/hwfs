function btn_altCancle_onclick(obj:Button,  e:ClickEventInfo)
{
	if(gfn_isNull(pReceiveNum)){
		alert("상계 처리를 한 후 취소가 가능합니다.");
		return;
	}
	var rtVal = gfn_confirm("상계처리된 데이터를  취소 하시겠습니까?");	
	if (rtVal == false) 
	{	
		return;
	}
	
	ds_advanceData.setColumn(0, "recdStatus", "D");
	for(var i=0; i<ds_listAdvance.rowcount; i++){
		if(!gfn_isNull(ds_listAdvance.getColumn(i, "altNum"))){
			ds_listAdvance.setColumn(i, "recdStatus", "D");
			ds_listAdvance.setColumn(i, "rowStatus",  "CANCEL");	//전체 대체취소
		}
	}		
    var strSvcID    = "cancle";
    var strURL      = "sm/sar/arDebitOffsetClearPop/cancleList.xdo";
    var strInDs     = "ds_listAr=ds_listAr ds_listAdvance=ds_listAdvance ds_receiveData=ds_receiveData ds_advanceData=ds_advanceData";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	
}


