function btn_addArnum_onclick(obj:Button,  e:ClickEventInfo)
{
	if (gfn_validCheck(ds_receiveData, ds_searchValid, true) == false) return;
	
	var arrArg = new Array();
	//조회 조건값을 다시 팝업으로 전달.
	arrArg[0] = ds_receiveData.getColumn(0, "inqueryStartDate");
	arrArg[1] = ds_receiveData.getColumn(0, "inqueryEndDate");
	arrArg[2] = ds_receiveData.getColumn(0, "muCd");
	arrArg[3] = ds_receiveData.getColumn(0, "inqueryTmCd");
	arrArg[4] = ds_receiveData.getColumn(0, "ccCd");
	arrArg[5] = ds_receiveData.getColumn(0, "ccNm");
	arrArg[6] = ds_receiveData.getColumn(0, "custCd");
	arrArg[7] = ds_receiveData.getColumn(0, "custNm");

	var strId       = "ArNumPopup";
	var strURL      = "X_SAR::ArNumPopup.xfdl";
	var objArgument = {arrSchVal:arrArg};
	var nWidth      = -1;
	var nHeight     = -1;
	var strStyle    = "";	
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";
	var bResizable  = true;

	// 리턴값을  Dataset으로 받을 경우
 	var dsObj = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);
 
 	for (i=0; i< dsObj.getRowCount(); i++)
	{
		if(ds_listAr.findRow("arNum", dsObj.getColumn(i, "arNum"))==-1){
			var nRow = ds_listAr.addRow();
			ds_listAr.copyRow(nRow, dsObj, i);	//채권 내역
		}
	}
	fn_changeAmtRe();
}
