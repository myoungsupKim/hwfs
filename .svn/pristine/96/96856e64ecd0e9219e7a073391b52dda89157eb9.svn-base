<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>update</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
UPDATE 
 	SCC_DESC
SET 
	OUTLINE = #OUTLINE#
	, IMG = #IMG#
	, CONTENTS = #CONTENTS#
	, WRITER = #WRITER#
	, WDAY = #WDAY#
	, UPDATOR = #UPDATOR#
	, UDAY = #UDAY#
	, REMARK = #REMARK#
	, USE_YN = #USE_YN#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 SEQ = #SEQ#
AND          MENU_ID=#MENU_ID#	]]></statement>
