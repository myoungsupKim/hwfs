<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT  SYS_ID, SYS_NAME, SORT_SEQ, SYS_TYPE, USE_YN FROM SCC_SYSTEM
WHERE   SYS_TYPE = #SYS_TYPE#
AND     USE_YN = 'Y'
ORDER BY SORT_SEQ
	]]></statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_cbo">
	</output>
</query>
