/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
    // Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
    gfn_formOnLoad(this, false);
    this.onkeydown.addHandler(gfn_formKeyDown);

    // 화면에 default readonly component의 색상을 회색으로 변경 한다.
    for (var i = 0, iLen = this.div_testInfoRegDtl.components.length; i < iLen; i++) {
        if (this.div_testInfoRegDtl.components[i].readonly == true) {
            this.div_testInfoRegDtl.components[i].style.backgroundColor = "#ecececff";
        }
    }

    btn_save.enable = false;
    //수정 버튼 컨트롤 ( 시험 완료면 수정 불가 ) 20171120 맹수영 요청자:김민채
    btn_modify.enable = false;
    
    //if (SCAuth.getMaskAuth(this) == '1')
    //{
    //    btn_modify.enable = false;
    //}
    //else
    //{
    //    btn_modify.enable = true;
    //}
    //trace("SCAuth.getMaskAuth(this) >>>>>>>>>>>> " + SCAuth.getMaskAuth(this));

    // 의뢰번호
    argArgs = this.getOwnerFrame().fv_Contents;
    if (typeof argArgs == "undefined" || argArgs == null)
    {
        gfn_alert("의뢰번호가 없습니다.");
        return;
    }
    //trace(argArgs[0]);

    // 초기값 셋팅
    fn_init();
}

