	div_testInfoRegDtl.cbo_specmClass.readonly = true;
	div_testInfoRegDtl.cbo_specmClass.enable = false;
	div_testInfoRegDtl.edt_specmTypeNm.readonly =true;
	div_testInfoRegDtl.edt_specmTypeNm.enable = false;
	div_testInfoRegDtl.btn_searchSpecmTypePop.enable = false;
	div_testInfoRegDtl.cbo_specmType.readonly =true;
	div_testInfoRegDtl.cbo_specmType.enable = false;
	div_testInfoRegDtl.cbo_insptPurp2.readonly =true;
	div_testInfoRegDtl.cbo_insptPurp2.enable = false;
	if (Number(ds_testInfoProgress.getColumn(0, "procClass")) < "160")
	{
		div_testInfoRegDtl.edt_specmNm.enable = true;
		div_testInfoRegDtl.btn_searchSpecmPop.enable = true;
		div_testInfoRegDtl.cbo_specmClass.readonly = false;
		div_testInfoRegDtl.cbo_specmClass.enable = true;
		div_testInfoRegDtl.edt_specmTypeNm.enable = true;
		div_testInfoRegDtl.btn_searchSpecmTypePop.enable = true;
		div_testInfoRegDtl.cbo_specmType.readonly = false;
		div_testInfoRegDtl.cbo_specmType.enable = true;
		div_testInfoRegDtl.cbo_insptPurp2.readonly =false;
		div_testInfoRegDtl.cbo_insptPurp2.enable = true;
	} else 
	{
		for(var i=0;i<ds_userList2.rowcount;i++)
		{
			if(ds_userList2.getColumn(i,"sabun") ==  gfn_getUserId())
			{
				div_testInfoRegDtl.edt_specmNm.enable = true;
				div_testInfoRegDtl.btn_searchSpecmPop.enable = true;
				div_testInfoRegDtl.cbo_specmClass.readonly = false;
				div_testInfoRegDtl.cbo_specmClass.enable = true;
				div_testInfoRegDtl.edt_specmTypeNm.enable = true;
				div_testInfoRegDtl.btn_searchSpecmTypePop.enable = true;
				div_testInfoRegDtl.cbo_specmType.readonly = false;
				div_testInfoRegDtl.cbo_specmType.enable = true;
				div_testInfoRegDtl.cbo_insptPurp2.readonly =false;
				div_testInfoRegDtl.cbo_insptPurp2.enable = true;
				break;
			}
		}
	}
	
}

/***************************************************************************
 * 설명   : 검체가 한개 이상인 경우 시험항목 정보의 입력을 체크 한다.
 ***************************************************************************/
function fn_checkTestInfo()
{
    ds_testArticleListTmp.clearData();
    ds_testArticleListTmp.copyData(ds_testArticleList);
    ds_testArticleListTmp.applyChange();
    ds_testArticleListTmp.filter("");

    var nCheckCnt = 0;
    var chkSpecmNm = "";
    for (var j = 0; j < ds_specmArticleList.rowcount; j++)
    {
        chkSpecmNm = "";
        ds_testArticleListTmp.filter("seqNum == '" + ds_specmArticleList.getColumn(j, "seqNum") + "'");	// 검체코드 일련번호
        if (ds_testArticleListTmp.rowcount == 0)
        {
            chkSpecmNm = ds_specmArticleList.getColumn(j, "specmNm");
            break;
        }
    }
    return chkSpecmNm;
}

