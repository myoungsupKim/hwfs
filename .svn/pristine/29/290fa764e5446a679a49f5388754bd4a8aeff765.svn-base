// 	{
// 		gfn_alert("장기보존여부를 선택하여 주십시오!.");
// 		return;
// 	}
    if (gfn_isNull(ds_specmArticleList.getColumn(nRow, "insptPurp2")))
    {
        gfn_alert("검사목적2를 선택하여 주십시오!.");
