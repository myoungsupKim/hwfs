            ds_testArticleList.rowposition = nLastRow - 1;

            // appendData는 상태가 normal이므로 임의로 상태값을 변경 한다.
            ds_testArticleList.updatecontrol = false;
            for (var i = 0; i < ds_testArticleList.rowcount; i++)
