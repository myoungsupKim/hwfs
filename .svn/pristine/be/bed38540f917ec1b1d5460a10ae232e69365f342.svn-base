<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>update</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
UPDATE HLDC_FS_VOC_MASTER 
                        SET   NM_KOR             = #NM_KOR#
                               , TRANSACTION_GUBUN = #TRANSACTION_GUBUN#
                               , TRANSCTION_DATE   = #TRANSCTION_DATE#
                               , TRANSCTION_DESC   = #TRANSCTION_DESC#
                               , HOWTO_T1          = #HOWTO_T1#
                               , HOWTO_T2          = #HOWTO_T2#
                               , HOWTO_T3          = #HOWTO_T3#
                               , HOWTO_T4          = #HOWTO_T4#
                               , HOWTO_T5          = #HOWTO_T5#
                         WHERE  SEQNO           = #SEQNO#	</statement>
	<input default-name="ds_listIn">
		<col name="NM_KOR" size="255" type="VARCHAR" description="" /> 
		<col name="TRANSACTION_GUBUN" size="10" type="VARCHAR" description="" /> 
		<col name="TRANSCTION_DATE" size="12" type="VARCHAR" description="" /> 
		<col name="TRANSCTION_DESC" size="255" type="VARCHAR" description="" /> 
		<col name="HOWTO_T1" size="2" type="VARCHAR" description="" /> 
		<col name="HOWTO_T2" size="2" type="VARCHAR" description="" /> 
		<col name="HOWTO_T3" size="2" type="VARCHAR" description="" /> 
		<col name="HOWTO_T4" size="2" type="VARCHAR" description="" /> 
		<col name="HOWTO_T5" size="2" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_list">
	</output>
</query>
