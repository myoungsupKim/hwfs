/*----------------------------------------------------------------------------------------------
 * 설명      : AM검색후 삭제 하는 경우
 *---------------------------------------------------------------------------------------------*/ 
function edt_serAm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13) btn_teamCd_onclick();
	if ((e.keycode == 46) || (e.keycode == 8)) 
	{
		edt_serAm.uCode = "";   //코드
		edt_serAm.value = "";    //명	
		ds_list.clearData();
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명      : AM 선택 버튼을 누른 경우
 *---------------------------------------------------------------------------------------------*/
function btn_searchAm_onclick(obj:Button,  e:ClickEventInfo)
{
	var rtnVal = gfn_dialog("CodePop"				// dialogId
					   , "X_BMS::AmPop.xfdl"			// Url
					   , null			// 배열
					   , null, null, null, null, null, false, true, -1);
 
	if (typeof rtnVal != "undefined" && rtnVal != null) 
	{
		if (rtnVal[0] == "OK")
		{	
			edt_serAm.uCode = rtnVal[1];
			edt_serAm.value = rtnVal[2]; 
			ds_list.clearData();
		}
	}
}
  
/*
	해당 일자를 클릭 하는 경우 시간별 내역을 화면에 보여 주는 팝업 
*/
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{ 
	if (e.col > 1)
	{
		var objDataSet = eval(obj.binddataset);
		var nCnt = gfn_nvl(objDataSet.getColumn(e.row, e.col+2),"0");
		
		//trace("nCnt:"+nCnt);
	 
		if (nCnt == "0") return;
		
		var chk_type =""
		if (chk_st.value==1) 
		{ 
			chk_type = "1"
		}
		if (chk_ok.value==1) 
		{ 
			chk_type = chk_type + "2";
		}
		
		//trace("Header:" +    obj.getCellProperty( "Head", e.col, "text" ) );
