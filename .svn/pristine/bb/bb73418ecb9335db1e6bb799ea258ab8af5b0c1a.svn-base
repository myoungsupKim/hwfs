/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
    // 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
    if (nErrorCode < 0) {
        return;
    }

    switch(strSvcID) {
        // 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            break;
        case "searchInfo" :
            //trace(ds_list.saveXML());
            break;
        case "searchMainUpjang" :
            //trace(ds_list.saveXML());
            if(ds_mainUpjang.getRowCount() != -1) {
                ds_search.setColumn(0, "mainUpjangCd", ds_mainUpjang.getColumn(0, "mainUpjangCd"));
                ds_search.setColumn(0, "mainUpjangNm", ds_mainUpjang.getColumn(0, "mainUpjangNm"));

                div_main.edt_mainUpjangNm.setUpjangCode(ds_mainUpjang.getColumn(0, "mainUpjangCd"));
                div_main.edt_mainUpjangNm.setUpjangName(ds_mainUpjang.getColumn(0, "mainUpjangNm"));
            }

            break;
        // 저장
        case "save" :
            gfn_alert("msg.save.success");  // 저장 되었습니다.
            fn_search();

            break;
    }
}

