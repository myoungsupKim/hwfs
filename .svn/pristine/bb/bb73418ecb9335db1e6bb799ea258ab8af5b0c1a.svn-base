function fn_save()
{
    // v_ds_check 미사용시 변경사항 체크
    if (gfn_dsIsUpdated(ds_realEstate) == false && gfn_dsIsUpdated(ds_guaranteeInsur) == false && gfn_dsIsUpdated(ds_guarantor) == false
         && gfn_dsIsUpdated(ds_cashGuarantee) == false && gfn_dsIsUpdated(ds_pledge) == false && gfn_dsIsUpdated(ds_accRcvAlnt) == false) {
        gfn_alert("msg.save.nochange"); // 변경된 내역이 없습니다.
