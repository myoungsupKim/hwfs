//MU가 변경될 때에 대한 이벤트
function fn_muItemChangedEvent(obj, evt) {
    edt_tmNm.setTmCode("");
    div_main.edt_mainUpjangNm.setUpjangCode("");
    div_upjang.edt_upjangNm.setUpjangCode("");

    if(edt_muNm.getMuCode() != "2004") {
        div_main.visible = false;
        div_upjang.position.left = div_main.position.left;
    } else { // 식재일 경우
        div_main.visible = true;
        div_upjang.position.left = div_main.position.right;
    }
}

//TM이 변경될 때에 대한 이벤트
