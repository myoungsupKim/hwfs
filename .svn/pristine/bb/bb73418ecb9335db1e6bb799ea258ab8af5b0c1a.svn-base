// 담보목록 행변경시
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
    if(obj.getRowCount() == -1) {
        return;
    }

    if (gfn_dsIsUpdated(ds_realEstate) == true || gfn_dsIsUpdated(ds_guaranteeInsur) == true || gfn_dsIsUpdated(ds_guarantor) == true || gfn_dsIsUpdated(ds_cashGuarantee) == true || gfn_dsIsUpdated(ds_pledge) == true || gfn_dsIsUpdated(ds_accRcvAlnt) == true) {
         // 해당 row의 위치를 이동하면 변경된 데이터가 사라집니다.@계속 진행 하시겠습니까?
        if (!gfn_confirm("confirm.before.moveropos")) return false;
    }

    ds_search.setColumn(0, "custCd", ds_list.getColumn(e.newrow, "custCd"));
    ds_search.setColumn(0, "seq", ds_list.getColumn(e.newrow, "seq"));

    ds_realEstate.clearData();
    ds_guaranteeInsur.clearData();
    ds_guarantor.clearData();
    ds_cashGuarantee.clearData();
    ds_pledge.clearData();
    ds_accRcvAlnt.clearData();

    var strSvcID    = "searchInfo";
    var strURL      = "sm/saa/accdDamboInfoReg/selectDambo.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_realEstate=dsRealEstate ds_guaranteeInsur=dsGuaranteeInsur ds_guarantor=dsGuarantor ds_cashGuarantee=dsCashGuarantee ds_pledge=dsPledge ds_accRcvAlnt=dsAccRcvAlnt";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

