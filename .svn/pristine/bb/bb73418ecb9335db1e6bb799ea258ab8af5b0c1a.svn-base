/*--------------------------------------------------------------------------------------------------
 * 설명      : 거래처 미등록 체크
 *------------------------------------------------------------------------------------------------*/
function fn_checkCustNm(nTabPage, nRow, obj:Dataset)
{
	var bRtn = true;
	
	//fn_insert() 적용시에는 제외
	if (fv_rowType == "I") {
		//trace("AccdDamboInfoReg 화면 >> fn_checkCustNm(거래처 미등록 체크) : (fv_rowType == 'I')제외,  nTabPage=[" + nTabPage + "], nRow=[" + nRow + "], strCustCd=[" + strCustCd + "], strCustNm=[" + strCustNm + "]");
		return bRtn;	
	}
	if (nRow < 0) {
		//trace("AccdDamboInfoReg 화면 >> fn_checkCustNm(거래처 미등록 체크) : (nRow < 0)제외,  nTabPage=[" + nTabPage + "], nRow=[" + nRow + "], strCustCd=[" + strCustCd + "], strCustNm=[" + strCustNm + "]");
		return bRtn;	
	}
	
	
	//if (gfn_isNull(obj.getColumn(nRow, "custCd"))) {
	if (gfn_isNull(obj.getColumn(nRow, "custCd")) || gfn_isNull(obj.getColumn(nRow, "custNm"))) {
		bRtn = false;
		
		var strCustCd = obj.getColumn(nRow, "custCd")
		var strCustNm = obj.getColumn(nRow, "custNm")
		trace("AccdDamboInfoReg 화면 >> fn_checkCustNm(거래처 미등록 체크) : nTabPage=[" + nTabPage + "], nRow=[" + nRow + "], strCustCd=[" + strCustCd + "], strCustNm=[" + strCustNm + "]");
		
		if (nTabPage == 1) {
			if (tab_dambo.tabindex != 0) { tab_dambo.tabindex = 0; }
			tab_dambo.tabpage1.grd_list.setCellPos(1);
		} else if (nTabPage == 2) {
			if (tab_dambo.tabindex != 1) { tab_dambo.tabindex = 1; }
			tab_dambo.tabpage2.grd_list.setCellPos(1);
		} else if (nTabPage == 3) {
			if (tab_dambo.tabindex != 2) { tab_dambo.tabindex = 2; }
			tab_dambo.tabpage3.grd_list.setCellPos(1);
		} else if (nTabPage == 4) {
			if (tab_dambo.tabindex != 3) { tab_dambo.tabindex = 3; }
			tab_dambo.tabpage4.grd_list.setCellPos(1);
		} else if (nTabPage == 5) {
			if (tab_dambo.tabindex != 4) { tab_dambo.tabindex = 4; }
			tab_dambo.tabpage5.grd_list.setCellPos(1);
		} else if (nTabPage == 6) {
			if (tab_dambo.tabindex != 5) { tab_dambo.tabindex = 5; }
			//tab_dambo.tabpage6.grd_list.setCellPos(1);    //거래처 숨겨짐..
		} else {
			//
		}
		gfn_alert("거래처가 미등록 되어 있습니다. @먼저 거래처를 등록 하세요.");
	}
	return bRtn;

}


/*--------------------------------------------------------------------------------------------------
 * 설명      : 거래처 미등록 체크
 *--------------------------------------------------------------------------------------------------
