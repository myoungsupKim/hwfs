/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
            
		// 기준업장목록 조회
        case "searchStd" :
            //trace(ds_listStd.saveXML());
            if (ds_listStd.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_listStd, fv_objColInfo);
            }
            fn_searchStdSet();
            break;

        
		// 추가목록  조회
        case "searchListAdd" :
            //trace(ds_listAdd.saveXML());
            if (ds_listAdd.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_listAdd, fv_objColInfo);
            }
            // 엑셀업로드 관련 변수 초기화
			fv_excelFlag = false;
            break;

		// 삭제목록  조회
        case "searchListDel" :
            //trace(ds_listDel.saveXML());
            if (ds_listDel.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_listDel, fv_objColInfo1);
            }
            // 엑셀업로드 관련 변수 초기화
			fv_excelFlag2 = false;			
            break;

        // 저장 : 추가분
        case "saveAdd" :
			//trace("처리건수 : " + fv_processCnt);
			gfn_alert("msg.action.success");	// 처리 되었습니다.
			fn_searchStd();				// 저장 후 재조회
			fn_searchAdd();				// 저장 후 재조회
            break;    


        // 저장 : 삭제분
        case "saveDel" :
			//trace("처리건수 : " + fv_processCnt);
			gfn_alert("msg.action.success");	// 처리 되었습니다.
			fn_searchStd();				// 저장 후 재조회
			fn_searchDel();				// 저장 후 재조회
            break;    

            
    }
}


