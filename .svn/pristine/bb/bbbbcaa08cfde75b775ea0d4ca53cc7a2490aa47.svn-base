/*----------------------------------------------------------------------------------------------
 * 설명      : 자재 추가 팝업에서 전달되어진 Dataset
 *---------------------------------------------------------------------------------------------*/
function fn_itemRerutn(dsObj)
{
	// 등록 TYPE GET
	var v_type = ds_searchRow.getColumn(ds_searchRow.rowposition, "schType");
	var v_stdUpjang = ds_searchRow.getColumn(ds_searchRow.rowposition, "selStdUpjang");
	
	var iRow = 0;
	var iFindRowIdx = 0;

	if(dsObj != null)
	{
		for(var i=0; i<dsObj.rowcount; i++)
		{	
			// Dataset 복사
			if (v_type == "+")	{
				// 중복자재 검사
				iFindRowIdx = ds_listAdd.findRowExpr("itemCode == '"+dsObj.getColumn(i, "itemCode")+"'", 0, ds_listAdd.rowcount);
		
				if (iFindRowIdx == -1)
				{
					// 중복자재가 없는경우만 추가
					ds_listAdd.addRow();
					iRow = ds_listAdd.rowposition;
					ds_listAdd.copyRow(iRow, dsObj, i);
					// 단가업장코드 복사
					ds_listAdd.setColumn(iRow, "stdUpjang", v_stdUpjang);
					ds_listAdd.setColumn(iRow, "exceptionType", v_type);
				}
			
			} else {
				// 중복자재 검사
				iFindRowIdx = ds_listDel.findRowExpr("itemCode == '"+dsObj.getColumn(i, "itemCode")+"'", 0, ds_listDel.rowcount);
		
				if (iFindRowIdx == -1)
				{
					// 중복자재가 없는경우만 추가
					ds_listDel.addRow();
					iRow = ds_listDel.rowposition;
					ds_listDel.copyRow(iRow, dsObj, i);
					// 단가업장코드 복사
					ds_listDel.setColumn(iRow, "stdUpjang", v_stdUpjang);
					ds_listDel.setColumn(iRow, "exceptionType", v_type);
				}
				
			}
		} // end for
		
	}

}

/*----------------------------------------------------------------------------------------------
 * 설명      : 예외설정에 대한 저장 처리 
 * 
 *---------------------------------------------------------------------------------------------*/
function fn_saveAdd()
{
	var nRowCnt = ds_listAdd.getRowCount();	// DataSet Count
	
	// 저장할 자료 여부 CHECK 
	if(nRowCnt == 0) 
	{
		gfn_alert("msg.save.nodata", "저장할 데이터");	// 저장할 데이터가 없습니다.
		return;
	}

	// 조회조건 기준으로 CHECK
	if (gfn_validCheck(ds_searchRow, ds_searchRowValid, true) == false) return;


	// 변경내역 여부 CHECK 
  	if (gfn_dsIsUpdated(ds_listAdd) == false) 
  	{
  		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
  		return;
  	}

	// 필수입력 CHECK : 엑셀업로드용
	//if (gfn_validCheck(ds_listAdd, ds_listAddValid, true) == false) return;
	if (fv_excelFlag == true) {
		if( !fn_saveAddChk() ) return;
	}
		
	var sMsg = "예외설정에 추가할 자재를 저장합니다.\n계속 진행 하시겠습니까?";
	
	if (!gfn_confirm(sMsg, "", "C")) return;

	// Key SET
	fv_stdUpjang = ds_listStd.getColumn(ds_listStd.rowposition, "stdUpjang");


    var strSvcID    = "saveAdd";
	if (fv_excelFlag == true) {
		var strURL      = "fm/fms/salePriceException/saveExcel.xdo";
		var strInDs     = "ds_list=ds_listAdd:A";
	} else {
		var strURL      = "fm/fms/salePriceException/saveList.xdo";
		var strInDs     = "ds_list=ds_listAdd:U";
	}
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}


/*----------------------------------------------------------------------------------------------
 * 설명      : 엑셀 업로드 시 자재코드 관련 기본 CHECK
 *---------------------------------------------------------------------------------------------*/
function fn_saveAddChk()
{
	var nRowCnt = ds_listAdd.getRowCount();	// DataSet Count
	var v_itemCode = "";
	
	this.setWaitCursor(true);

	for (var i = 0 ; i < nRowCnt ; i++)
	{
		v_itemCode = gfn_transNullToEmpty(ds_listAdd.getColumn(i, "itemCode"));
		
		// 필수입력 CHECK
		if (v_itemCode=="") {
			this.setWaitCursor(false);
			sMsg = "자재코드는 필수입력 사항입니다.";
			gfn_alert(sMsg, "", "A");
			ds_listAdd.selectRow(i);
			return false;
		}	
	}
	this.setWaitCursor(false);
	return true;
}



/*----------------------------------------------------------------------------------------------
 * 설명      : 예외설정에 대한 삭제 처리 
 * 
 *---------------------------------------------------------------------------------------------*/
function fn_delAdd()
{
	var nRowCnt = ds_listAdd.getRowCount();	// DataSet Count
	var iCnt = 0;

	for (var i = 0 ; i < nRowCnt ; i++)
	{
 		// Dataset이 선택된 것만 적용
 		if (ds_listAdd.getSelect(i))	
 		{
 			iCnt++;
 		}
	}

	if(iCnt == 0)
	{
		sMsg = "삭제할 자재 정보가 없습니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}	

	// 조회조건 기준으로 CHECK
	if (gfn_validCheck(ds_searchRow, ds_searchRowValid, true) == false) return;


	var sMsg = "선택한 자재를 삭제합니다.\n계속 진행 하시겠습니까?";

	var rtVal = gfn_confirm(sMsg, "", "C");	// 선택된 자료를 삭제 하시겠습니까?

	iCnt = 0;
	if (rtVal == true) 
	{	
		// Key SET
		fv_stdUpjang = ds_listStd.getColumn(ds_listStd.rowposition, "stdUpjang");

		// 삭제용 dataset Clear
		ds_deleteItem.clearData();
		
		// 삭제할 대상 dataset 생성
		for (var i = 0 ; i < nRowCnt ; i++)
		{
			// Dataset이 선택된 것만 적용
			if (ds_listAdd.getSelect(i))	
			{
				// 신규추가된 ROW 해당 ROW를 삭제
				if (ds_listAdd.getRowType(i) != Dataset.ROWTYPE_INSERT)
				{
					//var bSucc = ds_listAdd.deleteRow(i);
					//trace("bSucc : " + bSucc);
					// 기존 자료인 경우
					iCnt++;
					ds_deleteItem.addRow();
					ds_deleteItem.setRowType(ds_deleteItem.rowposition, Dataset.ROWTYPE_DELETE);
					ds_deleteItem.setColumn(ds_deleteItem.rowposition, "stdUpjang", ds_searchRow.getColumn(ds_searchRow.rowposition, "selStdUpjang"));	// 단가업장		
					ds_deleteItem.setColumn(ds_deleteItem.rowposition, "itemCode", ds_listAdd.getColumn(i, "itemCode"));								// 자재코드
					ds_deleteItem.setColumn(ds_deleteItem.rowposition, "exceptionType", ds_listAdd.getColumn(i, "exceptionType"));						// 예외유령
				}
			}
		}
	
		if (iCnt == 0) {	// TR 필요없으면 메시지 호출 후 종료
			var bSucc = ds_listAdd.deleteSelectedRows();
			trace("bSucc : " + bSucc);
			gfn_alert("msg.delete.success");// 삭제 되었습니다.
			return;
		}
		
		// Transaction 바로 호출해서 삭제 처리 함 
		var strSvcID    = "saveAdd";
		var strURL      = "fm/fms/salePriceException/saveList.xdo";
		var strInDs     = "ds_list=ds_deleteItem:A";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}

}


