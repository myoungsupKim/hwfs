<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[Á¶¸®¹ý]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT B.CUISINE_TYPE AS CD
       , (SELECT CODE_NAME FROM SCC_COMMON_CODE WHERE GROUP_CODE = 'FS0003' AND CODE = B.CUISINE_TYPE) NM
       , COUNT(*) CNT
<isNotEqual col="DT_GB" value="ALL">
       , FSM_TERM_MENU_EVAL_NAME_FUN(#MENU_CLASS# , #COST_TYPE#, #MENU_TYPE#,#MEAL#,#MENU_DATE#,#MENU_DATE#,'AND C.CUISINE_TYPE= ''' || B.CUISINE_TYPE || '''') INFO
</isNotEqual>
<isEqual col="DT_GB" value="ALL">
       , FSM_TERM_MENU_EVAL_NAME_FUN(#MENU_CLASS# , #COST_TYPE#, #MENU_TYPE#,#MEAL#,#MENU_FDATE#,#MENU_TDATE#,'AND C.CUISINE_TYPE= ''' || B.CUISINE_TYPE || '''') INFO
</isEqual>
  FROM FSM_TERM_MENU_RECIPE A,
       FSR_STD_RECIPE_MST B
 WHERE A.RECIPE_CD = B.RECIPE_CD
   AND A.TERM_MENU_CD IN (
                           SELECT TERM_MENU_CD
                             FROM FSM_TERM_MENU_MST
                            WHERE MENU_CLASS = #MENU_CLASS#
                              AND COST_TYPE  = #COST_TYPE#
                              AND MENU_TYPE  = #MENU_TYPE#
                              AND MEAL       = #MEAL#
<isNotEqual col="DT_GB" value="ALL">
                              AND MENU_DATE = #MENU_DATE#
</isNotEqual>
<isEqual col="DT_GB" value="ALL">
                              AND MENU_DATE BETWEEN #MENU_FDATE# AND #MENU_TDATE#
</isEqual>
                  )
   AND CUISINE_TYPE NOT IN (
                            SELECT CODE_NAME
                              FROM SCC_COMMON_CODE
                             WHERE GROUP_CODE = 'FS0037'
                          )
GROUP BY B.CUISINE_TYPE
HAVING COUNT(*)>1
ORDER BY COUNT(*) DESC,B.CUISINE_TYPE	</statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_list">
	</output>
</query>
