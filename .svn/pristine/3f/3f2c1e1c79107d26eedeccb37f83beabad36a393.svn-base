<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[일괄적용 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT '0' AS CHK
       , UPJANG
       , UJ.UPJANGNM AS UPJANG_NM
FROM   FMS_UPJANG_V UJ
WHERE  UPJANG <> #UPJANG#
AND    MAIN_UPJANG =  #MAIN_UPJANG#	]]></statement>
	<input default-name="ds_input">
	</input>
	<output default-name="ds_list">
	</output>
</query>
