/*------------------------------------------------------------(60)
	1. Function ID : fn_Search()
	2. 개       요 : 업무프로세스에 따른 조회작업
	3. 기       능 : 검색조건에 해당하는 정보를 조회
	4. 인 수 설 명 : 
*/
function fn_search()
{
	ds_list.rowposition = -1;
	
	// action 정보 초기화 
	fsp_clear(this);
	
	// 조회 Action 추가 
	fsp_addSearch(this, "fm/fms:FMS00170E_S001");
	
	ds_param.setColumn(0, "CUSTNM", gfn_trim(ed_SCustNm.value));
	// 테스트를 위해 '1'로 설정!!!
	//ds_param.setColumn(0, "p_MaskAuth", wrapQuote(SCAuth.getMaskAuth(this))); 
	ds_param.setColumn(0, "p_MaskAuth", SCAuth.getMaskAuth(this));

	if ( gfn_length(gfn_trim(ed_Upjang.userdata))== 0 ) {
		ds_param.setColumn(0, "MAIN_UPJANG", ed_MainUpjang.userdata);
		ds_param.setColumn(0, "UPJANG", "");
	}
	else {
		ds_param.setColumn(0, "MAIN_UPJANG", "");
		ds_param.setColumn(0, "UPJANG", ed_Upjang.userdata);
	}

	// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", "ds_cond=ds_param", "ds_list=ds_list", "", "afterSearch");
	http.Sync = false;
}

function afterSearch(){
	//개인정보보호 접근로그를 쌓는다. 조회조건과 조회건수
	SCPersInfo.accessReadLog(this, ds_param.saveXML(), ds_list.getRowCount());	
}

/*------------------------------------------------------------
	1. Function ID : fn_Insert()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_insert()
{
	if ( gfn_length(gfn_trim(ed_Upjang.userdata))== 0 ) {
		g_Message("EE","사업장을 입력하십시오.");
		return;
	}

	var row = ds_list.addRow();
	ds_list.setColumn(row, "UPJANG", ed_Upjang.userdata);
	ds_list.setColumn(row, "USE_YN", "Y");
	
	//신규행을 발생한 경우에 반드시 처리
	ds_list.setColumn(row, "ADDR_INFO", comp_addr.newData());
	
	//btn_popup.enable = true;
	ed_CUSTNM.setFocus();
}
/*------------------------------------------------------------
	1. Function ID : fn_Save()
	2. 개       요 : 업무프로세스에 따른 정보 저장작업
	3. 기       능 : 저장버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_save()
{
	
	if ( gfn_length(gfn_trim(ed_CUSTNM.value))== 0 ) {
		g_Message("EE","고객사명을 입력하십시오.");
		return false;
	}
	//if ( G_fn_chkCustNo(me_SAUPNO.value,"") == false ) {
	if ( gfn_isCompRegNo(me_SAUPNO.value) == false ) {
		g_Message("EE","사업자등록번호를 확인하십시오.");
		return false;
	}
	if ( gfn_length(gfn_trim(ed_DRNAME.value))== 0 ) {
		g_Message("EE","대표자명을 입력하십시오.");
		return false;
	}
	if ( gfn_length(gfn_trim(cbo_CUST_CLASS.value))== 0 ) {
		g_Message("EE","고객군을 입력하십시오.");
		return false;
	}		
	if ( me_FST_REG_DATE.value > me_LST_DEAL_DATE.value ) {
		g_Message("EE","계약일자를 확인하시기 바랍니다.");
		return false;
	}	
	
	// action 정보 초기화 
	fsp_clear(this);
	
	// 저장 Action 추가 
	fsp_addSave(this, "fm/fms:FMS00170E_I001", "fm/fms:FMS00170E_U001", "", "", "", "", "");
	fsp_addSave(this, "fm/fms:FMS00170E_I002", "fm/fms:FMS00170E_U002", "", "", "", "", "");
	
	fsp_callService(this, "", "", "ds_cond=ds_list:U", "" , "", "afterSave");
	
}

function afterSave(errCode, errMsg) {
	if ( errCode == 0 ) {
		g_Message("02");
	}
	else
		g_Message("EE", errMsg);
		
	//개인정보보호 접근로그를 쌓는다. 처리한 개인정보칼럼을 나열한다. 
	SCPersInfo.accessWriteLog(this, "DRNAME, CUSTNM, ADDR_INFO, TEL_NO, HP_NO, PART_NAME, ACNTCD, BANK_DRNM, ");	
}

