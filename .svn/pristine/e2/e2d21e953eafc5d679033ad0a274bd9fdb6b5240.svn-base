<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT LCODE , SCODE , SNM , DESCRIPTION1 , USE_YN  FROM HLDC_ST_VOC_ITEM
WHERE (1=1)
<isNotNull col="LCODE">
AND LCODE = #LCODE# 
</isNotNull>
ORDER BY SCODE	</statement>
	<input default-name="ds_cond">
		<col name="LCODE" size="20" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_listIn">
	</output>
</query>
