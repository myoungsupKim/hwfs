function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(obj, false);

	//초기화
	fsp_init(this);

	// 그리드 포맷 설정
	rdo_gubun.value = "4";
	gd_list.formatid = "format_4";
	
	// 당년 자동 셋팅
	yy.value = gfn_left(gfn_today(),4);
	
	// 팀, 업장 edt_tm.userdata
	ds_Cond.addRow();
	ds_Cond.setColumn(0, "INPUT_YY", yy.value); 
	ds_Cond.setColumn(0, "TM_CD", application.g_Tm_Cd);
	ds_Cond.setColumn(0, "UPJANG", edt_upjang.getUpjangCode()); 
	ds_Cond.setColumn(0, "CC_CD", application.g_Cc_Cd);
	//alert(application.g_Tm_Cd);
	v_ds_check = "ds_List";
	ta_Sql.visible = (application.g_AuthLevel == "999");
	
	// 사업부
	//Static25.visible = (application.g_AuthLevel == "999");
	//cbo_mu.visible = (application.g_AuthLevel == "999");
	
	// 팀조회
	fn_ComboDataLoad();
	// 월 추정 적용정보 조회.
	fn_BizClassCall();
	// 월추정 버전정보 조회.
	fn_BizClassCall2();
	
	// 점장이면 관리만 아니면 관리, 재무 모두 보여주기
	fn_sql_common("C", fstrQuery1, " AND CHARGE_SABUN = '" + application.g_EmpNo + "'", "", "", "", -1, ds_AUTH_CD);
	
	ds_CostType.clearData();
	if (Number(ds_AUTH_CD.getColumn(0, "CHARGE_SABUN_CNT")) == 0) {
		ds_CostType.insertRow(0);
		ds_CostType.setColumn(0, "CODE", "M");
		ds_CostType.setColumn(0, "CODE_NAME", "관리");
		ds_CostType.insertRow(1);
		ds_CostType.setColumn(1, "CODE", "U");
		ds_CostType.setColumn(1, "CODE_NAME", "재무");
		fn_setCondPosition("A");
	} else {
		ds_CostType.insertRow(0);
		ds_CostType.setColumn(0, "CODE", "M");
		ds_CostType.setColumn(0, "CODE_NAME", "관리");
		fn_setCondPosition("B");
	}
}

