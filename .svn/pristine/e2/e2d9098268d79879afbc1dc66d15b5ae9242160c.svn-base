	ds_cbo_GBN2.clearData();
	
	fn_ClearData();
}

function fn_sum(arg)
{
	var temp;
	if( rdo_gubun.value == 1 )
	{
		temp = Math.round(ds_List.getCaseSum("L_ACCTNM== '매출액'",arg) 
					    - ds_List.getCaseSum("L_ACCTNM== '매출원가'",arg));
	}
	else
	{
		temp = Math.round(ds_List.getCaseSum("M_ACCTNM== '매출액'",arg) 
					    - ds_List.getCaseSum("M_ACCTNM== '재료비'",arg) 
						- ds_List.getCaseSum("M_ACCTNM== '경비'",arg) 
						//- ds_List.getCaseSum("M_ACCTNM== '노무비'",arg));
						- ds_List.getCaseSum("M_ACCTNM== '인건비'",arg));
	}

	return temp;
}

// 업장변경시 CC_CD를 다시 받아온다.
function fn_CcCd(obj, data){
	//trace("==>"+data.ccCd+"<===");
	ds_Cond.addRow();
	ds_Cond.setColumn(0, "CC_CD", data.ccCd);
	ds_Cond.setColumn(0, "UPJANG", data.upjang);
	
	fn_ClearData();
	
	ds_cbo_GBN.clearData();
	ds_cbo_GBN2.clearData();
	
	// 월 추정 적용정보 조회.
	fn_BizClassCall();
	// 월추정 버전정보 조회.
	fn_BizClassCall2();
}

