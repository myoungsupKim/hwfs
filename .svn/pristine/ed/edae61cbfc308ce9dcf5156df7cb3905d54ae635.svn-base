<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>update</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
	MERGE INTO FSI_DUTY_MST A
	USING (SELECT '300006' AS CENTER_CODE  --경인센터 등록/수정 시 송탄/광주도 동일하게 설정 20180914 김호석
	            , #CUSTCD# AS CUSTCD
	            , #ITEM_CODE# AS ITEM_CODE
	            , #START_DATE# AS START_DATE
	         FROM DUAL) B 
	   ON (A.CENTER_CODE = B.CENTER_CODE
	       AND A.CUSTCD = B.CUSTCD
	       AND A.ITEM_CODE = B.ITEM_CODE
	       AND A.START_DATE = B.START_DATE)           
	       WHEN MATCHED THEN
	            UPDATE
			    SET A.REP_ITEM_CODE = #REP_ITEM_CODE#
			      , A.LIMIT_DUTY_QTY = #LIMIT_DUTY_QTY#
			      , A.REMARK = #REMARK#
			      , A.USE_YN = DECODE(#USE_YN#, 1, 'Y', 'N')
			      , A.MAIN_YN = DECODE(#MAIN_YN#, 1, 'Y', 'N')
			      , A.UUSER = #g_EmpNo#
			      , A.UDATE = SYSDATE
			      , A.MD_PRICE = #MD_PRICE#
	       WHEN NOT MATCHED THEN
	            INSERT(
					  A.CENTER_CODE
					, A.CUSTCD
					, A.ITEM_CODE
					, A.START_DATE
					, A.END_DATE
					, A.REP_ITEM_CODE
					, A.MARKET_PRICE
					, A.LIMIT_DUTY_QTY
					, A.REMARK
					, A.USE_YN
					, A.CUSER
					, A.CDATE
					, A.UUSER
					, A.UDATE
					, A.MAIN_YN
					, A.MD_PRICE
				 ) VALUES (
				      '300006' --경인센터 등록/수정 시 송탄/광주도 동일하게 설정 20180914 김호석
				    , #CUSTCD#
				    , #ITEM_CODE#
				    , #START_DATE#
				    , #END_DATE#
				    , #REP_ITEM_CODE#
				    , #MARKET_PRICE#
					, #LIMIT_DUTY_QTY#
					, #REMARK#
					, DECODE(#USE_YN#, 1, 'Y', 'N')
					, #g_EmpNo#
					, SYSDATE
					, #g_EmpNo#
					, SYSDATE
					, DECODE(#MAIN_YN#, 1, 'Y', 'N')
					, #MD_PRICE#
				 )
	</statement>
	<input default-name="ds_list">
	</input>
	<output default-name="ds_out">
	</output>
</query>