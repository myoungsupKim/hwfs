function btn_Hide_Search_OnClick(obj:Button,  e:ClickEventInfo)
{
	if(edt_muNm.getMuCode() == "2003"){
		if( obj.userdata == "0" ) {	// 숨기기
			Div00.position.top = 76;
			Div01.position.top = 76;
			Div02.position.top = 76;
			DivH00.visible = false;
			obj.userdata = "1";
			obj.text = "상세조건보이기";
		} else {	// 보이기
			Div00.position.top = 241;
			Div01.position.top = 241;
			Div02.position.top = 241;
			DivH00.visible = true;
			obj.userdata = "0";
			obj.text = "상세조건숨기기";
		}
	}else{
		if( obj.userdata == "0" ) {	// 숨기기
			Div00.position.top = 76;
			Div01.position.top = 76;
			Div02.position.top = 76;
			DivH.visible = false;
			obj.userdata = "1";
			obj.text = "상세조건보이기";
		} else {	// 보이기
			Div00.position.top = 241;
			Div01.position.top = 241;
			Div02.position.top = 241;
			DivH.visible = true;
			obj.userdata = "0";
			obj.text = "상세조건숨기기";
		}
	}
	
}

