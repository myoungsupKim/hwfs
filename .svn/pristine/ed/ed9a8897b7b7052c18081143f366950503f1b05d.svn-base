/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 조회
        case "search" :
            //trace(ds_list.saveXML());
            if (ds_list.getRowCount() == 0) {
				gfn_loadDatasetInfo (ds_list, fv_listColInfo);
				// 상세정보 Clear
				ds_upjangInfo.clearData();
				ds_guarantyAmtInfo.clearData();
				ds_upjangMenu.clearData();
				ds_plinfoList.clearData();
            }
            break;
            
        case "searchDetail" :
            if(ds_upjangInfo.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_upjangInfo, fv_upjangInfoColInfo);
			}
			if(ds_upjangMenu.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_upjangMenu, fv_upjangMenuColInfo);
			}
			
            break;        

        case "searchPlDetail" :
			if(ds_plinfoList.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_plinfoList, fv_plinfoListColInfo);
			}
			
            break;        
    }
}


