function fn_searchDetail() {
	fv_upjangInfoColInfo = gfn_getDatasetInfo(ds_upjangInfo);
	fv_upjangMenuColInfo = gfn_getDatasetInfo(ds_upjangMenu);
		
	if(ds_search.getColumn(0, "muCd") == "2003"){
		ds_search2.setColumn(0, "year", Div02.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.value);	
	}else{
		ds_search2.setColumn(0, "year", Div01.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.value);
	}

	var strSvcID    = "searchDetail";
    var strURL      = "fs/fso/upjangInfoCycleRept/selectListDetail.xdo";
