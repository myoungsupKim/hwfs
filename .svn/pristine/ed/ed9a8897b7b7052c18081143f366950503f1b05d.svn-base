 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/
// 상세조건 CHECK
function fn_searchCheck()
{
	///외식용
	if(ds_search.getColumn(0, "muCd") == "2003"){
		// 계약만료일 CHECK
		if (DivH00.cal_schCtrtEndDt1c.text != "" || DivH00.cal_schCtrtEndDt2c.text != "") {	
			if (DivH00.cal_schCtrtEndDt1c.text == "") {
				gfn_alert("계약만료 시작일은 필수입력 사항입니다", "A");	
				DivH00.cal_schCtrtEndDt1c.setFocus();
				return false;
			}
			if (DivH00.cal_schCtrtEndDt2c.text == "") {
				gfn_alert("계약만료 종료일은 필수입력 사항입니다", "A");	
				DivH00.cal_schCtrtEndDt2c.setFocus();
				return false;
			}
		
			if (DivH00.cal_schCtrtEndDt1c.text > DivH00.cal_schCtrtEndDt2c.text) {
				gfn_alert("msg.err.validator.date.great");	// 종료일이 시작일보다 빠릅니다.
				DivH00.cal_schCtrtEndDt1c.setFocus();
				return false;
			}
		
		}
		
		// 면적 CHECK
		var v_area1 = gfn_nvl(ds_search.getColumn(0, "contractArea1"), "");
		var v_area2 = gfn_nvl(ds_search.getColumn(0, "contractArea2"), "");
		
