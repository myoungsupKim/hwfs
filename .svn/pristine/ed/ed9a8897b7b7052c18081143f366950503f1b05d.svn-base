//MU가 변경될 때에 대한 이벤트
function fn_muItemChangedEvent(obj, evt)
{
    edt_tmNm.setTmCode("");
    edt_upjangNm.setUpjangCode("");
        
    ds_search.setColumn(0,"muCd", edt_muNm.getMuCode());
    ds_search.setColumn(0,"muNm", edt_muNm.getMuName());
    
    ds_search2.setColumn(0,"muCd", edt_muNm.getMuCode());
    ds_search2.setColumn(0,"muNm", edt_muNm.getMuName());
    
	if(ds_search.getColumn(0, "muCd") == "2003"){
		DivH00.visible = true;
		Div02.visible = true;
		
		DivH.visible = false;
		Div01.visible = false;	
	}else{
		DivH00.visible = false;
		Div02.visible = false;
		
		DivH.visible = true;
		Div01.visible = true;
	}
	
	if(ds_search.getColumn(0, "muCd") == "2003"){
		Div02.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.value = fv_sYear;
		Div02.tab_upjangInfoCycleRept.tabpage2.cbo_amtUnit.index = 0;	
	}else{
		Div01.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.value = fv_sYear;
		Div01.tab_upjangInfoCycleRept.tabpage2.cbo_amtUnit.index = 0;
	}
    
    // 급식
	if(gfn_indexOf(ds_search2.getColumn(0, "muNm"), "FS", 0) >= 0) {
		ds_search.setColumn(0, "srchType", "001");
		ds_search2.setColumn(0, "saupType", "C");
	// 식재
	}else if(gfn_indexOf(ds_search2.getColumn(0, "muNm"), "식재", 0) >= 0) {
		ds_search.setColumn(0, "srchType", "002");
		ds_search2.setColumn(0, "saupType", "E");	
	// 외식
	}else if(gfn_indexOf(ds_search2.getColumn(0, "muNm"), "외식", 0) >= 0) {
		ds_search.setColumn(0, "srchType", "001");
		ds_search2.setColumn(0, "saupType", "A");
	}else{
		ds_search.setColumn(0, "srchType", "002");
		ds_search2.setColumn(0, "saupType", "");
	}
	
	ds_list.clearData();
	ds_upjangInfo.clearData();
	ds_upjangMenu.clearData();
	ds_guarantyAmtInfo.clearData();
	ds_plinfoList.clearData();
		
	
}

//TM이 변경될 때에 대한 이벤트
function fn_tmChangedEvent(obj, data)
{
    edt_upjangNm.setUpjangCode("");   
    ds_search.setColumn(0,"tmCd", edt_tmNm.getTmCode());
