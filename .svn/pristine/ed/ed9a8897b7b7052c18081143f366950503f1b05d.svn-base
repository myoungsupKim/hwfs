    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_searchPlDetail
 ******************************************************************************************
 */
function fn_searchPlDetail() {
	if(ds_list.rowcount==0) {
		gfn_alert("업장목록을 조회하여 주십시요.");
 		return;
  	}
  	
  	if(ds_list.rowposition < 0) {
		gfn_alert("업장목록을 선택하여 주십시요.");
 		return;
  	}
	
	if(ds_search.getColumn(0, "muCd") == "2003"){
		if (gfn_nvl(Div02.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.value) == "") {
			gfn_alert("조회년도는 필수입력 사항입니다.", "A");
			Div02.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.setFocus();
			return;
		}	
	}else{
		if (gfn_nvl(Div01.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.value) == "") {
			gfn_alert("조회년도는 필수입력 사항입니다.", "A");
			Div01.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.setFocus();
			return;
		}
	}
	
	
	// 열람권한에 따라 조회가 안될 수 도 있으므로 해당 DataSet은 Clear
	ds_plinfoList.clearData();
	fv_plinfoListColInfo = gfn_getDatasetInfo(ds_plinfoList);
	
	if(ds_search.getColumn(0, "muCd") == "2003"){
		ds_search2.setColumn(0, "year", Div02.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.value);	
	}else{
		ds_search2.setColumn(0, "year", Div01.tab_upjangInfoCycleRept.tabpage2.cal_searchYear.value);
	}
	
	if(ds_search.getColumn(0, "muCd") == "2003"){
		ds_search2.setColumn(0, "schGubun", Div02.tab_upjangInfoCycleRept.tabpage2.cbo_gubun.value);	
	}else{
		ds_search2.setColumn(0, "schGubun", Div01.tab_upjangInfoCycleRept.tabpage2.cbo_gubun.value);
	}


	var strSvcID    = "searchPlDetail";
    var strURL      = "fs/fso/upjangInfoCycleRept/selectPlDetail.xdo";
	var strInDs     = "ds_search2=ds_search2:A";
    var strOutDs    = "ds_plinfoList=ds_plinfoList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}




