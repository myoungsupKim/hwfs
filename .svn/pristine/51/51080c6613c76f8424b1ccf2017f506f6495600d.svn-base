	if ( (e.keycode == 13) && (e.fromreferenceobject == ed_NmKor) ) fn_search();
}
function fn_ChkSaveNull()
{
    if (ds_List.rowcount < 1)
	{
		g_Message("EE", "저장할 자료가 없습니다.");
		return true;
	}

	if (ffn_ChkNull(ed_MainUpjang.userdata))
	{
		g_Message("EE", "본사" + fn_AlertMsg("005"));
		if (ed_MainUpjang.enable) ed_MainUpjang.setFocus();
		return true;
	}
	/*
	if (ffn_ChkNull(ed_Upjang.userdata))
	{
		g_Message("EE", "사업장" + fn_AlertMsg("005"));
		if (ed_Upjang.enable) ed_Upjang.setFocus();
		return true;
	}
	*/

    for (i=0; i<ds_List.rowcount; i++)
    {
	    if (ffn_ChkNull(ds_List.getColumn(i,"NM_KOR")))
		{
			g_Message("EE", "이름" + fn_AlertMsg("005"));
			ds_List.rowposition = i;
			ed_NM_KOR.setFocus();
			return true;
		}
		if (ffn_ChkNull(ds_List.getColumn(i,"UPJANG")))
		{
			g_Message("EE", "사업장" + fn_AlertMsg("005"));
			ds_List.rowposition = i;
			ed_UpjangInput.setFocus();
			return true;
		}
		if (ffn_ChkNull(ds_List.getColumn(i,"WORK_SDT")))
		{
			g_Message("EE", "근무시작일" + fn_AlertMsg("005"));
			ds_List.rowposition = i;
			me_WORK_SDT.setFocus();
			return true;
		}
		if (ffn_ChkNull(ds_List.getColumn(i,"WORK_TMA")))
		{
			g_Message("EE", "출근" + fn_AlertMsg("005"));
			ds_List.rowposition = i;
			ed_WORK_TMA.setFocus();
			return true;
		}
		if (ffn_ChkNull(ds_List.getColumn(i,"WORK_TMB")))
		{
			g_Message("EE", "퇴근" + fn_AlertMsg("005"));
			ds_List.rowposition = i;
			ed_WORK_TMB.setFocus();
			return true;
		}
		if (ffn_ChkNull(ds_List.getColumn(i,"WORK_TMC")))
		{
			g_Message("EE", "정상근무시간" + fn_AlertMsg("005"));
			ds_List.rowposition = i;
			ed_WORK_TMC.setFocus();
			return true;
		}
		if (ffn_ChkNull(ds_List.getColumn(i,"PER_DTC")))
		{
			g_Message("EE", "입사일" + fn_AlertMsg("005"));
			ds_List.rowposition = i;
			me_PER_DTC.setFocus();
			return true;
		}
    }

	return false;
}

//근무 시작시작 종료시간 체크함수
function fn_chkStartimeEndtime()
{
	var rowCount = ds_List.getRowCount();
	var startTime, endTime;
	
	for(var i=0; i<rowCount; i++)
	{
		
		startTime 	= ds_List.getColumn(i,"WORK_TMA");
		endTime 	= ds_List.getColumn(i,"WORK_TMB");
		//trace(i + "/" + startTime + "/" + endTime);
		
		if(startTime > endTime)
		{
			ds_List.rowposition = i;
			ds_List.selectRow(i);
			ed_WORK_TMA.setFocus();
			return false;
		}
	}
	return true;
}

//입사일 시작일 체크함수
function fn_chkEntDtStartDt()
{
	var rowCount = ds_List.getRowCount();
	var entDt, startDt;
	
	for(var i=0; i<rowCount; i++)
	{
		
		entDt 	= ds_List.getColumn(i,"PER_DTC");
		startDt = ds_List.getColumn(i,"WORK_SDT");
		//trace(i + "/" + startTime + "/" + endTime);
		
		if(startDt < entDt)
		{
			ds_List.rowposition = i;
			ds_List.selectRow(i);
			me_PER_DTC.setFocus();
			return false;
		}
	}
	return true;
}

//시작일 종료일 체크함수
function fn_chkStDtEdDt()
{
	var rowCount = ds_List.getRowCount();
	var stDt, edDt;
	
	for(var i=0; i<rowCount; i++)
	{
		
		stDt = ds_List.getColumn(i,"WORK_SDT");
		edDt = ds_List.getColumn(i,"WORK_EDT");
		//trace(i + "/" + startTime + "/" + endTime);
		
		if(stDt > edDt)
		{
			ds_List.rowposition = i;
			ds_List.selectRow(i);
			me_WORK_SDT.setFocus();
			return false;
		}
	}
	return true;
}

// 데이터셋의 getRowType이 INSERT 일때만 수정 가능
function ds_List_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	if ( obj.getRowType(e.newrow) == Dataset.ROWTYPE_INSERT ) 
	{
		ed_NM_KOR.enable = true;
		ed_UpjangInput.enable = true;
		btn_UpjangInput.enable = true;
	} else {
		ed_NM_KOR.enable = false;
		ed_UpjangInput.enable = false;
		btn_UpjangInput.enable = false;
	}
}

function fn_CallPopUp(obj:Button, e:ClickEventInfo)
{
	switch(obj.name)
	{
		case "btn_AddrPop":
			fn_sql_common_setup(strQuery_ZIPCD
							, strQuery_ZIPCDKeyField
							, strQuery_ZIPCDValueField
							, strQuery_ZIPCDKeyFieldNM
							, strQuery_ZIPCDValueFieldNM
							, ""
							, ""
							, ""
							, strQuery_ZIPCDCaption);
							
			if (gfn_length(gfn_trim(gds_sql_common.getColumn(0, "ret_code")))!= 0 && gfn_length(gfn_trim(gds_sql_common.getColumn(0, "ret_code")))!= null)
			{
				ed_Zipcd.value = gds_sql_common.getColumn(0, "ret_code");
				ed_Addr1.value = gds_sql_common.getColumn(0, "ret_name");
			}
			break;
			
		case "btn_SaupPop":
			if (ds_CustO.rowposition >= 0)
			{
				if (gfn_length(gfn_trim(ed_Saupno.value))== 10)
				{
					var sRtn;
					sRtn = fn_ChkSaupNo(ed_Otcustid.value,ed_Saupno.value);
					if (gfn_length(gfn_trim(sRtn))> 0)
					{
						g_Message("EE","이미 등록된 사업자번호입니다.\n(" + sRtn + ")");
						ed_Saupno.setFocus();
					}
					else
						g_Message("01","사용가능한 사업자번호입니다.");
				}
				else
				{
					g_Message("EE","사업자번호 10자리를 정확히 입력하십시요.");
					ed_Saupno.setFocus();
				}
			}
			break;
			
		default:
			break;
	}
