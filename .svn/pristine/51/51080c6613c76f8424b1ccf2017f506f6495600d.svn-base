/*------------------------------------------------------------
	1. Function ID : fn_Save()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명 : 
*/
function fn_save()
{
	//필수항목 
	if (fn_ChkSaveNull()) return;
	
	//출퇴근 시간 유효성 체크
	if(!fn_chkStartimeEndtime())
	{
		g_Message("EE", "출퇴근 시간이 정확하지 않습니다.");
		return;
	}
	
	//입사일 근무시작일 유효성 체크
	if(!fn_chkEntDtStartDt())
	{
		g_Message("EE", "입사일과 근무시작일이 정확하지 않습니다.");
		return;
	}
	
	//입사일 근무시작일 유효성 체크
	if(!fn_chkStDtEdDt())
	{
		g_Message("EE", "입사일과 종료일이 정확하지 않습니다.");
		return;
	}
	
	// 데이타체크
	var sRtn = fn_ChkData();
	if (sRtn != null)
	{
		g_Message("EE", sRtn);
		return;
	}
	
	if (!g_Confirm("02", '변경사항을 저장하시겠습니까?!'))
	{
		return;
	}

	//저장(신규,수정) 트랜젝션 호출
	fn_SaveTransaction();
}
