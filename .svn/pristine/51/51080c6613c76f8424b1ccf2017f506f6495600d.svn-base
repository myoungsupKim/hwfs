	if(! gfn_dsIsUpdated(ds_List))
	{
		return fn_AlertMsg("007");
	}
}
function fn_edBtn_OnClick(obj:Button, e:ClickEventInfo)
{
	var sAddWhere = "";
	
	switch(obj.name)
	{
		case "btn_MainUpjang":
			sAddWhere = " AND A.MAIN_UPJANG = A.UPJANG";
			break;

		case "btn_Upjang":
			sAddWhere = " AND A.MAIN_UPJANG LIKE '" + ed_MainUpjang.userdata + "'";
			//사업장인 경우 관리업장만 조회
			if (application.g_UserKind == '사업장')
				sAddWhere += " AND ( A.UPJANG = " + application.g_Upjang + " OR EXISTS (SELECT 1 FROM FMS_USER_UPJANG B WHERE B.USE_YN = 'Y' AND B.UPJANG = A.UPJANG AND B.SABUN = '" + application.g_EmpNo + "') )";
			break;
			
		case "btn_UpjangInput":
			sAddWhere = " AND A.MAIN_UPJANG LIKE '" + ed_MainUpjang.userdata + "'";
			//사업장인 경우 관리업장만 조회
			if (application.g_UserKind == '사업장')
				sAddWhere += " AND ( A.UPJANG = " + application.g_Upjang + " OR EXISTS (SELECT 1 FROM FMS_USER_UPJANG B WHERE B.USE_YN = 'Y' AND B.UPJANG = A.UPJANG AND B.SABUN = '" + application.g_EmpNo + "') )";
			break;	
		
		default:
			break;
	}
	//fm공통 팝업클릭 호출
	if (ffn_btn_OnClick(obj, sAddWhere)) fn_Trigger_edBtn(eval("ed_" + gfn_subStr(obj.name, 4)));
}
function fn_edBtn_OnKeyDown(obj:Edit, e:KeyEventInfo)
{
	if (ffn_ed_OnKeyDown(obj,e.keycode)) fn_Trigger_edBtn(obj);	
}
function fn_Trigger_edBtn(obj)
{
	switch(obj)
	{
		case ed_MainUpjang:
			ed_Upjang.userdata = ""; ed_Upjang.value = "";
			ds_List.clearData();
			break;
		case ed_Upjang:
			ds_List.clearData();
			break;
		case ed_UpjangInput:
		    ds_List.setColumn(ds_List.rowposition, "UPJANG", ed_UpjangInput.userdata);
		    ds_List.setColumn(ds_List.rowposition, "UPJANGNM", ed_UpjangInput.value);
		    ds_List.setColumn(ds_List.rowposition,"SABUN",ed_UpjangInput.userdata + fn_getSeqNo(ds_SEQ,"FMO_PERSONAL_S"));
			break;	
		default:
			break;
	}
}
