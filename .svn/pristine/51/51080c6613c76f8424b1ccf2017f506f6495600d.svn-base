function fn_search()
{
	//필수항목 
	if (fn_ChkManNull()) return;
	
	// 초기화
	ds_Cond.clearData();
	ds_List.clearData();
	ds_Cond.addRow();
	ds_Cond.setColumn(0, "MAIN_UPJANG",   ed_MainUpjang.userdata);
	ds_Cond.setColumn(0, "UPJANG",        ed_Upjang.userdata);	
	ds_Cond.setColumn(0, "WORK_GB",	      cbo_WorkGb.value);
	ds_Cond.setColumn(0, "NM_KOR",        ed_NmKor.value);
	ds_Cond.setColumn(0, "VIEW_NM",       "FMO00790E.xml");

	// 조회
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "FMO00790E_T001";
	strURL   = "U_svc::" + "fm/fmo/FMO00790E_T001.jsp";
	// 파라메터 설정
	strArgument  = "p_MaskAuth=" + wrapQuote(SCAuth.getMaskAuth(this));
	
	// 데이터셋 설정
	strInDatasets  = "ds_Cond=ds_Cond";
	strOutDatasets = "ds_List=ds_List";
	// 콜백함수 지정
	strCallbackFunc = "fn_CallBack_Search";
	// 서버 호출
	jsp_transaction(this, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, false, false, true);
}
