<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[요청자의 사번을 이용한 업장 코드 검색]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT SABUN, UPJANG
  FROM SCC_USERINFO
 WHERE SABUN = #SABUN#	]]></statement>
	<input default-name="ds_ReqSabunUpjang">
	</input>
	<output default-name="ds_ReqSabunUpjang">
	</output>
</query>
