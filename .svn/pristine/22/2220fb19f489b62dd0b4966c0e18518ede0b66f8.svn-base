function fn_changeAmt()
{
	var sumGiftAmt=0;
	var sumUseAmt=0;
	var sumCommAmt=0;
	var sumCommVatAmt=0;
	var sumBillingAmt=0;
	
	for(i=0; i < ds_list.rowcount; i++){
		if(ds_list.getColumn(i, "chk")=="1"){
			sumGiftAmt = sumGiftAmt+ ds_list.getColumn(i, "giftAmt");
			sumUseAmt = sumUseAmt+ ds_list.getColumn(i, "useAmt");
			sumCommAmt = sumCommAmt+ ds_list.getColumn(i, "commAmt");
			sumCommVatAmt = sumCommVatAmt+ ds_list.getColumn(i, "commVatAmt");
			sumBillingAmt = sumGiftAmt - sumCommAmt - sumCommVatAmt;
		}
	}
	msk_sumGiftAmt.value = sumGiftAmt;
	msk_sumUseAmt.value = sumUseAmt;
	msk_sumCommAmt.value = sumCommAmt;
	msk_sumCommVatAmt.value = sumCommVatAmt;
	
	if(ds_giftKindMst.getColumn(ds_giftKindMst.rowposition, "arTypeCd") == "002"){
		msk_sumBillingAmt.value = sumBillingAmt;
	}else{
		msk_sumBillingAmt.value = sumUseAmt;
	}
}

