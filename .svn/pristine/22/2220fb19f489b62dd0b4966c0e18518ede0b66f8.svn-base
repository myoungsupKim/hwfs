	// 타 칼럼값보다 작은지 비교           - comparemin:Column6(Dataset 칼럼ID):종료일(비교대상의 메시지 치환할 명칭)
	// 타 칼럼값과 같은 값인지 비교        - equalto:Column5(Dataset 칼럼ID):금액1(비교대상의 메시지 치환할 명칭)
	// 사이의 값인지 비교                   - range:40(from 숫자):100(to 숫자)
	// 코드값이 목록내의 값인지 비교       - code:1(목록 숫자):2(목록 숫자):3(목록 숫자)
	// 이메일 여부 체크(함수 체크시 예제)  - isEmail:true(체크여부 true/false)
	
	// 오류시 focus 지정                      - focus:cal_toDate(Componet ID로 form에서부터의 경로)
	
	//수수료 금액 = 공급가액 + 부가세

	if(msk_sumCommAmt.value != msk_supplyAmt.value){
		alert("수수료 공급가액과 수수료 부가세 정보의 공급가액과 같아야 합니다.");
		return;
	}
	if(msk_sumCommVatAmt.value != msk_vatAmt.value){
		alert("수수료 부가세 금액과 수수료 부가세 정보의 부가세 금액과 같아야 합니다.");
		return;
	}
	if(ds_search.getColumn(0, "arAmt")== "0"){
		alert("청구 금액을 선택해 주시기 바랍니다.");
		return;
	}
	if(gfn_isNull(ds_search.getColumn(0, "vatCd"))){
		alert("부가세 구분을 선택해 주시기 바랍니다.");
		return;
	}
	
	// 저장시 Validation Check	
	if (gfn_validCheck(ds_search, ds_listValid) == false)	return;

	if(!confirm("저장하시겠습니까?")){
		return false;
	}

	//ds_list 의 일부정보 셋팅하자
	ds_search.setColumn(0,"taxBillNum", edt_taxBillNum.value);  //승인번호
	
	ds_search.setColumn(0,"custCd", ds_list.getColumn(0,"custCd"));              //거래처코드
	ds_search.setColumn(0,"commAcctCd", ds_list.getColumn(0,"commAcctCd"));      //수수료계정
	ds_search.setColumn(0,"differAcctCd", ds_list.getColumn(0,"differAmtAcct")); //차이금액계정
	ds_search.setColumn(0,"vatAcctCd", ds_list.getColumn(0,"vatAcctCd"));        //부가세계정코드
	
    var strSvcID    = "save";
    var strURL      = "sm/sar/giftBillingPop/saveList.xdo";
    var strInDs     = "ds_list=ds_list:A ds_search=ds_search";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

