 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	if(p_billNo != ""){
		o_billNo = p_billNo;
	}else{
		o_billNo = "";
	}
	
	// 초기값 셋팅
	ds_search.addRow();
	fn_init();
	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";

	//달력 초기화
	cal_occurDateFr.fn_initMonthCal(this, null, null, ds_search, "occurDateFr");
	cal_occurDateTo.fn_initMonthCal(this, null, null, ds_search, "occurDateTo");	
	cal_occurDate.fn_initMonthCal(this, fv_sToday, null, ds_search, "occurDate");
	cal_taxShetDate.fn_initMonthCal(this, fv_sToday, null, ds_search, "taxShetDate");
	fn_giftKidMst();
	
	if(giftBillingNum != ""){	
		ds_search.copyRow(0, opener.ds_list, p_billNo);
		
        //enable 처리
        MuSearchComponent01.enable = false;
        TmSearchComponent01.enable = false;
        UpjangSearchComponent01.enable = false;
        
        MuSearchComponent01.setMuCode(ds_search.getColumn(0,"muCd"));
        cal_occurDateFr.enable = false;
        cal_occurDateTo.enable = false;
        cbo_giftKindCd.enable  = false;
        
        //상품권청구내역
        cal_occurDate.enable   = false;
        edt_ccCdSch.enable = false;
        btn_ccCdSch.enable = false;
        
        //수수료 부분 제어
		cbo_vatCd.enable=false;
		msk_supplyAmt.enable=false;
		msk_vatAmt.enable=false;
		btn_invoice.enable=false;        
		
		btn_billing.visible = false;
		btn_billingCancle.visible = true;
	}else{
		ds_search.setColumn(0,"giftBillingNum","");
		SCCombo.bind(this, "ComCode", "cbo_giftKindCd", ds_giftType, {groupCode:"GIFT_TYPE", useYn:"Y", defVal:ds_search.getColumn(0,"giftKindCd")}, SCCombo.CHOOSE);
		SCPopup.bind(this, "CcCode", 	"edt_ccNmSch", 	"btn_ccCdSch", 		null, {buCd:"2000"}, {after:"fn_popCcCdSchAfter"}); 	//CC코드팝업(입력)	

		SCCombo.bind(this, "ComCode", "cbo_vatCd", null, {groupCode:"TAX_CD", useYn:"Y", attr01:"Y"}, SCCombo.CHOOSE);						//부가세구분
	}
		
	if(giftBillingNum != ""){
		fn_search();
	} else {
	    fn_clearSearch();
	}
}
