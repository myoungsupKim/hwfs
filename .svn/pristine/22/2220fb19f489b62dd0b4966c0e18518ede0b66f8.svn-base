/*----------------------------------------------------------------------------------------------
 * 전자세금계산서 버튼 클릭 함수
 *---------------------------------------------------------------------------------------------*/
function btn_invoice_onclick(obj:Button,  e:ClickEventInfo)
{
	var fileDialog = new FileDialog();
	var vfile = fileDialog.open("FileOpen", FileDialog.LOAD);
	vfile.open(VirtualFile.openRead | VirtualFile.openText);
	
	var xml = vfile.read(0, "utf-8");
	vfile.close();
	
	//Xml Read & Parsing
	var domDoc;
	var domPar = new DomParser;
	domDoc = domPar.parseFromString(xml);
	
	//DomDocument Read
	getChildNode(domDoc);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : DomDocument Read(재귀함수)
 *---------------------------------------------------------------------------------------------*/
function getChildNode(pElement){

	if(pElement.childNodes.length == 0){
		return;
	}
 
 	var domElemenet;

	for(var i=0; i<pElement.childNodes.length; i++){
		domElemenet = pElement.childNodes[i];
		
		//Total 공급가액 / 세액
		if(domElemenet.nodeName.trim() == "ChargeTotalAmount"){
			ds_search.setColumn(0, "supplyAmt", domElemenet.textContent);
			//msk_supplyAmt.value = domElemenet.textContent;
		}
		if(domElemenet.nodeName.trim() == "TaxTotalAmount"){
			ds_search.setColumn(0, "vatAmt", domElemenet.textContent);
			//msk_vatAmt.value = domElemenet.textContent;
		}
		//승인정보
		if(pElement.nodeName.trim() == "TaxInvoiceDocument"){
			if(domElemenet.nodeName.trim() == "IssueID"){
				ds_search.setColumn(0, "taxBillNum", domElemenet.textContent);
			}
		}
		
		//하위 노드 존재시 함수 호출
		if(domElemenet.childNodes.length > 0){
			getChildNode(domElemenet);
		}
	}
}

