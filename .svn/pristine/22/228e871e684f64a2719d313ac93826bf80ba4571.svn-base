		var vRtn = gfn_dialog("CCS00010P"			// dialogId
				   , "U_CCS::CCS00010P.xfdl"		// Url
				   , {sParam:arrParam}					// 배열
				   , null, null, null, null, null, false, true, -1);
		
		//var vRtn = gfn_dialog("", "U_CCS::CCS00010P.xfdl", sParam);
		ds_list.setColumn(e.row, "IMGE_CNT", vRtn);
	}
}
