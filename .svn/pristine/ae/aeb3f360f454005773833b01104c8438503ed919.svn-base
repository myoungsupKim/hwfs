function tab_authMapping_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{
	if (e.postindex == 2 || e.postindex == 3)
	{
		sta_bu.visible = false;
		cbo_bu.visible = false;
		sta_mu.visible = false;
		cbo_mu.visible = false;
		sta_tm.visible = false;
		edt_tm.visible = false;
		btn_tm.visible = false;
	}
	else
	{
		sta_bu.visible = true;
		cbo_bu.visible = true;
		sta_mu.visible = true;
		cbo_mu.visible = true;
		sta_tm.visible = true;
		edt_tm.visible = true;
		btn_tm.visible = true;
	}
}

