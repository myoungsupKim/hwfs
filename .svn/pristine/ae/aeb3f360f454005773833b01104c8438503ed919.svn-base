function fn_searchAuthDeptAuthList()
{
	ds_search.setColumn(0, "useCd", "DEPT");

	if (gfn_dsIsUpdated(ds_authDept_CcMapList) == true) 
	{
		var retValue = gfn_confirm("confirm.before.search");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
		if (retValue == false) return;
	}
	// 조회시 Validation Check
	//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(fv_grd_authDept_list01);

    var strSvcID    = "searchAuthDeptAuthList";
    var strURL      = "sc/sca/authMappingMgmt/selectAuthList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_authDept_authList=ds_authList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

