/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		//권한별 CC :: 권한목록 조회
        case "searchAuthDeptAuthList" :
			ds_authDept_UnCcMapList.clearData();
			ds_authDept_CcMapList.clearData();
			fn_authDept_isChange();
            break;
        //권한별 CC :: 부서목록(미등록,등록)
        case "searchAuthDeptList" :
			fn_authDept_isChange();
			break;
        // 권한별 CC :: 저장
        case "saveAuthDeptAuthList" :
			fn_authDept_isChange();
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;
		//CC별 권한관리 :: CC목록
		case "searchDeptAuthCcList" :
			ds_deptAuth_UnAuthMapList.clearData();
			ds_deptAuth_AuthMapList.clearData();
			fn_deptAuth_isChange();
			break;
        //CC별 권한관리 :: 미등록, 등록 권한목록 조회
        case "searchDeptAuthList" :
			fn_deptAuth_isChange();
			break;
        // CC별 권한관리 :: 저장
        case "saveDeptAuthAuthList" :
			fn_deptAuth_isChange();
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			break;
		
		//권한별 사용자군 :: 권한목록조회
		case "searchAuthCustAuthList" :
			ds_authCust_UnCustMapList.clearData();
			ds_authCust_CustMapList.clearData();
			fn_authCust_isChange();
			break;
		//권한별 사용자군 :: 권한목록(등록/미등록)
		case "searchAuthCustList" :
			fn_authCust_isChange();
			break;
        // 권한별 사용자군 :: 저장
        case "saveAuthCustAuthList" :
			fn_authCust_isChange();
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;
		// 사용자군 권한관리 :: 사용자군조회
		case "searchAuthCustUserGroupList" :
			ds_custAuth_UnAuthMapList.clearData();
			ds_custAuth_AuthMapList.clearData();
			fn_custAuth_isChange();
			break;
        //사용자군별 권한관리 :: 미등록, 등록 권한목록 조회
        case "searchCustAuthList" :
			fn_custAuth_isChange();
			break;
        // 권한별 사용자군 :: 저장
        case "saveCustAuthAuthList" :
			fn_custAuth_isChange();
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;
    }
}


