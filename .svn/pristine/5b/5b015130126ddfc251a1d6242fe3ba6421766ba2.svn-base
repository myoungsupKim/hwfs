<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[업장레시피조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT RECIPE_CD
     , RECIPE_NAME
  FROM FSR_UPJANG_RECIPE_MST
 WHERE UPJANG = #UPJANG#
   AND RECIPE_CD = #RECIPE_CD#
    </statement>
