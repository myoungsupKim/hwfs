function fn_parentFunctionCall(obj, e, prtGubun)
{
	if (gfn_isNull(opener)) {
		gfn_alert("비 정상적인 호출이 발생 하였습니다.\n본 창을 다시 실행시켜 주시기 바랍니다.");
		this.close();
	}
	
	var objName = obj.name;
	// 출고증발행 종류 선택 후 값을 넘겨준다. - 출고증발생 / 출고증 Fax발송
	if (objName == "btn_releasePublish" || objName == "btn_releaseFaxTrans") {
		eval("opener."+objName+"_onclick(obj, e, "+prtGubun+");");
	} else {
		eval("opener."+objName+"_onclick(obj, e);");
	}
}
 
