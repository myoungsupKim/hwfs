/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";


/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_sToday = gfn_today();
var fv_funeralSaleDateSumColInfo;		// ds_funeralSaleDateSum 데이터셋 컬럼정보
var fv_funeralSaleMonthSumColInfo;		// ds_funeralSaleMonthSum 데이터셋 컬럼정보
var fv_funeralSaleYearSumColInfo;	// ds_funeralSaleYearSum 데이터셋 컬럼정보

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	//SCCombo.bind(this, "ComCode", "grd_deadGuest.funeralParlor", ds_parlorType, {groupCode:"FUNERAL_PARLOR_TYPE", useYn:"Y", defVal:"004"});	// 빈소,호실
	
	ds_search.clearData();
	var nRow= ds_search.addRow();
	ds_search.setColumn(nRow, "muCd", g_Mu_Cd);
	ds_search.setColumn(nRow, "muNm", g_Mu_Nm);
	ds_search.setColumn(nRow, "tmCd", g_Tm_Cd);
	ds_search.setColumn(nRow, "tmNm", g_Tm_Nm);
	ds_search.setColumn(nRow, "upjangCd", g_Upjang);
	ds_search.setColumn(nRow, "upjangNm", g_UpjangNm);
	ds_search.setColumn(nRow, "funeralHallCd", g_Upjang);
	
	// 입관일 오늘 날짜로 설정
	cal_saleFrom.fn_initMonthCal(this, fv_sToday, "", ds_search, "saleoutFrom");
	cal_saleTo.fn_initMonthCal(this, fv_sToday, "", ds_search, "saleoutTo");
	
	rdo_searchType.value = "01";	// 빈소별
	
	// 급식, 식재, 외식, 구매 사업부는 팀, 업장 변경 불가.
	if((gfn_indexOf(ds_search.getColumn(0,"muNm"), "FS", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "식재", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "외식", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "구매", 0) >= 0)) {
		
		edt_upjangNm.enable = false;
		
	// FC사업지원본부는 사업부/팀/업장 변경 가능	
	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FC", 0) >= 0) {
		edt_upjangNm.enable = true;
	}
}



/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 부서 세팅 
 *---------------------------------------------------------------------------------------------*/
//MU가 변경될 때에 대한 이벤트
function fn_muItemChangedEvent(obj, evt)
{
    edt_tmNm.setTmCode("");
    edt_upjangNm.setUpjangCode("");
        
    ds_search.setColumn(0,"muCd", edt_muNm.getMuCode());
    ds_search.setColumn(0,"muNm", edt_muNm.getMuName());
}

//TM이 변경될 때에 대한 이벤트
function fn_tmChangedEvent(obj, data)
{
    edt_upjangNm.setUpjangCode("");   
    ds_search.setColumn(0,"tmCd", edt_tmNm.getTmCode());
    ds_search.setColumn(0,"tmNm", edt_tmNm.getTmName());
}

//upjang이 변경될 때에 대한 이벤트
function fn_upjangChangedEvent(obj, data) {
    ds_search.setColumn(0,"upjangCd", edt_upjangNm.getUpjangCode());
    ds_search.setColumn(0,"upjangNm", edt_upjangNm.getUpjangName());
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_search()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 고인 목록 조회
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_search()
{
	if(rdo_searchType.value == "01") {  // 빈소별 조회
			// 조회시 Validation Check
			//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

			// Grid sort 표시 초기화(필수)
			gfn_clearSortMark(tab_funeralSaleSum.tabpage1.grd_saleDateSum);
			gfn_clearSortMark(tab_funeralSaleSum.tabpage2.grd_saleMonthSum);
			gfn_clearSortMark(tab_funeralSaleSum.tabpage3.grd_saleYearSum);
			
			//서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
			fv_funeralSaleDateSumColInfo  = gfn_getDatasetInfo(ds_funeralSaleDateSum);
			fv_funeralSaleMonthSumColInfo = gfn_getDatasetInfo(ds_funeralSaleMonthSum);
			fv_funeralSaleYearSumColInfo  = gfn_getDatasetInfo(ds_funeralSaleYearSum);
			
			// 조회조건 Param
			ds_search.setColumn(0, "funeralHallCd", ds_search.getColumn(0, "upjangCd"));
			ds_search.setColumn(0, "saleoutFrom", cal_saleFrom.mae_cal.value);
			ds_search.setColumn(0, "saleoutTo", cal_saleTo.mae_cal.value);
			ds_search.setColumn(0, "searchType", rdo_searchType.value);
		//	ds_search.setColumn(0, "maskAuth", SCAuth.getMaskAuth(this));
			
			var strSvcID    = "searchFuneralSaleSumList";
			var strURL      = "fs/fsf/funeralSaleSum/selectFuneralSaleSumList.xdo";
			var strInDs     = "ds_search=ds_search:A";
			var strOutDs    = "ds_funeralSaleDateSum=ds_funeralSaleDateSum ds_funeralSaleMonthSum=ds_funeralSaleMonthSum ds_funeralSaleYearSum=ds_funeralSaleYearSum";
			var strArg      = "";
			var strCallback = "fn_callBack";
			var bAsync      = true;
			
			gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
		
	}else{  // 품목별 조회
		fn_searchItem();
	}
}		
/*
 ******************************************************************************************
 * 함  수  명   : fn_Close()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 단기 버튼 이벤트
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_Close()
{
	this.close();
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID) {
		case "searchFuneralSaleSumList" :
            if(ds_funeralSaleDateSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralSaleDateSum, fv_funeralSaleDateSumColInfo);
			}
			if(ds_funeralSaleMonthSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralSaleMonthSum, fv_funeralSaleMonthSumColInfo);
			}
			if(ds_funeralSaleYearSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralSaleYearSum, fv_funeralSaleYearSumColInfo);
			}
			break;
			
		case "searchFuneralSaleSumItemList" :
            if(ds_funeralSaleDateSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralSaleDateSum, fv_funeralSaleDateSumColInfo);
			}
			if(ds_funeralSaleMonthSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralSaleMonthSum, fv_funeralSaleMonthSumColInfo);
			}
			if(ds_funeralSaleYearSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralSaleYearSum, fv_funeralSaleYearSumColInfo);
			}
			break;	
    }
}


/************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역 (필수)
************************************************************************************************/
/*
 ******************************************************************************************
 * 함  수  명   : fn_searchEnter()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 조회조건 enter 입력 이벤트
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_searchEnter(obj, e:KeyEventInfo)
{	
	if(e.keycode==13){
		fn_search();
	}
}

/*
 ******************************************************************************************
 * 함  수  명   : rdo_searchType_onitemchanged()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 간단/상세 변경시 Grid 설정 이벤트
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function rdo_searchType_onitemchanged(obj:Radio, e:ItemChangeEventInfo)
{
	// 빈소별
	if(e.postvalue == "01") {
		
		tab_funeralSaleSum.tabpage1.grd_saleDateSum.setCellProperty("Head", 2, "text", "빈소");
		tab_funeralSaleSum.tabpage2.grd_saleMonthSum.setCellProperty("Head", 2, "text", "빈소");
		tab_funeralSaleSum.tabpage3.grd_saleYearSum.setCellProperty("Head", 2, "text", "빈소");
		
		tab_funeralSaleSum.tabpage1.grd_saleDateSum.setCellProperty("Body", 2, "text", "bind:funeralParlorNm");
		tab_funeralSaleSum.tabpage2.grd_saleMonthSum.setCellProperty("Body", 2, "text", "bind:funeralParlorNm");
		tab_funeralSaleSum.tabpage3.grd_saleYearSum.setCellProperty("Body", 2, "text", "bind:funeralParlorNm");
		
		fn_search();
		
	// 품목별
	}else {
	
		tab_funeralSaleSum.tabpage1.grd_saleDateSum.setCellProperty("Head", 2, "text", "품목");
		tab_funeralSaleSum.tabpage2.grd_saleMonthSum.setCellProperty("Head", 2, "text", "품목");
		tab_funeralSaleSum.tabpage3.grd_saleYearSum.setCellProperty("Head", 2, "text", "품목");
		
		tab_funeralSaleSum.tabpage1.grd_saleDateSum.setCellProperty("Body", 2, "text", "bind:detailGoodsNm");
		tab_funeralSaleSum.tabpage2.grd_saleMonthSum.setCellProperty("Body", 2, "text", "bind:detailGoodsNm");
		tab_funeralSaleSum.tabpage3.grd_saleYearSum.setCellProperty("Body", 2, "text", "bind:detailGoodsNm");
		
		fn_searchItem();
	}
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_searchItem()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 매출집계  품목별로 조회
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_searchItem()
{
	// 조회시 Validation Check
	//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(tab_funeralSaleSum.tabpage1.grd_saleDateSum);
    gfn_clearSortMark(tab_funeralSaleSum.tabpage2.grd_saleMonthSum);
    gfn_clearSortMark(tab_funeralSaleSum.tabpage3.grd_saleYearSum);
    
    //서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
    fv_funeralSaleDateSumColInfo  = gfn_getDatasetInfo(ds_funeralSaleDateSum);
	fv_funeralSaleMonthSumColInfo = gfn_getDatasetInfo(ds_funeralSaleMonthSum);
	fv_funeralSaleYearSumColInfo  = gfn_getDatasetInfo(ds_funeralSaleYearSum);
    
    // 조회조건 Param
    ds_search.setColumn(0, "funeralHallCd", ds_search.getColumn(0, "upjangCd"));
	ds_search.setColumn(0, "saleoutFrom", cal_saleFrom.mae_cal.value);
	ds_search.setColumn(0, "saleoutTo", cal_saleTo.mae_cal.value);
	ds_search.setColumn(0, "searchType", rdo_searchType.value);
//	ds_search.setColumn(0, "maskAuth", SCAuth.getMaskAuth(this));
	
	var strSvcID    = "searchFuneralSaleSumItemList";
    var strURL      = "fs/fsf/funeralSaleSum/selectFuneralSaleSumItemList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_funeralSaleDateSum=ds_funeralSaleDateSum ds_funeralSaleMonthSum=ds_funeralSaleMonthSum ds_funeralSaleYearSum=ds_funeralSaleYearSum";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

function tab_funeralSaleSum_onchanged(obj:Tab, e:TabIndexChangeEventInfo)
{ 
	if(e.postindex == 1){
		tab_funeralSaleSum.tabpage2.grd_saleMonthSum.setCellProperty("Head", 1, "text", "판매월");
		tab_funeralSaleSum.tabpage2.grd_saleMonthSum.setCellProperty("Body", 1, "text", "bind:saleDate");
	}else if(e.postindex == 2){
		tab_funeralSaleSum.tabpage3.grd_saleYearSum.setCellProperty("Head", 1, "text", "판매년도");
		tab_funeralSaleSum.tabpage3.grd_saleYearSum.setCellProperty("Body", 1, "text", "bind:saleDate");
	}
}
