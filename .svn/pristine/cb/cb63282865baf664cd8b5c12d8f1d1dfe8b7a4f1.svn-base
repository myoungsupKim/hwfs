function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(obj, false);

	//var str_Gubun;
	
	fsp_init(this);

   //사업장 권한 셋팅#############################################################################
	fn_SetAuth("edt_Tm","edtBtn_Tm","edt_Upjang", "edtBtn_Upjang");
    //사업장 권한 셋팅############################################################################# 

	
	try
	{
		//chk_TM_CD.Value = strchk_TM_CD; 
		//chk_UPJANG.Value = strchk_UPJANG; 
		//chk_ACCT_TYPE.Value = strchk_ACCT_TYPE; 
		//chk_ACCT_CD.Value = strchk_ACCT_CD; 
		
		cbo_Acct_type.value = strAcct_type;
		rdo_Acct.value = strAcct;
		
		edt_Tm.userdata = strTM_CD;
		edt_Tm.value = strTM_NAME;
			
		edt_Upjang.userdata = strUPJANG;
		edt_Upjang.value = strUPJANG_NAME;
		
		edt_Acct1.userdata = strACCT_CD;
		edt_Acct1.value = strACCT_NAME;
	  
		
		fsp_clear(this);
		// 서버에서 조회할 정보 추가 
		fsp_addSearch(this, "ma/mac:MAC_CD_DTL_S001");
		// 서버 호출 
		http.Sync = true;
		fsp_callService(this, "", "", "", "ds_cd_out=ds_cd_out","MST_CD='C04'","");
		http.Sync = false;
		
		rdo_Data_Class.index = 0;
		
		fsp_clear(this);
		// 실적결산월 가져옴.
		me_Date1.value = fn_searchEisClose(ds_close, "MAM") ;
		me_Date2.value = fn_searchEisClose(ds_close, "MAM") ;
		
		chk_TM_CD.value 		= false;
		chk_UPJANG.value 		= false;
		chk_ACCT_TYPE.value 	= true;
		chk_ACCT_CD.value 		= true;
		
		grd_Main.setFormatColProperty(0,"size",20);
		grd_Main.setFormatColProperty(1,"size",0);
		grd_Main.setFormatColProperty(2,"size",0);
		grd_Main.setFormatColProperty(3,"size",90);
		grd_Main.setFormatColProperty(4,"size",80);
		grd_Main.setFormatColProperty(5,"size",170);
	}
	catch(e)
	{
		//Login 팀 정보 
		edt_Tm.userdata = g_Tm_Cd;
		edt_Tm.value = g_Tm_Nm;

		// Login 사업장 정보
		edt_Upjang.userdata = g_Upjang;
		edt_Upjang.value = g_UpjangNm;

		fsp_clear(this);
		// 서버에서 조회할 정보 추가 
		fsp_addSearch(this, "ma/mac:MAC_CD_DTL_S001");
		// 서버 호출 
		http.Sync = true;
		fsp_callService(this, "", "", "", "ds_cd_out=ds_cd_out","MST_CD='C04'","");
		http.Sync = false;

		rdo_Data_Class.index = 0;


		fsp_clear(this);
		// 실적결산월 가져옴.
		me_Date1.value = fn_searchEisClose(ds_close, "MAM") ;
		me_Date2.value = fn_searchEisClose(ds_close, "MAM") ;

		chk_TM_CD.value 		= "N";
		chk_UPJANG.value 		= "N";
		chk_ACCT_TYPE.value 	= "Y";
		chk_ACCT_CD.value 		= "Y";

		grd_Main.setFormatColProperty(0,"size",20);
		grd_Main.setFormatColProperty(1,"size",0);
		grd_Main.setFormatColProperty(2,"size",0);
		grd_Main.setFormatColProperty(3,"size",90);
		grd_Main.setFormatColProperty(4,"size",80);
		grd_Main.setFormatColProperty(5,"size",170);
		
		fv_popup = false;
	}
}
