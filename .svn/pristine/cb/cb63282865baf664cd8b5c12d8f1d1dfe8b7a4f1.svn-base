/*
 ******************************************************************************************
 * 함  수  명   : fn_Search()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 조회버튼 입력 시 처리되는 작업
 * 작  성  자   : 
 * 작성  일자   : 
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_search()
{
   if (me_Date2.value < me_Date1.value) { 
	  g_Message("EE","TO-연월이 FROM-연월 이전일수 없습니다.");
	   me_Date2.value = me_Date1.value;
	 }  
   
   // 계정 조회구간 체크
   if ((gfn_length(gfn_trim(edt_Acct1.value))!= 0 ) && (gfn_length(gfn_trim(edt_Acct2.value))== 0 )) {
	   edt_Acct2.userdata = edt_Acct1.userdata;
	   edt_Acct2.value = edt_Acct1.value;
	 } 
	
	if ((gfn_length(gfn_trim(edt_Acct2.value))!= 0 ) && (gfn_length(gfn_trim(edt_Acct1.value))== 0 )) {
	   edt_Acct1.userdata = edt_Acct2.userdata;
	   edt_Acct1.value = edt_Acct2.value;
	 } 
	 
	var v_ACCT_GUBUN = (chk_ACCT_CD.value+rdo_Acct.value);
	var v_ACCT_LVL;
	 
	if (gfn_length(gfn_trim(edt_Acct1.value))!= 0 ) {
        v_ACCT_LVL = rdo_Acct.value ;
       // ALERT(v_ACCT_LVL);
    }
    else {
        v_ACCT_LVL = ""; 
    }  
 
    ds_all_out.clearData();
    
    fsp_clear(this);
    
    if ((rdo_Data_Class.text == "전월") || (rdo_Data_Class.text == "전년")){
        
		fsp_addSearch(this, "ma/mam:MAM20060V_S002");
		
		http.Sync = true;
