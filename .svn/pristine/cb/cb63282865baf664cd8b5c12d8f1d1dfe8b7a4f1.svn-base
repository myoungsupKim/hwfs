function grd_Main_OnCellDblClick(obj:Grid, e:GridClickEventInfo)
{
	var strPopupFlag = "P";	// popup flag
	var arg = {strPopupFlag:strPopupFlag,
			   strDate:gfn_subStr(me_Date1.value,0,4),
			   strData_Class:rdo_Data_Class.value,
			   strAcct:rdo_Acct.value,
			   strAcct_type:cbo_Acct_type.value};
			   
	  var v_PARA ;
	  
	  
	  if (chk_TM_CD.value == "Y")
	  {
		arg["strchk_TM_CD"] = chk_TM_CD.value;
		arg["strTM_CD"] 	= ds_all_out.getColumn(e.row,"TM_CD");
		arg["strTM_NAME"]   = ds_all_out.getColumn(e.row,"TM_NAME");
	  }
	  else
	  {
		arg["strchk_TM_CD"] = chk_TM_CD.value;
		arg["strTM_CD"] 	= edt_Tm.userdata;
		arg["strTM_NAME"]   = edt_Tm.value;
	  }	  
	  
	  if (chk_UPJANG.value == "Y")
	  {
		arg["strchk_UPJANG"] 	= chk_UPJANG.value;
		arg["strUPJANG"] 	 	= ds_all_out.getColumn(e.row,"UPJANG");
		arg["strUPJANG_NAME"]   = ds_all_out.getColumn(e.row, "UPJANG_NAME");
	  }
	  else
	  {
		arg["strchk_UPJANG"] 	= chk_UPJANG.value;
		arg["strUPJANG"] 	 	= edt_Upjang.userdata;
		arg["strUPJANG_NAME"]   = edt_Upjang.value;
	  }
	  
	  if (chk_ACCT_TYPE.value == "Y")
	  {
		arg["strchk_ACCT_TYPE"]   = chk_ACCT_TYPE.value;
		arg["strACCT_TYPE"] 	  = cbo_Acct_type.value;
	  }
	  else
	  {
		arg["strchk_ACCT_TYPE"]   = chk_ACCT_TYPE.value;		
	  }	  
	  
	  if (chk_ACCT_CD.value == "Y")
	  {
		arg["strchk_ACCT_CD"] = chk_ACCT_TYPE.value;
		arg["strACCT_CD"]     = ds_all_out.getColumn(e.row, "ACCT_CD");
		arg["strACCT_NAME"]   = ds_all_out.getColumn(e.row, "ACCT_NAME");
	  }
	  else
	  {
		arg["strchk_ACCT_CD"]   = chk_ACCT_TYPE.value;
	  }
	
    
    // 경비추이 보고서 호출  
    if (rdo_Data_Class.value == "M") {
        g_Message("EE",	"전월대비는 추이보고서가 없습니다. 계획대비로 변경합니다.");
        rdo_Data_Class.value = 'P';
     }
     
     gfn_dialog("", "U_MAM::MAM20080V.xfdl", arg);	  
    
  
}

