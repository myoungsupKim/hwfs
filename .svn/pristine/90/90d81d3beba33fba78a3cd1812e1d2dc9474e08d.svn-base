/******************************************************************************************
 * 함  수  명   : btn_addMenu1Joint_onclick(obj:Button,  e:ClickEventInfo)
 * 기      능   : 개별식단 배정
 * 작  성  자   : 김민수
 * 작성  일자   : 2015-03-13  
 ******************************************************************************************/
function btn_addMenu1Joint_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_menuJointList.rowcount == 0) return;
	
	for(var i=0; i<ds_menuJointList.getRowCount(); i++)
	{
		if(ds_menuJointList.getSelect(i) == true)
		{
			var nRow = i;
			ds_menu1JointList.insertRow(0);
			ds_menu1JointList.copyRow(0, ds_menuJointList, nRow);
			ds_menu1JointList.setColumn(0, "JOINT_CLASS", "01");
			ds_menu1JointList.setColumn(0, "CDATE", gfn_today());
			ds_menu1JointList.setColumn(0, "JOINT_WEEKSEQ", 1);
		}
	}
	
	for(var i=ds_menuJointList.getRowCount(); i >= 0; i--)
	{
		if(ds_menuJointList.getSelect(i) == true)
		{
			ds_menuJointList.deleteRow(i);
		}
	}	
}

