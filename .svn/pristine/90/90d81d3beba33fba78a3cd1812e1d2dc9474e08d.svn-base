/******************************************************************************************
 * 함  수  명   : fn_search()
 * 기      능   : 조회
 * 작  성  자   : 김민수
 * 작성  일자   : 2015-02-02  
 ******************************************************************************************/
function fn_search() 
{
	if(gfn_isNull(ds_cond.getColumn(0, "JOINT_YYMM")))
	{
		g_Message("EE", "공유년월을 입력해주세요.");
		cal_jointYymm.setFocus();
		return;
	}

	// 데이터 초기화
	ds_upjangList.clearData();
	ds_menu1JointList.clearData();
	ds_menu2JointList.clearData();
	ds_menu3JointList.clearData();
	ds_menu4JointList.clearData();

	fsp_clear(this);
	fsp_addSearch(this,"fs/fsm:FSM00600E_S001");
	fsp_callService(this,"","","ds_cond=ds_cond","ds_upjangList=ds_upjangList","","fn_callBackSearch");
}

function fn_callBackSearch() 
{
	ds_upjangList.filter("REG_YN=='N'");
	CopyF(ds_menuJointList,ds_upjangList);

	ds_upjangList.filter("JOINT_CLASS=='01'");
	CopyF(ds_menu1JointList,ds_upjangList);
	ds_upjangList.filter("JOINT_CLASS=='02'");
	CopyF(ds_menu2JointList,ds_upjangList);
	ds_upjangList.filter("JOINT_CLASS=='03'");
	CopyF(ds_menu3JointList,ds_upjangList);
	ds_upjangList.filter("JOINT_CLASS=='04'");
	CopyF(ds_menu4JointList,ds_upjangList);
}

/******************************************************************************************
 * 함  수  명   : fn_save(obj , e)
 * 기      능   : 저장
 * 작  성  자   : 김민수
 * 작성  일자   : 2015-02-02  
 ******************************************************************************************/
function fn_save() 
{
	// 개별공유 유효성 검사
	if (gfn_validCheck(ds_menu1JointList, ds_menu1JointListValid, true) == false) return;

	// 기간공유 유효성 검사
	if (gfn_validCheck(ds_menu2JointList, ds_menu2JointListValid, true) == false) return;

	// 오븐공유 유효성 검사
	if (gfn_validCheck(ds_menu3JointList, ds_menu3JointListValid, true) == false) return;

	// 신규레시피공유 유효성 검사
	if (gfn_validCheck(ds_menu4JointList, ds_menu4JointListValid, true) == false) return;

	if (!confirm("공유대상 사업장 정보를 저장하시겠습니까?", "확인", "default")) return;
	
	jsp_transaction(
		  this                                 // 해당 스크립트를 호출하는 Form 객체
		, "FSM00600E_T001"                     // 서비스 ID
		, "U_svc::fs/fsm/FSM00600E_T001.jsp"   // 서비스 URL
		, "ds_menu1JointList=ds_menu1JointList:U ds_menu2JointList=ds_menu2JointList:U ds_menu3JointList=ds_menu3JointList:U ds_menu4JointList=ds_menu4JointList:U"    // 서버로 전송할 DataSet 객체 Str
		, ""                                   // 서버로부터 전송 받을 DataSet 객체 String
		, ""                                   // 서버로 전송할 기타 Argument 정보 String
		, "fn_callBackSave"                    // 서버에서 처리가 완료된 후 Callback 받을 Function 명
		, true                                 // 에러 발생 시 Callback 함수 호출 여부
		, true                                 // 처리중입니다. 메시지 창 보여줄지 여부 설정하기
		, true                                 // 동기식으로 설정할지 여부
	);
}

function fn_callBackSave(errCode, errMsg) 
{
	if(errCode!=0)
	{
		g_Message("EE", "서비스 오류:시스템 관리자에게 문의하십시오!");
		return;
	}
	else
	{
		g_Message("02", "저장 되었습니다.");
	}
}

