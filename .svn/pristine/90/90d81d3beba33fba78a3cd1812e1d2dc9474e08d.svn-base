/******************************************************************************************
 * 함  수  명   : grd_menuJointList_oncelldblclick(obj:Grid, e:GridClickEventInfo)
 * 기      능   : 공유구분 더블클릭시 자동 등록
 * 작  성  자   : 김민수
 * 작성  일자   : 2015-03-13  
 ******************************************************************************************/
function grd_menuJointList_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	var nRow = e.row;
	
	// 공유이력 팝업창
	if(e.col < 3 || e.col == 8)
	{
		var v_arg = {};
		v_arg["fa_UPJANG"]       = gfn_trim(ds_menuJointList.getColumn(nRow, "UPJANG"));
		v_arg["fa_JOINT_YYMM"]   = gfn_trim(ds_menuJointList.getColumn(nRow, "JOINT_YYMM"));

		gfn_dialog("", "U_FSM::FSM00600P.xfdl", v_arg);		
	}
	// 개별식단
	else if(e.col == 3)
	{
		ds_menu1JointList.insertRow(0);
		ds_menu1JointList.copyRow(0, ds_menuJointList, nRow);
		ds_menu1JointList.setColumn(0, "JOINT_CLASS", "01");
		ds_menu1JointList.setColumn(0, "CDATE", gfn_today());
		ds_menuJointList.deleteRow(nRow);
	}
	// 기간식단
	else if(e.col == 4)
	{
		ds_menu2JointList.insertRow(0);
		ds_menu2JointList.copyRow(0, ds_menuJointList, nRow);
		ds_menu2JointList.setColumn(0, "JOINT_CLASS", "02");
		ds_menu2JointList.setColumn(0, "CDATE", gfn_today());
		ds_menuJointList.deleteRow(nRow);
	}
	// 오븐식단
	else if(e.col == 5)
	{
		// 오븐 보유여부 체크
		if(ds_menuJointList.getColumn(nRow, "OVEN_DUTY_YN") != 'Y')
		{
			g_Message("EE", "오븐 보유업장이 아닙니다.");
			return;
		}
	
		ds_menu3JointList.insertRow(0);
		ds_menu3JointList.copyRow(0, ds_menuJointList, nRow);
		ds_menu3JointList.setColumn(0, "JOINT_CLASS", "03");
		ds_menu3JointList.setColumn(0, "CDATE", gfn_today());
		ds_menuJointList.deleteRow(nRow);
	}
	// 레시피공유
	else if(e.col == 6)
	{
		ds_menu4JointList.insertRow(0);
		ds_menu4JointList.copyRow(0, ds_menuJointList, nRow);
		ds_menu4JointList.setColumn(0, "JOINT_CLASS", "04");
		ds_menu4JointList.setColumn(0, "CDATE", gfn_today());
		ds_menuJointList.deleteRow(nRow);
	}
}

