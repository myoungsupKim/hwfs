 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	//외부 화면에서 호출하는 경우
	var arrParam = gfn_getScreenParams();
	if (arrParam.length > 0 && (arrParam[0] == "U" || arrParam[0] == "D")) extCallParam = arrParam;
	
	//저장/삭제권한을 승인/승인취소 권한으로 이용한다.
	div_cmnBtn.btn_delete.text = "승인취소";
	div_cmnBtn.btn_save.text = "승인";
	div_cmnBtn.btn_delete.position.x -= 15;
	div_cmnBtn.btn_delete.position.width += 15;
	div_cmnBtn.btn_search.position.x -= 15;
	
	//조회조건 초기화
	var nRow = ds_search.addRow();
	
	//기간 설정
	cal_fromDate.fn_initMonthCal(this, gfn_today(), "", ds_search, "fromDate", "Y");
	cal_toDate.fn_initMonthCal(this, gfn_today(), "", ds_search, "toDate", "Y");
	spn_fromTime.value = "000000000";
	spn_toTime.value = "235959000";
	
	//로그구분, 시스템구분, 사용자구분, 소속부서의 COMBO에 바인딩한다.
	SCCombo.bind(this, "ComCode", "cbo_logClass", ds_logClass, {groupCode:"ACES_LOG_CLASS", useYn:"Y", set1:"A", defVal:""}, SCCombo.CHOOSE, "fn_triggerEvent");
	SCCombo.bind(this, "ComCode", "cbo_sysType", ds_sysType, {groupCode:"시스템구분", useYn:"Y", defVal:"firstrow"}, SCCombo.CHOOSE, "fn_triggerEvent");
	SCCombo.bind(this, "ComCode", "cbo_userType", ds_userType, {groupCode:"USER_TYPE", useYn:"Y", defVal:""}, SCCombo.CHOOSE, "fn_triggerEvent");
	SCPopup.bind(this, "CcCode", "edt_ccCd", "btn_ccCd", null, {muCd:""});
	
	//미승인으로 설정
	rdo_approv.value = 0;
	rdo_approv.updateToDataset ();
	fn_setApprovButton (rdo_approv.value);
	
	//작업건수 >=로 설정
	cbo_workCntType.index = 0;
}

