/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 조회
        case "search" :
            //trace(ds_list.saveXML());
            // 페이지 설정	: 페이지정보 DS, 조회 Function
            ds_pageInfo.setColumn(ds_pageInfo.rowposition, "totalCount", fv_totalCount);		// 전체 조회 대상 건수
			ObjPaging.fn_pageLoad("ds_pageInfo", "fn_searchPage");
            break;

		// 승인/승인취소
        case "approve" :
		case "approveCancel" :
			gfn_alert("msg.action.success");	// 정상적으로 처리 되었습니다.
			fn_searchPage ();
			break;
			
    }
}


/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 콤보박스(시스템 구분)에 따른 Trigger Event
 *             현재 선택된 시스템 구분에 해당하는 시스템ID를 조회하여 바인딩한다.
 *---------------------------------------------------------------------------------------------*/
function fn_triggerEvent(p)
{
	if (p.oid == "cbo_logClass")
	{
		//승인대상 로그 설정
		var approvLog = "";
		var isFirst = true;
		for (var i=0; i<ds_logClass.getRowCount(); i++) {
			if (gfn_isNull(ds_logClass.getColumn(i, "code"))) continue;
			if (isFirst) isFirst=false; else approvLog += ","; 
			approvLog += "'" + ds_logClass.getColumn(i, "code") + "'";
		}
		//alert (approvLog);
		ds_search.setColumn(ds_search.rowposition, "approvLog",   approvLog);
		bnlLogClassLoaded = true;
	}
	else if (p.oid == "cbo_sysType")
		bnlSysTypeLoaded = true;
	else if (p.oid == "cbo_userType")
		bnlUserTypeLoaded = true;
	
	//외부 화면에서 호출인 경우 처리
	if (bnlLogClassLoaded && bnlSysTypeLoaded && bnlUserTypeLoaded) fn_setExtCallParam (extCallParam);
}

/**
 * 검색조건 중 실행일자의 기간을 선정한다.
 * @param intType 기간타입으로 0:금일, 1:금주, 2:금월
 * @return none
 */
function fn_setSearchDate (intType)
{
	//검색일자 설정
	var strFrom = gfn_today();
	var strTo = strFrom;
	
	if (intType == 0) {			//금일
	}else if (intType == 1) {	//금주
		strFrom = gfn_getThisWeekBegin();
	}else if (intType == 2) {	//금월		
		strFrom = gfn_getThisMonthBegin();
	}
	cal_fromDate.fn_setValue (strFrom);
	cal_toDate.fn_setValue (strTo);
}

/**
 * 승인/승인취소 처리한다.
 * @param bApproval true는 승인, false는 승인취소
 * @return none
 */
function fn_approve (bApproval)
{
	//그리드에서 선택한 로우 값을 얻는다.
	var arrSelRows = SCDataset.getSelectedRows(ds_list, "chk");

	if(arrSelRows.length == 0) 
	{
		gfn_alert("msg.err.grid.noselect", "처리할 row");	// 선택된 항목이 없습니다.
		return;
	}

	//처리 대상인지를 확인 : 첫번째 건만 확인하면 된다.
	if (bApproval && !gfn_isNull(ds_list.getColumn(0, "approvDate"))) {
		alert ("승인 대상이 아닙니다. 이미 승인 되었습니다.");
		return;
	}else if (!bApproval && gfn_isNull(ds_list.getColumn(0, "approvDate"))) {
		alert ("승인취소 대상이 아닙니다. 승인되어 있지 않습니다.");
		return;
	}
	
	if (bApproval)	//승인
		var rtVal = gfn_confirm("confirm.before.approve");			// 선택된 자료를 승인처리 하시겠습니까?
	else
		var rtVal = gfn_confirm("confirm.before.approve.cancel");	// 선택된 자료를 승인취소처리 하시겠습니까?
		
	if (rtVal == true) 
	{	
		//처리할 데이터를 생성
		ds_approvList.clearData ();
		ds_approvList.enableevent = false;
		for (var i=0; i<arrSelRows.length; i++) {
			var row = ds_approvList.addRow ();
			ds_approvList.setColumn(row, "acesDtm", ds_list.getColumn(arrSelRows[i], "acesDtm"));
			ds_approvList.setColumn(row, "sysType", ds_list.getColumn(arrSelRows[i], "sysType"));
			ds_approvList.setColumn(row, "prgId",   ds_list.getColumn(arrSelRows[i], "prgId"));
		}
		ds_approvList.enableevent = true;
		//alert(ds_approvList.saveXML());
		
		if (bApproval) {	//승인
			var strSvcID    = "approve";
			var strURL      = "sc/scp/persAccessLog/approveList.xdo";
		}else{				//승인취소
			var strSvcID    = "approveCancel";
			var strURL      = "sc/scp/persAccessLog/approveCancelList.xdo";
		}
		var strInDs     = "ds_list=ds_approvList:U";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}

}

/*----------------------------------------------------------------------------------------------
 * 설명      : 승인/미승인에 따라 처리 버튼 활성화 한다.
 *---------------------------------------------------------------------------------------------*/
function fn_setApprovButton(value)
{
	if (value == 0) { //미승인인 경우는 승인 처리 가능토록 설정
		div_cmnBtn.btn_delete.enable = false;
		div_cmnBtn.btn_save.enable = true;
	} else {
		div_cmnBtn.btn_delete.enable = true;
		div_cmnBtn.btn_save.enable = false;
	}
}

/*-------------------------------------------------------------------------------------------------------------
 * 설명      : 메뉴 등록시 parameter를 등록하거나, 화면에서 해당 메뉴를 호출하는 경우 실행되는 함수 ==> 외부에서 호출하는 경우
 *-------------------------------------------------------------------------------------------------------------*/
function fn_getScreenParams(arrParam) 
{
	if (arrParam.length > 0 && (arrParam[0] == "U" || arrParam[0] == "D")) extCallParam = arrParam; else extCallParam = new Array();
    //trace("arrArg : " + arrParam);
    //for ( var i=0; i<arrParam.length; i++ ) {
    //    trace("arrParam[" + i + "] : " + arrParam[i]);
    //}
    
    fn_setExtCallParam (extCallParam);
 }

/*-------------------------------------------------------------------------------------------------------------
 * 설명      : 외부에서 화면 호출 시 초기 파라미터를 설정한다.
 *-------------------------------------------------------------------------------------------------------------*/
 function fn_setExtCallParam (arrParam)
 {
	if (arrParam.length == 0) return;

	//외부 호출 파라미터 설정
	//기간 설정
	cal_fromDate.fn_setValue(arrParam[1]);
	cal_toDate.fn_setValue(arrParam[2]);
	//ds_search.setColumn (ds_search.rowposition, "fromDate", arrParam[1]);
	//ds_search.setColumn (ds_search.rowposition, "fromDate", arrParam[2]);
	spn_fromTime.value = arrParam[3];
	spn_toTime.value = arrParam[4];

	//로그구분, 시스템구분, 사용자구분, 소속부서의 COMBO에 바인딩한다.
	ds_search.setColumn (ds_search.rowposition, "sysType", arrParam[5]);
	ds_search.setColumn (ds_search.rowposition, "userType", arrParam[6]);
	//ds_search.setColumn (ds_search.rowposition, "ccCd", arrParam[7]);
	edt_ccCd.userdata = arrParam[7];
	edt_ccCd.value = arrParam[8];
	//SCPopup.bind(this, "CcCode", "edt_ccCd", "btn_ccCd", arrParam[8], {muCd:arrParam[7]});
	ds_search.setColumn (ds_search.rowposition, "acesUser", arrParam[9]);
	edt_user.setUserName (arrParam[10]);

	//승인구분 설정
	rdo_approv.value = arrParam[11];
	rdo_approv.updateToDataset ();
	fn_setApprovButton (rdo_approv.value);
	
	//작업건수 >=로 설정
	cbo_workCntType.index = 0;
	
	ds_search.setColumn (ds_search.rowposition, "workCnt", "");
	
	cbo_logClass.value = "";
	
	//조회 처리
	fn_search();
 }
 
