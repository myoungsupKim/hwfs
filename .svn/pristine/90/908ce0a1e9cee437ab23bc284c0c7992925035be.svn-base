    <Script type="xscript4.0"><![CDATA[/*
  화 면 명 : 외부 발주처 등록 관리
  파 일 명 : BasClientMgmt.xfdl
  기    능 : 외부 발주처를 등록 관리 한다. 
              등록후 외부 발주 품목을 등록 할수 있다.

*/

/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";
 

/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_sToday = gfn_today();
var RetVal="";
 
/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_orderplceMst";
	ds_orderplceMst.clearData();
	
	// 권한설정
	edt_tm.applyAuthLevel(cbo_mu);
	edt_upjang.applyAuthLevel(edt_tm);
	edt_upjang.setForcedRoleSet("OutletUpjang");
	
	edt_intm.applyAuthLevel(cbo_inmu);
	edt_inupjang.applyAuthLevel(edt_intm);
	edt_inupjang.setForcedRoleSet("OutletUpjang");
	
	 
	edt_serch_VendorNm.value="";
	edt_serch_PrcgNm.value="";
	
	fn_search();

}
 
/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	 
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_orderplceList);
    ds_orderplceMst.clearData();

    var strSvcID    = "search";
    var strURL      = "rc/bms/basOrderplceMgmt/selectOrderplceList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_orderplceMst=ds_orderplceMst";
    var strArg      = "outletCd=" + gfn_nvl(edt_upjang.getUpjangCode(),"")
                    + " vendorNm=" + edt_serch_VendorNm.value + " prcgNm=" + edt_serch_PrcgNm.value  
                    + " maskAuth=" + SCAuth.getMaskAuth(this);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	

	if (gfn_validCheck(grd_orderplceList, ds_orderplceMstValid) == false)	return;
	var nRow = ds_orderplceMst.addRow();	//행추가
	
	 
	
	ds_orderplceMst.setColumn(nRow,"mu",gfn_nvl(cbo_mu.getMuCode(),""));	//사업부
	edt_intm.applyAuthLevel(cbo_inmu);
	
	ds_orderplceMst.setColumn(nRow,"tm",gfn_nvl(edt_tm.getTmCode(),""));	//팀
	edt_inupjang.applyAuthLevel(edt_intm);
	
	edt_inupjang.setForcedRoleSet("OutletUpjang");
	
	ds_orderplceMst.setColumn(nRow,"outletCd",gfn_nvl(edt_upjang.getUpjangCode(),""));	//영업장
	 
	ds_orderplceMst.setColumn(nRow, "addrInfo", cmp_addr.newData());					//주소처리을위해 해당 필드 초기화
	ds_orderplceMst.setColumn(nRow, "useYn", 1);
	
	ds_orderplceMst.setColumn(nRow,"tm",gfn_nvl(edt_tm.getTmCode(),""));	//팀
	ds_orderplceMst.setColumn(nRow,"tmNm",gfn_nvl(edt_tm.getTmName(),""));	//팀명
	ds_orderplceMst.setColumn(nRow,"outletCd",gfn_nvl(edt_upjang.getUpjangCode(),""));	//영업장
	ds_orderplceMst.setColumn(nRow,"outletNm",gfn_nvl(edt_upjang.getUpjangName(),""));	//영업장명
	
	
	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_orderplceMst.rowposition;
	
	if(ds_orderplceMst.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		var bIsProcRow = false;
		if (ds_orderplceMst.getRowType(nRow) == Dataset.ROWTYPE_INSERT)
		{
			ds_orderplceMst.deleteRow(nRow);
		} else
		{
			ds_orderplceMst.setColumn(nRow, "useYn", 0);
			bIsProcRow = true;
		}
		
		if (!bIsProcRow) return;
		
		sMsg = gfn_getMessage("msg.action.success", "");
		sMsg = sMsg += "\r\r처리된 결과를 저장하시겠습니까?";
		
		//실제 저장처리를 수행할 지를 알려준다.
		if (gfn_confirm(sMsg, "", "C"))
		{
			fn_save();
		}
		else
		{
			sMsg = "변경된 자료는 나중에 저장하시기 바랍니다.";
			gfn_alert(sMsg, "", "A");
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (gfn_dsIsUpdated(ds_orderplceMst)==false )
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	// 저장시 Validation Check	
	if (gfn_validCheck(grd_orderplceList, ds_orderplceMstValid) == false)	return;
	
    var strSvcID    = "save";
    var strURL      = "rc/bms/basOrderplceMgmt/saveOrderplceMgmt.xdo";
    var strInDs     = "ds_orderplceMst=ds_orderplceMst:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

