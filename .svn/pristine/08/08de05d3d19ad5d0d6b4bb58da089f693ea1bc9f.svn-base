/**
 * 검색조건 중 실행일자의 기간을 선정한다.
 * @param intType 기간타입으로 0:금일, 1:금주, 2:금월
 * @return none
 */
function fn_setSearchDate (intType)
{
	//검색일자 설정
	var strFrom = gfn_today();
	var strTo = strFrom;
	
	if (intType == 0) {			//금일
	}else if (intType == 1) {	//금주
		strFrom = gfn_getThisWeekBegin();
	}else if (intType == 2) {	//금월		
		strFrom = gfn_getThisMonthBegin();
	}
	cal_fromDate.fn_setValue (strFrom);
	cal_toDate.fn_setValue (strTo);
}

/**
 * 개인정보열람승인 화면을 MDI로 오픈한다.
 * @param arrParams 조회조건
 * @return none
 */
function fn_openPersReadApprov (arrParam) 
{
	//prgId로 메뉴ID찾기
    var nRow = gds_menu.findRow("prgId", "PersReadApprov");
    if (nRow < 0) { gfn_alert("프로그램을 찾을 수 없습니다!!"); return; }

	var menuId = gds_menu.getColumn(nRow, "menuId");
	
	// MDI로 프로그램 오픈
	gfn_openMenu (menuId, arrParam);
}

