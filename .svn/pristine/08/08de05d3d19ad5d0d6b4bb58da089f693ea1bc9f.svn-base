/*----------------------------------------------------------------------------------------------
 * 설명      : Cell 클릭 시 개인정보열람승인 화면을 호출
 *---------------------------------------------------------------------------------------------*/
function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.col <= 3) return;
	
	//값이 0 이상이어야 함
	if (ds_list.getColumn(e.row, gfn_getCellColId(grd_list, e.col, "Body")) == 0) return;

	//시간대별은 당일만 볼 수 있음
	if (e.col > 4 && ds_search.getColumn(ds_search.rowposition, "fromDate") <> ds_search.getColumn(ds_search.rowposition, "toDate")) {
		gfn_alert ("사용자 시간대별 통계는 1일치의 열람이력만 볼 수있습니다. 합계 수치를 클릭하여 확인하십시오.");
		 return;
	}
	
	var arrParam = new Array();

	//조회조건 설정
	arrParam[0] = "U"; //사용자별 "U", 일자별 "D"
	arrParam[1] =  ds_search.getColumn(ds_search.rowposition, "fromDate");
	arrParam[2]   =  ds_search.getColumn(ds_search.rowposition, "toDate");
	if (e.col == 4) {
		arrParam[3] =  "000000000";
		arrParam[4] =  "235959000";
	}else{
		var sTime = gfn_getTitle(obj, e.col, "text");
		arrParam[3] =  sTime + "0000000";
		arrParam[4] =  sTime + "5959000";
	}
	arrParam[5] =  gfn_isNull(ds_search.getColumn(ds_search.rowposition, "sysType"))  ? "" : ds_search.getColumn(ds_search.rowposition, "sysType");
	arrParam[6] =  gfn_isNull(ds_search.getColumn(ds_search.rowposition, "userType")) ? "" : ds_search.getColumn(ds_search.rowposition, "userType");
	arrParam[7] =  gfn_isNull(ds_search.getColumn(ds_search.rowposition, "ccCd")) ? "" : ds_search.getColumn(ds_search.rowposition, "ccCd");
	arrParam[8] =  edt_ccCd.text;
	arrParam[9] =  ds_list.getColumn(e.row, "acesUser");
	arrParam[10] =  ds_list.getColumn(e.row, "acesUserNm");
	
	//0 : 미승인, 1 : 승인, A : 전체
	arrParam[11] = "A";	
	
	//var sParam = "";
	//for (var i=0; i < arrParam.length; i++)
	//	sParam += arrParam[i] + ",";
	//alert(sParam);
	
	//개인정보열람승인 호출
	fn_openPersReadApprov (arrParam);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : Cell 클릭 시 개인정보열람승인 화면을 호출
 *---------------------------------------------------------------------------------------------*/
function grd_listDay_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.col <= 1) return;
	
	//값이 0 이상이어야 함
	if (grd_listDay.getCellText(e.row, e.col) == 0) return;
	
	var arrParam = new Array();

	//조회조건 설정
	arrParam[0] = "D"; //사용자별 "U", 일자별 "D"
	arrParam[1] =  ds_listDay.getColumn(e.row, "acesDtm").replace(/-/g, "");
	arrParam[2] =  ds_listDay.getColumn(e.row, "acesDtm").replace(/-/g, "");
	arrParam[3] =  "000000000";
	arrParam[4] =  "235959000";
	arrParam[5] =  gfn_isNull(ds_search.getColumn(ds_search.rowposition, "sysType"))  ? "" : ds_search.getColumn(ds_search.rowposition, "sysType");
	arrParam[6] =  gfn_isNull(ds_search.getColumn(ds_search.rowposition, "userType")) ? "" : ds_search.getColumn(ds_search.rowposition, "userType");
	arrParam[7] =  gfn_isNull(ds_search.getColumn(ds_search.rowposition, "ccCd")) ? "" : ds_search.getColumn(ds_search.rowposition, "ccCd");
	arrParam[8] =  edt_ccCd.text;
	arrParam[9] =  gfn_isNull(ds_search.getColumn(ds_search.rowposition, "acesUser")) ? "" : ds_search.getColumn(ds_search.rowposition, "acesUser");
	arrParam[10] =  edt_user.edt_userName.text;
	
	//0 : 미승인, 1 : 승인, A : 전체
	if (e.col == 2)
		arrParam[11] = "A";	
	else if (e.col == 3)
		arrParam[11] = "0";
	else
		arrParam[11] = "1";	

	//var sParam = "";
	//for (var i=0; i < arrParam.length; i++)
	//	sParam += arrParam[i] + ",";
	//alert(sParam);
	
	//개인정보열람승인 호출
	fn_openPersReadApprov (arrParam);
}
