/*
 ******************************************************************************************
 * 함  수  명   : fn_Search()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 조회버튼 입력 시 처리되는 작업
 * 작  성  자   : 
 * 작성  일자   : 
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_search()
{
    // 초기화
	ds_in.clearData();
	
	fsp_clear(this);
	
	//
	ds_Cond.addRow();
	var chkMuCd = gfn_isNull(ds_CondTemp.getColumn(0, "MU_CD"));
	var chkTmCd = gfn_isNull(ds_CondTemp.getColumn(0, "TM_CD"));
	var chkCcCd = gfn_isNull(ds_CondTemp.getColumn(0, "CC_CD"));
	var chkUpjang = gfn_isNull(edt_Upjang.getUpjangCode());
	
	// MU_CD
	if(chkMuCd == true){
		ds_Cond.setColumn(0, "MU_CD", application.g_Mu_Cd);
	}else{
		ds_Cond.setColumn(0, "MU_CD", ds_CondTemp.getColumn(0, "MU_CD"));
	}
	// TM_CD
	if(chkTmCd == true){
		//ds_Cond.setColumn(0, "TM_CD", application.g_Tm_Cd);
		ds_Cond.setColumn(0, "TM_CD", "");
	}else{
		ds_Cond.setColumn(0, "TM_CD", ds_CondTemp.getColumn(0, "TM_CD"));
	}
	// UPAJGN
	if(chkUpjang == true){
		ds_Cond.setColumn(0, "UPJANG", "");
	}else{
		ds_Cond.setColumn(0, "UPJANG", edt_Upjang.getUpjangCode());
	}
	
	// 본부의 팀전체 조회 옵션 추가 2016.02.16
	if(chk_bonbu.value){
		if(chkTmCd == false) 
		{
			ds_Cond.setColumn(0, "TM_CD", ds_CondTemp.getColumn(0, "TM_CD").substr(0,3));
			ds_Cond.setColumn(0, "UPJANG", "");
		}
	}

	// 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "ma/map:MAP30050V_S001");
	
	var sParam = "YYMM=" + wrapQuote(me_Date.value) +
				 " DATA_CLASS=" + wrapQuote(rdo_Data_Class.value) +
				 " CLOSE_TYPE=" + wrapQuote(rd_CostType.value) +
				 " MU_CD=" + wrapQuote(ds_Cond.getColumn(0, "MU_CD")) +
				 " TM_CD=" + wrapQuote(ds_Cond.getColumn(0, "TM_CD")) +
				 " UPJANG=" + wrapQuote(ds_Cond.getColumn(0, "UPJANG")) +
				 " SUB_JOB_CD=" + wrapQuote(cbo_Subjob.value) +
				 " LOC_CD=" + wrapQuote(cbo_Loc.value) +
				 " NEW_GUBUN=" + wrapQuote(cbo_New.value) +
				 " SALE_TYPE=" + wrapQuote(rd_SaleType.value) +
				 //블로킹사용자체크를위해추가
				 " UUSER=" + wrapQuote(gfn_getUserId());

	// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", "", "ds_out=ds_out", sParam,"fn_afterSearch");
	http.Sync = false;
}


