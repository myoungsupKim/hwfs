		//ext_ExcelImportByIndex(파일풀패스, 시트순번, 깡통데이타셑);
		ext_ExcelImportByIndex(sFullFileName, 0, "ds_Excel");

		fn_SetImpData();
		gd_list.enableredraw = true;
	}
}

function fn_SetImpData()
{
	var nRow;
	ds_ErrLst.clearData();

	dv_Prg.visible = true;
	SetWaitCursor(true);
	setCapture();
	for (i = 1; i < ds_Excel.rowcount; i++)
	{
		//세분류코드 등록여부
		if (gfn_length(gfn_trim(ds_Excel.getColumn(i,"Col06")))== 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(GetLastRow(ds_ErrLst)     , "errmsg", numFormat(i+1)+ "행) 세분류코드 미등록");
			continue;
		}
		nRow = ds_List.findRowExpr("ITEM_CLASS4=='" + ds_Excel.getColumn(i,"Col06") + "'");
		if (nRow < 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(GetLastRow(ds_ErrLst)     , "errmsg", numFormat(i+1)+ "행) 해당하는 세분류코드(" + ds_Excel.getColumn(i,"Col06") + " " + ds_Excel.getColumn(i,"Col07") + ")를 찾을 수가 없습니다");
			continue;
		}
		//운영율
		if (! isFinite(ds_Excel.getColumn(i,"Col11")))
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(GetLastRow(ds_ErrLst)     , "errmsg", numFormat(i+1)+ "행) " + ds_Excel.getColumn(i,"Col06") + " " + ds_Excel.getColumn(i,"Col07") + " 운영율 오류(" + ds_Excel.getColumn(i,"Col11") + ")");
			continue;
		}
		//올림수
		if (! isFinite(ds_Excel.getColumn(i,"Col12")))
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(GetLastRow(ds_ErrLst)     , "errmsg", numFormat(i+1)+ "행) " + ds_Excel.getColumn(i,"Col06") + " " + ds_Excel.getColumn(i,"Col07") + " 올림수 오류(" + ds_Excel.getColumn(i,"Col12") + ")");
			continue;
		}
		ds_List.rowposition = nRow;
		ds_List.setColumn(nRow, "OP_RATE", toNumber(ds_Excel.getColumn(i,"Col11")));
		ds_List.setColumn(nRow, "CEIL_VAL", toNumber(ds_Excel.getColumn(i,"Col12")));
	}
	releaseCapture();
	SetWaitCursor(false);
	dv_Prg.visible = false;
	ds_List.rowposition = 0;

	var sMsg;
	if (ds_ErrLst.rowcount < ds_Excel.rowcount)
	{
		sMsg = '정상적으로 업로드 되었습니다.';
		if (ds_ErrLst.rowcount > 0)
		{
			sMsg += '\n\n오류 데이타가 존재합니다.\n확인하시겠습니까?!';
			ds_ErrLst.rowposition = 0;
			div_Error.visible = (g_Confirm("04", sMsg));
			if (div_Error.visible) div_Error.grd_ErrLst.setFocus();
		}
		else
			g_Message("02", sMsg);
	}
}

