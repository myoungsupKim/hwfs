                     ELSE '미지정' END) NEW_GUBUN,
               V.TRX_DATE,
               NVL(SUM(DECODE(A.ACCT_NAME, '식음료매출액', V.QTY, 0)), 0) QTY,
               NVL(DECODE(TAX_KIND,'310',
                      ROUND(SUM(DECODE(A.ACCT_NAME, '식음료매출액', V.AMT,0))/1.1),
                      SUM(DECODE(A.ACCT_NAME, '식음료매출액', V.AMT,0)) ), 0) F_AMT,
               NVL(DECODE(TAX_KIND,'310',
                      ROUND(SUM(DECODE(A.ACCT_NAME, '상품매출', V.AMT,0))/1.1),
                      SUM(DECODE(A.ACCT_NAME, '상품매출', V.AMT,0)) ), 0) S_AMT,
               NVL(DECODE(TAX_KIND,'310',
                      ROUND(SUM(DECODE(A.ACCT_NAME, '기타매출', V.AMT,0))/1.1),
                      SUM(DECODE(A.ACCT_NAME, '기타매출', V.AMT,0)) ), 0) E_AMT
          FROM MAS_TRX V,
               MAS_BOND_CODE A,
               SCO_UPJANG_MST_V C
        WHERE (A.BOND_GUBUN = 'MACHUL')
        AND   (V.CR_ACCTCD  = A.ACCTCD)
        AND   (V.UPJANG     = C.UPJANG)
        AND   (V.TRX_DATE   BETWEEN #TRX_DATE#||'01' AND #TRX_DATE#||'31')
        <isNotNull col="TM_CD">  
        AND   (C.TM_CD      = #TM_CD#)
        </isNotNull>  
        <isNotNull col="UPJANG">  
        AND   (C.UPJANG     = #UPJANG#)
        </isNotNull>   
        <isNotNull col="SUB_JOB_CD">  
        AND   (C.SUB_JOB_CD = #SUB_JOB_CD#)
        </isNotNull>                
        <isNotNull col="LOC_CD">  
        AND   (C.LOC_CD     = #LOC_CD#)
        </isNotNull>                  
        GROUP BY C.SAUP_TYPE, C.TM_CD, V.UPJANG, V.TAX_KIND, V.TRX_DATE
        UNION ALL
        SELECT /*+ RULE*/
               C.SAUP_TYPE,
               'FICS' GUBUN,
               C.TM_CD,
               A.MAIN_UPJANG UPJANG,
               V.SUB_UPJANG,
               (CASE WHEN SUBSTR(A.FST_REG_DATE, 1, 4) &lt; SUBSTR(#TRX_DATE#, 1, 4)  THEN '기존'
                     WHEN SUBSTR(A.FST_REG_DATE, 1, 4) &gt;= SUBSTR(#TRX_DATE#, 1, 4) THEN '신규'
                     ELSE '미지정' END) NEW_GUBUN,               
               V.TRX_DATE,
               0 QTY,
               0 F_AMT,
               NVL(S_AMT, 0) S_AMT,
               0 E_AMT
        FROM (
               SELECT V.TRANS_UPJANG SUB_UPJANG,
                      V.TRANS_DATE TRX_DATE,
                      NVL(SUM(SALE_AMOUNT) , 0) S_AMT
                 FROM HLDC_PO_TRANSACTION V
                WHERE (V.TRANS_DATE BETWEEN #TRX_DATE#||'01' AND #TRX_DATE#||'31')
                  AND (V.TRANS_TYPE = 'I001' )
                  AND (V.SUBINV_CODE BETWEEN 'S1000000' AND 'S1099999' )
                GROUP BY V.TRANS_UPJANG,
                         V.TRANS_DATE
             ) V,
             FMS_UPJANG A,
             SCO_UPJANG_MST_V C
        WHERE (V.SUB_UPJANG = A.UPJANG)
        AND   (A.UPJANG = C.UPJANG)       
        <isNotNull col="TM_CD">  
        AND   (C.TM_CD      = #TM_CD#)
        </isNotNull>  
        <isNotNull col="UPJANG">  
        AND   (C.UPJANG     = #UPJANG#)
        </isNotNull>   
        <isNotNull col="SUB_JOB_CD">  
        AND   (C.SUB_JOB_CD = #SUB_JOB_CD#)
        </isNotNull>                
        <isNotNull col="LOC_CD">  
        AND   (C.LOC_CD     = #LOC_CD#)
        </isNotNull>          
       ) V
GROUP BY V.SAUP_TYPE, V.GUBUN, V.TM_CD, V.UPJANG, V.SUB_UPJANG, V.NEW_GUBUN
</statement>
