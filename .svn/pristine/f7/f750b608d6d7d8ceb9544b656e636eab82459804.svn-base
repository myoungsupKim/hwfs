/*----------------------------------------------------------------------------------------------
 * 설명      : 식권 환불 20150819 추가
 *---------------------------------------------------------------------------------------------*/
function fn_ticketPop()
{	                             
	var arrArg = new Array();
	var sReceiptNum = "";
	var sUpjangCd = "";
	var sAdvanceTypeCd = "";
	var sApplyAmt = 0;
	
	if(flag){
		if(Number(ds_list.getColumn(ds_list.rowposition, "applyAmt")) == 0) return;
		arrArg[0] = ds_list.getColumn(ds_list.rowposition, "receiptNum"); // 영수증 번호
		arrArg[1] = ds_list.getColumn(ds_list.rowposition, "upjangCd"); // 업장 
		arrArg[2] = ds_list.getColumn(ds_list.rowposition, "advanceTypeCd"); // 아이템 타입 (F05 = 선수식권, F10 = 외상식권)
		arrArg[3] = ds_list.getColumn(ds_list.rowposition, "applyAmt");
		arrArg[4] = flag;
		arrArg[5] = "R";
		arrArg[6] = ds_list.getColumn(ds_list.rowposition, "altNum");	//ADVANCE_ALT_NUM
		
	}else{
		for(var i=0; i<ds_list.rowcount; i++){
			if(ds_list.getColumn(i, "advanceTypeCd") == "F05" || ds_list.getColumn(i, "advanceTypeCd") == "F10"){ //f05선수  f10외상
				if(Number(ds_list.getColumn(i, "applyAmt")) > 0){
					if (sApplyAmt == 0){
						sReceiptNum = ds_list.getColumn(i, "receiptNum"); // 영수증 번호
						sUpjangCd = ds_list.getColumn(i, "upjangCd"); // 업장 
						sAdvanceTypeCd = ds_list.getColumn(i, "advanceTypeCd"); // 아이템 타입 (F05 = 선수식권, F10 = 외상식권)
					}
					sApplyAmt = sApplyAmt + ds_list.getColumn(i, "applyAmt");
				}
			}
		}
		arrArg[0] = sReceiptNum; // 영수증 번호
		arrArg[1] = sUpjangCd; // 업장 
		arrArg[2] = sAdvanceTypeCd; // 아이템 타입 (F05 = 선수식권, F10 = 외상식권)
		arrArg[3] = sApplyAmt;
	}	
		
		
	var strId =	"MAS10130P";
	var strURL =  "U_MAS::MAS10131P.xfdl";
	var objArgument = {arrSchVal:arrArg}	//objArgumentList
	var nWidth = 		-1		//Width
	var nHeight = 		-1		//Height
	var strStyle = 		""		//Style
	var nLeft = 		-1		//Left
	var nTop = 		-1		//Top
	var isModeless = 		false	//Modeless
	var bShowTitle = 		true	//ShowTitle
	var strAlign = 		"-1"	//Align
			//,true);	//Resizable
	
	var arrRtn = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);
	
	//trace(ds_MAS_TICKET_MST.saveXML());
	
	if(arrRtn != undefined){
		saveChk = true;
		
 	}		
	flag = false;
}

