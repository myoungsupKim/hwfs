function fn_chkSaveInfo(arg){
	if(ds_list.rowcount == 0){
		gfn_alert("msg.search.nodata");	// 조건에 해당하는 자료가 없습니다.
		return false;
	}
	
	if(arg == "PROC"){	//환불처리
		var iOccurAmt = 0;
		var iApplyAmt = 0;
		if(cmb_altTypeCd.value == "G02"){	//환불유형 G02 : +- 선수금 처리
			for(var i=0; i<ds_list.rowcount; i++){
				//ds_list.setColumn(i, "applyAmt", ds_list.getColumn(i, "occurAmt"));
				//ds_list.setColumn(i, "balAmt", 0);
				iOccurAmt += ds_list.getColumn(i, "occurAmt");
			}
			if(iOccurAmt != 0){
				gfn_alert("선수금잔액의 합이 0 이 아니므로\n[+- 선수금 처리]를 할 수 없습니다.");
				ds_list.reset();
				return false;
			}
		}
		
		for(var i=0; i<ds_list.rowcount; i++){
			iApplyAmt += ds_list.getColumn(i, "applyAmt");
		}
		if(iApplyAmt == 0){
			gfn_alert("환불금액의 합이 0 이므로 환불처리할 수 없습니다.");
			//ds_list.reset();
			return false;
		}
				
//		if (gfn_isNull(cal_occurDateFromSch.text)){
//			gfn_alert("msg.noinput", "발생일자");	// $1 을(를) 입력해 주십시요.
