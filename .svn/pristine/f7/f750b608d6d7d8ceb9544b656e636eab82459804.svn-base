/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg){
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0){
		return;
	}
	
    switch(strSvcID){
		case "cmb_altTypeCd" :
		
           if(ds_altTypeCd.rowcount < 2){
				if(gfn_isNull(ds_altTypeCd.getColumn(0, "typeCd"))){
					
					//데이터가 없을 경우 전체조회
					ds_altTypeCd.clearData();
					gfn_setSalesTypeCd(this, "altTypeCd2", "ds_altTypeCd", "008");
				}
            }else{
				cmb_altTypeCd.index = 0;
            }
            break;

     	// 대체유형Combo 조회
        case "altTypeCd2" :
			ds_altTypeCd.insertRow(0);
			//ds_altTypeCdSch.setColumn(0, "typeCd", "");
			ds_altTypeCd.setColumn(0, "typeNm", "선택");
			cmb_altTypeCd.index = 0;
            break;
			
        //조회
        case "search" :
			
			//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
			if(ds_list.getRowCount() == 0) gfn_loadDatasetInfo (ds_list, fv_objColInfo);
			fn_formSetInit(bCreateMode);
			//alert(bCreateMode);
			
			//급식용 사업부 세팅
			muChk = ds_search.getColumn(0, "inqueryMuCd");
			
			//조회 후 그리드 속성 세팅
			fn_setCellProperty(grd_list);
			
			//식권의 경우 공급가액 부가세 세팅
			fn_setAmtByTicket();
			//trace(ds_list.getColumn(0, "paySchdDate"));
            break;

        //조회조건 조회 후 지불형태 조회 (수정모드일때 사용)
        case "selectAdvanceAltMst" :

			var inqueryStartDate	= ds_search.getColumn(0, "inqueryStartDate");
			var inqueryEndDate		= ds_search.getColumn(0, "inqueryEndDate");
			var altDate				= ds_search.getColumn(0, "altDate");
			var paySchdDate			= ds_search.getColumn(0, "paySchdDate");
			
			cal_occurDateFromSch.fn_initMonthCal(this, inqueryStartDate, null, ds_search, "inqueryStartDate", "Y");
			cal_occurDateToSch.fn_initMonthCal(this, inqueryEndDate, null, ds_search, "inqueryEndDate");
			cal_altDate.fn_initMonthCal(this, altDate, null, ds_search, "altDate", "Y");
			cal_paySchdDate.fn_initMonthCal(this, paySchdDate, null, ds_search, "paySchdDate", "Y");
			
			var orgAltTypeCd = ds_search.getColumn(0, "altTypeCd");
			var orgPayGrp = ds_search.getColumn(0, "payGrp");

			//지급정보 활성/비활성화
			fn_setPayInfo(ds_search.getColumn(0, "altTypeCd"));
			
			//콤보조회
			fn_setCombo2();

			//지불형태 조회
			if(orgAltTypeCd != "H03"){
				fn_setPayType(orgPayGrp);
			}else{			
				if(bCreateMode == false) fn_search();
			}
			//fn_search();

			//조회 후 대체유형 세팅
			ds_search.setColumn(0, "altTypeCd", orgAltTypeCd);
			ds_search.setColumn(0, "payGrp", orgPayGrp);
			ds_search.setColumn(0, "advanceAltOccurClass", fv_advanceAltOccurClass);
			ds_search.setColumn(0, "paySchdDate", paySchdDate);
			
			
			if (ds_search.getColumn(0, "inqueryMuCd") == "2002"){
				UpjangSearchComponent01.visible = false;
				stc_cccdTitle.visible = false;
				stc_upjangTitle.visible = true;
				UpjangSearchComponent00.visible = true;				
			}else{
				UpjangSearchComponent01.visible = true;
				stc_cccdTitle.visible = true;
				stc_upjangTitle.visible = false;
				UpjangSearchComponent00.visible = false;			
			}
			
			//그리드 변경
			fn_setGridFormat(fv_advanceAltOccurClass);
            break;
        
            
        //지불형태 조회 후 조회 (수정모드일때 사용)
        case "cmb_payType" :
			cmb_payType.value = ds_search.getColumn(0, "payType");

			if(ds_payType.getRowCount()==0){
				cmb_payType.enable=false;
			}else{
				cmb_payType.enable=true;
			}
			//지불그룹을 본사카드 선택하면, 지급형태는 법인카드 지급/ 은행코드는 선택불가로 셋팅
			if(cmb_payGroup.value == "004"){
				cmb_payType.value = "004001";
				cmb_bankCd.enable = false;
				edt_bankAcctNo.enable = false;
				edt_bankAcctOwnr.enable = false;
				cal_paySchdDate.enable = false;
				ds_search.setColumn(0, "paySchdDate","");
			}else{
				cmb_bankCd.enable = true;
				edt_bankAcctNo.enable = false;
				edt_bankAcctOwnr.enable = true;
				cal_paySchdDate.enable = true;
				ds_search.setColumn(0, "paySchdDate",fv_sToday);
			}
			
			if(bCreateMode == false) fn_search();
            break;

        //선수금 잔액체크
		case "getAdvanceAltYn" :

			if(fv_advanceAltYn == "N"){
				gfn_alert("선수금 잔액이 부족합니다.");
				return;
			}else{
				//alert(ds_list.rowcount);
				ds_list.updatecontrol = false;
				for(var i=0; i<ds_list.rowcount; i++){
					ds_list.setRowType(i, Dataset.ROWTYPE_INSERT);
				}

				for(var i=0; i<ds_list.rowcount; i++){
					ds_list.setColumn(i, "altTypeCd", 	ds_search.setColumn(0, "altTypeCd"));
					ds_list.setColumn(i, "typeGrpCd", 	"008");
				}
				
				//식권 환불 저장
				if(fv_advanceAltOccurClass == "TO11"){
				
					ds_masTicketMst.clearData();
					
					var nRow = 0;
					if(ds_MAS_TICKET_MST.rowcount > 0){//환불
						for(var i=0; i<ds_MAS_TICKET_MST.rowcount; i++){
							ds_masTicketMst.addRow();
							ds_masTicketMst.setColumn(nRow, "receiptNum",       ds_list.getColumn(ds_list.rowposition, "receiptNum"));
							ds_masTicketMst.setColumn(nRow, "transDd",       	ds_search.getColumn(ds_search.rowposition, "altDate"));
							ds_masTicketMst.setColumn(nRow, "ticketSeq",        ds_MAS_TICKET_MST.getColumn(i, "TICKET_SEQ"));
							ds_masTicketMst.setColumn(nRow, "qty",              ds_MAS_TICKET_MST.getColumn(i, "QTY"));
							ds_masTicketMst.setColumn(nRow, "substId",          ds_MAS_TICKET_MST.getColumn(i, "SUBST_ID"));
							ds_masTicketMst.setColumn(nRow, "ticketGroupSeq",   ds_MAS_TICKET_MST.getColumn(i, "TICKET_GROUP_SEQ"));
							ds_masTicketMst.setColumn(nRow, "adjDate",          ds_MAS_TICKET_MST.getColumn(i, "ADJ_DATE"));
							ds_masTicketMst.setColumn(nRow, "price",            ds_MAS_TICKET_MST.getColumn(i, "PRICE"));
							ds_masTicketMst.setColumn(nRow, "adjDesc",          ds_MAS_TICKET_MST.getColumn(i, "ADJ_DESC"));
							ds_masTicketMst.setColumn(nRow, "upjang",           ds_MAS_TICKET_MST.getColumn(i, "UPJANG"));
							ds_masTicketMst.setColumn(nRow, "itemCode",         ds_MAS_TICKET_MST.getColumn(i, "ITEM_CODE"));
							ds_masTicketMst.setColumn(nRow, "itemType",         ds_MAS_TICKET_MST.getColumn(i, "ITEM_TYPE"));
							ds_masTicketMst.setColumn(nRow, "transType",        ds_MAS_TICKET_MST.getColumn(i, "TRANS_TYPE"));
							ds_masTicketMst.setColumn(nRow, "transGubun",       ds_MAS_TICKET_MST.getColumn(i, "TRANS_GUBUN"));
							ds_masTicketMst.setColumn(nRow, "remarkDesc",       "선수금 환불관리");
							ds_masTicketMst.setColumn(nRow, "attr02",           "R");
							nRow++;
						}
					}

					gfn_transaction(this
									,"saveAdvanceRefundProc"
									,"sm/cmn/advanceCommon/setAdvanceRefundProc.xdo"
									,"ds_search=ds_search:U ds_list=ds_list:U ds_masTicketMst=ds_masTicketMst:U"
									,""
									,""
									,"fn_callBack"
									,true);
				}else{
					//선수금환불처리
					gfn_saveAdvanceAltProc(this, "saveAdvanceRefundProc", "ds_search", "ds_list");
				}
			}
			break;
			
        //환불처리    
		case "saveAdvanceRefundProc" :
			gfn_alert("선수금 환불처리가 완료 되었습니다.");
			ds_list.updatecontrol = true;
			saveChk = false;
			this.opener.fn_search();
			//parent.parent.form.fn_search();
			this.close();
			break;
            
        //환불취소
		case "saveAdvanceRefundCancel" :
			gfn_alert("선수금 환불취소가 완료 되었습니다.");
			this.opener.fn_search();		
			//parent.parent.form.fn_search();
			this.close();
			break;
		
		case "searchBuycpnyCd" :	//카드환불
			cbo_receiveBankcd.enable=true;
			cbo_receiveBankcd.visible=true;	
			if(ds_buycpnyCd.findRow("custCd", ds_search.getColumn(0, "custCd")) > -1){
				cbo_receiveBankcd.index = ds_buycpnyCd.findRow("custCd", ds_search.getColumn(0, "custCd"));
			}
			break;
            
    }
}

