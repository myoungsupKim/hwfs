	// 수정모드
	if(!gfn_isNull(gv_altNum)){
		//F05:식권판매(선수),F10:식권판매(외상)
		if((advanceTypeCd == "F05" || advanceTypeCd == "F10")){
			return "#ffffffff";
		}else{
			return "#fffbccff";
		}
	}else{
		return "#fffbccff";
	}
}
