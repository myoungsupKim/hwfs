/*----------------------------------------------------------------------------------------------
 * 식권의 경우 공급가액 부가세 세팅
 *---------------------------------------------------------------------------------------------*/
function fn_setAmtByTicket(){

	var iOccurAmt	= 0;	//발생금액
	var iNetAmt		= 0;	//공급가액
	var iVatAmt		= 0;	//부가세
	var iApplyAmt	= 0;	//적용금액
	var iBalAmt		= 0;	//잔액
	var sTaxYn		= "";	//세금계산서
	var sAdvanceTypeCd	= "";
   
	ds_list.updatecontrol = false;
	for(var i=0; i<ds_list.rowcount; i++){
	
		sAdvanceTypeCd	= ds_list.getColumn(i, "advanceTypeCd");
		sTaxYn			= ds_list.getColumn(i, "taxYn");
		iOccurAmt		= ds_list.getColumn(i, "occurAmt");

		// 신규모드
		if(gfn_isNull(gv_altNum)){
			if(sTaxYn == "Y"){
				//부가세 식권의 경우 식권금액을 부가세 포함으로 보이게 수정(환불 전표 처리 시 부가세 마이너스 됨) 20180608 김호석				
				if(sAdvanceTypeCd == "F05" || sAdvanceTypeCd == "F10"){
				
					iNetAmt		= iOccurAmt; 
					iVatAmt		= gfn_round(iOccurAmt*0.1, 0);
					iApplyAmt	= 0;				
					iOccurAmt   = iNetAmt + iVatAmt;
				
				} else {				
					iNetAmt		= gfn_round(iOccurAmt / 1.1, 0);
					iVatAmt		= iOccurAmt - iNetAmt;
					iApplyAmt	= 0;				
				}
			}else{
				iNetAmt		= iOccurAmt;
				iVatAmt		= 0;
				iApplyAmt	= 0;
			}

		}else{
			
			if(sAdvanceTypeCd == "F05" || sAdvanceTypeCd == "F10"){
				
				iNetAmt		= ds_list.getColumn(i, "altSupplyAmt");
				iVatAmt		= ds_list.getColumn(i, "altVatAmt");
				iApplyAmt	= iNetAmt + iVatAmt;
				//부가세 식권의 경우 식권금액을 부가세 포함으로 보이게 수정(환불 전표 처리 시 부가세 마이너스 됨) 20180608 김호석				
				if(sTaxYn == "Y"){									
					iOccurAmt   = iNetAmt + iVatAmt;				
				}

			}else{
				iNetAmt		= iOccurAmt;
				iApplyAmt	= ds_list.getColumn(i, "applyAmt");
			}
		}
		iBalAmt	= iOccurAmt - iApplyAmt;
		
		ds_list.setColumn(i, "occurAmt",	iOccurAmt);
		ds_list.setColumn(i, "applyAmt",	iApplyAmt);
		ds_list.setColumn(i, "netAmt",		iNetAmt);
		ds_list.setColumn(i, "vatAmt",		iVatAmt);
		ds_list.setColumn(i, "balAmt",		iBalAmt);
		ds_list.setRowType(i, Dataset.ROWTYPE_NORMAL);
	}
	fn_setGridFormat(fv_advanceAltOccurClass);
	ds_list.updatecontrol = true;
}

