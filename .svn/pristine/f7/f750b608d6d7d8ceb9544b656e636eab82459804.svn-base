		if (gfn_isNull(cmb_altTypeCd.value)){
			gfn_alert("msg.noselect", "환불유형");	// $1 을(를) 선택해 주십시요.
			cmb_altTypeCd.setFocus();
			return false;
		}
		//미지급금환불일 경우
		if(cmb_altTypeCd.value == "H01"){
