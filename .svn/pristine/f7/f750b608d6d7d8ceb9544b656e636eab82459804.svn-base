				alert("카드 환불 채권에 대해서 대체 내역이 있습니다.");
				return;
			}
		}
	}
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 선수금환불(PROC)/취소(CANCEL) Setting
 *---------------------------------------------------------------------------------------------*/
function fn_setAdvanceAltInfo(arg){
	
	var iApplyAmt 	= 0;
	var sAltNum		= "";
	var sRowStatus	= "";
	
	//선수금환불
	if(arg == "PROC"){
		
		//신규/수정 구분
		if(gfn_isNull(gv_altNum)) sRowStatus = "INSERT";
		else sRowStatus = "UPDATE";
		
		//선수금대체마스터 세팅
		ds_search.setColumn(0, "rowStatus",				sRowStatus);
		ds_search.setColumn(0, "altAmt",				edt_applyAmtSum.value);
		//ds_search.setColumn(0, "advanceAltOccurClass",	"TO03");
		ds_search.setColumn(0, "typeGrpCd",				"008");
		//지불예정일 추가 20170404 맹수영
		ds_search.setColumn(0, "paySchdDate",				cal_paySchdDate.text);

		//선수금마스터, 선수금대체디테일 세팅
		for(var i=0; i<ds_list.rowcount; i++){
			iApplyAmt	= ds_list.getColumn(i, "applyAmt");
			sAltNum		= ds_list.getColumn(i, "altNum");

			if(ds_list.getRowType(i) > 1){	//행 타입(1:Normal, 2:insert, 4:Update, 8:Delete)
			
				if(iApplyAmt > 0){
					
					if(gfn_isNull(sAltNum)){
						ds_list.setColumn(i, "rowStatus",	"INSERT");
						ds_list.setColumn(i, "altNum", 		gv_altNum);
					}
					
					//입력 데이터 세팅
					ds_list.setColumn(i, "altDate",				cal_altDate.text);					// 환불일자
					ds_list.setColumn(i, "altTypeCd",			cmb_altTypeCd.value);				// 환불유형
					ds_list.setColumn(i, "advanceAltOccurClass","TO03");							// 대체발생구분
					ds_list.setColumn(i, "remark",				edt_remark.value);					// 적요
				}else{
					if(!gfn_isNull(sAltNum)){
						ds_list.setColumn(i, "rowStatus",	"DELETE");	//수정모드에서 적용금액이 0 이면 단건 환불취소
					}
				}
			}
		}
		
		//신규일 경우 조회조건데이터를 복사한다.
		if(sRowStatus == "INSERT"){
			ds_search.setColumn(0, "muCd", 			ds_search.getColumn(0,"inqueryMuCd"));
			ds_search.setColumn(0, "tmCd", 			ds_search.getColumn(0,"inqueryTmCd"));
			ds_search.setColumn(0, "ccCd", 			ds_search.getColumn(0,"inqueryCcCd"));
