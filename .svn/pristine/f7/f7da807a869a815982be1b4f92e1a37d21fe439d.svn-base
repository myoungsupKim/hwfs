	SCRadio.bind(this, "ComCode", "Tab00.tabpage.rdo_salaryKind", ds_rdoSalaryKind, {groupCode:"SALARY_TYPE", useYn:"Y"});
	
	SCCombo.bind(this, "ComCode", "cbo_sex", ds_cboSex, {groupCode:"GENDER_TYPE", useYn:"Y", set1:"S"}, SCCombo.ALL);
	SCCombo.bind(this, "ComCode", "Tab00.tabpage.cbo_progressStatus", ds_cboProgressStatus, {groupCode:"ALLC_TYPE", useYn:"Y", defVal:""});
	//SCCombo.bind(this, "ComCode", "grd_req_list.progressStatus", ds_cboProgressStatus, {groupCode:"ALLC_TYPE", useYn:"Y", defVal:"A"});
