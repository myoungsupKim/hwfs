/*----------------------------------------------------------------------------------------------
 * 설명      : 조회시 검색 시작나이 종료나이 크기 비교
 *---------------------------------------------------------------------------------------------*/
function msk_ageEnd_onkillfocus(obj:MaskEdit, e:KillFocusEventInfo)
{
	if(msk_ageStart.value > msk_ageEnd.value)
	{
		gfn_alert("시작나이가 종료나이 보다 큽니다. ");
		msk_ageEnd.value = "";
		return;
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 등록시 요청일자 마감일자 크기 비교
 *---------------------------------------------------------------------------------------------*/
function Tab00_tabpage_ObjCalendar_closeDate_onkillfocus(obj:ObjCalendar, e:KillFocusEventInfo)
{
	var rDate = ds_pool_req.getColumn(ds_pool_req.rowposition, "requestDate");
