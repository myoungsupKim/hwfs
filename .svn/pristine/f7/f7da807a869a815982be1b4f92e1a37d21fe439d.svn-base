function fn_report(obj:Button,  e:ClickEventInfo)
{
	//레포트에 전달할 파라메터 생성
	var arrParams	= SCReport.newParams ();
 	SCReport.setParams(arrParams, "reqeustUpjang", ds_pool_req.getColumn(ds_pool_req.rowposition, "requestUpjangCd"));
 	SCReport.setParams(arrParams, "reqeustNum", ds_pool_req.getColumn(ds_pool_req.rowposition, "requestNum"));

	//--------------------------------------------------
	//방법1)서버에서 Query에 의해 처리하는 경우 예
	//(필수) 레포트명
	SCReport.setParams (arrParams, SCReport.RPT_NAME, "/hwfs/fs/fsa/AdoptionReqList.ozr"); 
		

	//출력 옵션 설정
	//-창 타이틀 (디폴트는 "ReportViewer")
	SCReport.setParams (arrParams, SCReport.RPT_TITLE, "AdoptionReqList");
	
	SCReport.viewReport (this, arrParams, "", "", "PRT");
}
