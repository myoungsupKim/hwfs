//row 변경시 에티드 재설정
function ds_pool_req_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	var status = ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus");
	var rowType = ds_pool_req.getRowType(ds_pool_req.rowposition);
	
	var nRow = ds_pool_req.rowposition;
	if( nRow < 0 ){
		return ;
	}	
					
	//지원직종 > 지원직무 > 기타 체크 설정	
	var sPossDuty = ds_pool_req.getColumn(nRow, 'possDuty' );
	SCCheckbox.setValue("Tab00.tabpage.div_duty1", sPossDuty , SCCheckbox.ONLY_VALUE);
	SCCheckbox.setValue("Tab00.tabpage.div_duty2", sPossDuty , SCCheckbox.ONLY_VALUE);
	
	if(!sPossDuty == ''){
		if(sPossDuty.substr(sPossDuty.length-3,3) == "009")
		{
			Tab00.tabpage.Edit04.enable = true;
			
		}else{
			Tab00.tabpage.Edit04.enable = false;
			ds_pool_req.setColumn(ds_pool_req.rowposition, "possDutyEtc", "");
		}
	}else{
		Tab00.tabpage.Edit04.enable = false;
	}
	
	//상태값에 따른 버튼 에디트
	if(status == "W")//접수대기
	{
		if (ds_pool_req.getRowType(nRow) == Dataset.ROWTYPE_INSERT) {
			btn_adoptionReq.enable = false;
		} else {
			btn_adoptionReq.enable = true;
		}
			btn_adoptionEnd.enable = false;
			btn_adoptionFix.enable = false;
			//btn_report.visible = false;
	}else if(status == "R" || status == "A"){//접수완료,배정
		btn_adoptionReq.enable = false;
		btn_adoptionEnd.enable = true;
		
		if(status == "A")
		{
			btn_adoptionFix.enable = true;
			//btn_report.visible = false;
		}else{
			btn_adoptionFix.enable = false;
		}
		if(status == "R")
		{
			//btn_report.visible = true;
		}else{
			//btn_report.visible = false;
		}		
	}else{
		btn_adoptionReq.enable = false;
		btn_adoptionEnd.enable = false;
		btn_adoptionFix.enable = false;
		//btn_report.visible = false;
	}
	
	
	//상태값에 따른 채용지원정보 탭 에디트 
	if(ds_pool_req.rowcount > 0)
	{
		if(ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "W" || ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "C")
		{
			//Tab00.tabpage.enable = true;
			fn_componentEnable(true);
			
			//fn_edtBox_editable();
		}else{
			if(ds_pool_req.getRowType(ds_pool_req.rowposition) == 1)
			{
				//Tab00.tabpage.enable = false;
				fn_componentEnable(false);
			}else{
				//Tab00.tabpage.enable = true;
				fn_componentEnable(true);
			}	
		}	
		
		var nRow = ds_pool_req.rowposition;
		var ujCd = ds_pool_req.getColumn(nRow, "requestUpjangCd");
		var rqNm = ds_pool_req.getColumn(nRow, "requestNum");
		
		//입력중인 데이터를 저장 안하고 배정탭을 선택했을시 인력ds를 클리어 시켜줌
		if(ujCd == null && rqNm == null)
		{
			ds_pool.clearData();
		}else{
			fn_searchUpjangPool();
		}
	}	
	
}

