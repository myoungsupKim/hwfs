 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	//v_ds_check = "ds_pool_req";
	
	var nRow = ds_search.addRow();
	
	Tab00.tabpage.ObjCalendar_requestDate.fn_initMonthCal(this, gfn_today(), "", ds_pool_req, "requestDate", "Y");
	Tab00.tabpage.ObjCalendar_closeDate.fn_initMonthCal(this, gfn_today(), "", ds_pool_req, "closeDate");
	//ObjCalendar00.fn_initMonthCal(this, gfn_today(), "", ds_pool_req, "closeDate");
	SCRadio.bind(this, "ComCode", "Tab00.tabpage.rdo_sex", ds_rdoSex, {groupCode:"GENDER_TYPE", useYn:"Y"});
	SCRadio.bind(this, "ComCode", "Tab00.tabpage.rdo_dutyCook", ds_rdoDutyCook, {groupCode:"DUTY_TYPE", useYn:"Y", set1:"1"});
