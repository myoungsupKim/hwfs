//가능 직무 조리사/조리원 값 그리드에 세팅
function fn_dutyOnClickEvent() {	

	var sValAll = '';
	var sVal1 = SCCheckbox.getValue("Tab00.tabpage.div_duty1", SCCheckbox.ONLY_VALUE);
	var sVal2 = SCCheckbox.getValue("Tab00.tabpage.div_duty2", SCCheckbox.ONLY_VALUE);
	
	if( sVal1 == ''){
		sValAll = sVal2;
	}else if( sVal2 == ''){
		sValAll = sVal1;
	}else {
		sValAll = sVal1 + ','+sVal2;
	}
	
	if(sValAll.substr(sValAll.length-3,3) == "009")
	{
		Tab00.tabpage.Edit04.enable = true;
	}else{
		Tab00.tabpage.Edit04.enable = false;
		ds_pool_req.setColumn(ds_pool_req.rowposition, "possDutyEtc", "");
	}
	
	ds_pool_req.setColumn(ds_pool_req.rowposition, 'possDuty' , sValAll);
}

