************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
		case "searchUpjangAddr" :
			//ds_search.setColumn(0, 'maskAuth', SCAuth.getMaskAuth(this));
			ds_search.setColumn(0, "loginPersMask", g_PersInfoMask);
			ds_search.setColumn(0, 'maskAuth', "0");
			ds_search.setColumn(0, "upjangCd", fv_upjangCd);
			ds_search.setColumn(0, 'duty', "");
			ds_search.setColumn(0, 'sex', "");
			break;
        
		case "search" :
			
			if(ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "W" || ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "C")
			{
				btn_adoptionReq.enable = true;
			}else{
				btn_adoptionReq.enable = false;
			}
		
			if(ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "W" || ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "C")
			{
				//Tab00.tabpage.enable = true;
				fn_componentEnable(true);
				
				fn_edtBox_editable();
				fn_searchUpjangPool();
			}else{
				//Tab00.tabpage.enable = false;
				fn_componentEnable(false);
			}
			
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			
				
			
			if(ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "F" || ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "E")
			{
				var arrParam = new Array();
				var nRow = ds_pool_req.rowposition;
				arrParam[0] = "LINK";
				arrParam[1] = ds_pool_req.getColumn(nRow, "requestUpjangCd");
				arrParam[2] = ds_pool_req.getColumn(nRow, "requestNum");
			
				if(gfn_isNull(ds_pool_req.getColumn(nRow, "requestNum"))){
					gfn_alert("접수요청이후 마감을 실행해주세요.");
					return;
				}
	
				var dsObj = gfn_dialog("AdoptionPoolSearchPop"					// Dialog ID
									, "X_FSA::AdoptionSiteAppraisalPop.xfdl"		// Form URL
									, {fv_Contents:arrParam}			// 배열
									, null, null, null, null, null, false, true, -1);	
				// 다른업무화면 호출 - MENUID,
				//gfn_openMenu(strMenuID, arrParam);
				//gfn_openMenuForm("AdoptionSiteAppraisalPop.xfdl","X_FSA",arrParam,"채용만족도조사등록");
			}else{
				fn_search();
			}
			
            break;    
    }
			
}

