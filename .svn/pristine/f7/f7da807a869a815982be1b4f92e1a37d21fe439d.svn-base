	SCCheckbox.bind(this, "ComCode", "Tab00.tabpage.div_duty2", {groupCode:"DUTY_TYPE", useYn:"Y", set1:"2"}, {appearance:{usingDb:true}}, {onclick:"fn_dutyOnClickEvent"}); //조리사
	
