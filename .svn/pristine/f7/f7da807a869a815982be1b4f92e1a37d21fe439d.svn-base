/*----------------------------------------------------------------------------------------------
 * 설명      : 접수요청 버튼 클릭시 처리
 *---------------------------------------------------------------------------------------------*/
function btn_adoptionReq_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_pool_req.rowcount==0)
	{
		gfn_alert("선택된 채용이 없습니다.");
		return;
	}else{
		var rtVal = gfn_confirm("접수요청 하시겠습니까?");	// 선택된 자료를 삭제 하시겠습니까?
		if (rtVal == true) 
		{	
			ds_pool_req.setColumn(ds_pool_req.rowposition, "progressStatus", "R");
			fn_save();
			// 저장에서 리턴될경우 다시 작성중으로 변경
			ds_pool_req.setColumn(ds_pool_req.rowposition, "progressStatus", "W");
		}
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명      : 마감 버튼 클릭시 처리
 *---------------------------------------------------------------------------------------------*/
function btn_adoptionFix_onclick(obj:Button,  e:ClickEventInfo)
{
	
	
	if(ds_pool_req.rowcount==0)
	{
		gfn_alert("선택된 채용이 없습니다.");
		return;
	}else{
		var iFindRowIdx = ds_pool.findRow("goodYn", "", 0, -1);
		if(iFindRowIdx == -1)
		{
			var rtVal = gfn_confirm("마감 하시겠습니까?");	// 선택된 자료를 삭제 하시겠습니까?
			if (rtVal == true) 
			{	
				ds_pool_req.setColumn(ds_pool_req.rowposition, "progressStatus", "F");
				ds_pool_req.setColumn(ds_pool_req.rowposition, "lastCloseDate", fv_sToday);
				ds_pool_req.setColumn(ds_pool_req.rowposition, "closeDate", fv_sToday);
				
				for(var i=0; i<ds_pool.rowcount; i++)
				{
					if(ds_pool.getColumn(i, "goodYn")=="G"){
						ds_pool.setColumn(i, "adoptionStatus", "F"); // 마감
					}else{
						ds_pool.setColumn(i, "adoptionStatus", "R"); // 접수완료
					}
				}
				fn_save();
			}
		}else{
			gfn_alert("합격여부를 먼저 설정하여 주십시오.");
			return;
		}
	}
}
