function fn_delete()
{
	var nRow = ds_pool_req.rowposition;
	
	if(ds_pool_req.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	if(ds_pool_req.getRowType(nRow) == 2){
		ds_pool_req.deleteRow(nRow);
		Tab00.tabpage.AddrSearchComponent00.clearAll();
	}else{
		var rtVal = gfn_confirm("해당 행은 인력채용리스트등록 되어 있는 데이터 입니다. \n삭제 하시겠습니까?");	// 선택된 자료를 삭제 하시겠습니까?
		if (rtVal == true) 
		{	
			ds_pool_req.deleteRow(ds_pool_req.rowposition);
			Tab00.tabpage.AddrSearchComponent00.clearAll();
		}else{
			return;
		}
	}	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (gfn_dsIsUpdated(ds_pool_req) == false && gfn_dsIsUpdated(ds_pool) == false) {
        gfn_alert("msg.save.nochange"); // 변경된 내역이 없습니다.
        return;
    }
	
	// 저장시 Validation Check
	var orgRow = ds_pool_req.rowposition;
	if (gfn_validCheck(grd_req_list, ds_pool_reqValid) == false){
		ds_pool_req.setColumn(orgRow, "progressStatus", "W");
		//ds_pool_req.rowposition = orgRow;
		return;
	}

	//INSERT 시 필요한 업장코드와 채용번호를 넣어준다.
	for(var i=0; i < ds_pool.getRowCount(); i++)
	{
		if(ds_pool.getRowType(i) == 4)
		{
			ds_pool.setColumn(i, "requestUpjangCd", ds_pool_req.getColumn(ds_pool_req.rowposition, "requestUpjangCd"));
			ds_pool.setColumn(i, "requestNum", ds_pool_req.getColumn(ds_pool_req.rowposition, "requestNum"));
		}
	}
	
	// 접수마감 하지 않더라도 합격, 불합격 여부 선택하여 저장하면 상태값 변경
	for(var i=0; i<ds_pool.rowcount; i++)
	{
		if(ds_pool.getColumn(i, "goodYn")=="G"){
			ds_pool.setColumn(i, "adoptionStatus", "F");
		}else{
			ds_pool.setColumn(i, "adoptionStatus", "R");
		}
	}
	
	
	
    var strSvcID    = "save";
    var strURL      = "fs/fsa/adoptionReqList/saveList.xdo";
    var strInDs     = "ds_pool_req=ds_pool_req:U ds_pool=ds_pool:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

