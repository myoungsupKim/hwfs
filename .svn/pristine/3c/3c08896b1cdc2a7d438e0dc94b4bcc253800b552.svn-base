//기기/기물 마스터 행 추가, 삭제
function btn_machineMstAdd_onclick(obj:Button,  e:ClickEventInfo)
{
	if(ds_machineMst.getColumn(ds_machineMst.rowposition,'lev') == 3)
	{
		alert(" 더 이상의 하위데이터 생성은 불가합니다. ");
		return;
	}else{
		if(ds_machineMst.getRowType(ds_machineMst.rowposition) == 2)
		{
			alert("상위코드를 등록해야 하위코드 생성이 가능합니다.");
			return;
		}else{
			var nRow = ds_machineMst.insertRow(ds_machineMst.rowposition+1);
			ds_machineMst.setColumn(ds_machineMst.rowposition,'lev',ds_machineMst.getColumn(ds_machineMst.rowposition-1,'lev')+1);
			ds_machineMst.setColumn(ds_machineMst.rowposition,'mgrpCd',ds_machineMst.getColumn(ds_machineMst.rowposition-1,'mgrpCd'));
			ds_machineMst.setColumn(ds_machineMst.rowposition,'mgrpNm',ds_machineMst.getColumn(ds_machineMst.rowposition-1,'mgrpNm'));
			ds_machineMst.setColumn(ds_machineMst.rowposition,'mclassCd',ds_machineMst.getColumn(ds_machineMst.rowposition-1,'mclassCd'));
			ds_machineMst.setColumn(ds_machineMst.rowposition,'sclassCd',ds_machineMst.getColumn(ds_machineMst.rowposition-1,'sclassCd'));
			
			//alert("mgrpCd:"+ds_machineMst.getColumn(ds_machineMst.rowposition,'mgrpCd'));
			
		}	
	}
}
function btn_machineMstDel_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRow = ds_machineMst.rowposition;
	//alert(ds_grd.getRowType(nRow));
	
	if(nRow == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	if(ds_machineMst.getRowType(nRow) == 2){
		ds_machineMst.deleteRow(nRow);
	}else{
		var rtVal = gfn_confirm("등록되어 있는 코드입니다. 삭제 하시겠습니까?");	// 선택된 자료를 삭제 하시겠습니까?
		if (rtVal == true) 
		{	
			ds_machineMst.deleteRow(nRow);
		}
	}
	
}

