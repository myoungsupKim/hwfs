function grd_machineMst_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{	
	//중코드 소코드를 조회해온다.
	fn_cboMclassLoad();
	fn_cboSclassLoad();
	
	ds_search.clearData();
	
	var nRow = ds_search.addRow();
	
	ds_search.setColumn(nRow, "mgrpCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "mgrpCd"));
	ds_search.setColumn(nRow, "mclassCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "mclassCd"));
	ds_search.setColumn(nRow, "sclassCd", ds_machineMst.getColumn(ds_machineMst.rowposition, "sclassCd"));
	
	// Grid sort 표시 초기화(필수)
    gfn_clearSortMark(Tab00.tabpage6.grd_list);

    var strSvcID    = "search";
    var strURL      = "fs/fsm/machineAshisDisuse/selectDisuseList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_grdDisuse=ds_grdDisuse";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

