public class JobInfoDTO extends DefaultDTO implements Serializable {
	private static final long serialVersionUID = -1L;

	/** 시스템구분 */
	private String sysType;
	
	/** Batch Job명 */
	private String jobName;

	/** Batch Job 설명 */
	private String jobDesc;

	/** 시스템ID */
	private String sysId;
	
	/** Batch Job 등록여부 */
	private String ynRgst;

	/**
	 * jobName 얻기
	 *
	 * @return the jobName
	 */
	public String getJobName() {
		return jobName;
	}

	/**
	 * jobName 설정
	 *
	 * @param jobName the jobName to set
	 */
	public void setJobName(String jobName) {
		this.jobName = jobName;
	}

	/**
	 * jobDesc 얻기
	 *
	 * @return the jobDesc
	 */
	public String getJobDesc() {
		return jobDesc;
	}

	/**
	 * jobDesc 설정
	 *
	 * @param jobDesc the jobDesc to set
	 */
	public void setJobDesc(String jobDesc) {
		this.jobDesc = jobDesc;
	}

	/**
	 * ynRgst 얻기
	 *
	 * @return the ynRgst
	 */
	public String getYnRgst() {
		return ynRgst;
	}

	/**
	 * ynRgst 설정
	 *
	 * @param ynRgst the ynRgst to set
	 */
	public void setYnRgst(String ynRgst) {
		this.ynRgst = ynRgst;
	}

	/**
	 * sysType 얻기
	 *
	 * @return the sysType
	 */
	public String getSysType() {
		return sysType;
	}

	/**
	 * sysType 설정
	 *
	 * @param sysType the sysType to set
	 */
	public void setSysType(String sysType) {
		this.sysType = sysType;
	}

	/**
	 * sysId 얻기
	 *
	 * @return the sysId
	 */
	public String getSysId() {
		return sysId;
	}

	/**
	 * sysId 설정
	 *
	 * @param sysId the sysId to set
	 */
	public void setSysId(String sysId) {
		this.sysId = sysId;
	}
	
}
