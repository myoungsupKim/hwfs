<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<isEqual col="GUBUN" value="STD"> 
SELECT A.CODE,
       A.CODE_NAME,
       A.SORT_SEQ,
       nvl(B.PHF,'N') PHF
FROM   SCC_COMMON_CODE A,
       (
        SELECT '001' CODE,PHF_MGMT1 PHF FROM FSR_STD_RECIPE_MST WHERE RECIPE_CD=#RECIPE_CD#
        UNION ALL
        SELECT '002' CODE,PHF_MGMT2 PHF FROM FSR_STD_RECIPE_MST WHERE RECIPE_CD=#RECIPE_CD#
        UNION ALL
        SELECT '003' CODE,PHF_MGMT3 PHF FROM FSR_STD_RECIPE_MST WHERE RECIPE_CD=#RECIPE_CD#
        UNION ALL
        SELECT '004' CODE,PHF_MGMT4 PHF FROM FSR_STD_RECIPE_MST WHERE RECIPE_CD=#RECIPE_CD#
        UNION ALL
        SELECT '005' CODE,PHF_MGMT5 PHF FROM FSR_STD_RECIPE_MST WHERE RECIPE_CD=#RECIPE_CD#
        ) B
WHERE A.USE_YN='Y'
AND   A.GROUP_CODE='FS0018'
AND   A.CODE=B.CODE(+)
</isEqual>

<isEqual col="GUBUN" value="UPJANG"> 
SELECT A.CODE,
       A.CODE_NAME,
       A.SORT_SEQ,
       nvl(B.PHF,'N') PHF
FROM   SCC_COMMON_CODE A,
       (
        SELECT '001' CODE,PHF_MGMT1 PHF FROM FSR_UPJANG_RECIPE_MST WHERE UPJANG=#UPJANG# AND HALL_CLASS=#HALL_CLASS# AND RECIPE_CD=#RECIPE_CD#
        UNION ALL
        SELECT '002' CODE,PHF_MGMT2 PHF FROM FSR_UPJANG_RECIPE_MST WHERE UPJANG=#UPJANG# AND HALL_CLASS=#HALL_CLASS# AND RECIPE_CD=#RECIPE_CD#
        UNION ALL
        SELECT '003' CODE,PHF_MGMT3 PHF FROM FSR_UPJANG_RECIPE_MST WHERE UPJANG=#UPJANG# AND HALL_CLASS=#HALL_CLASS# AND RECIPE_CD=#RECIPE_CD#
        UNION ALL
        SELECT '004' CODE,PHF_MGMT4 PHF FROM FSR_UPJANG_RECIPE_MST WHERE UPJANG=#UPJANG# AND HALL_CLASS=#HALL_CLASS# AND RECIPE_CD=#RECIPE_CD#
        UNION ALL
        SELECT '005' CODE,PHF_MGMT5 PHF FROM FSR_UPJANG_RECIPE_MST WHERE UPJANG=#UPJANG# AND HALL_CLASS=#HALL_CLASS# AND RECIPE_CD=#RECIPE_CD#
        ) B
WHERE A.USE_YN='Y'
AND   A.GROUP_CODE='FS0018'
AND   A.CODE=B.CODE(+)
</isEqual>	</statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_recipe_phf">
		<col name="CODE" size="50" type="VARCHAR" description="" /> 
		<col name="CODE_NAME" size="100" type="VARCHAR" description="" /> 
	</output>
</query>
