function before_SaveCheck(str_Gubun)
{
       
       /*
        업무구분별로 addAction 지정세팅##########################################        
        */
        
        f_Saved_ID = 0;
        
	
        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
		
		
 		if (
 		     (ds_listInput.rowcount == 0)||
 		     (ds_listInput.getColumn(ds_listInput.rowposition, "APPROV_CONGUBN") == "N")
 		   )
 		{

			return;
		}

	    if (str_Gubun == "Master"){

			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))== "0")
			{
			
				g_Message("EE", "승인할 내역을 선택하여 주십시요.");
				edt_upjang.setFocus();
				return false;
			}
		
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "APPROV_CONGUBN"))== 0)
			{
			
				g_Message("EE", "승인여부가 누락되었습니다.");
				cbo_AUTO_AMT_YN.setFocus();
				return false;
			}
	        

				        
			if (
			     (ds_listInput.getColumn(ds_listInput.rowposition, "APPROV_CONGUBN") == "R")&&
			     (gfn_length(gfn_trim(ds_listInput.getColumn(ds_listInput.rowposition, "CONGUBN_DESC")))== 0)
			   )
			{
			
				g_Message("EE", "결재내역에 반려사유를 작성하여 주십시요.");
				edt_CONGUBN_DESC.setFocus();
				return false;
			}
							        


	        
	        //수정일때 Confirm체크##################################
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))!= "0")
               )
            {	        
				if (!g_Confirm("02", "폐기신청을 " + cbo_AUTO_AMT_YN.text + " 하시겠습니까?")) return false;
			}			
	        //수정일때 Confirm체크##################################
	        
	        
			//각 구분별 대체 마감체크##############################	        	        
			if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ"))) return false;			
			//각 구분별 대체 마감체크##############################	        
					        
		}			

        
		//저장#############################################################
	    if (str_Gubun == "Master"){

							           			
			// action 정보 초기화
			fsp_clear(this);	
			            	
			//식권정보 저장            
			fsp_addSingle(this, "ma/mas:MAS10300E_U001");			
			
			f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "TICKET_SEQ");
			fsp_callService(this, "", "", "ds_cond=ds_listInput:A", "",  "", "fn_AfterSaveSearch()");
	    }
	   
	    
		return true;
}


function fn_AfterSaveSearch(errCode, errMsg)
{
	if (errCode != 0){
		
		g_Message("EE");		
		return;
	}
		
    gfn_alert("msg.save.success");
	fn_search();	
	fn_insert();
		
	http.Sync = true;
	ds_list.rowposition = -1;
	ds_list.rowposition = ds_list.findRowExpr("TICKET_SEQ=" + f_Saved_ID);
	http.Sync = false;
	
}

