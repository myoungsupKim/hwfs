<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT CODE CORNER,
       CODE_NAME CORNER_NAME
FROM   SCC_COMMON_CODE
WHERE  GROUP_CODE='FS0022'
<isEqual col="MENU_TYPE" value="002">
AND    SET3='Y'
</isEqual>
<isEqual col="MENU_TYPE" value="001">
AND    CODE = '001'
</isEqual>
	</statement>
	<input default-name="ds_input">
		<col name="MENU_TYPE" size="255" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_Corner">
	</output>
</query>
