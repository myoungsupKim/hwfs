function fn_delete()
{

    // 삭제 확인메세지 호출
	if (!mfn_Call_DeleteMsg()){ return false; }		
		
    // 데이터셋이 Insert일 경우는 DeleteRow만 실행한다. 
	if(mfn_chk_rowtypeInsert_deleteRow(ds_nut_intake_list)){return false;}
					
	ds_nut_intake_list.deleteRow(ds_nut_intake_list.rowposition);
	
	// 트랜잭션 시작 
	fsp_clear(this);
		
	fsp_addSave(this
        , ""
        , ""
        , "fs/fsa:FSA00020E_D001"
        , ""
        , ""
        , ""
        , ""
        );
        
	// 서버 호출 
	fsp_callService(this, "", "", "ds_in=ds_nut_intake_list:U", "", "", "");

    // 트랜잭션 종료 
}

