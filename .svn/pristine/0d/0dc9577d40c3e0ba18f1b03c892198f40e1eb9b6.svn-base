//=========================================================================================

//=========================================================================================
// [ PART 3 ]
// Form Loading 시 작업 정의
//
//=========================================================================================

function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(obj, false);

	// action 정보 생성 초기화 
	fsp_init(this);

	// 데이터 변경체크
	v_ds_check = "ds_nut_intake_list";
	
	// 화면조회
	fn_search();
}

//=========================================================================================
// [ PART 4 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,도움말,form닫기)
//
//=========================================================================================
/*------------------------------------------------------------
	1. Function ID : fn_Search()
	2. 개       요 : 업무프로세스에 따른 조회작업
	3. 기       능 : 검색버튼 입력 시 처리되는 작업
	4. 인 수 설 명:
*------------------------------------------------------------*/
function fn_search()
{
	// action 정보 초기화 
	fsp_clear(this);
   	// 영양소 정보 조회
	fsp_addSearch(this, "fs/fsa:FSA00020E_S001");	
	// 서버 호출 
	fsp_callService(this, "", "", "", "ds_nut_intake_list=ds_nut_intake_list" , "", "fn_After_Search");
}

// 조회 후 콜백함수 
function fn_After_Search()
{
	// 영양소 정보 리스트 플래그 클리어
	ds_nut_intake_list.applyChange();
    // 필터적용
    fn_Filter_Apply();
}

//어떤 탭이 활성화 되었는지에 따라서 필터 적용
function fn_Filter_Apply()
{    
    var tabIndex = Tab00.tabindex;
    switch(tabIndex)
		{
		  case 0 : 
			  // TAB1 필터적용    
			  ds_nut_intake_list.filter("NUT_TYPE=='1'");
			  ds_nut_intake_list.rowposition = 0;
			  Tab00.tab1.ed_NUT_DESC.setFocus();
			  break;
		  case 1 :
			  // TAB2 필터적용    
			  ds_nut_intake_list.filter("NUT_TYPE=='2'");
			  ds_nut_intake_list.rowposition = 0;
			  Tab00.tab2.ed_NUT_DESC.setFocus();
			  break;
		  default :
			  ds_nut_intake_list.filter("");
		}
}

/*------------------------------------------------------------
	1. Function ID : fn_Insert()
	2. 개       요 : 업무프로세스에 따른 입력작업
	3. 기       능 : 입력버튼 입력 시 처리되는 작업
	4. 인 수 설 명:
*------------------------------------------------------------*/
function fn_insert()
{
  if(gfn_dsIsUpdated(ds_nut_intake_list))
  {
	var blnRtn = confirm("변경된 데이터가 있습니다. 저장하신 후에 신규로 입력하셔 주기기 바랍니다.\n\n변경된 데이터를 저장하시겠습니까?");
    if(blnRtn)
    	{
			fn_save();
			return true;
    	}
	else{
			return false;
		}
  }

  ds_nut_intake_list.addRow();
  if(this.Tab00.tabindex==0)
	{
	// 포커스 주기
	this.Tab00.tab1.ed_NUT_DESC.setFocus();
	// 구분값설정 - 일반일 경우 1
	this.Tab00.tab1.ed_NUT_TYPE.value=1;
	}
  else
    {
    this.Tab00.tab2.ed_NUT_DESC.setFocus();
    // 구분값설정 - 병원일 경우 2
	this.Tab00.tab2.ed_NUT_TYPE.value=2;
    }
}

/*------------------------------------------------------------
	1. Function ID : fn_Delete()
	2. 개       요 : 업무프로세스에 따른 정보 삭제작업
