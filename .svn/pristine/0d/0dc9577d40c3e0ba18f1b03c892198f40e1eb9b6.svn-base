    , ""
    , ""
    , ""
    , ""
    , ""
    );
    
	// 서버 호출 후 조회 
	fsp_callService(this, "", "", "ds_in=ds_nut_intake_list:U", "" , "", "fn_callback_search()");
	// 트랜잭션 종료 
}

