<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[--레시피검색및 등록
식단에 속한 개별 레시피삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM FSM_REAL_RECIPE_MST
WHERE 
	 UPJANG = #ds_Item.UPJANG#
	 AND MENU_CD = #ds_Item.MENU_CD#
	 AND RECIPE_CD = #RECIPE_CD#
	]]></statement>
	<input default-name="ds_list">
		<col name="UPJANG" size="22" type="NUMERIC" description="" /> 
		<col name="MENU_CD" size="20" type="VARCHAR" description="" /> 
		<col name="RECIPE_CD" size="20" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_Menu">
	</output>
</query>
