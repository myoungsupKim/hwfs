function magamCheck(){

	if(gfn_getLastDate(ds_list.getColumn(ds_list.rowposition, "CLOSE_DATE")) == cal_CLOSE_DATETO.value){
		return true;
	}

	fsp_clear(this);
	
	ds_cond.clear();
	
	ds_cond.addColumn("UPJANG","BIGDECIMAL",256);
	ds_cond.addColumn("CLOSE_GUBUN","STRING",256);
	ds_cond.addColumn("CLOSE_DATE","STRING",256);		
	ds_cond.addColumn("SYS_CLASS","STRING",256);		
	
	ds_cond.addRow();
	ds_cond.setColumn(ds_cond.rowposition, "UPJANG",  edt_upjang.getUpjangCode() );
	ds_cond.setColumn(ds_cond.rowposition, "CLOSE_GUBUN",  Get_chk_Id());
	ds_cond.setColumn(ds_cond.rowposition, "CLOSE_DATE",  cal_CLOSE_DATETO.value);
	if(Get_chk_Id() == "001"){ // 급식
		ds_cond.setColumn(ds_cond.rowposition, "SYS_CLASS",  11);
	}else if(Get_chk_Id() == "006"){ // 장례식
		ds_cond.setColumn(ds_cond.rowposition, "SYS_CLASS",  16);
	}else if(Get_chk_Id() == "008"){ // 폐식용유
		ds_cond.setColumn(ds_cond.rowposition, "SYS_CLASS",  19);
	}else if(Get_chk_Id() == "009"){ // 특판
		ds_cond.setColumn(ds_cond.rowposition, "SYS_CLASS",  17);
	}
	
	fsp_addSearch(this, "ma/mag:MAG10126E_S003");
	
	http.Sync = true;
	fsp_callService(this, "", "", "ds_cond=ds_cond","ds_magamChk=ds_magamChk","","");
	http.Sync = false;
	
	if(ds_magamChk.getColumn(0, "CNT") != 0){
		gfn_alert(cal_CLOSE_DATETO.value + " 이후에 발생한 매출마감이 있습니다.");
		return false;
	}else{
		return true;
	}
}

function btn_tmCls_onclick(obj:Button,  e:ClickEventInfo)
{
	// validation
	if( gfn_isNull(cbo_mu.getMuCode()) )
	{
		gfn_alert("사업부를 선택하세요.");
		return false;
	}

	if( gfn_isNull(edt_tmNm.getTmCode()) )
	{
		gfn_alert("팀을 선택하세요.");
		return false;
	}

	if( !chk_3.isChecked() && !chk_4.isChecked() )
	{
		gfn_alert("재고마감 또는 급식운영일지마감을 선택하세요.");
		return;
	}

	// 004 : 재고마감, 005 : 급식운영일지마감   
	var sType = "";

	if( chk_3.isChecked() )	sType = "004";
	else if( chk_4.isChecked() )	sType = "005";
	else return false;

	// 팝업 호출
	var args = {  v_muCd:cbo_mu.getMuCode()
				, v_tmCd:edt_tmNm.getTmCode()
				, v_tmNm:edt_tmNm.getTmName()
				, v_type:sType
			};

	gfn_dialog("", "U_MAG::MAG10126P.xfdl", args);
}]]></Script>
