function fn_search()
{
	if (!before_SelectCheck())
		return;
		
	if (numFormat(edt_upjang.getUpjangCode() )== "0")
	{
	
		gfn_alert("업장을 지정하여 주십시요.");
		edt_upjang.setFocus();
		return false;
	}
	//마감리스트############################################################
    fsp_clear(this);
    
	//fsp_addCsvSearch(this, "ma/mag:MAG10126E_S001");
    //fsp_addCSVSearch(this, "ma/mag:MAG10126E_S001"); 	
  fsp_addSearch(this, "ma/mag:MAG10126E_S001"); 	    

	ds_cond.clear();
	ds_cond.addColumn("MU_CD","STRING",256);
	ds_cond.addColumn("TM_CD","STRING",256);
	ds_cond.addColumn("UPJANG","BIGDECIMAL",256);
	ds_cond.addColumn("CLOSE_GUBUN","STRING",256);
	ds_cond.addColumn("CLOSE_DATEFR","STRING",256);		
	ds_cond.addColumn("CLOSE_DATETO","STRING",256);		
	
	
	ds_cond.addRow();
	ds_cond.setColumn(ds_cond.rowposition, "MU_CD",  cbo_mu.getMuCode() );
	ds_cond.setColumn(ds_cond.rowposition, "TM_CD",  edt_tmNm.getTmCode() );
	ds_cond.setColumn(ds_cond.rowposition, "UPJANG",  edt_upjang.getUpjangCode() );
	ds_cond.setColumn(ds_cond.rowposition, "CLOSE_GUBUN",  Get_chk_Id());
	ds_cond.setColumn(ds_cond.rowposition, "CLOSE_DATEFR",  cal_CLOSE_DATEFR.value);
	ds_cond.setColumn(ds_cond.rowposition, "CLOSE_DATETO",  cal_CLOSE_DATETO.value);
		
	fn_setSearchInfo();
	
