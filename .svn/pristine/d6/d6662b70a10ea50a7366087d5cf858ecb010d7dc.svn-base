function btn_MainUpjang_OnClick(obj:Button, e:ClickEventInfo)
{
	var str_Where="", str_DeptId="";

    if (obj.name == "edtBtn_DeptID") {
			str_Where += " AND EXISTS (SELECT 1 FROM FMS_UPJANG Z WHERE Z.DEPT_ID = V.DEPT_ID) ";
	}
	
	//권한에 따른 조회세팅 (파트추가)########################################################################			
	if (obj.name == "edtBtn_Part") {
	
		if (numFormat(edt_DeptID.userdata)!= "0") str_DeptId = " WHERE DEPT_ID = " + edt_DeptID.userdata;
		str_Where += "  AND (CODE IN (SELECT PART_CODE FROM FMS_UPJANG " + str_DeptId + ")) ";
		
	}
	
	if (obj.name == "edtBtn_ORD_SABUNQuery") {
	
		if (numFormat(edt_DeptID.userdata)!= "0") str_DeptId += " AND DEPT_ID   = "  + edt_DeptID.userdata;
		if (numFormat(edt_Part.userdata)!= "0") str_DeptId += " AND PART_CODE = '" + edt_Part.userdata + "'";
		
		str_Where += "  AND (V.SABUN IN (SELECT PART_SALES_SABUN FROM FMS_UPJANG WHERE 1=1 " + str_DeptId + ")) ";
		
	}	
	//권한에 따른 조회세팅 (파트추가)########################################################################
			
	
	
	fn_edtBtn_OnClick(obj, str_Where);
	fn_DataSetAllClear();
			
}



