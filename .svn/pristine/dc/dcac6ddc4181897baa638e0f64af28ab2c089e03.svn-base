function btn_CalcInsert_OnClick(obj:Button, e:ClickEventInfo)
{
	if( gfn_length(gfn_trim(ds_Line.getColumn(ds_Line.rowposition, "LINE_ID")))!= 0 ) {
		
		var v_ChkCnt = 0;
		for( var i=0; i<ds_Account.getRowCount(); i++ ) {
			if( ds_Account.getColumn(i, "CHK") == "1" ) {
				if( ds_Calc.findRow("ACCTCD", ds_Account.getColumn(i, "ACCTCD")) != -1 ) {
					g_Message("EE", "이미 수식에 반영된 계정이 존재합니다.\n삭제 또는 계정을 제외 후 추가해 주세요.");
					return;
				}
				v_ChkCnt++;
			}
		}
		if( v_ChkCnt == 0 ) {
			g_Message("EE", "선택되어진 계정과목이 없습니다.\n선택 후 추가하시기 바랍니다.");
			return;
		}
		
		var v_Row = 0;
		var v_Gubun = "";
		
		if( ds_Calc.getRowCount() == 0 ) {
			v_Gubun = "A";
		} else {
			if( g_Confirm("03", "추가하시려면 [확인],\n삽입하시려면 [취소] 버튼을 누르세요.") ) {
				v_Gubun = "A";
			} else {
				v_Gubun = "I";
			}
		}
		
		if( v_Gubun == "A" ) {
			for( var i=0; i<ds_Account.getRowCount(); i++ ) {
				if( ds_Account.getColumn(i, "CHK") == "1" ) {
					v_Row = ds_Calc.addRow();
					ds_Calc.setColumn(v_Row, "UPJANG",  ds_Input.getColumn(0, "MAIN_UPJANG")     );
					ds_Calc.setColumn(v_Row, "LINE_ID", ds_Line.getColumn(ds_Line.rowposition, "LINE_ID"));
					ds_Calc.setColumn(v_Row, "ACCTCD",  ds_Account.getColumn(i, "ACCTCD")        );
					ds_Calc.setColumn(v_Row, "ACCTNM",  ds_Account.getColumn(i, "ACCTNM")        );
					ds_Calc.setColumn(v_Row, "ADD_YN",  1);
				}
			}
		} else {
			for( var i=0; i<ds_Account.getRowCount(); i++ ) {
				if( ds_Account.getColumn(i, "CHK") == "1" ) {
					v_Row = ds_Calc.insertRow(ds_Calc.rowposition);
					ds_Calc.setColumn(v_Row, "UPJANG",  ds_Input.getColumn(0, "MAIN_UPJANG")     );
					ds_Calc.setColumn(v_Row, "LINE_ID", ds_Line.getColumn(ds_Line.rowposition, "LINE_ID"));
					ds_Calc.setColumn(v_Row, "ACCTCD",  ds_Account.getColumn(i, "ACCTCD")        );
					ds_Calc.setColumn(v_Row, "ACCTNM",  ds_Account.getColumn(i, "ACCTNM")        );
					ds_Calc.setColumn(v_Row, "ADD_YN",  1);
				}
