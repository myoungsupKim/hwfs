/*------------------------------------------------------------
	1. Function    : btn_LineInsert_OnClick(obj)
	2. Description : 손익계산서 양식라인에 계정코드 추가
	3. Arguments   : 
	4. History     : 2008. 02. 19 - 최초작성
*------------------------------------------------------------*/
function btn_LineInsert_OnClick(obj:Button, e:ClickEventInfo)
{
	if( ds_Account.getRowCount() > 0 ) {
		
		var v_ChkCnt = 0;
		for( var i=0; i<ds_Account.getRowCount(); i++ ) {
			if( ds_Account.getColumn(i, "CHK") == "1" ) {
				v_ChkCnt++;
			}
		}
		if( v_ChkCnt == 0 ) {
			g_Message("EE", "선택되어진 계정과목이 없습니다.\n선택 후 추가하시기 바랍니다.");
			return;
		}
		
		var v_Row = 0;
		var v_Gubun = "";
		if( ds_Line.getRowCount() == 0 ) {
			v_Gubun = "A";
		} else {
			if( g_Confirm("03", "추가하시려면 [확인],\n삽입하시려면 [취소] 버튼을 누르세요.") ) {
				v_Gubun = "A";
			} else {
				v_Gubun = "I";
			}
		}
		if( v_Gubun == "A" ) {
			for( var i=0; i<ds_Account.getRowCount(); i++ ) {
				if( ds_Account.getColumn(i, "CHK") == "1" ) {
					v_Row = ds_Line.addRow();
					ds_Line.setColumn(v_Row, "UPJANG",    ds_Input.getColumn(0, "MAIN_UPJANG"));
					ds_Line.setColumn(v_Row, "LINE_NAME", ds_Account.getColumn(i, "ACCTNM")   );
				}
			}
		} else {
			for( var i=0; i<ds_Account.getRowCount(); i++ ) {
				if( ds_Account.getColumn(i, "CHK") == "1" ) {
					v_Row = ds_Line.insertRow(ds_Line.rowposition);
					ds_Line.setColumn(v_Row, "UPJANG",    ds_Input.getColumn(0, "MAIN_UPJANG"));
					ds_Line.setColumn(v_Row, "LINE_NAME", ds_Account.getColumn(i, "ACCTNM")   );
				}
			}
		}
		ds_Calc.clearData();
	}
}
