/*------------------------------------------------------------
	1. Function    : fn_Init()
	2. Description : 기본 검색조건 셋팅
	3. Arguments   : 
	4. History     : 2008. 02. 18 - 최초작성
*------------------------------------------------------------*/
function fn_Init()
{
	// 권한에 따른 작업 분기
	if( g_UserKind == "본부" ) { // 본사 선택해야 함.
		// Default Focus Setting
		ed_MainUpjangNm.setFocus();
	} else if( g_UserKind == "본사" ) { // 본사 자동 셋팅.
		ds_Input.setColumn(0, "MAIN_UPJANG",   g_Main_Upjang  );
		ds_Input.setColumn(0, "MAIN_UPJANGNM", g_Main_UpjangNm);
		
		// 본사 조회 금지
		ed_MainUpjangNm.enable = false;
		btn_MainUpjang.enable  = false;
		
		// 메인사업장 셋팅
		ds_Input.setColumn(0, "MAIN_UPJANG",   g_Main_Upjang  );
		ds_Input.setColumn(0, "MAIN_UPJANGNM", g_Main_UpjangNm);
		
		// 기본양식 가져오기 버튼 활성화/비활성화 여부 체크
		fn_ChkDefault();
		
		// 공통코드 계정구분 콤보박스 리스트 조회
		fn_SearchAcctType();
		
		// 본사 사업장 계정리스트 조회
		fn_SearchAccount();
			
	} else if( g_UserKind == "사업장") { // 사업장 접근금지
