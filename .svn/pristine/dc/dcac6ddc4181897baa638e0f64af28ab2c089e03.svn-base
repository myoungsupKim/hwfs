/*
 ******************************************************************************************
 * 함  수  명   : fn_Search()
 * 기      능   : [조회버튼] 검색조건에 해당하는 정보를 조회
 ******************************************************************************************
 */
function fn_search()
{
	// 필수 검색조건 체크
	if( gfn_length(gfn_trim(ds_Input.getColumn(0, "MAIN_UPJANG")))== 0 ) {
		g_Message("EE", "본사를 선택해 주세요.");
		ed_MainUpjangNm.setFocus();
		return;
	}
	
	// 본사 사업장 손익계산표 양식 조회
	fn_SearchLine();
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_Insert()
 * 기      능   : [등록버튼] 등록 버튼 클릭 시 처리되는 작업
 ******************************************************************************************
 */
function fn_insert()
{
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_Save()
 * 기      능   : [저장버튼] 저장 버튼 클릭 시 처리되는 작업
 ******************************************************************************************
 */
function fn_save()
{
	if( gfn_dsIsUpdated(ds_Line)|| gfn_dsIsUpdated(ds_Calc)) {
		var v_Row = ds_Line.findRowExpr("LINE_NAME == null");
		
		if( v_Row != -1 ) {
			g_Message("EE", "손익계산서 출력명을 입력해 주세요.");
			ds_Line.rowposition = v_Row;
			return;
		} else {
			if( g_Confirm("02", "저장한 내용은 기존 손익양식에 즉시 적용되어\n과거 데이터가 조회되지 않을 수 있습니다.\n그래도 저장하시겠습니까?") ) {
				v_PreRow = ds_Line.rowposition;
				fsp_clear(this);
				fsp_callService(this, "FMO00110E_ACT", "saveLine", "ds_Line=ds_Line:U ds_Calc=ds_Calc:A", "", "", "fn_CallBackSave");
			}
		}
	} else {
		g_Message("EE", "변경된 내역이 없습니다.");
		return;
	}
}
/*------------------------------------------------------------
	1. Function    : fn_CallBackSave(errCode, errMsg)
	2. Description : fn_Save() CallBack Function
	3. Arguments   : 
	4. History     : 2008. 02. 19 - 최초작성
*------------------------------------------------------------*/
function fn_CallBackSave(errCode, errMsg)
{	
	if( errCode != 0 ) {
		return;
	} else {
		g_Message("02");
		fn_ChkDefault();
		fn_SearchLine();
		ds_Calc.clearData();
		return;
	}
}
