	3. Arguments   : 
	4. History     : 2008. 02. 19 - 최초작성
*------------------------------------------------------------*/
function fn_SearchLine()
{
	ds_Line.clearData();
	ds_Calc.clearData();
	fsp_clear(this);
	fsp_callService(this, "FMO00110E_ACT", "searchLine", "ds_Input=ds_Input", "ds_Line=ds_Line", "", "fn_CallBackSearchLine");
}
/*------------------------------------------------------------
	1. Function    : fn_CallBackSearchLine(errCode, errMsg)
	2. Description : fn_SearchLine() CallBack Function
	3. Arguments   : 
	4. History     : 2008. 02. 19 - 최초작성
*------------------------------------------------------------*/
function fn_CallBackSearchLine(errCode, errMsg)
{	
	if( errCode != 0 ) {
		return;
	} else {
		if(searchCnt == 0){
			cbo_AcctType.index = 0;
			searchCnt++;
		}
		
		if( ds_Line.getRowCount() > 0 ) {
			btn_Pop.enable = true;
		} else {
			btn_Pop.enable = false;
		}
		ds_Line.rowposition = v_PreRow;
		//  계산 공식 변경시 로우변경시 로직 ==>
		if(v_bln)
		{
			if( ds_Line.getRowCount() > 0 ) {
				if( gfn_length(gfn_trim(ds_Line.getColumn(v_PreRow, "LINE_ID")))!= 0 ) {
					ds_Input.setColumn(0, "LINE_ID", ds_Line.getColumn(v_PreRow, "LINE_ID"));
					fn_SearchCalc();
				} else {
					ds_Calc.clearData();
				}
			}
			v_bln=false;
		}
		//  <== 계산 공식 변경시 로우변경시 로직 
		v_PreRow = 0;
	}
}
