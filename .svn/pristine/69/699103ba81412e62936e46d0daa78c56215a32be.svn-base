<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT 
	 YY AS YY
	, STD_CD AS STD_CD
	, STD_NAME AS STD_NAME
	, STD_DESC AS STD_DESC
	, INC_DECR_STD AS INC_DECR_STD
	, STD_RATE AS STD_RATE
	, ATTR01 AS ATTR01
	, ATTR02 AS ATTR02
	, ATTR03 AS ATTR03
	, ATTR04 AS ATTR04
	, ATTR05 AS ATTR05
	, ATTR06 AS ATTR06
	, ATTR07 AS ATTR07
	, ATTR08 AS ATTR08
	, ATTR09 AS ATTR09
	, ATTR10 AS ATTR10
	, CUSER AS CUSER
	, CDATE AS CDATE
	, UUSER AS UUSER
	, UDATE AS UDATE
 FROM MAP_MAKEUP_STD1
 WHERE (1=1)
   AND  YY = #YY#	
  ORDER BY YY 
		  ,STD_CD]]></statement>
