	<statement>
<![CDATA[
UPDATE 
 	FMP_OTCUST_ITEM
SET 
	 OTCUSTCD = #OTCUSTCD#
	, ITEM_CLASS1 = #ITEM_CLASS1#
	, ITEM_CLASS2 = #ITEM_CLASS2#
	, ITEM_CLASS3 = #ITEM_CLASS3#
	, ITEM_CLASS4 = #ITEM_CLASS4#
	, ORI_ITEM_CODE = #ORI_ITEM_CODE#
	, ITEM_NAME = #ITEM_NAME#
	, ITEM_SIZE = #ITEM_SIZE#
	, PO_UOM = #PO_UOM#
	, UOM_CODE = #PO_UOM#
	, KG_CONVERT_RATE = #KG_CONVERT_RATE#	
	, TAX_CODE = #TAX_CODE#
	, CONSUM_YN = #CONSUM_YN#
	, CORN_YN = #CORN#
	, SPICE_YN = #SPICE#	
	, POINT_FLAG = #POINT_FLAG#
	, MIN_ORD_QTY = #MIN_ORD_QTY#
	, MULTIPLIER_QTY = #MULTIPLIER_QTY#
	, D_DAYS = #D_DAYS#
	, D_TIMES = #D_TIMES#
	, ORD_TYPE = #ORD_TYPE#
	, ORIGIN_TYPE = #ORIGIN_TYPE#
	, KEEPING_TYPE = #KEEPING_TYPE#
	, IMAGE_PATH = #IMAGE_PATH#
	, SIMAGE_PATH = #SIMAGE_PATH#
	, REMARK = #REMARK#
	, USE_YN = #USE_YN#
	, DEFAULT_FLAG = #DEFAULT_FLAG#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 ITEM_CODE = #ITEM_CODE#	]]></statement>
