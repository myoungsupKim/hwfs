	<statement>
<![CDATA[
INSERT INTO SCC_BBS_CON(
	 CON_ID
	, BBS_ID
	, TITLE
	, UPJANG
	, WRITE_SABUN
	, WRITE_TIME
	, CONTENTS
	, READ_CNT
	, SECRET_YN
	, PASSWORD
	, NOTICE_YN
	, NOTICE_EDATE
	, EDATE
	, PARENT_CON_ID
	, LEVELS
	, STEP
	, APPEND1
	, APPEND2
	, APPEND3
	, APPEND4
	, APPEND5
	, SAPPEND1
	, SAPPEND2
	, SAPPEND3
	, SAPPEND4
	, SAPPEND5
	, REMARK
	, USE_YN
	, ATTR01
	, ATTR02
	, ATTR03
	, ATTR04
	, ATTR05
	, ATTR06
	, ATTR09
	, ATTR10
	, CUSER
	, CDATE
	, UUSER
	, UDATE
	, READ_ALL_YN
 ) VALUES (
	 #CON_ID#
	, #BBS_ID#
	, #TITLE#
	, #UPJANG#
	, #WRITE_SABUN#
	, SYSDATE
	, SUBSTRB(#CONTENTS#, 1, 3900)
	, 0
	, #SECRET_YN#
	, #PASSWORD#
	, #NOTICE_YN#
	, #NOTICE_EDATE#
	, SUBSTR(TRIM(#EDATE#), 1, 8)
	, #PARENT_CON_ID#
	, #LEVELS#
	, #STEP#
	, #APPEND1#
	, #APPEND2#
	, #APPEND3#
	, #APPEND4#
	, #APPEND5#
	, #SAPPEND1#
	, #SAPPEND2#
	, #SAPPEND3#
	, #SAPPEND4#
	, #SAPPEND5#
	, #REMARK#
	, 'Y'
	, #ATTR01#
	, #ATTR02#
	, #ATTR03#
	, #ATTR04#
	, #ATTR05#
	, #ATTR06#
	, #IS_MOBILE#
	, #HTML_URL#
	, #g_EmpNo#
	, SYSDATE
	, #g_EmpNo#
	, SYSDATE
	, #READ_ALL_YN#
 )	]]></statement>
