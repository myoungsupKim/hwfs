function btn_appraisal_enable()
{
	if(rdo_ansr01.value != "" && rdo_ansr01.value != "" && rdo_ansr01.value != "" && rdo_ansr01.value != "" && rdo_ansr01.value != "")
	{
		if(rdo_ansr01.value == "001")
		{
			if(rdo_ansr02.value != "" && rdo_ansr03.value != "" && rdo_ansr04.value != "")
			{
				btn_appraisal.enable = true;
			}else{
				btn_appraisal.enable = false;
			}
		}else if(rdo_ansr01.value == "002"){
			if(rdo_ansr05.value != "" && rdo_ansr06.value != "" && rdo_ansr07.value != "" && rdo_ansr08.value != "")
			{
				btn_appraisal.enable = true;
			}else{
				btn_appraisal.enable = false;
			}
		}else{
			btn_appraisal.enable = true;
		}
	}else{
		btn_appraisal.enable = false;
	}
}

