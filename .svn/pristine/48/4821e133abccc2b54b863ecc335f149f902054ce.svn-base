//MU가 변경될 때에 대한 이벤트
function fn_muItemChangedEvent(obj, evt)
{
    edt_tmNm.setTmCode("");
    edt_upjangNm.setUpjangCode("");
        
    ds_search.setColumn(0,"muCd", edt_muNm.getMuCode());
    ds_search.setColumn(0,"muNm", edt_muNm.getMuName());
}

//TM이 변경될 때에 대한 이벤트
function fn_tmChangedEvent(obj, data)
{
    edt_upjangNm.setUpjangCode("");   
    ds_search.setColumn(0,"tmCd", edt_tmNm.getTmCode());
    ds_search.setColumn(0,"tmNm", edt_tmNm.getTmName());
}

//upjang이 변경될 때에 대한 이벤트
function fn_upjangChangedEvent(obj, data) {
    ds_search.setColumn(0,"upjangCd", edt_upjangNm.getUpjangCode());
    ds_search.setColumn(0,"upjangNm", edt_upjangNm.getUpjangName());
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 판매관리에서 호출 시 실행
 *---------------------------------------------------------------------------------------------*/
function fn_getScreenParams(arrArg)
{
    v_arg = arrArg;

    if (typeof v_arg != "undefined" && v_arg != null && v_arg != "")
    {
        edt_deadName.value             = arrArg[0];
		cal_borneOutFrom.mae_cal.value = arrArg[1];
		cal_borneOutTo.mae_cal.value   = arrArg[1];
		fn_search();
    }
}


/*
 ******************************************************************************************
 * 함  수  명   : fn_search()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 고인 Master 조회
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_search()
{
	var strSvcID    = "";
	var strURL      = "";
	var strInDs     = "";
	var strOutDs    = "";
	var strArg      = "";
	var strCallback = "";
	var bAsync      = true;
	
	// 조회조건 Param	
	ds_search.setColumn(0, "deadName", edt_deadName.value);
	ds_search.setColumn(0, "resideName", edt_resideName.value);
	ds_search.setColumn(0, "borneOutFrom", cal_borneOutFrom.mae_cal.value);
	ds_search.setColumn(0, "borneOutTo", cal_borneOutTo.mae_cal.value);
	ds_search.setColumn(0, "funeralHallCd", ds_search.getColumn(0, "upjangCd"));
		
	
	
	// v_ds_check 미사용시 변경사항 체크
	if (gfn_dsIsUpdated(ds_deadInfoReg))
	{
		var retValue = gfn_confirm("confirm.before.search");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
		if (retValue == false) return;
	}
	
	// 조회시 Validation Check
	//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

	// Grid sort 표시 초기화(필수)
	gfn_clearSortMark(tab_funeralPatronMstReg.tabpage1.grd_deadInfoReg);
	
	//서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
	fv_deadInfoRegColInfo = gfn_getDatasetInfo(ds_deadInfoReg);
	
	ds_deadInfoReg.clearData();
	
	strSvcID    = "search";
	strURL      = "fs/fsf/funeralPatronMstReg/selectList.xdo";
	strInDs     = "ds_search=ds_search:A";
	strOutDs    = "ds_deadInfoReg=ds_deadInfoReg";
	strArg      = "";
	strCallback = "fn_callBack";
	bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

function fn_insert(){
	var addRw = ds_deadInfoReg.addRow();
	ds_deadInfoReg.setColumn(addRw, "funeralHallCd", ds_search.getColumn(0, "upjangCd"));ds_deadInfoReg.setColumn(addRw, "guestNum", "0");	// 초기값
	ds_deadInfoReg.setColumn(addRw, "borneOutDate",	gfn_today());
	ds_deadInfoReg.setColumn(addRw, "enshrineDate",	gfn_today());
	ds_deadInfoReg.setColumn(addRw, "casketDate",	gfn_today());
	
	ds_deadInfoReg.setColumn(addRw, "borneOutDateTime",	(gfn_todayTime()).substr(8, 6));
	ds_deadInfoReg.setColumn(addRw, "enshrineDateTime",	(gfn_todayTime()).substr(8, 6));
	ds_deadInfoReg.setColumn(addRw, "casketDateTime",	(gfn_todayTime()).substr(8, 6));
	
	ds_deadInfoReg.setColumn(addRw, "persInfoAgreeYn", "1");
	
	SCPopup.Address.newData(ds_deadInfoReg, addRw, 'addrInfo');
	SCPopup.Address.newData(ds_deadInfoReg, addRw, 'addrInfo2');
}

function fn_delete(){
	if (this.ds_deadInfoReg.rowposition < 0) {
		gfn_alert("msg.err.grid.noselect");			// 선택된 항목이 없습니다.
	}else if(toNumber(ds_deadInfoReg.getColumn(ds_deadInfoReg.rowposition, "saleCnt"))>0) {
		gfn_alert("거래내역이 존재하여, 삭제할수 없습니다.");
	}else {
		var confirm = gfn_confirm("confirm.before.delete");			// 선택된 자료를 삭제 하시겠습니까?
		if (confirm)								// 삭제를 선택 하면
		{
			ds_deadInfoReg.deleteRow(ds_deadInfoReg.rowposition);
		}
	}
}


/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (!gfn_dsIsUpdated(ds_deadInfoReg)) 
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	// 저장시 Validation Check
	if (gfn_validCheck(ds_deadInfoReg, ds_deadInfoRegValid) == false)	return;
	
	var tmpKey = "";
	var curKey = "";
	for(var i=0; i<ds_deadInfoReg.rowcount; i++) {
	
		if(gfn_isNull(ds_deadInfoReg.getColumn(i, "funeralParlor"))||gfn_isNull(ds_deadInfoReg.getColumn(i, "theDeadNm"))) {
			gfn_alert("호실,고인명은 필수입력값입니다.");
			ds_deadInfoReg.rowposition = i;
			return;
		}
		
		curKey = ds_deadInfoReg.getColumn(i, "funeralParlor")+ds_deadInfoReg.getColumn(i, "enshrineDate");
		for(var j=0; j<ds_deadInfoReg.rowcount; j++) {
			if(i!=j) {
				tmpKey = ds_deadInfoReg.getColumn(j, "funeralParlor")+ds_deadInfoReg.getColumn(j, "enshrineDate");
				//trace(tmpKey +" ///// "+curKey);
				
				if(tmpKey == curKey) {
					gfn_alert("동일일자에 동일 빈소가 존재합니다.");
					ds_deadInfoReg.rowposition = j;
					return;
				}
			}
		}		
	}
	
	for(var k=0; k<ds_deadInfoReg.rowcount; k++) {
		 if(gfn_isNull(ds_deadInfoReg.getColumn(k, "borneOutDateTime"))) {
			ds_deadInfoReg.setColumn(k, "borneOutDateTime", "000000");
		 }
		 if(gfn_isNull(ds_deadInfoReg.getColumn(k, "enshrineDateTime"))) {
			ds_deadInfoReg.setColumn(k, "enshrineDateTime", "000000");
		 }	
	 	 if(gfn_isNull(ds_deadInfoReg.getColumn(k, "casketDateTime"))) {
			ds_deadInfoReg.setColumn(k, "casketDateTime", "000000");
	 	 }
	}
	
    var strSvcID    = "save";
    var strURL      = "fs/fsf/funeralPatronMstReg/saveList.xdo";
    var strInDs     = "ds_deadInfoReg=ds_deadInfoReg:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_Close()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 단기 버튼 이벤트
