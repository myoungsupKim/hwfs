/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	SCCombo.bind(this, "ComCode", "tab_funeralPatronMstReg.tabpage1.grd_deadInfoReg.funeralParlor", ds_parlorType, {groupCode:"FUNERAL_PARLOR_TYPE", useYn:"Y", defVal:"Y"});	// 빈소,호실
	SCCombo.bind(this, "ComCode", "tab_funeralPatronMstReg.tabpage1.grd_deadInfoReg.religion", ds_religion, {groupCode:"RELIGION_TYPE", useYn:"Y", defVal:"Y"});	   // 종교
	
	SCCombo.bind(this, "ComCode", "tab_funeralPatronMstReg.tabpage1.grd_deadInfoReg.relation", ds_familyType, {groupCode:"FAMILY_TYPE", useYn:"Y", defVal:"Y"}); // 가족유형,관계
	
	
	ds_search.clearData();
	var nRow= ds_search.addRow();		
	ds_search.setColumn(nRow, "muCd", g_Mu_Cd);
	ds_search.setColumn(nRow, "muNm", g_Mu_Nm);
	ds_search.setColumn(nRow, "tmCd", g_Tm_Cd);
	ds_search.setColumn(nRow, "tmNm", g_Tm_Nm);
	ds_search.setColumn(nRow, "upjangCd", g_Upjang);
	ds_search.setColumn(nRow, "upjangNm", g_UpjangNm);
	ds_search.setColumn(nRow, 'maskAuth', SCAuth.getMaskAuth(this));
	
	ds_search.setColumn(nRow, "loginPersMask", g_PersInfoMask);
	
	SCPopup.bind(this, SCPopup.AddrSearch, "tab_funeralPatronMstReg.tabpage1.grd_deadInfoReg.zipCode", "GRID:", null, 
					{addrKey:"addrKey", addrInfo:"addrInfo", addrClass:"addrClass", baseAddr:"baseAddr", detailAddr:"detailAddr", etcAddr:"etcAddr"});
					
	SCPopup.bind(this, SCPopup.AddrSearch, "tab_funeralPatronMstReg.tabpage1.grd_deadInfoReg.zipCode2", "GRID:", null, 
					{addrKey:"addrKey2", addrInfo:"addrInfo2", addrClass:"addrClass2", baseAddr:"baseAddr2", detailAddr:"detailAddr2", etcAddr:"etcAddr2"});
	
	
	
	// 발인일 오늘 날짜로 설정
	cal_borneOutFrom.fn_initMonthCal(this, fv_sToday, "", ds_search, "borneOutFrom");
	cal_borneOutTo.fn_initMonthCal(this, gfn_addDate(fv_sToday, 3), "", ds_search, "borneOutTo");
	
	var arrArg = gfn_getScreenParams();
	if(!gfn_isNull(arrArg)) {
		edt_deadName.value     = arrArg[0];
		cal_borneOutFrom.mae_cal.value = arrArg[1];
		cal_borneOutTo.mae_cal.value   = arrArg[1];
		fn_search();
	}
	
	// 급식, 식재, 외식, 구매 사업부는 팀, 업장 변경 불가.
	if((gfn_indexOf(ds_search.getColumn(0,"muNm"), "FS", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "식재", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "외식", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "구매", 0) >= 0)) {
		
		edt_upjangNm.enable = false;
		
	// FC사업지원본부는 사업부/팀/업장 변경 가능	
	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FC", 0) >= 0) {
		edt_upjangNm.enable = true;
	}
}



