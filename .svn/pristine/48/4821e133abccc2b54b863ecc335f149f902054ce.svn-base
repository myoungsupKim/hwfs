function fn_Close()
{
	this.close();
}


/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		case "search" :
            if(ds_deadInfoReg.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_deadInfoReg, fv_deadInfoRegColInfo);
			}
            break;
 
        
        // 저장
        case "save" :
			if(fv_dupCnt == "0") {
				gfn_alert("msg.save.success");	// 저장 되었습니다.
				fn_search();
			}else{
				gfn_alert("기존 안치일,발인일 구간에 동일 빈소는 제외처리 되었습니다.");
				fn_search();
			}	
            break;
    }
}


