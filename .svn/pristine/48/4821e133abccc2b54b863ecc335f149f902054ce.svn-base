{
	
	if(e.keycode==13){
		fn_search();
	}
}

function ds_deadInfoReg_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if(e.columnid == "borneOutDate"){
		if(ds_deadInfoReg.getColumn(e.row, "borneOutDate") < ds_deadInfoReg.getColumn(e.row, "casketDate")){
			gfn_alert("발인일은 입관일이후로 등록 가능합니다.");
			ds_deadInfoReg.setColumn(e.row, "borneOutDate", fv_sToday);
			return false;
		}
	}
}
