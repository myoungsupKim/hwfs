/*----------------------------------------------------------------------------------------------
 * 설명      : 일괄적용시 적용금액=잔액  일괄적용 아닐시 잔액에서 순차적으로 차감
 *---------------------------------------------------------------------------------------------*/
function cbo_allEditYn_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	var comboData  = obj.value;
	var allEditAmt = msk_allEditAmt.value;
	
	fn_change_receiveAmt_listAr(comboData, allEditAmt);
}

function fn_change_receiveAmt_listAr(allEditYn, allAmt)
{
    if (allAmt == "" || allAmt == "0" || allAmt == null && allEditYn == "N"){
        msk_allEditAmt.enable = true;
        //적용금액 일괄 잔액금액으로 셋팅
		for (var i = 0 ; i < ds_listAr.rowcount ; i ++){
			ds_listAr.setColumn(i, "receiveAmt", 0);
		}
    } else {
		if (allEditYn == "Y"){
			msk_allEditAmt.enable = false;
			
			//적용금액 일괄 잔액금액으로 셋팅
			for (var i = 0 ; i < ds_listAr.rowcount ; i ++){
				ds_listAr.setColumn(i, "receiveAmt", ds_listAr.getColumn(i, "arBalAmt"));
			}
		} else if (allEditYn == "N"){
			msk_allEditAmt.enable = true;
			
			//적용금액을 순차적으로 차감 일괄적용금액만큼
			for (var i = 0 ; i < ds_listAr.rowcount ; i ++){
				var arBalAmt = ds_listAr.getColumn(i, "arBalAmt");
				
				if (allAmt > arBalAmt){
					allAmt -= arBalAmt;
					ds_listAr.setColumn(i, "receiveAmt", ds_listAr.getColumn(i, "arBalAmt"));
				} else {
					ds_listAr.setColumn(i, "receiveAmt", allAmt);
					allAmt = 0;
				}
			}
		}
	}
}

function msk_addEditAmt_ontextchanged(obj:MaskEdit,  e:TextChangedEventInfo)
{
	var comboData  = cbo_allEditYn.value;
	var allEditAmt = obj.value;
	
	fn_change_receiveAmt_listAr(comboData, allEditAmt);
}
