			ds_listSuspense.setColumn(nRow, "notesPublishDate", dsObj.getColumn(i, "notesPublishDate"));
			ds_listSuspense.setColumn(nRow, "notesExpireDate",  dsObj.getColumn(i, "notesExpireDate"));
