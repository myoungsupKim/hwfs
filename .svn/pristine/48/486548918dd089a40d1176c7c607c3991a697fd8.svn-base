<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>update</type>
	<description><![CDATA[주기식단식수예측
UPDATE]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
UPDATE 
 	FSH_CYCLE_MENU_MST
SET 
	 EXPECT_MEAL_QTY = #EXPECT_MEAL_QTY#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE UPJANG = #UPJANG#
	 AND MENU_CD = #MENU_CD#
	</statement>
	<input default-name="ds_EXPECT_MEAL">
		<col name="CYCLE_CD" size="20" type="VARCHAR" description="" /> 
		<col name="UPJANG" size="22" type="NUMERIC" description="" /> 
		<col name="MENU_CD" size="20" type="VARCHAR" description="" /> 
		<col name="EXPECT_MEAL_QTY" size="22" type="NUMERIC" description="" /> 
	</input>
	<output default-name="ds_out">
	</output>
</query>
