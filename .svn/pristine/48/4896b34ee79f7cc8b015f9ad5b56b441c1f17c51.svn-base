<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[업장/식당에 딸린 끼니 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>

SELECT '000' AS CODE, '- 전체 -' AS CODE_NAME, -1 AS SORT_SEQ
  FROM DUAL
 UNION ALL
SELECT DISTINCT B.MEAL AS CODE, C.CODE_NAME, C.SORT_SEQ
  FROM FSA_HALL_MST A
     , FSA_HALL_DTL B
     , SCC_COMMON_CODE C
     , FSA_UPJANG_GRP E
 WHERE 1=1 
  	AND A.UPJANG = E.UPJANG
    AND E.GRP_SEQ = #UPJANG# --필수 검색조건(업장그룹)     
   --AND A.HALL_CD = #HALL_CD#
   AND A.UPJANG = B.UPJANG
   AND A.HALL_CD = B.HALL_CD
   AND C.GROUP_CODE = 'FS0023'
   AND B.MEAL = C.CODE
ORDER BY SORT_SEQ ASC 
</statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_Meal">
	</output>
</query>
