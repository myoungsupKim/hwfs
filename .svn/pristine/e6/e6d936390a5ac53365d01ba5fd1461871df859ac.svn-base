<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[주간식단표의 헤더 주간 날짜 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT 0 AS LEV, NULL AS MENU_NAME, 0 SEQ,
       TO_CHAR(TO_DATE(WEEK_DAY1, 'RRRR-MM-DD'),'YYYY.MM.DD') || '(' || TO_CHAR(TO_DATE(WEEK_DAY1, 'RRRR-MM-DD'),'dy','NLS_DATE_LANGUAGE=KOREAN') || ')' AS WEEK_DAY1, NULL WEEK_KCAL1,
       TO_CHAR(TO_DATE(WEEK_DAY2, 'RRRR-MM-DD'),'YYYY.MM.DD') || '(' || TO_CHAR(TO_DATE(WEEK_DAY2, 'RRRR-MM-DD'),'dy','NLS_DATE_LANGUAGE=KOREAN') || ')' AS WEEK_DAY2, NULL WEEK_KCAL2,
       TO_CHAR(TO_DATE(WEEK_DAY3, 'RRRR-MM-DD'),'YYYY.MM.DD') || '(' || TO_CHAR(TO_DATE(WEEK_DAY3, 'RRRR-MM-DD'),'dy','NLS_DATE_LANGUAGE=KOREAN') || ')' AS WEEK_DAY3, NULL WEEK_KCAL3,
       TO_CHAR(TO_DATE(WEEK_DAY4, 'RRRR-MM-DD'),'YYYY.MM.DD') || '(' || TO_CHAR(TO_DATE(WEEK_DAY4, 'RRRR-MM-DD'),'dy','NLS_DATE_LANGUAGE=KOREAN') || ')' AS WEEK_DAY4, NULL WEEK_KCAL4,
       TO_CHAR(TO_DATE(WEEK_DAY5, 'RRRR-MM-DD'),'YYYY.MM.DD') || '(' || TO_CHAR(TO_DATE(WEEK_DAY5, 'RRRR-MM-DD'),'dy','NLS_DATE_LANGUAGE=KOREAN') || ')' AS WEEK_DAY5, NULL WEEK_KCAL5,
       TO_CHAR(TO_DATE(WEEK_DAY6, 'RRRR-MM-DD'),'YYYY.MM.DD') || '(' || TO_CHAR(TO_DATE(WEEK_DAY6, 'RRRR-MM-DD'),'dy','NLS_DATE_LANGUAGE=KOREAN') || ')' AS WEEK_DAY6, NULL WEEK_KCAL6,
       TO_CHAR(TO_DATE(WEEK_DAY7, 'RRRR-MM-DD'),'YYYY.MM.DD') || '(' || TO_CHAR(TO_DATE(WEEK_DAY7, 'RRRR-MM-DD'),'dy','NLS_DATE_LANGUAGE=KOREAN') || ')' AS WEEK_DAY7, NULL WEEK_KCAL7
  FROM (SELECT (SELECT TO_CHAR(TO_DATE(#FROM_MENU_DATE#, 'YYYYMMDD'), 'YYYYMMDD') FROM DUAL) WEEK_DAY1,
               (SELECT TO_CHAR(TO_DATE(#FROM_MENU_DATE#, 'YYYYMMDD') + 1, 'YYYYMMDD') FROM DUAL) WEEK_DAY2,
               (SELECT TO_CHAR(TO_DATE(#FROM_MENU_DATE#, 'YYYYMMDD') + 2, 'YYYYMMDD') FROM DUAL) WEEK_DAY3,
               (SELECT TO_CHAR(TO_DATE(#FROM_MENU_DATE#, 'YYYYMMDD') + 3, 'YYYYMMDD') FROM DUAL) WEEK_DAY4,
               (SELECT TO_CHAR(TO_DATE(#FROM_MENU_DATE#, 'YYYYMMDD') + 4, 'YYYYMMDD') FROM DUAL) WEEK_DAY5,
               (SELECT TO_CHAR(TO_DATE(#FROM_MENU_DATE#, 'YYYYMMDD') + 5, 'YYYYMMDD') FROM DUAL) WEEK_DAY6,
               (SELECT TO_CHAR(TO_DATE(#FROM_MENU_DATE#, 'YYYYMMDD') + 6, 'YYYYMMDD') FROM DUAL) WEEK_DAY7
          FROM DUAL)	</statement>
	<input default-name="ds_Input">
	</input>
	<output default-name="ds_Header">
	</output>
</query>