 */
function btn_copy_onclick(obj:Button,  e:ClickEventInfo)
{
	var copyRowCnt = 0;
	ds_copyData.clearData();

	if (ds_listRoleMenu.getRowCount() < 1)
	{
		gfn_alert("sc.no.copydata");
		return;
	}
	
	for (var i=0,len=ds_listRoleMenu.getRowCount(); i<len; i++)
	{
		if (ds_listRoleMenu.getSelect(i))
		{
			copyRowCnt++;
		}
	}
	
	if (copyRowCnt > 0)
	{
		if (gfn_confirm("confirm.before.copy"))
		{
			var nRow;
			for (var i=0,len=ds_listRoleMenu.getRowCount(); i<len; i++)
