function grd_listRoleMenu_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (fv_buttonIdx == e.col)
	{
		var strId = "AuthByMenuRetvPop";
		var strURL = "X_SCA::AuthByMenuRetvPop.xfdl";
		var objArgument = {
			P_SYS_TYPE : cbo_sysType.value,
			P_SYS_ID : ds_listRoleMenu.getColumn(ds_listRoleMenu.rowposition, "sysId"),
			P_MENU_ID : ds_listRoleMenu.getColumn(ds_listRoleMenu.rowposition, "menuId")
		};
		var nWidth = 896;
		var nHeight = 630;
		var strStyle = "";
		var nLeft = -1;
		var nTop = -1;
		var isModeless = false;
		var bShowTitle = true;
		var strAlign = "-1";
	
		gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);
	}
	
}

/**
 * @class 조회/입력/삭제/저장/출력/엑셀의 헤더를 클릭시 전체선택/해제, 그 외는 정렬을 수행한다.
 * @param obj - Event Object
 * @param e - Event 발생정보
 * @return 
 */
function grd_listRoleMenu_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.cell >= 12 && e.cell <= 17)
	{
		switch (e.cell)
		{
			case 12 :
				fv_srcCheckAll = fn_doCheck(fv_srcCheckAll, "srcYn", "prgSrcYn");
				break;
			case 13 :
				fv_insCheckAll = fn_doCheck(fv_insCheckAll, "insYn", "prgInsYn");
				break;
			case 14 :
				fv_delCheckAll = fn_doCheck(fv_delCheckAll, "delYn", "prgDelYn");
				break;
			case 15 :
				fv_savCheckAll = fn_doCheck(fv_savCheckAll, "savYn", "prgSavYn");
				break;
			case 16 :
				fv_prtCheckAll = fn_doCheck(fv_prtCheckAll, "prtYn", "prgPrtYn");
				break;
			case 17 :
				fv_exlCheckAll = fn_doCheck(fv_exlCheckAll, "exlYn", "prgExlYn");
				break;
		}
	}
	else
	{
		gfn_gridSort(obj, e);
	}
}

