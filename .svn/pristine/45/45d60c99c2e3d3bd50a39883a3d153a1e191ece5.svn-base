 * @param p - 발생정보
 * @return 
 */
function fn_triggerEvent(p)
{
	if (p.oid == "cbo_sysType")
	{
		SCCombo.bind(this, "System", "cbo_sysId", null, {sysType:p.object.value, useYn:"Y", defVal:"firstrow"}, SCCombo.ALL);
		fn_searchRoleList(); //시스템별 권한을 조회한다.
	}
}

/**
 * @class 정의된 컬럼의 Check를 체크하거나 해제한다.
 * @param obj - Event Object
 * @param e - Event 발생정보
 * @return 
 */
function fn_doCheck(isCheck, columnId1, columnId2)
{
	for (var i=0,len=ds_listRoleMenu.getRowCount(); i<len; i++)
	{
		if (isCheck && (ds_listRoleMenu.getColumn(i, columnId2) == "1"))
		{
			ds_listRoleMenu.setColumn(i, columnId1, "0");
		}
		else if (!isCheck && (ds_listRoleMenu.getColumn(i, columnId2) == "1"))
		{
			ds_listRoleMenu.setColumn(i, columnId1, "1");
		}
	}
	
	return !isCheck;
}


function fn_sendDataset(ds) {
	//trace(ds.getRowCount());
	for (var i=0,len=ds_listRoleMenu.getRowCount(); i<len; i++) {
		ds_listRoleMenu.selectRow(i);
	}
	
	btn_rightToLeft.click();
	
	for (var i=0,len=ds.getRowCount(); i<len; i++) {
		var nRow = ds_listRoleMenu.addRow();
		ds_listRoleMenu.setColumn(nRow, "sysId", ds.getColumn(i, "sysId"));
		ds_listRoleMenu.setColumn(nRow, "role", cbo_role.value);
		ds_listRoleMenu.setColumn(nRow, "sysName", ds.getColumn(i, "sysName"));
		ds_listRoleMenu.setColumn(nRow, "menuId", ds.getColumn(i, "menuId"));
		ds_listRoleMenu.setColumn(nRow, "menuName", ds.getColumn(i, "menuName"));
		ds_listRoleMenu.setColumn(nRow, "parentMenuId", ds.getColumn(i, "parentMenuId"));
		ds_listRoleMenu.setColumn(nRow, "parentMenuName", ds.getColumn(i, "parentMenuName"));
		ds_listRoleMenu.setColumn(nRow, "sysType", ds.getColumn(i, "sysType"));
		ds_listRoleMenu.setColumn(nRow, "prgSrcYn", ds.getColumn(i, "srcYn"));
		ds_listRoleMenu.setColumn(nRow, "prgInsYn", ds.getColumn(i, "insYn"));
		ds_listRoleMenu.setColumn(nRow, "prgDelYn", ds.getColumn(i, "delYn"));
		ds_listRoleMenu.setColumn(nRow, "prgSavYn", ds.getColumn(i, "savYn"));
		ds_listRoleMenu.setColumn(nRow, "prgPrtYn", ds.getColumn(i, "prtYn"));
		ds_listRoleMenu.setColumn(nRow, "prgExlYn", ds.getColumn(i, "exlYn"));
		ds_listRoleMenu.setColumn(nRow, "srcYn", ds.getColumn(i, "srcYn")=="1"?"1":"");
		ds_listRoleMenu.setColumn(nRow, "insYn", ds.getColumn(i, "insYn")=="1"?"1":"");
		ds_listRoleMenu.setColumn(nRow, "delYn", ds.getColumn(i, "delYn")=="1"?"1":"");
		ds_listRoleMenu.setColumn(nRow, "savYn", ds.getColumn(i, "savYn")=="1"?"1":"");
		ds_listRoleMenu.setColumn(nRow, "prtYn", ds.getColumn(i, "prtYn")=="1"?"1":"");
		ds_listRoleMenu.setColumn(nRow, "exlYn", ds.getColumn(i, "exlYn")=="1"?"1":"");
		ds_listRoleMenu.setColumn(nRow, "useYn", 1);
		ds_listRoleMenu.setColumn(nRow, "rowPos", ds.getColumn(i, "rowPos"));
		
		var findRow = ds_listMenu.findRow("menuId", ds.getColumn(i, "menuId"));
		ds_listMenu.deleteRow(findRow);
		//ds_listMenu.setColumn(i, "isRoleMenuId", ds.getColumn(i, "menuId"));
		//ds_listMenu.setColumn(i, "chk", "");
	}
}
