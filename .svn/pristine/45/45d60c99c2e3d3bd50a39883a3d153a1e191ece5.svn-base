 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_listRoleMenu.saveXML());
            btn_getData.enable = true;
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;
		
		//시스템별 Role조회
		case "searchRole" :
			var defVal = ds_cbo_role.getColumn(0, "role");
			cbo_role.value = defVal;
			//fn_search();
			ds_copyData.clearData();
			break;
		case "searchGetMenu" :
			if (gfn_confirm("기존에 등록된 모든정보를 삭제하고 새로 가져오시겠습니까?")) {
				
			}
			break;
    }
}

