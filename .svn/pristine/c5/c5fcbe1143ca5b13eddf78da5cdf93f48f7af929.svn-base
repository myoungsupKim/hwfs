<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT COUNT(*) CNT
  FROM SCC_COMMON_CODE V
  WHERE (V.GROUP_CODE    = 'MA0122')
  AND   (V.CODE          = #ITEM_TYPE#)
  AND   (V.SET1          = 'Y')
</statement>
	<input default-name="ds_cond">
<col name="ITEM_TYPE" size="" type="NUMERIC" description="" />	
	</input>
	<output default-name="ds_list">
<col name="CNT" size="" type="NUMERIC" description="" />
	</output>
</query>