 * @param obj TextArea
 * @param e TextChangedEventInfo
 * @return none
 */
function Tab00_tabpage_txt_content_ontextchanged(obj:TextArea,  e:TextChangedEventInfo)
{
	//입력한 글자수를 표시
	Tab00.tabpage.edt_count.value = obj.getLength();
}
/*----------------------------------------------------------------------------------------------
 * 설명      : [∨][∧] 클릭시 행 이동 삭제 
 *---------------------------------------------------------------------------------------------*/
function Tab00_tabpage_btn_poolAdd_onclick(obj:Button,  e:ClickEventInfo)
{
	var nRow = null;
    var iFindRowIdx = 0;
    
    for(var i=0; i< ds_pool.rowcount; i++)
    {
		if(ds_pool.getColumn(i, "chk") == 1)
		{
			iFindRowIdx = ds_sendList.findRow("adoptionNum", ds_pool.getColumn(i, "adoptionNum"), 0, -1);
			
			if(iFindRowIdx == -1)
			{
				nRow = ds_sendList.addRow();
				ds_sendList.copyRow(nRow, ds_pool, i);
			}	
		}
    }
}

function Tab00_tabpage_btn_poolDel_onclick(obj:Button,  e:ClickEventInfo)
{
	for(var i=ds_sendList.rowcount-1; i>=0 ; i--)
    {
		if(ds_sendList.getColumn(i, "chk") == 1)
		{
			ds_sendList.deleteRow(i);
		}
    }
}
