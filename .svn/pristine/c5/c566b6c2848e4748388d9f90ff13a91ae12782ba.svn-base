<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[레시피유형 코드 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT '0' CODE, '- 전체 -' CODE_NAME, 0 SORT_SEQ
  FROM DUAL
 UNION ALL
SELECT CODE, CODE_NAME NM, SORT_SEQ
  FROM SCC_COMMON_CODE 
 WHERE GROUP_CODE='FS0002'
   AND USE_YN='Y'
 <isNotNull col="SET1">
   AND SET1 = #SET1#
 </isNotNull>
 ORDER BY SORT_SEQ	</statement>
	<input default-name="ds_RecipeType">
	</input>
	<output default-name="ds_RecipeType">
	</output>
</query>
