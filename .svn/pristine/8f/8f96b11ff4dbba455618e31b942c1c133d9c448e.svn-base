	SCReport.setParams(arrParams, "upjangCd", ds_list.getColumn(ds_list.rowposition, "upjangCd"));
	SCReport.setParams(arrParams, "stdRecipeCd", ds_list.getColumn(ds_list.rowposition, "stdRecipeCd"));
