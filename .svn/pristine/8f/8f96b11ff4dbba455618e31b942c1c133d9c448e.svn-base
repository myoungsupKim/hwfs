 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";
	
	//검색조건을 초기화 한다.
	var nRow = ds_search.addRow();
	
	//UpjangSearchComponent00.setForcedRoleSet("CostCenter");
	TmSearchComponent01.applyAuthLevel(MuSearchComponent01);
	schUpjang.applyAuthLevel(TmSearchComponent01);	
	
	//Dataset
	SCDataset.bind(this, "ComCode", "ds_muCd", {groupCode:"DATA_SET", code:"DEF_MU_CODE", useYn:"Y"});
	
	//Combo
	SCCombo.bind(this, "ComCode", "cbo_stand", ds_standType, {groupCode:"STAND_TYPE", useYn:"Y", defVal:"01"});		//레시피단계
	SCCombo.bind(this, "ComCode", "cbo_offerUnit", ds_offerUnit, {groupCode:"OFFER_UNIT", useYn:"Y", defVal:"A"});	//투입단위
	SCCombo.bind(this, "ComCode", "cbo_oferUnit", ds_oferUnit, {groupCode:"OFER_UNIT", useYn:"Y", defVal:"A"});			//투입단위
	SCCombo.bind(this, "ComCode", "cbo_recipeStatus", ds_recipeStatus, {groupCode:"RECIPE_STATUS", useYn:"Y"});		//레시피상태
	SCCombo.bind(this, "ComCode", "cbo_useYn", ds_useYn, {groupCode:"USE_YN", useYn:"Y"});							//사용여부
	SCCombo.bind(this, "ComCode", "cbo_mcalssCd", ds_mcalssCd, {groupCode:"MCALSS_CD", useYn:"Y"});					//중분류코드
	SCCombo.bind(this, "ComCode", "cbo_sclassCd", ds_sclassCd, {groupCode:"SCLASS_CD", useYn:"Y"});					//소분류코드
	SCCombo.bind(this, "ComCode", "cbo_upjangModifyAllowYn", ds_upjangModifyAllowYn, {groupCode:"UPJANG_MODIFY_ALLOW_YN", useYn:"Y"});	//업장수정허용여부
	
	SCCombo.bind(this, "ComCode", "cbo_schSclassCd", null, {groupCode:"SCLASS_CD", useYn:"Y"}, SCCombo.ALL);		//소분류코드(검색조건)
	SCCombo.bind(this, "ComCode", "cbo_schUseYn", null, {groupCode:"USE_YN", useYn:"Y", defVal:"Y"}, SCCombo.ALL);			//사용여부(검색조건)
	
	//Radio
	SCRadio.bind(this, "ComCode", "rdo_stdRecipeApplyBasis", ds_stdRecipeApplyBasis, {groupCode:"STD_RECIPE_APPLY_BASIS", useYn:"Y", defVal:"1"});	//표준레시피반영기준
	
	//Popup
	//SCPopup.bind(this, "Recipe", "edt_schRecipeNm", "btn_recipePop", null, {muCd:"", upjangType:""}, {before:"fn_recipeBeforeEvent", after:"fn_recipeAfterEvent"});
	
	//레시피 분류코드 검색(소분류 코드)
	//fn_sclassCdSearch();
	
	//Calendar
	cal_regDate.fn_initMonthCal(this, "", "", ds_list, "regDate");
	
	fn_searchRsr();
}

