/*----------------------------------------------------------------------------------------------
 * 설명      : 보정내역 삭제
 *---------------------------------------------------------------------------------------------*/
function btn_delete_onclick(obj:Button,  e:ClickEventInfo)
{
	// 보정 목록에서 삭제 : 선택한 행의 자재를 바로 삭제
	var nRow = ds_list2.rowposition;
	var v_status = ds_list2.getColumn(nRow, "status");
	
	if(fn_magamFlag())return;//매출마감여부
	
	if(nRow == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	// 매출등록(수정불가) 일 경우에는 취소 안됨
	if(v_status=="3") {
		sMsg = "매출등록된 보정자료는 삭제할 수 없습니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}	
	
	var sMsg = "선택한 보정자료를 삭제합니다.\n계속 진행 하시겠습니까?\n\n";
		sMsg = sMsg + " - 사업장   : " + ds_list2.getColumn(ds_list2.rowposition, "upjangNm") + "\n";
		sMsg = sMsg + " - 창고 : " + ds_list2.getColumn(ds_list2.rowposition, "subinvName") + "\n";
		sMsg = sMsg + " - 보정일자   : " + ds_list2.getColumn(ds_list2.rowposition, "transDate") + "\n";
		sMsg = sMsg + " - 매출금액   : " + gfn_numFormat(gfn_nvl(ds_list2.getColumn(ds_list2.rowposition, "saleAmount"),"0")) + "\n";

	var rtVal = gfn_confirm(sMsg, "", "C");	// 선택된 자료를 삭제 하시겠습니까?

	if (rtVal == true) 
	{	
		// 신규생성일 경우에는 해당 ROW를 삭제
		if (ds_list2.getRowType(nRow)==Dataset.ROWTYPE_INSERT) {
			var bSucc = ds_list2.deleteRow(nRow);
			if (bSucc) gfn_alert("msg.delete.success");	// 삭제 되었습니다.
			else gfn_alert("msg.err.grid.noselect");	// 선택된 항목이 없습니다.	
			return;
		}
	
		// 삭제용 dataset Clear
		ds_deleteRow.clearData();
		
		// 삭제용 dataset SET
		ds_deleteRow.addRow();
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "trNum", ds_list2.getColumn(nRow, "trNum"));					// TR_NUM
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "upjang", ds_list2.getColumn(nRow, "upjang"));					// UPJANG
		ds_deleteRow.setColumn(ds_deleteRow.rowposition, "transDate", ds_list2.getColumn(nRow, "transDate"));					// TRANS_DATE

		
		// Transaction 바로 호출해서 삭제 처리 함 
		var strSvcID    = "deleteRow";
		var strURL      = "fm/fmf/saleModifyWork/deleteRow.xdo";
		var strInDs     = "ds_deleteRow=ds_deleteRow:A";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
}


/*----------------------------------------------------------------------------------------------
 * 설명      : 조건 Dataset 이벤트 처리
 *---------------------------------------------------------------------------------------------*/
function ds_search_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
    switch(e.columnid)
    {
		case "schTdate1" :		// 매출일자_FROM
			ds_list.clearData();
			break;
			
		case "schTdate2" :		// 매출일자_TO
			ds_list.clearData();
			break;

		case "schUpjang" :		// 사업장
			ds_list.clearData();
			//if(!gfn_isNull(ds_search.getColumn(0,"schUpjang")))fn_magamYn();
			ds_search.setColumn(0,"schSubinv","");
			break;
	
		case "schSubinv" :		// 창고
			ds_list.clearData();
			//if(!gfn_isNull(ds_search.getColumn(0,"schUpjang")))fn_magamYn();
			break;

		case "schRdate1" :		// 보정기간_FROM
			ds_list2.clearData();
			break;
			
		case "schRdate2" :		// 보정기간_TO
			ds_list2.clearData();
			break;
			
		case "schStatus" :		// 상태
			ds_list2.clearData();
			break;
			
		case "schSdate" :
			if(!gfn_isNull(ds_search.getColumn(0,"schUpjang")))fn_magamYn();
			break;
	}	
	
}



