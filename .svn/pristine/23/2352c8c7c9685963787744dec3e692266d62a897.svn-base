 *---------------------------------------------------------------------------------------------*/
function fn_tranSave()
{
	var nRowCnt = ds_list2.getRowCount();	// DataSet Count
	var nRow= ds_search.rowposition;
	var sMsg = "";
	
	if(fn_magamFlag())return;//매출마감여부
	
	// 저장할 자료 여부 CHECK
	if(nRowCnt == 0) 
	{
		gfn_alert("msg.save.nodata", "저장할 데이터");	// 저장할 데이터가 없습니다.
		return;
	}
	
	 // v_ds_check 미사용시 변경사항 체크 : 엑셀업로드 시에는 CHECK X
 	if (gfn_dsIsUpdated(ds_list2) == false)
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	// 저장 시 Validation Check : 중복 CHECK	
	if( !fn_saveCheck() ) 
	{
		return;
	}

	sMsg = "보정내역을 저장합니다.\n\n계속 진행 하시겠습니까?\n";

	if (!gfn_confirm(sMsg, "", "C")) return;

	// 저장 Transaction
    var strSvcID    = "save";
    var strURL      = "fm/fmf/saleModifyWork/saveList.xdo";
	var strInDs     = "ds_list2=ds_list2:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 시 CHECK 
 *---------------------------------------------------------------------------------------------*/
function fn_saveCheck()
{
	var nRowCnt = ds_list2.getRowCount();	// DataSet Count
	var v_itemCode = "";
	
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		if (ds_list2.getRowType(i)==Dataset.ROWTYPE_INSERT || ds_list2.getRowType(i)==Dataset.ROWTYPE_UPDATE) 
		{
	
 			// 필수입력 CHECK
 			if (gfn_transNullToEmpty(ds_list2.getColumn(i, "ccNm")=="")) {
				sMsg = "관리부서는 필수입력 사항입니다.";
				gfn_alert(sMsg, "", "A");
				ds_list2.rowposition = i;
				return false;
			}	
			if (gfn_transNullToEmpty(ds_list2.getColumn(i, "transDate")=="")) {
				sMsg = "보정일자는 필수입력 사항입니다.";
				gfn_alert(sMsg, "", "A");
				ds_list2.rowposition = i;
				return false;
			}	
			if (gfn_transNullToEmpty(ds_list2.getColumn(i, "upjang")=="")) {
				sMsg = "사업장은 필수입력 사항입니다.";
				gfn_alert(sMsg, "", "A");
				ds_list2.rowposition = i;
				return false;
			}	
			if (gfn_transNullToEmpty(ds_list2.getColumn(i, "subinvCode")=="")) {
				sMsg = "창고는 필수입력 사항입니다.";
				gfn_alert(sMsg, "", "A");
				ds_list2.rowposition = i;
				return false;
			}	
			if (gfn_transNullToEmpty(ds_list2.getColumn(i, "itemCode")=="")) {
				sMsg = "보정구분은 필수선택 사항입니다.";
				gfn_alert(sMsg, "", "A");
				ds_list2.rowposition = i;
				return false;
			}	
			if (gfn_transNullToEmpty(ds_list2.getColumn(i, "itemCode")=="709999999996")) {
				// 보정구분 : 면세
				if (gfn_nvl(ds_list2.getColumn(i, "saleAmount"),"0")=="0") {
					sMsg = "매출가보정 금액은 필수입력 사항입니다.";
					gfn_alert(sMsg, "", "A");
					ds_list2.rowposition = i;
					return false;
				}	
			}
			if (gfn_transNullToEmpty(ds_list2.getColumn(i, "itemCode")!="709999999996")) {
				// 보정구분 : 과세
				if (gfn_nvl(ds_list2.getColumn(i, "saleAmount"),"0")=="0" && gfn_nvl(ds_list2.getColumn(i, "saleVat"),"0")=="0") {
					sMsg = "매출가보정 또는 부가세보정 금액을 입력하십시요.";
					gfn_alert(sMsg, "", "A");
					ds_list2.rowposition = i;
					return false;
				}	
			}
		}
	}	

	return true;

}

/*----------------------------------------------------------------------------------------------
 * 설명      : 매출금액 대비 부가세 자동계산
 *---------------------------------------------------------------------------------------------*/
function fn_calcVat(row)
{
	var v_saleAmount = 0;
	var v_saleVat = 0;

	v_saleAmount = ds_list2.getColumn(row, "saleAmount");
	
	if (v_saleAmount==0) return 0;
	
	v_saleVat = (v_saleAmount * 0.1);
	
	v_saleVat = gfn_round(v_saleVat, 0);

	return v_saleVat;
}

