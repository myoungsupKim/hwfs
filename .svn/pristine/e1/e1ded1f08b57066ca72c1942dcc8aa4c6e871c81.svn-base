function fn_SetImpData()
{
	var nRow;

	ds_ErrLst.clearData();

	dv_Prg.visible = true;
	SetWaitCursor(true);
	setCapture();
	for (i = 2; i < ds_Excel.rowcount; i++)
	{
		//상품코드 등록여부
		if (gfn_length(gfn_trim(ds_Excel.getColumn(i,"Col03")))== 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", numFormat(i+1)+ "행) 상품코드 미등록");
			continue;
		}
		nRow = ds_List.findRowExpr("ITEM_CODE=='" + ds_Excel.getColumn(i,"Col03") + "'");
		if (nRow < 0)
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", numFormat(i+1)+ "행) 해당하는 상품코드(" + ds_Excel.getColumn(i,"Col03") + " " + ds_Excel.getColumn(i,"Col04") + ")를 찾을 수가 없습니다");
			continue;
		}
		//운영단가
		if (! isNumeric(ds_Excel.getColumn(i,"Col12")))
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", numFormat(i+1)+ "행) " + ds_Excel.getColumn(i,"Col03") + " " + ds_Excel.getColumn(i,"Col04") + " 상품 운영단가오류(" + ds_Excel.getColumn(i,"Col12") + ")");
			continue;
		}
		//운영율
		if (! isNumeric(ds_Excel.getColumn(i,"Col13")))
		{
			ds_ErrLst.addRow();
			ds_ErrLst.setColumn(ds_ErrLst.rowposition, "errmsg", numFormat(i+1)+ "행) " + ds_Excel.getColumn(i,"Col03") + " " + ds_Excel.getColumn(i,"Col04") + " 상품 운영율오류(" + ds_Excel.getColumn(i,"Col13") + ")");
			continue;
		}
		gd_list.enableredraw = false;
		ds_List.rowposition = nRow;
		ds_List.setColumn(nRow, "OP_PRICE", toNumber(ds_Excel.getColumn(i,"Col12")));
		ds_List.setColumn(nRow, "OP_RATE", toNumber(ds_Excel.getColumn(i,"Col13")));
		gd_list.enableredraw = true;
	}
	releaseCapture();
	SetWaitCursor(false);
	dv_Prg.visible = false;
	ds_List.rowposition = 0;

	var sMsg;
	if (ds_ErrLst.rowcount < ds_Excel.rowcount)
	{
		sMsg = '정상적으로 업로드 되었습니다.';
		if (ds_ErrLst.rowcount > 0)
		{
			sMsg += '\n\n오류 데이타가 존재합니다.\n확인하시겠습니까?!';
			ds_ErrLst.rowposition = 0;
			div_Error.visible = (g_Confirm("04", sMsg));
			if (div_Error.visible) grd_ErrLst.setFocus();
		}
		else
			g_Message("02", sMsg);
	}
}

function ds_List_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if ( e.columnid == "OP_PRICE" )
	{
		//운영단가 입력 시 운영율 자동계산
		if (numFormat(obj.getColumn(e.row, "SALE_PRICE"))== "0")
		{
			obj.setColumn(e.row, "OP_RATE", 100 * e.newvalue);
		}
		else
		{
			//운영단가가 0이면 운영율도 0으로 설정...
			if (numFormat(e.newvalue)== "0")
				obj.setColumn(e.row, "OP_RATE", 0);
			else
				obj.setColumn(e.row, "OP_RATE", 100 * ((toNumber(e.newvalue) / toNumber(obj.getColumn(e.row, "SALE_PRICE"))) - 1));
		}
	}
	//운영금액 자동설정
	else if ( (e.columnid == "OP_RATE") || (e.columnid == "CEIL_VAL") )
	{
		if ( numFormat(obj.getColumn(e.row, "OP_PRICE"))== "0" )
			obj.setColumn(e.row, "OP_AMT", Math.ceil((toNumber(obj.getColumn(e.row,"SALE_PRICE")) + Math.round(toNumber(obj.getColumn(e.row,"SALE_PRICE")) * toNumber(obj.getColumn(e.row,"OP_RATE")) / 100)) / Math.pow(10,toNumber(obj.getColumn(e.row,"CEIL_VAL")))) * Math.pow(10,toNumber(obj.getColumn(e.row,"CEIL_VAL"))));
		else
			obj.setColumn(e.row, "OP_AMT", obj.getColumn(e.row,"OP_PRICE"));
	}
}

