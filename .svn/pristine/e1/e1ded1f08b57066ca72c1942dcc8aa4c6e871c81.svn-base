		var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
		// 경로 설정
		strSvcID = "FMU00240E_T003";
		strURL   = "U_svc::" + "fm/fmu/FMU00240E_T003.jsp";
		// 데이터셋 설정
		strOutDatasets = "ds_MultiUpj=ds_List";
		// 파라메터 설정
		strArgument  = "g_Upjang=" + wrapQuote(g_Upjang);
		strArgument += " g_EmpNo=" + wrapQuote(g_EmpNo);
		strArgument += " sMainUpjang=" + wrapQuote(ed_MainUpjang.userdata);
		strArgument += " sSdate=" + wrapQuote(me_Sdate.value);
		//strArgument += " sEdate=" + wrapQuote(me_Edate.value);
		// 서버 호출 
		http.Sync = true;
		Transaction(strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc);
		http.Sync = false;
ta_Sql.value = fa_Sql;
	}
	if (dv_MultiUpj.visible == false)
		dv_MultiUpj.visible = true;
	else
		dv_MultiUpj.visible = false;
}

function dv_MultiUpj_btn_Hidden_OnClick(obj:Button, e:ClickEventInfo)
{
	//필수항목 
	if (ds_List.rowcount < 1)
	{
		g_Message("EE","운영율 조회 후 작업하세요.");
		return;
	}
	if (ds_MultiUpj.findRowExpr("CHK=='1'") < 0)
	{
		g_Message("EE","작업할 업장을 선택하세요!");
		dv_MultiUpj.gd_list.setFocus();
		return;
	}
	if (! g_Confirm("01", '다중업장 등록 작업을 하시겠습니까?!')) return;
	
	ds_UpjLst.clearData();
	for (var iRow=0; iRow<ds_MultiUpj.rowcount; iRow++)
	{
		if (ds_MultiUpj.getColumn(iRow,"CHK") <> "1") continue;
		if (ds_MultiUpj.getColumn(iRow,"UPJANG") ==  ed_Upjang.userdata) continue;
		
		//
		ds_UpjLst.addRow();
		ds_UpjLst.setColumn(GetLastRow(ds_UpjLst), "UPJANG", ds_MultiUpj.getColumn(iRow,"UPJANG"));
	}
	
	// 파라미터셋팅
	ds_Cond.clearData();
	ds_Cond.addRow();
	ds_Cond.setColumn(0, "SDATE",  me_Sdate.value);
	ds_Cond.setColumn(0, "UPJANG",  ed_Upjang.userdata);
	
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "FMU00240E_T004";
	strURL   = "U_svc::" + "fm/fmu/FMU00240E_T004.jsp";
	// 데이터셋 설정U
	strInDatasets  = "ds_Cond=ds_Cond ds_List=ds_List ds_UpjLst=ds_UpjLst";
	strOutDatasets = "";
	// 파라메터 설정
	strArgument  = "g_Upjang=" + wrapQuote(g_Upjang);
	strArgument += " g_EmpNo=" + wrapQuote(g_EmpNo);
	strArgument += " sOtcust=" + wrapQuote(ed_Otcust.userdata);
	strCallbackFunc = "";
	// 서버 호출 
	jsp_transaction(this, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, false, false, true);
	
	g_Message("02","다중업장 등록작업이 완료되었습니다.");
	if (dv_MultiUpj.visible == true) dv_MultiUpj.visible = false;
}

function Button1_OnClick(obj:Button, e:ClickEventInfo)
