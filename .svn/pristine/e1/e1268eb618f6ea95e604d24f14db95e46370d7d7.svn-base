function btn_cancel_onclick(obj:Button,  e:ClickEventInfo)
{
	this.close();
}

function edt_value_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if (e.keycode == 13)
	{
		fn_search();
	}
}
