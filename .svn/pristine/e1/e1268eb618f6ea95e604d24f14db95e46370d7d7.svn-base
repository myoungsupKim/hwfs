function ds_list_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var sQty = 0;
	for (i=0;i<ds_list.rowcount;i++){
		sQty += Number(ds_list.getColumn(i,"wwQty"));
		//trace(amt);
		if(Number(obj.getSum(e.columnid)) > msk_SalQuantity.value){
			alert("회수수량은 총식수보다 클 수 없습니다.");
			obj.setColumn(e.row,e.columnid,0);
			return;
		}
	}
	msk_inQty.value = sQty;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : Return할 Array 생성 및 창 닫기
 *---------------------------------------------------------------------------------------------*/
function fn_return()
{
 	var arr = new Array();
 	var nRow = ds_list.rowposition;
//  	for ( var i = 0; i < ds_list.getColCount(); i++ ) {
//  		arr[ds_list.getColID(i)] = ds_list.getColumn(nRow, i);
//  	}
	for (var i=0; i <ds_list.rowcount; i++){
		if(ds_list.getColumn(i, "wwQty") > 0){
			arr["itemCode"] = ds_list.getColumn(i, "itemCode");
			arr["itemCodenm"] = ds_list.getColumn(i, "itemCodenm");
			arr["taxShetPublishYn"] = ds_list.getColumn(i, "taxShetPublishYn");
		}
	}
 	this.close(arr);
}
