 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	// 변경사항 체크
	if (gfn_dsIsUpdated(ds_list) == false) 
	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return;
	}
	
	if (Number(msk_SalQuantity.value) != Number(msk_inQty.value)){
			gfn_alert("총식수와 저장될 총회수수량이 일치하지 않습니다.\n확인하여 주십시요."); 	
			return false;
	}
	var amt = 0;
	for (i=0;i<ds_list.rowcount;i++){
		amt += Number(ds_list.getColumn(i,"uprice")) * Number(ds_list.getColumn(i,"wwQty"));
	}

	//예외처리 시 사용	
	if (g_EmpNo == "Z9952396") {
	
	} else{
		if (amt != arrSchVal[4]*arrSchVal[5]){
				gfn_alert("매출총액이 일치하지 않습니다.\n확인하여 주십시요."); 	
				return false;
		}
	}
	
	
    //부모창에 저장용 데이터셋 ds_list_ticket 삭제
	if(opener.ds_list_ticket.rowcount > 0){
		for(var i = opener.ds_list_ticket.rowcount-1; i >= 0; i--){
			if(arrSchVal[9] == opener.ds_list_ticket.getColumn(i, "tmpSeq")){
				opener.ds_list_ticket.deleteRow(i);
			}
		}
	}
	
	
	//저장용 데이터셋(하단)
	for (var i=0;i<this.ds_list.rowcount;i++){
		if(this.ds_list.getColumn(i,"wwQty") != 0){
			if((Number(ds_list.getColumn(i,"ooQty")) - Number(ds_list.getColumn(i,"altQty"))) - Number(ds_list.getColumn(i,"wwQty")) < 0){
				gfn_alert("총잔여수보다 회수수량이 큽니다.");
				ds_list.setColumn(i, "wwQty", 0);
				return false;
			}
			
			//예외처리 시 사용	
			if (g_EmpNo == "Z9952396") {
			
			} else {
				if(ds_list.getColumn(i,"uprice") != arrSchVal[5]){
					gfn_alert("단가가 일치하지 않습니다.\n확인하여 주십시요."); 	
					ds_list.setColumn(i,"wwQty", 0);
					return false;
				}
			}
			
			var row = opener.ds_list_ticket.addRow();
			opener.ds_list_ticket.setColumn(row, "uprice", this.ds_list.getColumn(i, "uprice"));
			opener.ds_list_ticket.setColumn(row, "wwQty", this.ds_list.getColumn(i, "wwQty"));
			opener.ds_list_ticket.setColumn(row, "itemType", this.ds_list.getColumn(i, "itemType"));
			opener.ds_list_ticket.setColumn(row, "itemCode", this.ds_list.getColumn(i, "itemCode"));
			
			opener.ds_list_ticket.setColumn(row, "upjangCd", arrSchVal[0]);
			opener.ds_list_ticket.setColumn(row, "occurDate", arrSchVal[1]);
			opener.ds_list_ticket.setColumn(row, "remarkDesc", arrSchVal[6]);
			opener.ds_list_ticket.setColumn(row, "custCd", arrSchVal[7]);
			opener.ds_list_ticket.setColumn(row, "muCd", arrSchVal[8]);
			opener.ds_list_ticket.setColumn(row, "tmpSeq", arrSchVal[9]);
		}
	}
	
	fn_return();
}

