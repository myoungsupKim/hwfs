	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
        case "search" :    		// 코드분류 조회
            fn_searchTicketHistory();
            grd_list.setFocus();
            break;
            		
        case "save" :			// 저장
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_return();
            break;    
        case "masTicketMstGroupSeq" :
			
			break;
			
	    case "searchTicketHistory" : 
	        break;
    }
}


