	cal_borneOutTo.fn_initMonthCal(this, gfn_addDate(fv_sToday, 3), "", ds_search, "borneOutTo");
	
	var arrArg = gfn_getScreenParams();
	if(!gfn_isNull(arrArg)) {
		edt_theDeadNm.value    = arrArg[0];
		cal_borneOutFrom.mae_cal.value = arrArg[1];
		cal_borneOutTo.mae_cal.value   = arrArg[1];
		cal_casketFrom.mae_cal.value   = arrArg[2];
		cal_casketTo.mae_cal.value     = arrArg[2];
