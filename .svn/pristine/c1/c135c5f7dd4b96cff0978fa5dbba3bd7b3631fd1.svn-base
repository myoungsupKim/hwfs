/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (!gfn_dsIsUpdated(ds_goodsSaleout) && !gfn_dsIsUpdated(ds_goodsReturn)) 
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	
	// 결제수단이 등록되어있는지 체크
	ds_search.setColumn(0, "funeralHallCd", ds_deadGuest.getColumn(ds_deadGuest.rowposition,"funeralHallCd"));
	ds_search.setColumn(0, "guestNum", ds_deadGuest.getColumn(ds_deadGuest.rowposition,"guestNum"));
	
	strSvcID    = "searchPayWayYn";
	strURL      = "fs/fsf/funeralDeadSaleGoodsOutReturn/selectPayWayYn.xdo";
	strInDs     = "ds_search=ds_search:A";
	strOutDs    = "ds_payWayYn=ds_payWayYn";
	strArg      = "";
	strCallback = "fn_callBack";
	bAsync      = false;
	
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

	// 결제수단 등록되어있으면 리턴
	if(ds_payWayYn.getColumn(0, "cnt") > 0){
		gfn_alert("결제수단이 등록되어 있어 수정 하실 수 없습니다.");
		return false;
	}
	
	// 저장시 Validation Check	
 	if(!gfn_validCheck(tab_saleGoodsOutReturn.tabpage1.grd_goodsSaleout, ds_goodsSaleoutValid)) {
 		return;
 	}
 	
 	if(!gfn_validCheck(tab_saleGoodsOutReturn.tabpage2.grd_goodsreturn, ds_goodsReturnValid)) {
 		return;
 	}
 	
	for(var i=0; i<ds_goodsSaleout.rowcount; i++) {
		if((gfn_isNull(ds_goodsSaleout.getColumn(i, "amt")))
			||(toNumber(ds_goodsSaleout.getColumn(i, "amt"))<=0)) {
			gfn_alert("고인별판매 금액을 입력하여 주십시요.");
			ds_goodsSaleout.rowposition = i;
			return;
		}else if((gfn_isNull(ds_goodsSaleout.getColumn(i, "ea")))
			||(toNumber(ds_goodsSaleout.getColumn(i, "ea"))<=0)) {
			gfn_alert("고인별판매 개수를 입력하여 주십시요.");
			ds_goodsSaleout.rowposition = i;
			return;
		}
	}
	
	for(var i=0; i<ds_goodsReturn.rowcount; i++) {
		if((gfn_isNull(ds_goodsSaleout.getColumn(i, "amt")))
			||(toNumber(ds_goodsReturn.getColumn(i, "amt"))<=0)) {
			gfn_alert("고인별 반품 금액을 입력하여 주십시요.");
			ds_goodsReturn.rowposition = i;
			return;
		}else if((gfn_isNull(ds_goodsSaleout.getColumn(i, "ea")))
			||(toNumber(ds_goodsReturn.getColumn(i, "ea"))<=0)) {
			gfn_alert("고인별 반품 개수를 입력하여 주십시요.");
			ds_goodsReturn.rowposition = i;
			return;
		}
	}

    var strSvcID    = "save";
    var strURL      = "fs/fsf/funeralDeadSaleGoodsOutReturn/saveGoodsSaleoutReturnList.xdo";
    var strInDs     = "ds_goodsSaleout=ds_goodsSaleout:U ds_goodsReturn=ds_goodsReturn:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

