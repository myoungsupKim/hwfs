function fn_search()
{
	if(ds_search.getColumn(0, "tmCd")=="")
	{
		gfn_alert("팀을 선택하여 주십시오.");
		edt_tmNm.setFocus();
		return;
	}
	if(ds_search.getColumn(0, "upjangCd")=="")
	{
		gfn_alert("장례식장을 선택하여 주십시오.");
		edt_upjangNm.setFocus();
		return;
	}
	
	if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;
	
	if (cal_borneOutFrom.text > cal_borneOutTo.text) {
		gfn_alert("msg.err.validator.date.great");	// 종료일이 시작일보다 빠릅니다.
		cal_borneOutFrom.setFocus();
		return;
	}
	
	// v_ds_check 미사용시 변경사항 체크
 	if(gfn_dsIsUpdated(ds_goodsSaleout)||gfn_dsIsUpdated(ds_goodsReturn))
 	{
 		var retValue = gfn_confirm("confirm.before.search");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		if (retValue == false) return;
 	}
	
	
	ds_goodsSaleout.clearData();
	ds_goodsReturn.clearData();
	
	// 조회시 Validation Check
	//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_deadGuest);
    
    //서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
	fv_deadGuestColInfo = gfn_getDatasetInfo(ds_deadGuest);
    
    // 조회조건 Param
	ds_search.setColumn(0, "funeralHallCd", ds_search.getColumn(0,"upjangCd"));
	ds_search.setColumn(0, "guestNum", edt_guestNum.value);
	ds_search.setColumn(0, "theDeadNm", edt_theDeadNm.value);
	//ds_search.setColumn(0, "casketFrom", cal_casketFrom.mae_cal.value);
	//ds_search.setColumn(0, "casketTo", cal_casketTo.mae_cal.value);
	ds_search.setColumn(0, "borneOutFrom", cal_borneOutFrom.mae_cal.value);
	ds_search.setColumn(0, "borneOutTo", cal_borneOutTo.mae_cal.value);
	
	var strSvcID    = "searchDeadGuest";
    var strURL      = "fs/fsf/funeralDeadSaleGoodsOutReturn/selectDeadGuestList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_deadGuest=ds_deadGuest";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

