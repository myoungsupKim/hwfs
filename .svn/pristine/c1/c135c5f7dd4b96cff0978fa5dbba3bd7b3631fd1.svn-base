	// 입관일 오늘 날짜로 설정
	//cal_casketFrom.fn_initMonthCal(this, fv_sToday, "", ds_search, "casketFrom");
	//cal_casketTo.fn_initMonthCal(this, fv_sToday, "", ds_search, "casketTo");
	
