	SCCombo.bind(this, "ComCode", "tab_saleGoodsOutReturn.tabpage2.grd_goodsreturn.unit", ds_unitType, {groupCode:"UNIT_TYPE", useYn:"Y", defVal:"001"});		// 단위
	
