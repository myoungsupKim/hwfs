/*----------------------------------------------------------------------------------------------
 * 설명      : 대상자재 검색 버튼 클릭 
 *---------------------------------------------------------------------------------------------*/
function btn_item_onclick(obj:Button,  e:ClickEventInfo)
{
	// 조회조건 기준으로 CHECK
	if (gfn_validCheck(ds_search, ds_itemValid, true) == false) return;

	// 기간 CHECK
	if (cal_fromDate.text > cal_toDate.text) {
 		gfn_alert("msg.err.validator.date.great");	// 종료일이 시작일보다 빠릅니다.
 		cal_fromDate.setFocus();
		return;
	}
	
	// 센터명을 ds_search에 복사 
	var nRow= ds_search.rowposition;
	ds_search.setColumn(nRow, "schCenterName", cbo_centerCode.text);

	fn_itemPop();
}

