/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		if(strSvcID=="save")
		{
			fv_errorFlag="Y";
			fv_errorMsg = strErrorMsg;
			fn_processStep();
			return;
		}
		else 
		{
			return;
		}
	}
	
    switch(strSvcID)
    {
		// 센터 조회
        case "searchCenter" :
            //trace(ds_center.saveXML());
            fn_center_init();
            break;
    
    
		// 조회
        case "search" :
            //trace(ds_list.saveXML());
            if (ds_list.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_list, fv_objColInfo);
            }
            break;
            
        // 저장
        case "save" :
			if (fv_processCnt > 0) {	
				fv_errorFlag="";
				fv_errorMsg="";
			} 
            if (ds_listRow.getRowCount() > 0) {
				// 선택한 ROW에 대한 결과 Dataset 복사
				var nRow = ds_list.findRowExpr("prId=="+fv_row);
				ds_list.setColumn(nRow, "itemCode2", ds_listRow.getColumn(0, "itemCode"));
				ds_list.setColumn(nRow, "itemName2", ds_listRow.getColumn(0, "itemName"));
				ds_list.setColumn(nRow, "itemSize2", ds_listRow.getColumn(0, "itemSize"));
				ds_list.setColumn(nRow, "itemUnit2", ds_listRow.getColumn(0, "itemUnit"));
				ds_list.setColumn(nRow, "salePrice2", ds_listRow.getColumn(0, "salePrice"));
				ds_list.setColumn(nRow, "saleAmt2", ds_listRow.getColumn(0, "saleAmt"));
				ds_list.setColumn(nRow, "custCd2", ds_listRow.getColumn(0, "custCd"));
				ds_list.setColumn(nRow, "custNm2", ds_listRow.getColumn(0, "custNm"));
				ds_list.setColumn(nRow, "flag", "Y");
            }
			fn_processStep();
            break;  			
    }
}

