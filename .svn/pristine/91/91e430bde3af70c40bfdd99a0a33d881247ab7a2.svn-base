/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	/*시스템 통제 시간 체크 - 특정일자에 사용을 할 수 없게 한다.*/
	mfn_GetSystemWorkCheck("PO");
	if (ds_common.getColumn(0, 0) == "Y")
	{
		g_Message("EE", "시스템 변경 작업중이라 현 화면을 사용할 수 없습니다. \n사용불가시간(" + ds_common.getColumn(0, 1) + " ~ " + ds_common.getColumn(0, 2) + ") ");
		return false; 
	}
	
	var nRowCnt = ds_list.getRowCount();	// DataSet Count
	var iCnt = 0;
	var sMsg = "";

	// 저장시 Validation Check
	if (gfn_validCheck(ds_search, ds_saveValid, true) == false) return;

	// 대상자재 및 변경자재 비교
	if (ds_search.getColumn(0, "schItemCode") == ds_search.getColumn(0, "schItemCode2")) {
		sMsg = "대상자재와 변경자재가 동일합니다.\n\n작업을 취소합니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}

	// CHECK 개수 
	for (var i = 0 ; i < nRowCnt ; i++)
	{
		// Dataset이 선택된 것만 적용, 한번 저장이 된건은 제외(flag == 1)
		if (ds_list.getColumn(i, "chk")=="1" && ds_list.getColumn(i, "flag")=="") {
			iCnt++;
		}
	}

	// 저장할 자료 여부 CHECK
	if(iCnt == 0) 
	{
		gfn_alert("msg.save.nodata", "저장할 데이터");	// 저장할 데이터가 없습니다.
		return;
	}


	// 기본 CHECK 진행
	if (!fn_itemCheck()) return;


	sMsg = "주문신청을 변경합니다.\n계속 진행 하시겠습니까?\n\n";
	sMsg = sMsg + " - 대상자재를 변경자재로 일괄 적용합니다.\n";
	sMsg = sMsg + " ※ 여신관리여부에 따라 여신정보(잔액)을 체크합니다.";
	
	
	if (!gfn_confirm(sMsg, "", "C")) return;

	// Progress Bar 처리 
	fn_processSet();
}

