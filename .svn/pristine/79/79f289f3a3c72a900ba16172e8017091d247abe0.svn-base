/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		 
        case "search" :
            
            break;
        case "searchList":
			//예약이 검색이 완료 되면 해당 예약의 레시피 내역이 있는지 검색 한다.
			if (ds_reservMst.rowcount>0)
			{
				fn_searchRecipeList();
			}
			break;
        
        case "searchRecipeList":
			if (ds_cst.rowcount<=0)
			{
				var nIdx = ds_cst.addRow();
				ds_cst.setColumn(nIdx,"useYn","1");
				ds_cst.setColumn(nIdx,"cstDate",fv_sToday);
				ds_cst.setColumn(nIdx,"fnbClass",'F');
				ds_cst.setColumn(nIdx,"seq",1);
				ds_cst.setColumn(nIdx,"reservNum",ds_reservMst.getColumn(ds_reservMst.rowposition,"reservNum") );
				
				ds_cst.updatecontrol = false;
				var bSucc = ds_cst.setRowType(0, Dataset.ROWTYPE_NORMAL);
				ds_cst.updatecontrol = true;
			}
			
			if( ds_cstDetail.rowcount > 0 )
			{
				for( var i = 0; i < ds_cstDetail.rowcount; i++ )
				{
					var salePrice	= ds_cstDetail.getColumn(i, "salePrice");	// 판매가
					var cst			= ds_cstDetail.getColumn(i, "cst");			// 원가
					var maxPsn		= ds_cstDetail.getColumn(i, "maxPsn");		// 예측인원
					
					var expectAmt = toNumber(salePrice*maxPsn);	// 예상매출
					var expectCst = toNumber(cst*maxPsn);		// 예상원가
					var cstPer = Math.round((expectCst / expectAmt) * 100, 2);	// 원가율
					
					ds_cstDetail.setColumn(i,"expectAmt", expectAmt);
					ds_cstDetail.setColumn(i,"expectCst",  expectCst);
					ds_cstDetail.setColumn(i,"cstPer", cstPer);
				}
				ds_cstDetail.applyChange();
			}
			break;
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_searchRecipeList();
            break;    
    }
}

