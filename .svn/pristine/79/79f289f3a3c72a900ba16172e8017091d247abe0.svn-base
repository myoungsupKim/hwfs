/*----------------------------------------------------------------------------------------------
 * 설명      : 왼쪽 검색 내역을 선택 하면 상세내역을 검색 한다.
 *---------------------------------------------------------------------------------------------*/
function ds_reservList_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	var reservNum = ds_reservList.getColumn(ds_reservList.rowposition,"reservNum");
	if (gfn_nvl(reservNum,"") == "") return;
	
	//변경 또는 입력중에 그리드 ROW를 변경 하는 경우
	if(ds_reservList.rowposition == -1) return;

	if (fv_gridFlag==true)
	{
		fv_gridFlag=false;
		return;
	}
	
	if (gfn_dsIsUpdated(ds_cst) || gfn_dsIsUpdated(ds_cstDetail))
 	{
		var retValue = gfn_confirm("confirm.before.moveropos");	// 해당 row의 위치를 이동하면 변경된 데이터가 사라집니다.@계속 진행 하시겠습니까?
		if (retValue == false) 
		{
			fv_gridFlag=true;
			ds_reservList.rowposition = e.oldrow;
			return;
		}
	}
 
	ds_paramInfo.clearData();
	var nRow = ds_paramInfo.addRow();
	ds_paramInfo.setColumn(nRow, "reservNum",  reservNum);
	ds_paramInfo.setColumn(nRow, "fnbClass",  "F");
	ds_paramInfo.setColumn(nRow, "maskAuth", SCAuth.getMaskAuth(this));
	fn_selectList();
	
	
     
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 선택이 되는 경우 사전 사후 원가 계산
 *---------------------------------------------------------------------------------------------*/
function ds_cstDetail_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if (e.columnid == "choiceStatus")
	{
		 
		if (gfn_nvl(ds_cst.getColumn(ds_cst.rowposition,"cstNm"),"") == "")
		{
			gfn_alert("원가명칭을 입력 하십시오!");	// 저장 되었습니다.
			obj.enableevent = false;
			obj.setColumn(e.row,"choiceStatus",0);
			obj.enableevent = true;
			edt_cstNm.setFocus();
			return;
		}
/*
		if (gfn_nvl(ds_cst.getColumn(ds_cst.rowposition,"psn"),"0") == 0)
		{
			gfn_alert("실인원수를 입력 하십시오!");	// 저장 되었습니다.
			obj.enableevent = false;
			obj.setColumn(e.row,"choiceStatus",0);
			obj.enableevent = true;
			msk_cstPsn.setFocus();
			return;
		}
*/		 
		
		if (obj.getColumn(e.row,"choiceStatus") == 1)
		{
			var cst = obj.getColumn(e.row,"cst");
			var psn = obj.getColumn(e.row,"psn");
			var maxPsn = obj.getColumn(e.row,"maxPsn");
			
			obj.setColumn(e.row,"expectCst", cst * psn);
			obj.setColumn(e.row,"exeCst", cst * maxPsn);
		}
		else
		{
			obj.setColumn(e.row,"expectCst", 0);
			obj.setColumn(e.row,"exeCst", 0);
		}

		fn_totalAmt();
	}

	if( e.columnid == "maxPsn" )
	{
		var salePrice	= ds_cstDetail.getColumn(e.row, "salePrice");	// 판매가
		var cst			= ds_cstDetail.getColumn(e.row, "cst");			// 원가
		var maxPsn		= ds_cstDetail.getColumn(e.row, "maxPsn");		// 예측인원

		var expectAmt = toNumber(salePrice*maxPsn);	// 예상매출
		var expectCst = toNumber(cst*maxPsn);		// 예상원가
		var cstPer = Math.round((expectCst / expectAmt) * 100, 2);	// 원가율

		ds_cstDetail.setColumn(e.row,"expectAmt", expectAmt);
		ds_cstDetail.setColumn(e.row,"expectCst",  expectCst);
		ds_cstDetail.setColumn(e.row,"cstPer", cstPer);
	}
}

function fn_totalAmt()
{
	var fnbAmt = 0;
	for(var j = 0 ; j <= ds_cstDetail.getRowCount()-1 ; j++)
	{
		fnbAmt = fnbAmt + Number(ds_cstDetail.getColumn(j, "exeCst"));
	}
	
	
	var psn = Number(gfn_nvl(ds_cst.getColumn(ds_cst.rowposition,"psn"),""));
	var cst = fnbAmt / psn;
	
	ds_cst.setColumn(ds_cst.rowposition,"amt",fnbAmt);
	ds_cst.setColumn(ds_cst.rowposition,"uprice",cst);
}
function ds_cst_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if (e.columnid == "psn")
	{
		var nPsn = Number(gfn_nvl(ds_cst.getColumn(ds_cst.rowposition,"psn"),""));
		for(var j = 0 ; j <= ds_cstDetail.getRowCount()-1 ; j++)
		{
			
			ds_cstDetail.setColumn(j, "maxPsn",nPsn );
			
			if (ds_cstDetail.getColumn(j,"choiceStatus") == 1)
			{
				var cst = ds_cstDetail.getColumn(j,"cst");
				var psn = ds_cstDetail.getColumn(j,"psn");
				var maxPsn = ds_cstDetail.getColumn(j,"maxPsn");
				
				ds_cstDetail.setColumn(j,"expectCst", cst * psn);
				ds_cstDetail.setColumn(j,"exeCst", cst * maxPsn);
			}
			else
			{
				ds_cstDetail.setColumn(j,"expectCst", 0);
				ds_cstDetail.setColumn(j,"exeCst", 0);
			}
		}
		
		fn_totalAmt();
	}
