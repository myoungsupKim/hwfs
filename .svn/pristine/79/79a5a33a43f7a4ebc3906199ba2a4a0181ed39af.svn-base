<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT  CUST_ID  ,CUSTNM
 FROM FMS_CUSTOM
 WHERE (1=1)
    AND UPJANG = #UPJANG#
    AND USE_YN = 'Y'	]]></statement>
	<input default-name="ds_sin">
		<col name="UPJANG" size="50" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_sout">
		<col name="CUST_ID" size="22" type="NUMERIC" description="" /> 
		<col name="CUSTNM" size="100" type="VARCHAR" description="" /> 
	</output>
</query>
