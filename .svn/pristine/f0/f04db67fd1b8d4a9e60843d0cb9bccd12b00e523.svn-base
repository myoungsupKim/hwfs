function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";
	
	//검색조건을 초기화한다.
	var nRow = ds_search.addRow();
	ds_search.setColumn(nRow, "backNm");
	
	//Combo
	SCCombo.bind(this, "ComCode", "cbo_backClsfiCd", ds_backClsfiCd, {groupCode:"BACK_CLSFI_CD", useYn:"Y"}); 	//반송분류코드
	SCCombo.bind(this, "ComCode", "cbo_recdStatus", ds_recdStatus, {groupCode:"RECD_STATUS", useYn:"Y"}); 		//레코드상태
}

