/*----------------------------------------------------------------------------------------------
 * 설명      : ds_list RowPos 이벤트 (입력필드 키값 활성화 비활성화)
 *---------------------------------------------------------------------------------------------*/
function ds_list_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	var rowType = obj.getRowType(obj.rowposition);
	
	if(rowType == 1 || rowType == 4){
		edt_backCd.enable = false;
	}else{
		edt_backCd.enable = true;
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 반송사유 코드 onkillfocus 이벤트(코드 중복체크)
 *---------------------------------------------------------------------------------------------*/
 function edt_backCd_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	if(!fn_existsSearch()){
		obj.value = null;
		obj.setFocus();
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 반송사유 코드 등록 중복체크(동기로 실행)
 *---------------------------------------------------------------------------------------------*/
function fn_existsSearch(){
	
	var nRow = ds_list.rowposition;
	var backCd = ds_list.getColumn(nRow, "backCd");
	
	if(backCd == undefined || backCd == null || backCd == ""){
		return true;
	}
	
	var strSvcID    = "search";
    var strURL      = "/sm/ssi/cardBackReasonCd/selectExists.xdo";
    var strInDs     = "";
    var strOutDs    = "fv_existsCnt=fv_existsCnt";
    var strArg      = "backCd="+backCd;
    var strCallback = "";
    var bAsync      = false;		//동기실행
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
    if(fv_existsCnt > 0){
		gfn_alert("msg.err.duplicatedata", "반송사유 코드");	// $1 은(는) 이미 등록된 자료입니다.
		fv_existsCnt = 0;
		
		return false;
    }
    
    return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 반송사유(검색조건) onkeydown 이벤트
 *---------------------------------------------------------------------------------------------*/
function edt_schBackNm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	if(e.keycode == 13){
		obj.updateToDataset();
		fn_search();
		obj.setFocus();
	}
}
