//MU가 변경될 때에 대한 이벤트
function fn_muItemChangedEvent(obj, evt)
{
    edt_tmNm.setTmCode("");
    edt_upjangNm.setUpjangCode("");
    
    // 데이터셋 클리어전 사업부선택한 값
	var selectMuCd = edt_muNm.getMuCode();
    // ds_search 데이터셋 클리어
    ds_search.clearData();
    ds_grd.clearData();
    
	var nRow = ds_search.addRow();
    ds_search.setColumn(nRow, "muCd", selectMuCd );
    ds_search.setColumn(nRow, "schYYMM", fv_schYyyyMm );
    ds_search.setColumn(nRow, "useYn", fv_useYn );
    
	ds_search.setColumn(nRow, "upjangCd", g_Upjang);
	ds_search.setColumn(nRow, "upjangNm", g_UpjangNm);
	
    fn_cboLclassLoad();
	fn_cboMclassLoad();
	fn_cboSclassLoad();
	
	fn_search();
}
