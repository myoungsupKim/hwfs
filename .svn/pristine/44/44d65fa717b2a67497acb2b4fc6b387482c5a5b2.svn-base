<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
  SELECT NVL(GAMU, '') GAMU, NVL(COST_GAMU, '') COST_GAMU FROM MAS_ADVANCE WHERE ADV_ID = #ADV_ID#
  </statement>
	<input default-name="ds_cond">
		<col name="ADV_ID" size="" type="NUMERIC" description="" /> 
	</input>
	<output default-name="ds_list">
		<col name="GAMU" size="255" type="VARCHAR" description="" /> 
		<col name="COST_GAMU" size="255" type="VARCHAR" description="" /> 		
	</output>
</query>