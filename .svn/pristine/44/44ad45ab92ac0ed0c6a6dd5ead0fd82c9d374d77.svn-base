    //alert(ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "signProgressStatus"));	   
    SCGw.setUserButton (this, fv_option, gfn_nvl(ds_gdspaymList.getColumn(ds_gdspaymList.rowposition, "signProgressStatus"),""));

}
	
/*----------------------------------------------------------------------------------------------
