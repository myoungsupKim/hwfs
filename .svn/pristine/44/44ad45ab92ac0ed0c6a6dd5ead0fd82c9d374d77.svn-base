 *---------------------------------------------------------------------------------------------*/
function btn_fiscalSlipCreate_onclick(obj:Button,  e:ClickEventInfo)
{
	//v_ds_check 미사용시 변경사항 체크
 	if ( !gfn_dsIsUpdated(ds_gdspaymList))
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	var chkCnt = ds_gdspaymList.findRow("chk",'1');
	//체크된 항목이 하나도 없으면 경고를 한다.
	if(chkCnt<0)
	{
		gfn_alert("msg.noselect","체크박스");
		return;
	}
	 
	for ( var i=0; i<ds_gdspaymList.rowcount; i++ ) 
	{
		if ( ds_gdspaymList.getRowType(i) == Dataset.ROWTYPE_UPDATE ) 
		{
			if ( ds_gdspaymList.getColumn(i, "signProgressStatus") !=  "COMPLETE" ) {
				gfn_alert("결재 승인완료 상태가 아닙니다.");	//
				return;
			}		
					
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "slipStatus"),"") ==  "P" )	//전표확정
			{
				gfn_alert("전표 확정상태 입니다.");
				return; 
			}
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "slipStatus"),"") ==  "A" )	//전표요청
			{
				gfn_alert("전표 요청상태 입니다.");
				return; 
			}
			if ( gfn_nvl(ds_gdspaymList.getColumn(i, "slipStatus"),"") ==  "B" )	//전표요청
			{
				gfn_alert("전표 취소요청 상태 입니다.");
				return; 
			}
		}
	}	
	

	// 저장시 Validation Check
	if ( !fn_chk() ) return;
	
	
	// 저장시 Validation Check
	if (gfn_validCheck(ds_gdspaymList, ds_gdspaymListValid) == false)	return;

	var retValue = gfn_confirm("ft.gds.confirm.before.if", " ");	// 전표 I/F하시겠습니까?
	if (retValue == false) return;
	
	ds_search.setColumn(0, "draft", "N" );	

