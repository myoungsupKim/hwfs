function ds_input_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	if (e.columnid == "JOB")	// 직군변경시
	{
		if (gfn_length(e.newvalue)==0)
		{
			Copy(ds_cbo_SUB_JOB, ds_SUB_JOB1);
			cbo_Sub_Job.index = 0;
		}
		else
		{
			//SUB 직군코드
			fn_sql_common("C", strMAQuery_MASUBJOB,"","AND JOB_CD = '" + e.newvalue + "'ORDER BY SUB_JOB_CD","",cbo_Sub_Job,-1,ds_cbo_SUB_JOB);			
			ds_cbo_SUB_JOB.insertRow(0);
			ds_cbo_SUB_JOB.setColumn(0,"SUB_JOB_NAME","전체");
			ds_cbo_SUB_JOB.setColumn(0,"SUB_JOB_CD","");
			cbo_Sub_Job.index = 0;			
		}
	}
