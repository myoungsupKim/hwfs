<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>insert</type>
	<description><![CDATA[권한입력]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
INSERT INTO SCC_BBSROLE
(
      BBSROLE
    , ROLE_NAME
    , CONTENTS
    , REMARK
    , USE_YN
    , ATTR01
    , ATTR02
    , ATTR03
    , ATTR04
    , ATTR05
    , ATTR06
    , ATTR07
    , ATTR08
    , ATTR09
    , ATTR10
    , CUSER
    , UUSER
    , INSIDE_BASEAUTH_YN
    , EXTR_BASEAUTH_YN
)
VALUES
(
      #BBSROLE#
    , #ROLE_NAME#
    , #CONTENTS#
    , #REMARK#
    , DECODE(#USE_FLAG#, '1', 'Y', 'N')
    , #ATTR01#
    , #ATTR02#
    , #ATTR03#
    , #ATTR04#
    , #ATTR05#
    , #ATTR06#
    , #ATTR07#
    , #ATTR08#
    , #ATTR09#
    , #ATTR10#
    , #CUSER#
    , #UUSER#
    , DECODE(#INSIDE_BASEAUTH_YN#, '1', 'Y', 'N')
    , DECODE(#EXTR_BASEAUTH_YN#, '1', 'Y', 'N')
)	]]></statement>
