 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	var time = gfn_todayTime().substr(8);

	if(time > 180000){
		if(!(gfn_today() < edt_from.value) && !(gfn_today() < edt_to.value))
		{
			var sMsg = "물류요청서 등록은 당일 18:00 까지 등록 가능합니다."
			gfn_message("A", sMsg);
			return;
		}
	}
	
 	// v_ds_check 미사용시 변경사항 체크	
	var ds_check 

	ds_check = ds_list;	

 	if (gfn_dsIsUpdated(ds_check) == false) 
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}

	//조회시 Validation Check
	if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;
 	
 	
	// 저장시 Validation Check	
	if (gfn_validCheck(grd_list, ds_listValid) == false)	return;
	
	for(var i=0; i<ds_list.getRowCount(); i++)
	{
		if(ds_list.getRowType(i) == 2 && ds_list.getColumn(i,"clsfiClass") != "추가기재" && gfn_isNull(ds_list.getColumn(i, 'vendorNm'))){
			gfn_alert("업체명은 필수 입니다.");
			ds_list.rowposition = i;
			return;
		}
		
		if(gfn_length(ds_list.getColumn(i, "requestDate")) < 8)
		{
			gfn_alert("요청일자는 필수 입니다.");
			ds_list.rowposition = i;
			return;
		}
	}
	
	// 분류 체크
	if (fn_classCheck() == false) 	return;
 	
	var rtVal = gfn_confirm("confirm.before.save");	// 자료를 저장 하시겠습니까?
	
	for(var i=0; i<ds_list.getRowCount(); i++)
	{
		trace(i + " : " + ds_list.getRowType(i));
		if(ds_list.getRowType(i) == 2 ) {
			ds_list.setColumn(i, 'centerCode', ds_search.getColumn(0, "centerCode"));
			//ds_list.setColumn(i, 'requestDate', cal_requestFromDate.text);
		}
	}
	
	if(rtVal == true)
	{
		var strSvcID    = "save";
		var strURL      = "fm/fmc/distributionRequestRptReg/saveList.xdo";
		
//		if(ds_listMast.getRowCount() == 1){
			var strInDs     = "ds_list=ds_list:U";
			var strURL      = "fm/fmc/distributionRequestRptReg/saveList.xdo";
		
//		}else{
//			// MASTER 정보가 없는 경우 (신규)
//			var nRow= ds_listMast.addRow();
//			ds_listMast.setColumn(nRow, "centerCode", ds_search.getColumn(0, "centerCode"));
			
//			var strInDs     = "ds_list=ds_list:U ds_listMast=ds_listMast";
//			var strURL      = "fm/fmc/distributionRequestRptReg/saveMast.xdo";
//			ds_listMast.addRow();
//		}
		
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}	
}

