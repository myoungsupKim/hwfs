function grd_list_OnMouseOver(obj:Grid, e:GridMouseEventInfo)
{
	
	var strMsg = ""
	if(e.col == 11)
	{
		strMsg = ds_list.getColumn(e.row,"centerRequestFact");
	}
	if(e.row == -1)	 strMsg = ""
	grd_list.tooltiptext = strMsg;
	
}

function ds_search_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	ds_list.clearData();
	ds_listMast.clearData();
}

function fn_showTstContent(ds)
{
	var strId       = "TstContent";
	var strURL      = "U_SCC::SCC99999P1.xfdl";
	var objArgument = {
		P_TST_CONTENT : ds.getColumn(ds.rowposition, "centerRequestFact")
	};
	var nWidth      = 790;
	var nHeight     = 440;
	var strStyle    = "";
	var nLeft       = -1;
	var nTop        = -1;
	var isModeless  = false;
	var bShowTitle  = true;
	var strAlign    = "-1";
	
	var centerRequestFact = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);
	
	if (centerRequestFact != undefined) ds.setColumn(ds.rowposition, "centerRequestFact", centerRequestFact);
}

function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	fn_search();
}

function btn_Ok_onclick(obj:Button,  e:ClickEventInfo)
{
	var rowNum;
	
	for(var i=0; i<ds_list.getRowCount(); i++)
	{
		if(ds_list.getColumn(i,"chk") == 1)
		{
			rowNum = opener.ds_list.addRow();
			opener.ds_list.copyRow(rowNum,ds_list,i);
			//요청일자는 무조건 내일일자로 셋팅 - 이재미
			opener.ds_list.setColumn(rowNum,"requestDate",gfn_addDate(gfn_today(),1));
		}
	}
	
	this.close();
}
