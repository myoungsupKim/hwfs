function grd_copyPaste_onkeydown(obj:Grid, e:KeyEventInfo)
{
	grd_list.setCellProperty('Body',1, 'edittype', 'text');
	// Ctrl + C
	if(e.ctrlKey == true && e.keycode == 67) 
	{
		//복사(Ctrl + C)
		gfn_gridCopy(obj, e);
	}
	// Ctrl + V	: Paste
	else if(e.ctrlKey == true && e.keycode == 86 && gfn_length(cal_requestFromDate.text) != '0') {		
		//복사(Ctrl + V)
		obj.setCellPos(1);
		gfn_gridPaste(obj, e);
	}
	grd_list.setCellProperty('Body',1, 'edittype', 'combo');
}

