    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

function fn_classCheck()
{
	var ret;
	for(var i=0; i<ds_list.getRowCount(); i++)
	{	
		if(ds_list.getRowType(i)==2 || ds_list.getRowType(i)==4)
		{
			ret = false;
			for(var j=0; j<ds_clasfiClass.getRowCount(); j++ )
			{
				if (ds_list.getColumn(i,'clsfiClass') == ds_clasfiClass.getColumn(j,'value')) 	ret = true;
			}
			if(ret != true)
			{
				gfn_message("A", "분류 값이 올바르지 않습니다.");
				grd_list.setCellPos(grd_list.getBindCellIndex("Body", 'clsfiClass'));
				ds_list.rowposition = i;
				grd_list.showEditor(true);
				grd_list.setFocus();
				return false;
			}
		}
	}
	return true;
}



