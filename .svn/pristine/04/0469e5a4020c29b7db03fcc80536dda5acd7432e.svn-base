function fn_loadMast()
{
	fv_objColInfo2 = gfn_getDatasetInfo(ds_listMast);
	ds_listMast.clear();

    var strSvcID    = "searchMast";
    var strURL      = "fm/fmc/distributionRequestRptReg/selectMast.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_listMast=ds_listMast";
