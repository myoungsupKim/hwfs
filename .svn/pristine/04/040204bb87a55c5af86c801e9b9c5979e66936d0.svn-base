*/

/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";


/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_sToday = gfn_today();


/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";
	
	//검색조건을 초기화 한다.
	var nRow = ds_search.addRow();
	ds_search.setColumn(nRow, "muCd", g_Mu_Cd);
	ds_search.setColumn(nRow, "ccCd", g_Cc_Cd);
	ds_search.setColumn(nRow, "ccNm", g_Cc_Nm);
	ds_search.setColumn(nRow, "g_PersInfoMask", g_PersInfoMask);
	ds_search.setColumn(nRow, "p_MaskAuth", SCAuth.getMaskAuth(this));
	
	//Combo
	SCCombo.bind(this, "ComCode", "cbo_sysClass", ds_sysClass, {groupCode:"SYS_CLASS", useYn:"Y"}, SCCombo.ALL); 	//시스템구분
	
	//Popup
	SCPopup.bind(this, "CardComCode", "edt_cardComNm", "btn_cardComSearchPop", null, {groupCode:""}, {after:"fn_cardPopupSearchAfter"});	//카드사사코드 팝업(입력)
	SCPopup.bind(this, "CcCode", "edt_ccNm", "btn_cc",	null, {}, {after:"fn_popCcCdAfter"}); 												//원가부서CC코드팝업
	
	//Dataset
	SCDataset.bind(this, "ComCode", "ds_muCd", {groupCode:"DATA_SET", code:"DEF_MU_CODE", useYn:"Y"}, "fn_triggerDatasetEvent");
	
	//Calendar(검색조건 바인드)
	cal_occurDateFrom.fn_initMonthCal(this, gfn_addMonth(fv_sToday, -1), "", ds_search, "occurDateFrom");
	cal_occurDateTo.fn_initMonthCal(this, fv_sToday, "", ds_search, "occurDateTo");
	
	//카드처리유형검색
	fn_searchCardProcType();
	
}

