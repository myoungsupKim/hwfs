/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 조회
        case "search" :
            //trace(ds_list.saveXML());
            break;
        
        //카드처리유형조회
        case "searchCardProcType" :
			ds_cardProcType.insertRow(0);
			ds_cardProcType.setColumn(0, "cardProcType", "");
			ds_cardProcType.setColumn(0, "cardProcTypeNm", "전체");
			ds_search.setColumn(0, "cardProcType", "");
			break;
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;    
    }
}


