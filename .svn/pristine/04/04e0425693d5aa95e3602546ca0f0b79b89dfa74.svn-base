/*
 ******************************************************************************************
 * 함  수  명   : fn_addRow()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 행추가 이벤트
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_addRow(obj:Button,  e:ClickEventInfo)
{	
	// 보유기기
	if(obj.name == "btn_addUtensil") {
		var addRw = ds_utensil.addRow();
		ds_utensil.setColumn(addRw, "upjangCd", ds_search.getColumn(0, "upjangCd"));
		ds_utensil.setColumn(addRw, "utensilClass", "001");
	
	// 고객사
	}else if(obj.name == "btn_addCustInfo") {
		var addRw = ds_custMgnt.addRow();
		ds_custMgnt.setColumn(addRw, "upjang", ds_search.getColumn(0, "upjangCd"));	
		ds_custMgnt.setColumn(addRw, "useYn", "Y");	
		ds_custMgnt.setColumn(addRw, "mainMenuPrice", 0);		
		ds_custMgnt.setColumn(addRw, "payDate", 0);		
		ds_custMgnt.setColumn(addRw, "releasePriority", 0);
	
	// 식단가
	}else if(obj.name == "btn_addMpriceInfo") {
		var addRw = ds_mpriceInfo.addRow();
		ds_mpriceInfo.setColumn(addRw, "upjang", ds_search.getColumn(0, "upjangCd"));
		ds_mpriceInfo.setColumn(addRw, "stJobcd", ds_accountingInfo.getColumn(0,"jobCd"));
		ds_mpriceInfo.setColumn(addRw, "stSubJobcd", ds_accountingInfo.getColumn(0,"subJobCd"));		
		ds_mpriceInfo.setColumn(addRw, "gubunCd1", tab_upjangPackMgnt.tabpage6.cbo_gubun1.value);
		ds_mpriceInfo.setColumn(addRw, "gubunCd2", tab_upjangPackMgnt.tabpage6.cbo_gubun2.value);
		ds_mpriceInfo.setColumn(addRw, "mpriceOpendd", fv_sToday);
		ds_mpriceInfo.setColumn(addRw, "mealUprice", "0");
		ds_mpriceInfo.setColumn(addRw, "useYn", "Y");
	
	// 대표식단가 / 대표 식수
	}else if(obj.name == "btn_addRespMealList") {
		var addRw = ds_respMealList.addRow();
		ds_respMealList.setColumn(addRw, "upjang", ds_search.getColumn(0, "upjangCd"));
	}
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_deleteRow()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 행삭제 이벤트
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_deleteRow(obj:Button,  e:ClickEventInfo)
{
	if(obj.name =="btn_deleteUtensil") {
		if(ds_utensil.getColumn(ds_utensil.rowposition, "regYn")=="Y") {
			gfn_alert("기존코드는 수정만 가능합니다.");
			return;
		}else {
			ds_utensil.deleteRow(ds_utensil.rowposition);
		}
	}else if(obj.name =="btn_deleteCustInfo") {
		
		if(ds_custMgnt.getColumn(ds_custMgnt.rowposition, "regYn")=="Y") {
			gfn_alert("기존코드는 수정만 가능합니다.");
			return;
		}else {
			ds_custMgnt.deleteRow(ds_custMgnt.rowposition);
		}
	
	// 식단가
	}else if(obj.name =="btn_deleteMpriceInfo") {
		if(ds_mpriceInfo.getColumn(ds_mpriceInfo.rowposition, "regYn")=="Y") {
			gfn_alert("기존코드는 수정만 가능합니다.");
			return;
		}else {
			ds_mpriceInfo.deleteRow(ds_mpriceInfo.rowposition);
		}
	
	// 대표식단가 / 대표 식수
	}else if(obj.name =="btn_deleteRespMealList") {
		ds_respMealList.deleteRow(ds_respMealList.rowposition);
	}
}

