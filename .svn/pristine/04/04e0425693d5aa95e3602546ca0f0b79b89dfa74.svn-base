 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function tab_upjangPackMgnt_tabpage6_cbo_gubun1_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if((toNumber(ds_accountingInfo.getColumn(ds_accountingInfo.rowposition, "upjang")) != 0)
			&& (e.preindex <> e.postindex)) {
		// insert 상태가 아닌 경우	
		if(ds_mpriceInfo.getRowType(ds_mpriceInfo.rowposition) != 2){
				gfn_alert("기존의 식구분을 변경할 수 없습니다.");
				obj.index = e.preindex;
				
				ds_mpriceInfo.setColumn(ds_mpriceInfo.rowposition, "gubunCd1", e.prevalue);
				ds_mpriceInfo.setColumn(ds_mpriceInfo.rowposition, "gubunCd1nm", e.pretext);
				return;
		}else {
			ds_mpriceInfo.setColumn(ds_mpriceInfo.rowposition, "gubunCd1", e.postvalue);
			ds_mpriceInfo.setColumn(ds_mpriceInfo.rowposition, "gubunCd1nm", e.posttext);
		}
	}	
}

