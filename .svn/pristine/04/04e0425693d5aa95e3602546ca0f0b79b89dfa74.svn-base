                  "18":경영자,"99":전산관리자(현업),"999":전산관리자(전산)
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_GetSaveAuthBy_AuthLevelLocal(real_UPJANG)
{
	var expUpjang = ['161527', '161535', '161538', '161541', '100851'];
    if(toNumber(g_AuthLevel) != 10 && toNumber(g_AuthLevel) != 14) {
        //식수제외업장 권한 임시 지정 : 추후 메뉴관리자(fs)권한으로 변경 필요 20160810 맹수영 (CH201607_00047)
        if (g_EmpNo != '201600733' && g_EmpNo != '201100570') {
			if ( expUpjang.indexOf(g_Upjang) == -1 ) {
				if (real_UPJANG <> g_Upjang) {
					gfn_alert("해당 사용자는 소속업장에 대해서만 저장/삭제 가능합니다. 확인하여 주십시요.");
					return false;
				}
			}
		}
    }    
	return true;
}	

