function fn_saveUtensil() {

	if (!gfn_dsIsUpdated(ds_utensil)) {
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
 	
 	// 저장시 Validation Check	
	//if (gfn_validCheck(grd_list, ds_listValid) == false)	return;
	
	for(var i=0; i<ds_utensil.rowcount; i++) {

		if(gfn_isNull(ds_utensil.getColumn(i, "mgmtvendor"))) {
			gfn_alert((i + 1) + "행에 정수기관리업체가 존재하지 않습니다. 확인하여 주십시요.");
			ds_utensil.rowposition = i;
			return;
		}
		
		if(gfn_isNull(ds_utensil.getColumn(i, "filterMgmtvendor"))){
		    gfn_alert((i + 1) + "행에 필터관리업체가 존재하지 않습니다. 확인하여 주십시요.");
			ds_utensil.rowposition = i;
			return;
		}
					
		if(gfn_isNull(ds_utensil.getColumn(i, "retalYn"))){
			gfn_alert((i + 1) + "행에 렌탈여부가 존재하지 않습니다. 확인하여 주십시요.");
			ds_utensil.rowposition = i;
			return;
		}
										
		if(gfn_isNull(ds_utensil.getColumn(i, "mgmtmnbd"))){	
			gfn_alert((i + 1) + "행에 관리주체가 존재하지 않습니다. 확인하여 주십시요.");
			ds_utensil.rowposition = i;
			return;
		}
				
		if(ds_utensil.getColumn(i, "quantity") == "0"){
			gfn_alert((i + 1) + "행에 수량이 존재하지 않습니다. 확인하여 주십시요.");
			ds_utensil.rowposition = i;
			return;
		}
							
		if(gfn_isNull(ds_utensil.getColumn(i, "contractOpendd"))){	
			gfn_alert((i + 1) + "행에 계약개시일이 존재하지 않습니다. 확인하여 주십시요.");
			ds_utensil.rowposition = i;
			return;
		}																	
		
		// insert 일때 중복체크
		if (ds_utensil.getRowType(i) == 2){			
			for (var j=0; j<ds_utensil.rowcount; j++){				
				if ( (i != j) &&
					 (ds_utensil.getColumn(i, "mgmtvendor") == ds_utensil.getColumn(j, "mgmtvendor"))  ){													
							
					gfn_alert((i + 1) + "행에 데이터가 중복됩니다. 확인하여 주십시요.");
					ds_utensil.rowposition = i;
					return;									 
				}				
			}					
		}		
	}
	
	/*/정수기보유현황 저장######################################################
	if (str_Gubun == "Purifier"){
	
		// action 정보 초기화 
		tit_clearActionInfo(this);
		//alert(ds_list.contents);
		tit_addSaveActionInfo(this, "ma/mas:MAS10020E_I002", "ma/mas:MAS10020E_U002", "", "", "", "", "");
		tit_callService(this, "", "", "ds_cond=ds_purifier:U", "",  "", "fn_SearchGubun(1)");
	}*/	
	
	
