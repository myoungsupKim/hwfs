function fn_saveCustInfo()
{
	if (!gfn_dsIsUpdated(ds_custMgnt)) {
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	// 저장시 Validation Check	
	//if (gfn_validCheck(grd_list, ds_listValid) == false)	return;	
	
	for (var i=0;i<ds_custMgnt.rowcount; i++){

		if (gfn_isNull(ds_custMgnt.getColumn(i, "custcd"))) {
			gfn_alert((i + 1) + "행에 거래처가 존재하지 않습니다. 확인하여 주십시요.");
			ds_custMgnt.rowposition = i;
			return;
		}
		
		if (ds_custMgnt.getColumn(i, "releasePriority") == "0") {		
			gfn_alert((i + 1) + "행에 우선순위가 존재하지 않습니다. 확인하여 주십시요.");
			ds_custMgnt.rowposition = i;
			return;
		}
		
		if (ds_custMgnt.getColumn(i, "mainMenuPrice") == "0") {
			gfn_alert((i + 1) + "행에 대표식단가가 존재하지 않습니다. 확인하여 주십시요.");
			ds_custMgnt.rowposition = i;
			return;
		}
							
		if (gfn_isNull(ds_custMgnt.getColumn(i, "payGubunCd"))) {
			gfn_alert((i + 1) + "행에 식대지급일구분이 존재하지 않습니다. 확인하여 주십시요.");
			ds_custMgnt.rowposition = i;
			return;
		}
		
		if (toNumber(ds_custMgnt.getColumn(i, "payDate")) > 31) {	
		
			gfn_alert((i + 1) + "행에 식대지급일이 31일 보다 클수 없습니다. 확인하여 주십시요.");
			ds_custMgnt.rowposition = i;
			return;
		}				

		if((ds_custMgnt.getColumn(i, "payGubunCd") == "999") && (toNumber(ds_custMgnt.getColumn(i, "payDate")) != 0)) {		
			gfn_alert((i + 1) + "행에 식대지급일구분이 즉시인경우 일자는 0이어야합니다. 확인하여 주십시요.");
			ds_custMgnt.rowposition = i;
			return;
		}									
									
		if((ds_custMgnt.getColumn(i, "payGubunCd") <> "999") && (toNumber(ds_custMgnt.getColumn(i, "payDate")) == 0)) {
		
			gfn_alert((i + 1) + "행에 식대지급일이 존재하지 않습니다. 확인하여 주십시요.");
			ds_custMgnt.rowposition = i;
			return;
		}																						
		
		// insert 일때 중복체크
		if(ds_custMgnt.getRowType(i) == 2){		
			for (var j=0; j<ds_custMgnt.rowcount; j++) {
				if ( (i != j) && (ds_custMgnt.getColumn(i, "custCd") == ds_custMgnt.getColumn(j, "custCd")) ){
					gfn_alert((i + 1) + "행에 데이터가 중복됩니다. 확인하여 주십시요.");
					ds_custMgnt.rowposition = i;
					return;
				}			
			}
		}			
	}
	
	/*/고객사현황 저장###########################################################
	if (str_Gubun == "Cust"){
	
		// action 정보 초기화 
		tit_clearActionInfo(this);
		//alert(ds_list.contents);
		tit_addSaveActionInfo(this, "ma/mas:MAS10020E_I004", "ma/mas:MAS10020E_U005", "", "", "", "", "");
		tit_callService(this, "", "", "ds_cond=ds_SCO_UPJANG_CUST:U", "",  "", "fn_SearchGubun(5)");
	}*/	
		
    var strSvcID    = "saveCustInfo";
    var strURL      = "fs/fsb/upjangPackMgnt/saveCustInfo.xdo";
    var strInDs     = "ds_search=ds_search:A ds_custMgnt=ds_custMgnt:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

