<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>

/* 자재재고보유량 상세조회 */
SELECT B.CENTER_NAME
     , B.TM_NM
     , B.JOB_NAME
     , B.SUB_JOB_NAME
     , B.UPJANGNM
     , B.SUBINV_NAME
<isEqual col="UNIT" value="1">
     , SUM(A.UOM_QTY / C.UOM_CONVERT_RATE) AS BONHAND_QTY
     , SUM(A.IN_QTY / C.UOM_CONVERT_RATE)  AS IN_QTY
     , SUM(A.OUT_QTY / C.UOM_CONVERT_RATE) AS OUT_QTY
     , SUM(A.UOM_QTY / C.UOM_CONVERT_RATE) + SUM(A.IN_QTY / C.UOM_CONVERT_RATE) - SUM(A.OUT_QTY / C.UOM_CONVERT_RATE) AS AONHAND_QTY
</isEqual>
<isEqual col="UNIT" value="0">
     , SUM(A.UOM_QTY) AS BONHAND_QTY
     , SUM(A.IN_QTY)  AS IN_QTY
     , SUM(A.OUT_QTY) AS OUT_QTY
     , SUM(A.UOM_QTY) + SUM(A.IN_QTY) - SUM(A.OUT_QTY) AS AONHAND_QTY
</isEqual>
  FROM (
        SELECT A.SUBINV_CODE AS SUBINV_CODE
             , A.ITEM_CODE AS ITEM_CODE
             , A.ONHAND_QTY AS UOM_QTY
             , 0 AS IN_QTY
             , 0 AS OUT_QTY
          FROM HLDC_PO_ONHAND_MONTH A
         WHERE ONHAND_YM = (SELECT MAX(Z.CLS_YM) AS CLS_YM
                              FROM HLDC_PO_CLOSING Z
                             WHERE Z.CLS_YM &lt; SUBSTR(#SDATE#, 1, 6)
                               AND Z.SUBINV_CODE = 'C1000001'       -- 무조건 한화빌딩 식자재 창고로 조회
                               AND Z.CLS_FLAG    = 'Y'
                           )
            AND SUBINV_CODE IN (
			       					SELECT A.SUBINV_CODE
			                       FROM HLDC_PO_SUBINVENTORY A
			                          , ST_UPJANG B
			                          , HLDC_SC_DEPT_V C
			                       WHERE A.UPJANG = B.UPJANG
			                       	 AND B.DEPT_ID = C.DEPT_ID									
									<isNotNull col="MU">
									   AND C.MU_CD = #MU#
									</isNotNull>
									<isNotNull col="TM_CD">
									   AND C.TM_CD = #TM_CD#
									</isNotNull>
									<isNotNull col="UPJANG">
									   AND A.UPJANG = #UPJANG#
									</isNotNull>			
			                       	 AND A.SUBINV_CODE LIKE 'C%'		                             
			                         )		
		   AND A.ITEM_CODE = #ITEM_CODE#
         UNION ALL
        SELECT A.SUBINV_CODE AS SUBINV_CODE
             , A.ITEM_CODE AS ITEM_CODE
             , DECODE(SUBSTR(TRANS_TYPE, 1, 1), 'I', 1, -1) * UOM_QTY AS UOM_QTY
             , 0 AS IN_QTY
             , 0 AS OUT_QTY
          FROM (
          	  SELECT DT.MG_NO AS TRANS_ID,
                       DT.TRANS_DATE,
                       CASE
                                 WHEN SUBSTR(DT.MOVEMENT_TYP, 0, 1) = '3'
                        AND    DT.GR_QTY IS NULL THEN 'O002' /* 재고이동 - 이동출고   */
                                 WHEN SUBSTR(DT.MOVEMENT_TYP, 0, 1) = '3'
                        AND    DT.GR_QTY IS NOT NULL THEN 'I002' /*재고이동 - 이동입고  */
                                 WHEN DT.MOVEMENT_TYP = '251'
                        OR     DT.MOVEMENT_TYP = '252' THEN 'O001' /* POS 매출 유형  */
                                 WHEN DT.MOVEMENT_TYP ='701'
                        OR     DT.MOVEMENT_TYP = '702' THEN 'O004' /* 재고실사 */
                                 WHEN ((DT.MOVEMENT_TYP ='197'
                                        OR     DT.MOVEMENT_TYP = '198')
                                AND    (NVL(GI_BUY_AMT, 0) != 0
                                        OR     NVL(GI_MGT_AMT, 0) != 0)) THEN 'O004' /* 재고실사 */
                        WHEN DT.MOVEMENT_TYP ='261' OR DT.MOVEMENT_TYP = '262' THEN 'O003'      /* 대표창고 수불 단수차 원가 처리 2018.04.11 최학진 */
                                 ELSE 'I001'
                       END AS TRANS_TYPE,                       
                        DT.PO_NO AS PO_NO,
                        DT.PO_LNO AS PO_LNO,
                        HD.MU_CD AS TRANS_MU_CD,  
                        HD.CC_CD AS TRANS_DEPT_ID,
                        HD.SHOP_CD AS TRANS_UPJANG,
                        DT.WH_CD AS SUBINV_CODE,
                        DT.ITEM_CD AS ITEM_CODE,
                             CASE
                               WHEN MOD(TO_NUMBER(DT.MOVEMENT_TYP), 2)=0 THEN DECODE(DT.GR_QTY, NULL, DT.GI_QTY*-1, DT.GR_QTY*-1)
                               WHEN MOD(TO_NUMBER(DT.MOVEMENT_TYP), 2)=1 THEN DECODE(DT.GR_QTY, NULL, DT.GI_QTY, DT.GR_QTY)
                             END AS UOM_QTY                 

                      FROM   EPROCUSR.ESIMGHD HD,

                      /* 수불헤더*/
                        (SELECT PDT.SYS_ID ,
                                     PDT.COMP_CD ,
                                     PDT.MG_DATE AS TRANS_DATE ,
                                     PDT.MG_NO ,
                                     PDT.IVT_INSP_NO ,
                                     PDT.MOVEMENT_TYP ,
                                     PDT.PROC_TYP_CD ,
                                     PDT.PURC_UNIT_RATE ,
                                     PDT.GR_QTY,
                                     PDT.GR_BUY_AMT,
                                     PDT.GR_PURC_QTY,
                                     PDT.GR_MGT_PRICE,
                                     PDT.GR_MGT_AMT,
                                     PDT.GR_BUY_PRICE ,
                                     PDT.GI_QTY,
                                     PDT.GI_BUY_AMT,
                                     PDT.GI_PURC_QTY,
                                     PDT.GI_MGT_PRICE,
                                     PDT.GI_MGT_AMT,
                                     PDT.GI_BUY_PRICE ,
                                     PDT.PO_NO ,
                                     PDT.PO_LNO ,
                                     PDT.WH_CD ,
                                     PDT.CC_WH_CD ,
                                     PDT.MV_WH_CD ,
                                     PDT.ITEM_CD ,
                                     PDT.VD_CD ,
                                     PDT.JRNL_TYP_CD
                              FROM   EPROCUSR.ESIMGDT PDT
                              WHERE  PDT.SYS_ID = '100'
                              AND    PDT.COMP_CD = 'HFC'
                              AND    PDT.MOVEMENT_TYP NOT IN ('561', '562', '998', '999', '901', '902', '801', '802', '111', '112', '211', '212') /* 기초 , 기말 재고,배송입출고,  직송 가상입출고  제외 */
                              AND    PDT.STS != 'D'

                      ---------------------------
                              AND    PDT.MOVEMENT_TYP NOT IN ('701', '702')
                              AND PDT.MG_DATE BETWEEN TO_CHAR(ADD_MONTHS(TO_DATE(
                                                                                (SELECT NVL(MAX(Z.CLS_YM), '000101') AS CLS_YM
						                                                                    FROM HLDC_PO_CLOSING Z
						                                                                   WHERE Z.CLS_YM &lt; SUBSTR(#SDATE#, 1, 6)
						                                                                     AND Z.SUBINV_CODE = 'C1000001'
						                                                                     AND Z.CLS_FLAG    = 'Y')
						                                                             ||'01', 'YYYYMMDD'), 1), 'YYYYMMDD')
                                                    AND TO_CHAR(TO_DATE(#SDATE#, 'YYYYMMDD') - 1, 'YYYYMMDD')
                                 --AND PDT.WH_CD LIKE 'C%'                                            
		   						 AND  PDT.ITEM_CD = #ITEM_CODE#
								 AND  PDT.WH_CD IN (
						       					SELECT A.SUBINV_CODE
						                       FROM HLDC_PO_SUBINVENTORY A
						                          , ST_UPJANG B
						                          , HLDC_SC_DEPT_V C
						                       WHERE A.UPJANG = B.UPJANG
						                       	 AND B.DEPT_ID = C.DEPT_ID									
												<isNotNull col="MU">
												   AND C.MU_CD = #MU#
												</isNotNull>
												<isNotNull col="TM_CD">
												   AND C.TM_CD = #TM_CD#
												</isNotNull>
												<isNotNull col="UPJANG">
												   AND A.UPJANG = #UPJANG#
												</isNotNull>			
						                       	 AND A.SUBINV_CODE LIKE 'C%'		                             
						                         )		 
                              

                      ---------------------------
                              UNION ALL
                        SELECT PDT.SYS_ID ,
                                     PDT.COMP_CD ,
                                     PA.INSP_DATE AS TRANS_DATE ,
                                     PDT.MG_NO ,
                                     PDT.IVT_INSP_NO ,
                                     PDT.MOVEMENT_TYP ,
                                     PDT.PROC_TYP_CD ,
                                     PDT.PURC_UNIT_RATE ,
                                     PDT.GR_QTY,
                                     PDT.GR_BUY_AMT,
                                     PDT.GR_PURC_QTY,
                                     PDT.GR_MGT_PRICE,
                                     PDT.GR_MGT_AMT,
                                     PDT.GR_BUY_PRICE ,
                                     PDT.GI_QTY,
                                     PDT.GI_BUY_AMT,
                                     PDT.GI_PURC_QTY,
                                     PDT.GI_MGT_PRICE,
                                     PDT.GI_MGT_AMT,
                                     PDT.GI_BUY_PRICE ,
                                     PDT.PO_NO ,
                                     PDT.PO_LNO ,
                                     -- PDT.WH_CD || '' AS WH_CD, IF PDT.MOVEMENT_TYP IN ('701', '702') THEN PDT.WH_CD=PA.WH_CD 2017/06/21 최학진대리확인받음
                                     PA.WH_CD  AS WH_CD,  
                                     PDT.CC_WH_CD || '' AS CC_WH_CD,
                                     PDT.MV_WH_CD || '' AS MV_WH_CD ,
                                     PDT.ITEM_CD || '' AS ITEM_CD,
                                     PDT.VD_CD || '' AS VD_CD,
                                     PDT.JRNL_TYP_CD
                              FROM   EPROCUSR.ESIMGDT PDT,
                                     EPROCUSR.ESIMIHD PA
                              WHERE  PDT.SYS_ID = '100'
                              AND    PDT.COMP_CD = 'HFC'
                              AND    PA.SYS_ID = '100'
                              AND    PA.COMP_CD = 'HFC'
                              AND    PA.STS != 'D'

                      ---
                              AND    PA.SYS_ID = PDT.SYS_ID
                              AND    PA.COMP_CD = PDT.COMP_CD
                              AND    PA.IVT_INSP_NO = PDT.IVT_INSP_NO

                      ---
                              AND    PDT.MOVEMENT_TYP NOT IN ('561', '562', '998', '999', '901', '902', '801', '802', '111', '112', '211', '212') /* 기초 , 기말 재고,배송입출고,  직송 가상입출고  제외 */
                              AND    PDT.STS != 'D'

                      ---------------------------
                              AND    PDT.MOVEMENT_TYP IN ('701', '702')                                            
		   					  AND  PDT.ITEM_CD = #ITEM_CODE#
                              AND  PA.INSP_DATE BETWEEN TO_CHAR(ADD_MONTHS(TO_DATE(
                                                                                  (SELECT NVL(MAX(Z.CLS_YM), '000101') AS CLS_YM
						                                                                    FROM HLDC_PO_CLOSING Z
						                                                                   WHERE Z.CLS_YM &lt; SUBSTR(#SDATE#, 1, 6)
						                                                                     AND Z.SUBINV_CODE = 'C1000001'
						                                                                     AND Z.CLS_FLAG    = 'Y')
						                                                             ||'01', 'YYYYMMDD'), 1), 'YYYYMMDD')
                                                    AND TO_CHAR(TO_DATE(#SDATE#, 'YYYYMMDD') - 1, 'YYYYMMDD')
                                 
								AND  PA.WH_CD IN (
			       					SELECT A.SUBINV_CODE
			                       FROM HLDC_PO_SUBINVENTORY A
			                          , ST_UPJANG B
			                          , HLDC_SC_DEPT_V C
			                       WHERE A.UPJANG = B.UPJANG
			                       	 AND B.DEPT_ID = C.DEPT_ID									
									<isNotNull col="MU">
									   AND C.MU_CD = #MU#
									</isNotNull>
									<isNotNull col="TM_CD">
									   AND C.TM_CD = #TM_CD#
									</isNotNull>
									<isNotNull col="UPJANG">
									   AND A.UPJANG = #UPJANG#
									</isNotNull>			
			                       	 AND A.SUBINV_CODE LIKE 'C%'		                             
			                         )		 

                      ---------------------------
                                     ) DT
                       

                      WHERE  HD.SYS_ID = '100'
                      AND    HD.COMP_CD = 'HFC'
                      AND    HD.BU_CD IN ('1000', '2000')
                      AND    HD.SYS_ID = DT.SYS_ID
                      AND    HD.COMP_CD = DT.COMP_CD
                      AND    HD.WH_CD = DT.WH_CD
                      AND    HD.ITEM_CD = DT.ITEM_CD 
					<isNotNull col="UPJANG">
					   AND HD.SHOP_CD = #UPJANG#
					</isNotNull>
          
          
          ) A     
         WHERE TRANS_DATE BETWEEN TO_CHAR(ADD_MONTHS(TO_DATE(
                                                             (SELECT NVL(MAX(Z.CLS_YM), '000101') AS CLS_YM
                                                                    FROM HLDC_PO_CLOSING Z
                                                                   WHERE Z.CLS_YM &lt; SUBSTR(#SDATE#, 1, 6)
                                                                     AND Z.SUBINV_CODE = 'C1000001'
                                                                     AND Z.CLS_FLAG    = 'Y')
                                                             ||'01', 'YYYYMMDD'), 1), 'YYYYMMDD')
                              AND TO_CHAR(TO_DATE(#SDATE#, 'YYYYMMDD') - 1,'YYYYMMDD')
         UNION ALL 
        SELECT A.SUBINV_CODE AS SUBINV_CODE
             , A.ITEM_CODE AS ITEM_CODE
             , 0 AS UOM_QTY
             , DECODE(SUBSTR(TRANS_TYPE,0,1), 'I',UOM_QTY, 0) AS IN_QTY
             , DECODE(SUBSTR(TRANS_TYPE,0,1), 'O',UOM_QTY, 0) AS OUT_QTY
          FROM (
             SELECT DT.MG_NO AS TRANS_ID,
                       DT.TRANS_DATE,
                       CASE
                                 WHEN SUBSTR(DT.MOVEMENT_TYP, 0, 1) = '3'
                        AND    DT.GR_QTY IS NULL THEN 'O002' /* 재고이동 - 이동출고   */
                                 WHEN SUBSTR(DT.MOVEMENT_TYP, 0, 1) = '3'
                        AND    DT.GR_QTY IS NOT NULL THEN 'I002' /*재고이동 - 이동입고  */
                                 WHEN DT.MOVEMENT_TYP = '251'
                        OR     DT.MOVEMENT_TYP = '252' THEN 'O001' /* POS 매출 유형  */
                                 WHEN DT.MOVEMENT_TYP ='701'
                        OR     DT.MOVEMENT_TYP = '702' THEN 'O004' /* 재고실사 */
                                 WHEN ((DT.MOVEMENT_TYP ='197'
                                        OR     DT.MOVEMENT_TYP = '198')
                                AND    (NVL(GI_BUY_AMT, 0) != 0
                                        OR     NVL(GI_MGT_AMT, 0) != 0)) THEN 'O004' /* 재고실사 */
                        WHEN DT.MOVEMENT_TYP ='261' OR DT.MOVEMENT_TYP = '262' THEN 'O003'      /* 대표창고 수불 단수차 원가 처리 2018.04.11 최학진 */
                                 ELSE 'I001'
                       END AS TRANS_TYPE,                       
                        DT.PO_NO AS PO_NO,
                        DT.PO_LNO AS PO_LNO,
                        HD.MU_CD AS TRANS_MU_CD,  
                        HD.CC_CD AS TRANS_DEPT_ID,
                        HD.SHOP_CD AS TRANS_UPJANG,
                        DT.WH_CD AS SUBINV_CODE,
                        DT.ITEM_CD AS ITEM_CODE,
                             CASE
                               WHEN MOD(TO_NUMBER(DT.MOVEMENT_TYP), 2)=0 THEN DECODE(DT.GR_QTY, NULL, DT.GI_QTY*-1, DT.GR_QTY*-1)
                               WHEN MOD(TO_NUMBER(DT.MOVEMENT_TYP), 2)=1 THEN DECODE(DT.GR_QTY, NULL, DT.GI_QTY, DT.GR_QTY)
                             END AS UOM_QTY                 

                      FROM   EPROCUSR.ESIMGHD HD,

                      /* 수불헤더*/
                        (SELECT PDT.SYS_ID ,
                                     PDT.COMP_CD ,
                                     PDT.MG_DATE AS TRANS_DATE ,
                                     PDT.MG_NO ,
                                     PDT.IVT_INSP_NO ,
                                     PDT.MOVEMENT_TYP ,
                                     PDT.PROC_TYP_CD ,
                                     PDT.PURC_UNIT_RATE ,
                                     PDT.GR_QTY,
                                     PDT.GR_BUY_AMT,
                                     PDT.GR_PURC_QTY,
                                     PDT.GR_MGT_PRICE,
                                     PDT.GR_MGT_AMT,
                                     PDT.GR_BUY_PRICE ,
                                     PDT.GI_QTY,
                                     PDT.GI_BUY_AMT,
                                     PDT.GI_PURC_QTY,
                                     PDT.GI_MGT_PRICE,
                                     PDT.GI_MGT_AMT,
                                     PDT.GI_BUY_PRICE ,
                                     PDT.PO_NO ,
                                     PDT.PO_LNO ,
                                     PDT.WH_CD ,
                                     PDT.CC_WH_CD ,
                                     PDT.MV_WH_CD ,
                                     PDT.ITEM_CD ,
                                     PDT.VD_CD ,
                                     PDT.JRNL_TYP_CD
                              FROM   EPROCUSR.ESIMGDT PDT
                              WHERE  PDT.SYS_ID = '100'
                              AND    PDT.COMP_CD = 'HFC'
                              AND    PDT.MOVEMENT_TYP NOT IN ('561', '562', '998', '999', '901', '902', '801', '802', '111', '112', '211', '212') /* 기초 , 기말 재고,배송입출고,  직송 가상입출고  제외 */
                              AND    PDT.STS != 'D'

                      ---------------------------
                              AND    PDT.MOVEMENT_TYP NOT IN ('701', '702')
                              AND PDT.MG_DATE BETWEEN #SDATE# AND #EDATE#
                                                                                      
		   					  AND  PDT.ITEM_CD = #ITEM_CODE#
                              AND PDT.WH_CD IN (
			       					SELECT A.SUBINV_CODE
			                       FROM HLDC_PO_SUBINVENTORY A
			                          , ST_UPJANG B
			                          , HLDC_SC_DEPT_V C
			                       WHERE A.UPJANG = B.UPJANG
			                       	 AND B.DEPT_ID = C.DEPT_ID									
									<isNotNull col="MU">
									   AND C.MU_CD = #MU#
									</isNotNull>
									<isNotNull col="TM_CD">
									   AND C.TM_CD = #TM_CD#
									</isNotNull>
									<isNotNull col="UPJANG">
									   AND A.UPJANG = #UPJANG#
									</isNotNull>			
			                       	 AND A.SUBINV_CODE LIKE 'C%'		                             
			                         )		
                              

                      ---------------------------
                              UNION ALL
                        SELECT PDT.SYS_ID ,
                                     PDT.COMP_CD ,
                                     PA.INSP_DATE AS TRANS_DATE ,
                                     PDT.MG_NO ,
                                     PDT.IVT_INSP_NO ,
                                     PDT.MOVEMENT_TYP ,
                                     PDT.PROC_TYP_CD ,
                                     PDT.PURC_UNIT_RATE ,
                                     PDT.GR_QTY,
                                     PDT.GR_BUY_AMT,
                                     PDT.GR_PURC_QTY,
                                     PDT.GR_MGT_PRICE,
                                     PDT.GR_MGT_AMT,
                                     PDT.GR_BUY_PRICE ,
                                     PDT.GI_QTY,
                                     PDT.GI_BUY_AMT,
                                     PDT.GI_PURC_QTY,
                                     PDT.GI_MGT_PRICE,
                                     PDT.GI_MGT_AMT,
                                     PDT.GI_BUY_PRICE ,
                                     PDT.PO_NO ,
                                     PDT.PO_LNO ,
                                     -- PDT.WH_CD || '' AS WH_CD, IF PDT.MOVEMENT_TYP IN ('701', '702') THEN PDT.WH_CD=PA.WH_CD 2017/06/21 최학진대리확인받음
                                     PA.WH_CD  AS WH_CD,  
                                     PDT.CC_WH_CD || '' AS CC_WH_CD,
                                     PDT.MV_WH_CD || '' AS MV_WH_CD ,
                                     PDT.ITEM_CD || '' AS ITEM_CD,
                                     PDT.VD_CD || '' AS VD_CD,
                                     PDT.JRNL_TYP_CD
                              FROM   EPROCUSR.ESIMGDT PDT,
                                     EPROCUSR.ESIMIHD PA
                              WHERE  PDT.SYS_ID = '100'
                              AND    PDT.COMP_CD = 'HFC'
                              AND    PA.SYS_ID = '100'
                              AND    PA.COMP_CD = 'HFC'
                              AND    PA.STS != 'D'

                      ---
                              AND    PA.SYS_ID = PDT.SYS_ID
                              AND    PA.COMP_CD = PDT.COMP_CD
                              AND    PA.IVT_INSP_NO = PDT.IVT_INSP_NO

                      ---
                              AND    PDT.MOVEMENT_TYP NOT IN ('561', '562', '998', '999', '901', '902', '801', '802', '111', '112', '211', '212') /* 기초 , 기말 재고,배송입출고,  직송 가상입출고  제외 */
                              AND    PDT.STS != 'D'

                      ---------------------------
                              AND    PDT.MOVEMENT_TYP IN ('701', '702')
                              AND PA.INSP_DATE BETWEEN #SDATE# AND #EDATE#
                                                                                                  
		   					  AND  PDT.ITEM_CD = #ITEM_CODE#
                              AND PA.WH_CD IN (
			       					SELECT A.SUBINV_CODE
			                       FROM HLDC_PO_SUBINVENTORY A
			                          , ST_UPJANG B
			                          , HLDC_SC_DEPT_V C
			                       WHERE A.UPJANG = B.UPJANG
			                       	 AND B.DEPT_ID = C.DEPT_ID									
									<isNotNull col="MU">
									   AND C.MU_CD = #MU#
									</isNotNull>
									<isNotNull col="TM_CD">
									   AND C.TM_CD = #TM_CD#
									</isNotNull>
									<isNotNull col="UPJANG">
									   AND A.UPJANG = #UPJANG#
									</isNotNull>			
			                       	 AND A.SUBINV_CODE LIKE 'C%'		                             
			                         )		 

                      ---------------------------
                                     ) DT
                       

                      WHERE  HD.SYS_ID = '100'
                      AND    HD.COMP_CD = 'HFC'
                      AND    HD.BU_CD IN ('1000', '2000')
                      AND    HD.SYS_ID = DT.SYS_ID
                      AND    HD.COMP_CD = DT.COMP_CD
                      AND    HD.WH_CD = DT.WH_CD
                      AND    HD.ITEM_CD = DT.ITEM_CD
                      AND HD.CC_CD IN (SELECT DEPT_ID FROM HLDC_SC_DEPT_V WHERE FS_UPJANG = 'Y')
						<isNotNull col="UPJANG">
						   AND HD.SHOP_CD = #UPJANG#
						</isNotNull>
					  AND DT.TRANS_DATE BETWEEN #SDATE# AND #EDATE#
                      
          ) A
          
       ) A
     , (
        SELECT A.UPJANG
             , A.UPJANGNM
             , A.DEPT_ID
             , A.TM_CD
             , A.TM_NM
             , A.MU_CD
             , A.JOB_CD
             , (SELECT JOB_NAME FROM MAS_JOB Z WHERE A.JOB_CD = Z.JOB_CD) AS JOB_NAME
             , A.SUB_JOB_CD
             , (SELECT SUB_JOB_NAME FROM MAS_JOB_SUB Z WHERE A.JOB_CD = Z.JOB_CD AND A.SUB_JOB_CD = Z.SUB_JOB_CD) AS SUB_JOB_NAME
             , B.CENTER_CODE
             , C.CENTER_NAME
             , D.SUBINV_CODE
             , D.SUBINV_NAME
             , A.AP_UNITPRICE_UPJANG
          FROM SCO_UPJANG_MST_V A
             , HLDC_PO_UPJANG_CENTER B
             , HLDC_PO_CENTER C
             , HLDC_PO_SUBINVENTORY D
         WHERE A.AP_UNITPRICE_UPJANG = B.UPJANG
           AND B.CENTER_CODE = C.CENTER_CODE
           AND A.UPJANG = D.UPJANG
           AND A.FS_UPJANG = 'Y'
       ) B
     , HLDC_PO_ITEM_MST C
<isNotNull col="CUSTCD">
     , PO_TREAT_UPJANG_V D
</isNotNull>
 WHERE A.SUBINV_CODE = B.SUBINV_CODE
   AND A.ITEM_CODE = C.ITEM_CODE
   AND A.ITEM_CODE = #ITEM_CODE#
<isNotNull col="CENTER_CODE">
   AND B.CENTER_CODE = #CENTER_CODE#
</isNotNull>
<isNotNull col="MU">
   AND B.MU_CD = #MU#
</isNotNull>
<isNotNull col="TM_CD">
   AND B.TM_CD = #TM_CD#
</isNotNull>
<isNotNull col="UPJANG">
   AND B.UPJANG = #UPJANG#
</isNotNull>
<isNotNull col="CUSTCD">
   AND B.AP_UNITPRICE_UPJANG = D.UPJANG
   AND A.ITEM_CODE = D.ITEM_CODE
   AND D.CUSTCD = #CUSTCD#
</isNotNull>
<isNotNull col="JOB_CD">
