    <Script type="xscript5.0"><![CDATA[include "lib::sz_fsp_xp.xjs";

this.form_onload = function (obj:Form, e:LoadEventInfo)
{
	this.fsp_init();
 
//	trace("테스트 업무화면");
	this.parent.btnGoTop.bringToFront();

	//application.goMain.gfn_goTop();
}

/*******************************************************************************
* 기      능   : 조회
* 설      명   : undefined
********************************************************************************/
this.fn_search = function ()
{
	this.fsp_clear();
	this.fsp_addSearch("sample:FSP_USER_SELECT", false);
	// connect the server 
	this.fsp_callService(
		""
		,""
		,"ds_cond=ds_cond"	// inDs
		,"ds_list=ds_list"	// outDs
		,""	// args
		,"fn_afterSearch"
		,true
	);
}

/*******************************************************************************
* 기      능   : search callback
********************************************************************************/
this.fn_afterSearch = function (ErrorCode, ErrorMsg)
{
	this.alert(this.ds_list.getRowCount()+"건 조회");
}

/*******************************************************************************
* 기      능   : 뒤로 가기 처리
********************************************************************************/
