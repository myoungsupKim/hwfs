}

// 팝업테스트
this.Button02_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var objCalArg = this.edtYM.value;
	
	application.goMain.gfn_POP_create("PopCalMonth", "cmmn::CmmnCalMonthPop.xfdl", "fn_callbackPOP", this, objCalArg, "POPx", application.goMain.divBase);
	return false;
}

/** 
 * 팝업 callBack
 * @param  : popname - 팝업을 구분짓는 id
 rtn : 팝업이 닫히면서 전달된 값
 * @return : 없음
 */
this.fn_callbackPOP = function (popname, rtn)
{
	switch (popname)
	{
		case "PopCalMonth":				// 년월
			this.edtYM.set_value(rtn);
			break;
	}
}

this.frmExam_001_onvscroll = function(obj:Form, e:nexacro.ScrollEventInfo)
{
//	trace("vScroll 이벤트");
//	trace("e.type>>>"+e.type);

	if (e.pos == 0)
	{
		this.parent.btnGoTop.set_visible(false);
	}
	else
	{
		this.parent.btnGoTop.set_visible(true);
	}
}

this.Calendar00_ondropdown = function(obj:Calendar, e:nexacro.EventInfo)
{
	var arg = new Array();
	arg[0] = this.ds_year;
	arg[1] = "년도선택";
	arg[2] = "";
	arg[3] = "year";
	application.goMain.gfn_POP_create("Year", "cmmn::CmmnCalendarTest.xfdl", "fn_callbackPOP", this, "", "POP", application.goMain.divBase);
	return false;
}

this.frmExam_001_onslide = function(obj:Form, e:nexacro.SlideEventInfo)
{
//	trace("e.yaccvalue :: "+e.yaccvalue);
//	trace("e.ydeltavalue :: "+e.ydeltavalue);
		// type = TouchDown/TouchMove/TouchUp
	if (e.yaccvalue > 0)
	{
		this.parent.parent.divBottom.set_visible(true);
		this.parent.parent.divMain.set_height(833);
		this.parent.parent.divBottom.bringToFront();
	}
	else if (e.yaccvalue < 0)
	{
		this.parent.parent.divBottom.set_visible(false);
		this.parent.parent.divMain.set_height(923);
	}
}
