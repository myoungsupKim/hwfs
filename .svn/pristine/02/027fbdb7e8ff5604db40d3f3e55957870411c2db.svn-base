/*----------------------------------------------------------------------------------------------
 * 설명      : (+) GRID 라인 추가 작업
 *---------------------------------------------------------------------------------------------*/
function fn_fnbTableAdd(obj:Grid, e:Dataset)
{
	if (gfn_nvl(edt_upjang.getUpjangCode(),"") == "") 
	{
		gfn_alert("msg.noselect","영업장");
		return;
	}
	
	if ( e.name == "ds_assignKey" )
	{
		var strGrp = gfn_nvl(ds_assignKeyGrp.getColumn(ds_assignKeyGrp.rowposition,"outletCd"),"");
		if (strGrp == "") 
		{
			gfn_alert("msg.err.grid.noselect", "테이블 그룹 선택");	// 선택된 항목이 없습니다.
			return;
		}
		var nRow = e.addRow();	
		e.setColumn(nRow , "useYn","1");
		e.setColumn(nRow , "outletCd", ds_assignKeyGrp.getColumn(ds_assignKeyGrp.rowposition,"outletCd"));
		e.setColumn(nRow , "outletNm", ds_assignKeyGrp.getColumn(ds_assignKeyGrp.rowposition,"outletNm"));
		e.setColumn(nRow , "posNum", ds_assignKeyGrp.getColumn(ds_assignKeyGrp.rowposition,"posNum"));
		e.setColumn(nRow , "picPlaceGrp", ds_assignKeyGrp.getColumn(ds_assignKeyGrp.rowposition,"picPlaceGrp"));
		e.setColumn(nRow , "picPlaceSubGrp", ds_assignKeyGrp.getColumn(ds_assignKeyGrp.rowposition,"picPlaceSubGrp"));
		e.setColumn(nRow , "picPlaceGrpNm", ds_assignKeyGrp.getColumn(ds_assignKeyGrp.rowposition,"picPlaceGrpNm"));
		e.setColumn(nRow , "grpPic", ds_assignKeyGrp.getColumn(ds_assignKeyGrp.rowposition,"picPlace"));
		//그리드의 Focus 이동
		obj.setCellPos(obj.getBindCellIndex("Body", "cdNum"));
		obj.showEditor(true);
		obj.setFocus();

	}
	else 
	{
	   
		var nRow = e.addRow();	
		e.setColumn(nRow , "useYn","1");		
		e.setColumn(nRow , "outletCd", gfn_nvl(edt_upjang.getUpjangCode(),""));
		e.setColumn(nRow , "outletNm", gfn_nvl(edt_upjang.getUpjangName(),""));

		//그리드의 Focus 이동 
		obj.setCellPos(obj.getBindCellIndex("Body", "outletNm"));
		obj.showEditor(true);
		obj.setFocus();
		
	}
	
}

