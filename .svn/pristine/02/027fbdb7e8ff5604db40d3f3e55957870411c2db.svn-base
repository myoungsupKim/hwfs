/*----------------------------------------------------------------------------------------------
 * 설명      : (-) 식음 테이블 그룹 삭제
 *---------------------------------------------------------------------------------------------*/
function fn_fnbTableDelete(obj:Grid, e:Dataset)
{
	var nRow = e.rowposition;
	if(e.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		
		var bIsProcRow = false;
		if (e.getRowType(nRow) == Dataset.ROWTYPE_INSERT)
		{
			e.deleteRow(nRow);
			//ds_assignKeyGrp 을 삭제 하는 경우 하위 그룹은 모두 삭제 처리 한다.
			if (e.name == "ds_assignKeyGrp")
			{
				for(var i = ds_assignKey.getRowCount()-1 ; i >= 0 ; i--)
				{
					 if (ds_assignKey.getRowType(i) == Dataset.ROWTYPE_INSERT)
					 {
						ds_assignKey.deleteRow(i);
					 }
					 else 
					 {
						ds_assignKey.setColumn(i, "useYn", 0);
					 }
				}
				 
			}
		} else
		{
			e.setColumn(nRow, "useYn", 0);
			bIsProcRow = true;
		}
		
		if (!bIsProcRow) return;
		
		//ds_assignKeyGrp 을 삭제 하는 경우 하위 그룹은 모두 삭제 처리 한다.
		if (e.name == "ds_assignKeyGrp")
		{
			for(var i = ds_assignKey.getRowCount()-1 ; i >= 0 ; i--)
			{
				 if (ds_assignKey.getRowType(i) == Dataset.ROWTYPE_INSERT)
				 {
					ds_assignKey.deleteRow(i);
				 }
				 else 
				 {
					ds_assignKey.setColumn(i, "useYn", 0);
				 }
			}
			 
		}
		
		sMsg = gfn_getMessage("msg.action.success", "");
		sMsg = sMsg += "\r\r처리된 결과를 저장하시겠습니까?";
		
		//실제 저장처리를 수행할 지를 알려준다.
		if (gfn_confirm(sMsg, "", "C"))
		{
			fn_save();
		}
		else
		{
			sMsg = "변경된 자료는 나중에 저장하시기 바랍니다.";
			gfn_alert(sMsg, "", "A");
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 테이블 그룹 선택시 (상위 그리트 클릭시 하위 그리드 자료 찾기)
 *---------------------------------------------------------------------------------------------*/
 
function ds_assignKeyGrp_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	//신규 추가행에 대해서만 처리한다.
	if (obj.getRowType(obj.rowposition) == Dataset.ROWTYPE_INSERT)
	{
		if (e.columnid == "picPlaceSubGrp")
		{
// 			//이미 등록된 그룹코드가 존재하는지를 확인한다.
			 
			ds_assignKeyChk.clearData();
			var strSvcID    = "searchCheckGrp";
			var strURL      = "rc/sle/posMenuKeyMgmt/selectGrpList.xdo";
			var strInDs     = "";
			var strOutDs    = "ds_assignKeyGrpChk=ds_assignKeyGrp";
			var strArg      = "outletCd="        + gfn_nvl(obj.getColumn(e.row, "outletCd"),"")
							+ " posNum="         + gfn_nvl(obj.getColumn(e.row, "posNum"),"")  
							+ " picPlaceGrp="    + gfn_nvl(obj.getColumn(e.row, "picPlaceGrp"),"")  
							+ " picPlaceSubGrp=" + gfn_nvl(obj.getColumn(e.row, "picPlaceSubGrp"),"")  
							+ " useYn="          + "0"
							+ " maskAuth="       + SCAuth.getMaskAuth(this);
			var strCallback = "fn_callBack";
			var bAsync      = false;
			
			gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
			if (ds_assignKeyGrpChk.rowcount > 0)
			{
				var sMsg = "이미 등록된 코드가 존재합니다. " ;
				gfn_alert(sMsg, "", "A");
				obj.setColumn(e.row, "picPlaceGrp", "");
				obj.setColumn(e.row, "picPlaceSubGrp", "");
				grd_assignKeyGrp.setCellPos(grd_assignKeyGrp.getBindCellIndex("Body", "picPlaceGrp"));
				grd_assignKeyGrp.setFocus();
				return;	
			}

 
			var iFindRowIdx = obj.findRowExpr( "outletCd == '" + obj.getColumn(e.row, "outletCd")
										     + "' && posNum == '" + obj.getColumn(e.row, "posNum") 
										     + "' && picPlaceGrp == '" + obj.getColumn(e.row, "picPlaceGrp") 
										     + "' && picPlaceSubGrp == '" + obj.getColumn(e.row, "picPlaceSubGrp") +"'");
			 
			var sMsg = "";
			//trace('iFindRowIdx:' + iFindRowIdx);
			//자기 자신을 제외하고 검색된 행이 존재할 시에는 중복 메시지를 표시한다.
			if (iFindRowIdx != e.row && iFindRowIdx != -1)
			{
				sMsg = "이미 등록된 코드가 존재합니다.\r\r행 번호 : " + (iFindRowIdx+1) + "\r입력 값 : " + obj.getColumn(e.row, "picPlaceGrp");
				gfn_alert(sMsg, "", "A");
				obj.setColumn(e.row, "picPlaceGrp", "");
				obj.setColumn(e.row, "picPlaceSubGrp", "");
				
				grd_assignKeyGrp.setCellPos(grd_assignKeyGrp.getBindCellIndex("Body", "picPlaceGrp"));
				grd_assignKeyGrp.setFocus();
				return;	
			}
			
			//정상입력 이면 해당 그룹의 자료는 넣어 준다.
			
		}
	}
	 
}

