function grd_assignKey_onexpanddown(obj:Grid, e:GridMouseEventInfo)
{ 
	//메뉴찾기(업장 전달)
	
	if (e.col == 7)
	{
		var outletCd = ds_assignKey.getColumn(ds_assignKey.rowposition,"outletCd");
		var outletNm = ds_assignKey.getColumn(ds_assignKey.rowposition,"outletNm");
		var arrParam = new Array();
		arrParam[0] = gfn_nvl( outletCd,"");					// 업장 코드를 전송 한다.
		arrParam[1] = gfn_nvl( outletNm,"");					// 업장 명를 전송 한다.
		
		arrParam[2] ="";
		arrParam[3] ="";
		
		arrParam[4] ="";
		arrParam[5] ="";
		
		arrParam[6] ="";
		arrParam[7] ="";
		
		var rtnVal = gfn_dialog("MenuPop"				// dialogId
					   , "X_SLE::MenuPop.xfdl"	// Url
					   , {fv_Contents:arrParam}			// 배열 
					   , null, null, null, null, null, false, true, -1);


		if (rtnVal instanceof Object)
		{
			arr = rtnVal.datas;
			
			for(var i=0; i<rtnVal.dataRow; i++)
			{
				if (i==0)
				{
					var nRow = ds_assignKey.rowposition
					ds_assignKey.setColumn(nRow , "cdNum", gfn_trim(arr[i].menuCd));
					ds_assignKey.setColumn(nRow , "cdNumNm", gfn_trim(arr[i].menuNm));
				}
				else
				{
					fn_fnbTableAdd(grd_assignKey,ds_assignKey);
					var nRow = ds_assignKey.rowposition
					ds_assignKey.setColumn(nRow , "cdNum", gfn_trim(arr[i].menuCd));
					ds_assignKey.setColumn(nRow , "cdNumNm", gfn_trim(arr[i].menuNm));
