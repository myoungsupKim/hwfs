/*
 ******************************************************************************************
 * 함  수  명   : grd_Detail1_OnExpandEdit(obj,nRow,nCell,strVal,nPivotIndex)
 * 기      능   : 개선조치사항 달력
 * 작  성  자   : 박지영
 * 작성  일자   : 2008-03-20  
 ******************************************************************************************
 */
function grd_Detail1_OnExpandEdit(obj:Grid, e:GridMouseEventInfo)
{
	var retDate,strValue;
	
	strValue = ds_Detail.getColumn(e.row,"MGMT_DATE");
	
	//retDate = gfn_dialog("", "U_SCC::SCC_CALENDAR.xfdl","fa_Date="+wrapQuote(strValue),-1,-1,"Title=false Status=false");
	
	var nCellRect  = obj.getCellRect(e.row, e.cell); //left,right,top,bottom
	var nX = system.clientToScreenX(obj, nCellRect.left)
	var nY = system.clientToScreenY(obj, nCellRect.top);
	var strDate = gfn_callCalendarPopDiv(strValue, nX, nY+24);
	
	if(gfn_isNull(strDate))
		return;
	
	ds_Detail.setColumn(e.row,"MGMT_DATE",strDate);
}

function grd_list_BKimg(val)
{
	var camera = "'theme://images/new/grd_WF_Camerabtn.png'";
	var expand = "'theme://images/new/grd_WF_Expand.png'";
	switch(val)
	{
		case "camera1":
			//trace( "transparent URL(" + "'theme://images/new/grd_WF_Camerabtn.png'" + ") center middle" );
			return "transparent URL(" + camera + ") center middle";
			break;
		
		case "camera2":
			//trace( "#e4e4e4ff URL(" + "'theme://images/new/grd_WF_Camerabtn.png'" + ") center middle" );
			return "#e4e4e4ff URL(" + camera + ") center middle";
			break;
		
		case "selectcamera":
			return "#24a3a8ff URL(" + camera + ") center middle";
			break;
		
		case "popup1":
			//trace( "transparent URL(" + "'theme://images/new/grd_WF_ExpandImage.png'" + ") center middle" );
			return "transparent URL(" + expand + ") center middle";
			break;
		
		case "popup2":
			//trace( "#e4e4e4ff URL(" + "'theme://images/new/grd_WF_ExpandImage.png'" + ") center middle" );
			return "#e4e4e4ff URL(" + expand + ") center middle";
			break;
		
		case "selectpopup":
			return "#24a3a8ff URL(" + expand + ") center middle";
			break;

		default:
			return val;
			break;
	}
}


/**
 * 전자결재 버튼정보를 초기화 한다.
 * @param
