	var strMsg = "변경된 내역이 있습니다. 저장하시겠습니까?\n(취소하신경우 자료는 초기화됩니다.)";
	
	// 변경여부체크
	if (gfn_dsIsUpdated(ds_Master)==true || gfn_dsIsUpdated(ds_Detail)==true)
	{
		if (g_Confirm("02",strMsg))
		{
			fn_save();
			return;
		}
	}

	
	ds_input.setColumn(0,"TABLE_TYPE",ds_list.getColumn(e.row,"TABLE_TYPE"));
	ds_input.setColumn(0,"FORM_CD",ds_list.getColumn(e.row,"FORM_CD"));
	ds_input.setColumn(0,"MST_UPJANG",ds_list.getColumn(e.row,"UPJANG"));
	ds_input.setColumn(0,"CHECK_DATE",ds_list.getColumn(e.row,"CHECK_DATE"));
	
	// 의뢰정보 클릭 시 전자결재 상신 또는 상신상세 버튼 연동
    fn_gwUserButtonInit();
	
	fn_SearchSub();
}

