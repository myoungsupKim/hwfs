/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	var ds_check = ds_list;	
	
 	if (gfn_dsIsUpdated(ds_list) == false && gfn_dsIsUpdated(ds_file) == false && gfn_dsIsUpdated(ds_listMast) == false)
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}

	var rtVal = gfn_confirm("confirm.before.save");	// 선택된 자료를 저장 하시겠습니까?

	if(rtVal == true)
	{
	
		// 업데이트 된 row를 찾아 각 DataSet에 넣어준다.
		for(var i=0; i<ds_list.getRowCount(); i++)
		{
			if(gfn_isUpdatedRow(ds_list, i))	
			{
				ds_list.setColumn(i,'transDate',gfn_today());	// 처리일자
				ds_list.setColumn(i,'procUser',g_Name);			// 처리자
			}
		}
		
		
		
		// 저장시 Validation Check	
		//if (gfn_validCheck(grd_list, ds_listValid) == false)	return;
		
		var strSvcID    = "save";
		var strURL      = "fm/fmc/distributionRequestRptProc/saveList.xdo";
		var strInDs     = "ds_list=ds_list:U ds_file=ds_file:A ds_listMast=ds_listMast:U";	// 파일정보 전체를 가져와야 함
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}	
}

