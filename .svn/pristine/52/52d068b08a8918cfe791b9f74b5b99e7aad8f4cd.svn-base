 *---------------------------------------------------------------------------------------------*/
function cbo_procDesc_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if(ds_search.getColumn(0,'procDesc') == '처리')
	{
		edt_procUser.enable="true"
		cal_transFromDate.enable="true"
		cal_transToDate.enable="true"
	}
	else
	{
		edt_procUser.enable="false"
		cal_transFromDate.enable="false"
		cal_transToDate.enable="false"
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 해당 row의 첨부파일을 찾는다.
 *---------------------------------------------------------------------------------------------*/
function ds_listMast_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{		
	ds_file.clearData();
	if(ds_listMast.rowcount > 0 && ds_listMast.getColumn(ds_listMast.rowposition, 'fileId'))
	{
		var strSvcID    = "searchFile";
		var strURL      = "fm/fmc/distributionRequestRptProc/searchFile.xdo";
		var strInDs     = "";
		var strOutDs    = "ds_file=ds_file";
		var strArg      = "refSeq=" + wrapQuote(ds_listMast.getColumn(ds_listMast.rowposition, 'fileId'));
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);	
	}
}



/*----------------------------------------------------------------------------------------------
 * 설명      : 물류요청목록을 더블클릭하면 해당 날짜의 요청목록상세를 불러온다.
 *---------------------------------------------------------------------------------------------*/
function grd_listMast_oncelldblclick(obj:Grid, e:GridClickEventInfo)
{
	// v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_list) == true) 
 	{
 		var retValue = gfn_confirm("confirm.before.search");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		if (retValue == false) return;
 	}

	// 상세조건 초기화
	ds_searchRow.clearData();
	ds_searchRow.addRow();	
	ds_searchRow.setColumn(0,'requestDate', ds_listMast.getColumn(ds_listMast.rowposition, 'reqDate'));
	ds_searchRow.setColumn(0,'centerCode', ds_listMast.getColumn(ds_listMast.rowposition, 'centerCode'));
	
	//gfn_clearSortMark(grd_list);
    
    //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장 
	//fv_objColInfo = gfn_getDatasetInfo(ds_list);

    var strSvcID    = "search";
    var strURL      = "fm/fmc/distributionRequestRptProc/selectList.xdo";
    var strInDs     = "ds_search=ds_searchRow";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 물류요청목록의 첨부파일을 클릭하면 해당 파일을 다운 받는다.
 *---------------------------------------------------------------------------------------------*/
function grd_listMast_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if(e.cell == 4) {
		if(ds_file.rowcount > 0){
			ds_file.setColumn(0,'chk',1);
			ObjAtchFile.btn_multiDown.click();
		}
	}
	
	if(e.cell == 5)
	{
		//레포트에 전달할 파라메터 생성
		var arrParams	= SCReport.newParams ();
		
		//--------------------------------------------------
		//방법1)서버에서 Query에 의해 처리하는 경우 예
		//(필수) 레포트명
		SCReport.setParams (arrParams, SCReport.RPT_NAME, "/IFIS/물류요청서.ozr");

		//출력 옵션 설정
		//-창 타이틀 (디폴트는 "ReportViewer")
		SCReport.setParams (arrParams, SCReport.RPT_TITLE, "물류요청서처리");
		
		//레포트에 맞게 전달되는 개발자 정의 파라미터
		SCReport.setParams (arrParams, "PARAM1", ds_listMast.getColumn(ds_listMast.rowposition,"centerCode"));
		SCReport.setParams (arrParams, "PARAM2", ds_listMast.getColumn(ds_listMast.rowposition,"reqDate"));	
		
		SCReport.setParams (arrParams, "zoom", 120);
		//--------------------------------------------------
		SCReport.viewReport (this, arrParams);
	}
}

