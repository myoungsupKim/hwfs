function fn_init()
{
	v_ds_check = "ds_list";
	
	//조회조건을 초기화한다.
	var nRow= ds_search.addRow();

	//ds_search.setColumn(nRow, "fromDate", gfn_addMonth(gfn_today(), -1)); //한달전 부터
	ds_search.setColumn(nRow, "fromDate", gfn_today()); // 오늘 부터
	ds_search.setColumn(nRow, "toDate", gfn_today());
	
	//달력 설정
	cal_requestFromDate.fn_initMonthCal(this, "", "", ds_search, "fromDate");	
	cal_requestToDate.fn_initMonthCal(this, "", "", ds_search, "toDate");	
	
	// 센터정보 COMBO 초기화 : 조건
	fn_searchCenter();

	// 단일 첨부파일 지정하기(true/false)
	ObjAtchFile.setSingleFile(true);
	
	// 첨부파일 연결
    ObjAtchFile.setDataset("ds_file");
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{	
	// v_ds_check 사용시 변경사항 체크
	if(gfn_length(v_ds_check) != 0)
	{	
		// v_ds_check 설정기준 변경사항 체크
		if (gfn_isDataChange() == true) 
		{		
			// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
			if (!gfn_confirm("confirm.before.movepage")) return false;
		}
	}
    
	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}


