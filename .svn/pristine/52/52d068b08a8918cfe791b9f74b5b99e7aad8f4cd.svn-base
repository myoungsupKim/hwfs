 *---------------------------------------------------------------------------------------------*/
function btn_fileSave_onclick(obj:Button,  e:ClickEventInfo)
{
	// 첨부 파일 추가가 있을때만
	if(ObjAtchFile.fn_countDs("insert") > 0)
	{	
		var rtn = ObjAtchFile.fn_uploadFile();		
		if (rtn < 0)	return;
	}
	
	var strSvcID    = "fileSave";
	var strURL      = "fm/fmc/distributionRequestRptProc/saveAttachFile.xdo";
	var strInDs     = "ds_file=ds_file:A";
	var strOutDs    = "ds_file=ds_file";
	var strArg      = "reqDate=" + wrapQuote(ds_listMast.getColumn(ds_listMast.rowposition, 'reqDate'));
		strArg     += " centerCode=" + wrapQuote(ds_listMast.getColumn(ds_listMast.rowposition, 'centerCode'));
	var strCallback = "fn_callBack";
	var bAsync      = true;
	
	gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 센터 조회
        case "searchCenter" :
            //trace(ds_center.saveXML());
            fn_center_init();
            break;
    
    
		// 코드분류 조회
        case "search" :
			if (ds_list.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				//gfn_loadDatasetInfo (ds_list, fv_objColInfo);
            }
            //if(ds_file.rowcount > 0 ) ds_listMast.setColumn(ds_listMast.rowposition,"fileNm",ds_file.getColumn(0,'orgnlFileNm'));
            //trace(ds_list.saveXML());
            break;
        
        // 저장
        case "save" :
			trace("처리건수 : " + fv_processCnt);
			
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;  
        
        //삭제
        case "delete" :
			trace("처리건수 : " + fv_processCnt);
			gfn_alert("msg.delete.success"); //삭제 되었습니다.
			break;
		
		//파일저장
		case "fileSave" :
			// 첨부파일 저장시 첨부파일 참조일련번호가 전달됨
			if(!gfn_isNull(fv_RefSeq)){
				ds_listMast.setColumn(ds_listMast.rowposition,"fileId",fv_RefSeq);
				ds_listMast.setColumn(ds_listMast.rowposition,"fileNm",edt_FileAddNm.value);
			}
			gfn_message("A", "파일이 저장 되었습니다.");
			break;
		
    }
}


