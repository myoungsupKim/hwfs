	<statement name="selectFsale"><![CDATA[SELECT 
    CENTER_CODE         ,
    UPJANG              ,
    ITEM_CODE           ,
    CONTRACT_START      ,
    CONTRACT_ID         ,
    CUSTCD              ,
    AP_UNITPRICE_UPJANG ,
    PO_CONTRACT_ID      ,
    SALE_PRICE_START    ,
    SALE_PRICE_END      ,
    CENTER_FLAG         ,
    CONTRACT_PRICE AS UNIT_PRICE  ,
    MARGIN_RATE         ,
    MARGIN_PRICE		,
    SALE_PRICE          ,
    CONTRACT_END        ,
    REMARK              ,
    USE_YN              ,
    CONTRACT_END_RK     ,
    ORI_CONTRACT_PRICE  ,
    ORI_SALE_PRICE      ,
    SYS_REMARK          ,
    CONTRACT_PRICE_S    ,
    CONSTRACT_COST      ,
    DISTRIBUTION_PRICE  ,
    PO_PRICE            ,
    ADD_RATE            ,
    ADD_PRICE           ,
    STRATEGY_RATE       ,
    STRATEGY_PRICE      ,
    PLANNING_RATE       ,
    PLANNING_PRICE      ,
    STD_SALE_RATE       ,
    STD_SALE_PRICE      ,
    CREATE_DATE         ,
    SUBSTRB(CREATE_BY,1,8) AS CREATE_BY,
    UPDATE_DATE         ,
    SUBSTRB(UPDATE_BY,1,8) AS UPDATE_BY,
    NEGO_PRICE          ,
    VD_SN               ,
    APPLY_SD            ,
    SUBSIDY_RATE        ,
    SUBSIDY_PRICE       ,
    OP_RATE             ,
    OP_PRICE            ,
    UPJANG_GRP          ,
    STD_MARGIN_RATE     ,
    OLD_CUSTCD          ,
    DUTY_YN             ,
    PB_YN               ,
    MANUF_YN            ,
    ECO_ITEM_YN         ,
    TOT_AMT             ,
    BOX_QTY				,
    PROC_GB             ,
    PROC_YN             ,
    'foodistapp' AS LOGIN_SABUN   ,
    'foodistapp' AS LOGIN_USERID  ,
    '' AS LOGIN_PERS_MASK 
  FROM APP_PO_CONTRACT_FSALE
 WHERE PROC_YN = 'N']]></statement>
	<statement name="selectItem"><![CDATA[SELECT ITEM_CODE,       
       '0000' AS OTCUSTCD, 
       BARCODE,         
       ITEM_CLASS1,     
       ITEM_CLASS2,     
       ITEM_CLASS3,     
       ITEM_CLASS4,     
       ITEM_NAME,     
       ITEM_SIZE,       
       PO_UOM,          
       KG_CONVERT_RATE, 
       MIN_ORD_QTY,     
       MAX_ORD_QTY,     
       MULTIPLIER_QTY,  
       D_DAYS,          
       D_TIMES,         
       TAX_CODE,        
       ORIGIN_TYPE,     
       ORIGIN_NAT,      
       ORIGIN_DTL,      
       POINT_FLAG,      
       KEEPING_TYPE,    
       CONSUM_YN,       
       SPICE_YN,        
       CORN_YN,         
       UNUSE_YN,        
       STP_YN,          
       USE_YN,          
       CREATE_DATE,     
       SUBSTRB(CREATE_BY,1,8) AS CREATE_BY,
       UPDATE_DATE,     
       SUBSTRB(UPDATE_BY,1,8) AS UPDATE_BY,
       ITEM_TYPE,       
       IMG_YN,          
       IMG_PATH,        
       KEEP_TYPE,       
       WIN_ITEM_CD,     
       PROC_GB,
       PROC_YN,
       'foodistapp' AS LOGIN_SABUN   ,
       'foodistapp' AS LOGIN_USERID  ,
       '' AS LOGIN_PERS_MASK 
  FROM APP_PO_ITEM_MST
 WHERE PROC_YN = 'N']]></statement>
	<statement name="selectFsale2"><![CDATA[SELECT
       '0' AS CENTER_CODE,        
       UPJANG,                    
       ITEM_CODE,                 
       SDATE AS CONTRACT_START,   
       NVL(EDATE,'99999999') AS CONTRACT_END,
       0 AS CUSTCD,
       AP_UNITPRICE_UPJANG,
       'N' AS CENTER_FLAG,
       0 AS UNIT_PRICE,
       0 AS MARGIN_PRICE,
       SALE_PRICE,
       USE_YN,
       CDATE AS CREATE_DATE,
       SUBSTRB(CUSER,1,8) AS CREATE_BY,
       UDATE AS UPDATE_DATE,
       SUBSTRB(UUSER,1,8) AS UPDATE_BY,
       '' AS VD_SN,
       '' AS APPLY_SD,
       '' AS UPJANG_GRP,
       'N' AS DUTY_YN,
       'N' AS PB_YN,
       'N' AS MANUF_YN,
       'N' AS ECO_ITEM_YN,
       0 AS TOT_AMT,
       0 AS BOX_QTY,
       PROC_GB,
       PROC_YN,
       'foodistapp' AS LOGIN_SABUN   ,
       'foodistapp' AS LOGIN_USERID  ,
       '' AS LOGIN_PERS_MASK 
  FROM APP_OTCUST_PRICE
 WHERE PROC_YN = 'N']]></statement>
	<statement name="selectItem2"><![CDATA[SELECT ITEM_CODE,      
        OTCUSTCD,
        NULL AS BARCODE,
        ITEM_CLASS1,
        ITEM_CLASS2,
        ITEM_CLASS3,
        ITEM_CLASS4,
        ITEM_NAME,
        ITEM_SIZE,
        PO_UOM,
        KG_CONVERT_RATE,
        MIN_ORD_QTY,
        MAX_ORD_QTY,
        MULTIPLIER_QTY,
        D_DAYS,
        D_TIMES,
        TAX_CODE,
        ORIGIN_TYPE,
        NULL AS ORIGIN_NAT,
        NULL AS ORIGIN_DTL,
        POINT_FLAG,
        KEEPING_TYPE,
        CONSUM_YN,
        '' AS SPICE_YN,
        '' AS CORN_YN,
        '' AS UNUSE_YN,
        '' AS STP_YN,
        USE_YN,
        CDATE AS CREATE_DATE,
        SUBSTRB(CUSER,1,8) AS CREATE_BY,
        UDATE AS UPDATE_DATE,
        SUBSTRB(UUSER,1,8) AS UPDATE_BY,
        NULL AS ITEM_TYPE,
        DECODE(IMAGE_PATH, NULL, 'N', 'Y') AS IMG_YN,
        IMAGE_PATH AS IMG_PATH,
        NULL AS KEEP_TYPE,
        NULL AS WIN_ITEM_CD,
        PROC_GB,
        PROC_YN,
       'foodistapp' AS LOGIN_SABUN   ,
       'foodistapp' AS LOGIN_USERID  ,
       '' AS LOGIN_PERS_MASK 
   FROM APP_OTCUST_ITEM_MST
 WHERE PROC_YN = 'N']]></statement>
	<statement name="updateFsale"><![CDATA[UPDATE APP_PO_CONTRACT_FSALE
   SET PROC_YN = 'Y'
 WHERE CENTER_CODE = :centerCode
   AND UPJANG = :upjang
   AND ITEM_CODE = :itemCode
   AND CONTRACT_START = :contractStart]]></statement>
	<statement name="updateItem"><![CDATA[UPDATE APP_PO_ITEM_MST
   SET PROC_YN = 'Y'
 WHERE ITEM_CODE = :itemCode]]></statement>
	<statement name="updateFsale2"><![CDATA[UPDATE APP_OTCUST_PRICE
   SET PROC_YN = 'Y'
 WHERE UPJANG = :upjang
   AND SDATE = :contractStart
   AND ITEM_CODE = :itemCode]]></statement>
	<statement name="updateItem2"><![CDATA[UPDATE APP_OTCUST_ITEM_MST
   SET PROC_YN = 'Y'
 WHERE ITEM_CODE = :itemCode]]></statement>
</hqml>
