			v_NQ=Math.round((toNumber(ds_ItemList.getColumn(i,"NEED_QTY"))*toNumber(ed_ratio.value))/100,2);
			ds_ItemList.setColumn(i,"NEED_QTY",v_NQ);
		}
		ed_ratio.value="";
	}
	else
	{
		ed_ratio.visible=true;
		st_Ratio.visible=true;
	}
}
