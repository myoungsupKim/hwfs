		var strURL      = "rc/sle/menuMgmt/selectList.xdo";
		var strInDs     = "";
		var strOutDs    = "ds_MenuList=ds_outletMenu";
		//var strArg      = "outletCd="      + tab_menuLog.tpg_menuLog.edt_outletNm.outletCd
		var strArg      = "outletCd="    + gfn_nvl(tab_menuLog.tpg_menuLog.edt_upjang.getUpjangCode(),"")
						+ " menuClass="  + gfn_nvl(tab_menuLog.tpg_menuLog.edt_menuClass.menuClassCd,"")
						+ " menuClsfi="  + gfn_nvl(tab_menuLog.tpg_menuLog.edt_menuClsfi.menuClsfiCd,"")
						+ " menuSclass=" + gfn_nvl(tab_menuLog.tpg_menuLog.edt_menuSclass.menuSclassCd,"")
						+ " menuNm="     + gfn_nvl(tab_menuLog.tpg_menuLog.edt_menuNm.value,"")
						+ " itemCd="     + gfn_nvl(tab_menuLog.tpg_menuLog.edt_itemCd.value,"");
        
        if(tab_menuLog.tpg_menuLog.chk_price.value == "1") strArg = strArg + " chkPrice="   + tab_menuLog.tpg_menuLog.chk_price.value;
		
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
	else
	{
		 
		// Grid sort 표시 초기화(필수)
