		var strArg      = "outletCd="    + gfn_nvl(tab_menuLog.tpg_outletMenu.edt_upjang.getUpjangCode(),"")
						+ " menuClass="  + gfn_nvl(tab_menuLog.tpg_outletMenu.edt_menuClass.menuClassCd,"")
						+ " menuClsfi="  + gfn_nvl(tab_menuLog.tpg_outletMenu.edt_menuClsfi.menuClsfiCd,"")
						+ " menuSclass=" + gfn_nvl(tab_menuLog.tpg_outletMenu.edt_menuSclass.menuSclassCd,"")
						+ " menuNm="     + gfn_nvl(tab_menuLog.tpg_outletMenu.edt_menuNm.value,"")
						+ " chkUseYn="   + "N" //2017.02.28 메뉴정보관리에서 사용인건만 조회하는 조건  추가 
						+ " itemCd="     + gfn_nvl(tab_menuLog.tpg_outletMenu.edt_itemCd.value,"");
		
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
		
	}
	else if (tab_menuLog.tabindex == 1)
	{ 
		//if (gfn_nvl(tab_menuLog.tpg_menuLog.edt_outletNm.outletCd) == "") 
		if (gfn_nvl(tab_menuLog.tpg_menuLog.edt_upjang.getUpjangCode()) == "") 
