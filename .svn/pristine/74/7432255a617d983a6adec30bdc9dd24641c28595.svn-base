/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
	
    switch(strSvcID)
    {
		// 코드분류 조회
		case "delete":
			 
			break;
        case "searchHistory":
			 
			break;
        case "search" :
          
            //검색후 오른쪽의 있는 자료는 모두 삭제 한다.
            fv_outletMenuListDelete();

            break;
       
        // 저장
        case "saveoutlet":
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			ds_outletMenuLog.clearData();
			fn_search();
			break;   
        case "save" :
			
			//ds_amList.setColumn(ds_amList.rowposition, "amcd", 0);
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			ds_menuLog.clearData();
			fn_search();
			break;    
