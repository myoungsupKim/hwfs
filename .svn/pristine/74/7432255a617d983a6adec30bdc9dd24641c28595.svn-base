/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (tab_menuLog.tabindex == 0) 
	{
		if (gfn_dsIsUpdated(ds_outletMenuLog)==false )
		{
			gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
			return;
		}
		
		// 저장시 Validation Check	
		if (gfn_validCheck(ds_outletMenuLog, ds_menuLogValid) == false)	return;

		//처리 여부를 확인 한다.
		if(gfn_nvl(tab_menuLog.tpg_outletMenu.cal_apprDate.fn_getValue(),"") == "")
		{
			gfn_alert("msg.noinput","외쪽 상단의 적용일자" );	// $1 을(를) 입력해 주십시요.
			return;
		}
		
		var msg="";
		msg = " 적용일자 : " + gfn_nvl(tab_menuLog.tpg_outletMenu.cal_apprDate.fn_getValue(),"") + "\r"
			+ " 처리건수 : " + ds_outletMenuLog.getRowCount() + "건\r"
		
		var retValue = gfn_confirm("ft.fdm.confirm.deliver",msg);	//ft.fdm.confirm.deliver  $1 처리하시겠습니까?
		if (retValue == false) return;
		
		//applyDd 값을 변경해준다. 적용일자로
		for(var i = 0; i <= ds_outletMenuLog.getRowCount()-1 ; i++)
		{
			ds_outletMenuLog.setColumn(i,"applyDd",gfn_nvl(tab_menuLog.tpg_outletMenu.cal_apprDate.fn_getValue(),""));
		}
		
		var strSvcID    = "saveoutlet";
		var strURL      = "rc/sle/menuLogMgmt/saveData.xdo";
		var strInDs     = "ds_menuLog=ds_outletMenuLog:U";
		var strOutDs    = "";
		var strArg      = "posYn="; 
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
	else if (tab_menuLog.tabindex == 1)
	{
		
		if (gfn_dsIsUpdated(ds_menuLog)==false )
		{
			gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
			return;
		}

		// 저장시 Validation Check	
		if (gfn_validCheck(ds_menuLog, ds_menuLogValid) == false)	return;

		//처리 여부를 확인 한다.
		if(gfn_nvl(tab_menuLog.tpg_menuLog.cal_apprDate.fn_getValue(),"") == "")
		{
			gfn_alert("msg.noinput","외쪽 상단의 적용일자" );	// $1 을(를) 입력해 주십시요.
			return;
		}
		
		//POS 미사용 업장에 한해 과거 날짜로 적용일자 지정이 가능하다.
		if(tab_menuLog.tpg_menuLog.cal_apprDate.fn_getValue() < gfn_today()){
			if(fv_posYn == "1")
			{
				alert("한화POS를 사용하지 않는 업장(외부POS사용)에 한해\r\r 오늘 이전날짜로 단가 변경이 가능합니다.");
				return;
			}
		}
		
		
		var msg="";
		msg = " 적용일자 : " + gfn_nvl(tab_menuLog.tpg_menuLog.cal_apprDate.fn_getValue(),"") + "\r"
			+ " 처리건수 : " + ds_menuLog.getRowCount() + "건\r"
		
		var retValue = gfn_confirm("ft.fdm.confirm.deliver",msg);	//ft.fdm.confirm.deliver  $1 처리하시겠습니까?
		if (retValue == false) return;
		
		//applyDd 값을 변경해준다. 적용일자로
		for(var i = 0; i <= ds_menuLog.getRowCount()-1 ; i++)
		{
			ds_menuLog.setColumn(i,"applyDd",gfn_nvl(tab_menuLog.tpg_menuLog.cal_apprDate.fn_getValue(),""));
		}
		
		var strSvcID    = "save";
		var strURL      = "rc/sle/menuLogMgmt/saveData.xdo";
		var strInDs     = "ds_menuLog=ds_menuLog:U";
		var strOutDs    = "";
		var strArg      = "";
		var strCallback = "fn_callBack";
		var bAsync      = true;
		
		gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	}
}

