function fn_search()
{
	//업장 선택은
	if (tab_menuLog.tabindex == 0)
	{	// Grid sort 표시 초기화(필수)
		gfn_clearSortMark(tab_menuLog.tpg_outletMenu.grd_outletMenuList);
