//메뉴 검색후 변경 리스트에 있는 자료는 삭제 처리 해준다.
function fv_outletMenuListDelete()
{
	if (ds_menuLog.getRowCount() == -1) return;
	for(var i = 0; i <= ds_menuLog.getRowCount()-1 ; i++)
	{	
		var menuCd = ds_menuLog.getColumn(i,"menuCd");
		var iFindRowIdx = ds_MenuList.findRow("menuCd",menuCd);
		if ( iFindRowIdx != -1)
		{
			ds_MenuList.deleteRow(iFindRowIdx);
		}
 	}
}
 
//선택된 메뉴코드를 왼쪽에 단가 수정 메뉴코드로 보낸다.
function btn_send_onclick(obj:Button,  e:ClickEventInfo)
{
	//처리방법
	/*
	 * 처리 방법
	 * 1. 선택된 메뉴를 모두 복사해서 수정위치로 보낸다.
	 * 2. 선택된 메뉴를 모두 삭제 한다.
	*/
	var grdCheck=false;
	if (ds_MenuList.getRowCount() == -1) return;
	for(var i = 0; i <= ds_MenuList.getRowCount()-1 ; i++)
	{
		if(ds_MenuList.getColumn(i,"chkYn") == true)
		{
			grdCheck=true;
			break;
		}
	}
	if(grdCheck==false) return;
	
 
	// 변경내역의 그리드의 선택 버튼을 모두 제거 한다.
	for(var i = ds_menuLog.getRowCount()-1 ; i >= 0 ; i--)
	{
		if(ds_menuLog.getColumn(i,"chkYn") == true)
		{
			ds_menuLog.setColumn(i,"chkYn","0");
		}
	}
	
	//1.서택된 자료 보내기
	for(var i = 0; i <= ds_MenuList.getRowCount()-1 ; i++)
	{
		if(ds_MenuList.getColumn(i,"chkYn") == true)
		{
			var nRowIdx = ds_menuLog.addRow();	//행추가
			var bSucc = ds_menuLog.copyRow(nRowIdx, ds_MenuList, i);	//해당 선택된 행 복사
			
		}
	}

	//2.선택된것을 삭제 한다.
	for(var i = ds_MenuList.getRowCount()-1 ; i >= 0 ; i--)
	{
		if(ds_MenuList.getColumn(i,"chkYn") == true)
		{
			ds_MenuList.deleteRow(i);
		}
	}
	//ds_MenuList.updatecontrol = false;
	//var bSucc = ds_MenuList.setRowType(nRowIdx, Dataset.ROWTYPE_NORMAL);
	//ds_MenuList.updatecontrol = true;
	//------------------------------------------
}

