function fn_selectInsptUser()
{
    ds_search.setColumn(0, "authCd", "101");	// 담당자권한 코드중 검사자
    
    var strSvcID    = "selectInsptUserList";
    var strURL      = "ls/lcm/userMgmt/selectUserList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_userList=ds_userList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync, false);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 조회
 *---------------------------------------------------------------------------------------------*/
function fn_doSearch()
{
	if (gfn_isNull(cal_fromDay.fn_getValue()) || gfn_isNull(cal_toDay.fn_getValue()))
	{
		gfn_alert("접수일자를 입력하여 주십시오!");
		return;
	}
	if (cal_fromDay.fn_getValue() > cal_toDay.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
	if (cal_fromDayTest.fn_getValue() > cal_toDayTest.fn_getValue())
	{
		gfn_alert("시험 시작일이 종료일보다 큽니다.");
		return;
	}
	
	if (gfn_dsIsUpdated(ds_testRegList))
	{
		// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
        var retValue = gfn_confirm("confirm.before.search");
		if (retValue == false) return;
    }
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_testRegList);
    gfn_clearSortMark(grd_specmList);
    gfn_clearSortMark(grd_testArticleList);

    // Dataset을 초기화한다.
    ds_testRegList.clearData();
    ds_specmList.clearData();
    ds_testArticleList.clearData();

    var strSvcID    = "selectList";
    var strURL      = "ls/lem/inspectRptIssueInquery/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_testRegList=ds_testRegList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 접수 리스트 클릭
 *---------------------------------------------------------------------------------------------*/
function grd_testRegList_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.cell == 1) return;	// 체크박스
	//if (e.oldrow == e.row) return;
	
	if (Number(ds_testRegList.getColumn(ds_testRegList.rowposition, "publishCnt")) > 0) {
		btn_rtpIssueHistory.enable = true;
	} else {
		btn_rtpIssueHistory.enable = false;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_specmList);
    gfn_clearSortMark(grd_testArticleList);

    // Dataset을 초기화한다.
    ds_specmList.clearData();
    ds_testArticleList.clearData();
    
    ds_search.setColumn(0, "reqNum", ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum"));

    var strSvcID    = "selectSpecmList";
    var strURL      = "ls/lem/inspectRptIssueInquery/selectSpecmList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_specmList=ds_specmList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 클릭
 *---------------------------------------------------------------------------------------------*/
function grd_specmList_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.oldrow == e.row) return;
	fn_setSpecmRemark(ds_specmList.rowposition);
	fn_searchDetail();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 클릭 후 시험항목 조회
 *---------------------------------------------------------------------------------------------*/
function fn_searchDetail()
{
	// Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_testArticleList);

    // Dataset을 초기화한다.
    ds_testArticleList.clearData();
    
    ds_search.setColumn(0, "reqNum", ds_specmList.getColumn(ds_specmList.rowposition, "reqNum"));
    ds_search.setColumn(0, "acceptNum", ds_specmList.getColumn(ds_specmList.rowposition, "acceptNum"));

    var strSvcID    = "selectTestArticleList";
    var strURL      = "ls/lem/inspectRptIssueInquery/selectDetailList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_testArticleList=ds_testArticleList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/***************************************************************************
 * 설명   : 시험항목 일지 상태권한 체크
 ***************************************************************************/
function fn_testDiaryWriteViewCheck(statusGubun)
{
	if (statusGubun == "VIEW")
	{
		// 시험일지조회 버튼 권한
		return (ds_testRegList.getColumn(ds_testRegList.rowposition, "progressStatus") >= '150');
	}
	else if (statusGubun == "WRITE")
	{
		// 시험항목 그리드 일지 작성 권한
		return (ds_testRegList.getColumn(ds_testRegList.rowposition, "progressStatus") == '150' || 
				ds_testRegList.getColumn(ds_testRegList.rowposition, "progressStatus") == '160');
	}
}

