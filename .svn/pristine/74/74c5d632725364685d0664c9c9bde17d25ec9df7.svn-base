/***************************************************************************
 * 설명   : 진행상황 ComboBox 변경 시
 ***************************************************************************/
function cbo_progressStatus_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	// 신청서발급 신청/재신청인 경우에 버튼 활성화
	if (obj.value == "210" || obj.value == "250")
	{
		// 로그인 사용자가 성적서발급승인 권한이 있으면...
		if ( gds_limsAuthLevels.findRow('authCd', '104') != -1 ) {
			div_cmnBtn.btn_delete.enable = true;
			div_cmnBtn.btn_save.enable = true;
		}
	}
	else
	{
		div_cmnBtn.btn_delete.enable = false;
		div_cmnBtn.btn_save.enable = false;
	}
	fn_search();
}

/***************************************************************************
 * 설명   : 검체별 비고 출력
 ***************************************************************************/
function fn_setSpecmRemark(nRow)
{
	var strRemark = "1. 상기 판정은 제시된 검사대상물에 한합니다." + String.fromCharCode(13, 10);
	strRemark += "2. 이 성적서는 당사의 사전 서면 동의없이 통보, 선전, 홍보, 소송용 등으로 사용될 수 없으며 용도 이외 사용을 금합니다." + String.fromCharCode(13, 10);
	strRemark += "3. 검사 항목 및 그에 따른 규격기준 검사방법은 당사 규정에 따릅니다.";
	
	if (gfn_isNull(ds_specmList.getColumn(nRow, "remark")))
	{
		ds_specmList.setColumn(nRow, "remark", strRemark);
	}
}

