/*----------------------------------------------------------------------------------------------
 * 설명      : 저장(승인) 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	if (!gfn_dsIsUpdated(ds_testRegList))
	{
		gfn_alert("의뢰정보를 선택하여 주십시오");	// 변경된 내역이 없습니다.
		return;
	}
	
	var bIsProcRow = false;
	// 선택된 행 체크
	for (var i = ds_testRegList.getRowCount()-1 ; i >= 0 ; i--)
	{
		if (ds_testRegList.getColumn(i, "chk") == 1)
		{
			bIsProcRow = true;
			break;
		}
	}

	if (!bIsProcRow)
	{
		gfn_alert("의뢰정보를 선택하여 주십시오.", "", "A");
		return;
	}
	
	if (!gfn_confirm("승인 하시겠습니까?")) return false;

	// 성적서발급승인완료
	if (cbo_progressStatus.value == "210")	// 성적서 발급 신청
	{
		ds_search.setColumn(0, "procClass", "230");	// 성적서 발급 승인
	}
	else if (cbo_progressStatus.value == "250")	// 성적서 재발급 신청
	{
		ds_search.setColumn(0, "procClass", "270");	// 성적서 재발급 승인
	}

    var strSvcID    = "saveAccept";
    var strURL      = "ls/ltm/testEndApprv/saveTestReqStatus.xdo";
    var strInDs     = "ds_testRegList=ds_testRegList:U ds_search=ds_search:A";
    var strOutDs    = "ds_retval=ds_retval";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제(반려) 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var bIsProcRow = false;
	// 선택된 행 체크
	for (var i = ds_testRegList.getRowCount()-1 ; i >= 0 ; i--)
	{
		if (ds_testRegList.getColumn(i, "chk") == 1)
		{
			bIsProcRow = true;
			break;
		}
	}

	if (!bIsProcRow)
	{
		gfn_alert("의뢰정보를 선택하여 주십시오.", "", "A");
		return;
	}
	
	if (!gfn_dsIsUpdated(ds_testRegList))
	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return;
	}
	
	if (gfn_validCheck(ds_testRegList, ds_testRegListValid) == false)	return;
	
	if (!gfn_confirm("반려 하시겠습니까?")) return false;

	// 성적서발급반려
	if (cbo_progressStatus.value == "210")	// 성적서 발급 신청
	{
		ds_search.setColumn(0, "procClass", "220");	// 성적서 발급 반려
	}
	else if (cbo_progressStatus.value == "250")	// 성적서 재발급 신청
	{
		ds_search.setColumn(0, "procClass", "260");	// 성적서 재발급 반려
	}

    var strSvcID    = "saveCancel";
    var strURL      = "ls/ltm/testEndApprv/saveTestReqStatus.xdo";
    var strInDs     = "ds_testRegList=ds_testRegList:U ds_search=ds_search:A";
    var strOutDs    = "ds_retval=ds_retval";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0)
	{
		gfn_alert(strErrorMsg);
		return;
	}

    switch(strSvcID)
    {
		// 조회
        case "selectList" :
			if (ds_testRegList.rowposition != -1) {
                // 강제로 첫 row 클릭
                grd_testRegList_oncellclick(grd_testRegList, evnentInfo);
            }
            break;
        // 검체목록 조회
        case "selectSpecmList" :
			fn_setSpecmRemark(0);
			if (ds_specmList.rowposition != -1) {
                fn_searchDetail();
            }
            break;
        case "selectInsptUserList" :	// 담당자
			ds_userList.insertRow(0);
			ds_userList.setColumn(0, "sabun", "");
			ds_userList.setColumn(0, "userNm", "전체");
			cbo_userList.index = 0;
            break;
        // 시험항목조회
        case "selectTestArticleList" :
            break;
        // 시험성적서 미리보기
        case "rptIssuePreview" :
			fn_inspectPreview();
			break;
		// 승인
        case "saveAccept" :
			gfn_alert("승인처리 되었습니다.");
			fn_doSearch();
            break;
        // 반려
        case "saveCancel" :
			gfn_alert("반려처리 되었습니다.");
			fn_doSearch();
			break;
    }
}

