//=========================================================================================
function fn_save()
{
	// 기본적으로 재고 등록은 당일에 한해서 등록 될 수 있다. 전일 재고 수정을 하기 위해서는 수불 재생성 체크를 해서 
	//  과거 데이터를 불러 와서 수정 해야 한다. 반영 버튼 시 해당 프로시져를 호출 하여 재 생성 한다.(FMS_SUBUL_CREATE_PRO)
	
//	if (me_Sdate.Value != AddDate(today(),0)) {
//			g_Message("EE", "과거 또는 미래 재고는 저장이 불가능 합니다. \n  수불 재 생성을 이용해 주시기 바랍니다." );
//			return;
//	}
//////////////////////////////
//필수항목 체크 
//////////////////////////////

	
	// 데이타체크
	ds_cond.clearData();	
	ds_cond.addRow();	
	
	var sRtn = fn_ChkData();
	if (sRtn != null)
	{
		g_Message("EE", sRtn);
		return;
	}
	if (! g_Confirm("02", '변경사항을 저장하시겠습니까?!')) return;		

   	// action 정보 초기화 
    fsp_clear(this);
	
	// 초기화
	// 파라미터셋팅
    // 서비스셋팅
    http.Sync = true;
    
    var actionName, cmdName, inData, outData, otherArg, callBackFnc;
    var singleSql, insertSql, updateSql, deleteSql, saveFlagColumn, keySqlName, keyIncrement, callbackSql;
    inData		= "ds_IUD=ds_list:U";
    outData		= "";   

	// 파라미터셋팅
	
    updateSql	= "fm/fmp:FMP00510E_U001";
    callbackSql	= "";
    //callbackSql	= "fm/fmp:FMP00510E_S001";
    
    // 서버에서 작업할 정보 추가 
    fsp_addSave(this, insertSql, updateSql, deleteSql, saveFlagColumn, keySqlName, keyIncrement, callbackSql);
	// 서버 호출 
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출    
    http.Sync = false;
    
    // 가져온 결과를 ds_list2에 담는다.
	ds_list2.copyData(ds_list);
}

//=========================================================================================
// [ PART 9]
//	1. Function ID : fn_Equal()
//	2. 개       요 : 실재고량을 입력하지 않은 재고조사일을 입력 했을 때 조회 되는 실재고량 초기 설정.
//	3. 기       능 : 실재고량 = 재고량.
//	4. 인 수 설 명 :
//=========================================================================================
function fn_chk_Equal()
{	
	SetWaitCursor(true);
	if (chk_Equal.value == "1") {	//체크가 선택되면
		for (var i = 0; i < ds_list.rowcount; i++){				
			ds_list.setColumn(i, "REAL_QTY", ds_list.getColumn(i,"DAY_QTY"));
		}			
	}
	else{	//체크가 해제되면
		
		for (var i = 0; i < ds_list2.rowcount; i++){				
			ds_list.setColumn(i, "REAL_QTY", ds_list2.getColumn(i,"REAL_QTY"));
			//ds_list.setColumn(i, "REAL_QTY", 0);
		}
		
	}
	
	SetWaitCursor(false);
	grd_list.setFocus();     	    
    grd_list.setCellPos(12);
}

//=========================================================================================
// [ PART 10]
// Data 저장 클릭 시 정검 체크 사항
//=========================================================================================
function fn_ChkData()
{
	if(ds_list.rowcount < 1)
		return "저장할 자료가 없습니다.";
	
	// 변경여부 확인
	if(! gfn_dsIsUpdated(ds_list))
	{
		return fn_AlertMsg("007");
	}
	
	//상품코드 미등록여부
	for (i=ds_list.rowcount-1; i>=0; i--)
	{
		if (ffn_ChkNull(ds_list.getColumn(i,"REAL_QTY")))
		{
			ds_list.rowposition = i; grd_list.setFocus(); grd_list.setCellPos(12);
			return " 실재고량을 입력하세요.";
		}		
	}
}
