			break;			
			
		default:
			break;
	}	
}

//=========================================================================================
// [ PART 7]
// 	1. Function ID : fn_Search()
//	2. 개       요 : 왼쪽의 상품 코드 조회 부분 조회 조건은 업장/대분류/중분류/소분류/아이템 별.
//	3. 기       능 : 조회.
//=========================================================================================
function fn_search()
{	
//////////////////////////////
//필수항목 체크 
//////////////////////////////

	if (gfn_length(gfn_trim(ds_SUBINV.getColumn(0,"SUBINV_CODE")))== 0)
	  {
		g_Message("EE","재고 관리 식당이 하나도 존재 하지 않습니다. \n\n 재고 관리 식당 설정을 확인 하시기 바랍니다.");
		return;
	  }

	ds_cond.clearData();
	ds_list.clearData();
	ds_list2.clearData();
	ds_cond.addRow();
	
	ds_cond.setColumn(0, "UPJANG", ed_Upjang.userdata);
	ds_cond.setColumn(0, "SDATE", me_Sdate.value);
	
	if (chk_SUBINV.value == "1") {									//전체 식당 조회 체크 박스.
		ds_cond.setColumn(0, "SUBINV_CODE", "");	
		grd_list.setFormatColProperty(3,"size",200);		
	}
	else{
		ds_cond.setColumn(0, "SUBINV_CODE", cbo_Subinv.value);	
		grd_list.setFormatColProperty(3,"size",0);
		//ds_input2.setColumn(0, "SUBINV_CODE", cbo_Subinv.value);	// 추가
	}
	
	if (chk_AMT.value == "1") {						     			//금액 보기 체크 박스
		grd_list.setFormatColProperty(11,"size",70);
		grd_list.setFormatColProperty(13,"size",70);
		grd_list.setFormatColProperty(15,"size",70);	
	}
	else{
		grd_list.setFormatColProperty(11,"size",0);
		grd_list.setFormatColProperty(13,"size",0);
		grd_list.setFormatColProperty(15,"size",0);
	}	
		
	if (chk_ZERO.value == "1"){										//재고량 0 제외 체크 박스.
		ds_cond.setColumn(0,"ZERO","1");		
	}
	else{
		ds_cond.setColumn(0,"ZERO","");		
	}	


	// 조회
	var actionName, cmdName, inData, outData, otherArg, callBackFnc;		
	
	// 파라미터셋팅	
	
    inData		= "ds_cond=ds_cond";
    outData		= "ds_chk=ds_chk";
    callBackFnc = "";
	//action 정보 초기화 
	fsp_clear(this); 
	ds_list.clearData();
    // 서버에서 조회할 정보 추가 
    
//-------Sync True--------
    http.Sync = true;
//-------Sync True-------- 

	fsp_addSearch(this, "fm/fmp:FMP00510E_S003");
	// 서버 호출     
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출 
    
    
    if (ds_chk.getColumn(0,"CNT") > 0 )
		{
			// 조회
			var actionName, cmdName, inData, outData, otherArg, callBackFnc;		
			
			http.Sync = true;
			// 파라미터셋팅
			inData		= "ds_cond=ds_cond";
			outData		= "ds_list=ds_list";
			callBackFnc = "fn_chk_Equal";
			//action 정보 초기화 
			fsp_clear(this); 
			// 서버에서 조회할 정보 추가 
			fsp_addSearch(this, "fm/fmp:FMP00510E_S001");
			// 서버 호출     
			fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출 	
			
			http.Sync = false;
		}
    else
		{
//ffn_alert('수불자료 자동재생성');

			//수불 기초 자료 생성.(데이터가 없다는 가정.)
			if (v_SearchCNT != 1)
			{			
				fn_CreateSubul();
			}
		}

//-------Sync False--------
    http.Sync = false;
//-------Sync False--------
 		
 		// 가져온 결과를 ds_list2에 담는다.
		ds_list2.copyData(ds_list);
}

//=========================================================================================
// [ PART 8]
//	1. Function ID : fn_Save()
//	2. 개       요 : 업무프로세스에 따른 입력작업
//	3. 기       능 : 입력버튼 입력 시 처리되는 작업
