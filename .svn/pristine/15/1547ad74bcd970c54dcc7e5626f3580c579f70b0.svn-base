function fn_edBtn_OnClick(obj:Button, e:ClickEventInfo)
{
	var sAddWhere = "";
	var sOnBoard  = "";
	
	switch(obj.name)
	{
		case "btn_CustCd":
			sAddWhere += " AND CUSTCD IN (SELECT CUSTCD FROM HLDC_PO_SPECIAL_ITEM) ";
			break;
			
		case "btn_PR_SABUN_CC_CD":
		
		    if (gfn_length(cbo_PR_SABUN_MU.value)!= 0)
			sAddWhere += " AND (MU_CD = '" + cbo_PR_SABUN_MU.value + "') ";
			break;			

		case "btn_ItemCd":
			sAddWhere += " AND A.SEASON_NUM = '" + cb_Season.value + "' ";
			break;
			
		default:
			break;
	}
	//fm공통 팝업클릭 호출
	if (ffn_btn_OnClick(obj, sAddWhere, sOnBoard)) fn_Trigger_edBtn(eval(sOnBoard + "ed_" + gfn_subStr(obj.name, 4)));
}

