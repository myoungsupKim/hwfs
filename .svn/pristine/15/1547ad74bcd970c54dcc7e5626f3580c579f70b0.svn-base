function fn_CallBack_Search(errCode, errMsg)
{

	if (errCode == 0)
	{
		if (ds_List_Detail.rowcount < 1)
		{
			g_Message("EE", "조회대상이 없습니다.");
			me_PrDate.setFocus();
		}
	}
	else
		g_Message("EE",fn_AlertMsg("002"));
}
function fn_CallBack_Save(errCode, errMsg)
{

	if (errCode == 0)
	{
		
		g_Message("02","정산작업" + fn_AlertMsg("000"));
		fn_search();
	}
	else
		g_Message("EE",fn_AlertMsg("002"));
}

function fn_CallBack_Cancel(errCode, errMsg)
{

	if (errCode == 0)
	{
		
		g_Message("02","취소작업" + fn_AlertMsg("000"));
		fn_search();
	}
	else
		g_Message("EE",fn_AlertMsg("002"));
}
