function fn_search()
{

	if (!before_SelectCheck())
		return;
	var search_cal = cal_yearMonth.fn_getValue();
    str_LastDate = fn_GetMonthLastDate(ds_cal, cal_yearMonth.fn_getValue() + "01");
   
   
   
	//리스트#############################################################
    fsp_clear(this);
    
    ds_cond.setColumn(0,"MU_CD","2004");
    
    if (chk_AllYN.value == "0"){
		if(ds_cond.getColumn(ds_cond.rowposition, "MU_CD") == "2004"){
			fsp_addSearch(this, "ma/mak:MAK10160S_S001");
		}else{
			fsp_addSearch(this, "ma/mak:MAK10160S_S003");
		}
	}
	else {
		if(ds_cond.getColumn(ds_cond.rowposition, "MU_CD") == "2004"){
			fsp_addSearch(this, "ma/mak:MAK10160S_S002");
		}else{
			fsp_addSearch(this, "ma/mak:MAK10160S_S004");
		}
	}

	if(chk_cc.value == "0")	
	{
		ds_cond.setColumn(ds_cond.rowposition,"ccChkYn",null);
		ds_cond.setColumn(ds_cond.rowposition,"schCc",null);
	}
	else
	{
		ds_cond.setColumn(ds_cond.rowposition,"ccChkYn","Y");
		ds_cond.setColumn(ds_cond.rowposition,"schCc",edt_ccCd.userdata);
	}
	
	ds_cond.setColumn(ds_cond.rowposition,"SUNSU_YN",chk_AllYN.value);
	ds_cond.setColumn(ds_cond.rowposition,"TRX_DATE_LAST",str_LastDate);
	ds_cond.setColumn(ds_cond.rowposition,"PART_USER",edt_ORD_SABUNQuery.userdata);
	
					
	fn_DataSetAllClear();
	grid_Slip.formatid = cbo_Disp.value;
	grid_Slip.treeinitstatus = "collapse,null";
	
	//trace(ds_cond.saveXML());

	http.Sync = true;

	fsp_callService(this, "", "", "ds_cond=ds_cond","ds_list=ds_list","","fn_SearchDetail");

	http.Sync = false;	

	var j = 0;
	var str_BeforeMonthTitle;

	//for (i=21;i<=32;i++)
	for (var i=0; i<12; i++)
	{
		str_BeforeMonthTitle = gfn_addMonth(str_LastDate, j);

		str_BeforeMonthTitle = "'" + gfn_subStr(str_BeforeMonthTitle, 2, 2)+ "년" + gfn_subStr(str_BeforeMonthTitle, 4, 2)+ "월";

		//grid_Slip.SetCellProp("head",i,"Text", str_BeforeMonthTitle);
		if (cbo_Disp.value == "Default") {
			grid_Slip.setCellProperty("head",21+i,"text", str_BeforeMonthTitle);
			grid_Excel.setCellProperty("head",21+i,"text", str_BeforeMonthTitle);
			grid_Slip_01.setCellProperty("head",19+i,"text", str_BeforeMonthTitle);
		}
		else {
			//grid_Slip.SetCellProp("head",20+(i*3),"Text", str_BeforeMonthTitle);
			grid_Slip.setCellProperty("head",20+i,"text", str_BeforeMonthTitle);
			grid_Excel.setCellProperty("head",20+i,"text", str_BeforeMonthTitle);
			grid_Slip_01.setCellProperty("head",19+i,"text", str_BeforeMonthTitle);
		}
		j--;
    }
    
    
    
	
	//g_debug("ds_list");
	//리스트############################################################
}

