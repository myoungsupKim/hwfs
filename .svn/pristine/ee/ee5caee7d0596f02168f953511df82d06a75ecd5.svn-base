		else if(BG == "2")
			return "#e4e4e4ff URL(" + "'theme://images/new/grd_WF_Plusbtn.png'" + ") center middle";
		else if(BG == "3")
			return "#24a3a8ff URL(" + "'theme://images/new/grd_WF_Plusbtn.png'" + ") center middle";
			
	}
	else if (BL_PLUS == "-")
	{
		if(BG == "1")
