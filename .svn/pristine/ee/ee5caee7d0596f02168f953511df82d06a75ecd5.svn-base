		if (gfn_isNull(ds_list.getColumn(i, "UPJANGNM"))) {
			var iIdx = ds_listDummy.addRow();

			ds_listDummy.copyRow(iIdx, ds_list, i);
			
			//Next Row 
			var nfindRow = i + 1;
			
			//var nfindRow = ds_list.findRowExpr("BL_PLUS=='+'&&MAIN_UPJANG2=='" + ds_listDummy.getColumn(iIdx, "MAIN_UPJANG2") + "'");
			if (nfindRow != -1) {
				ds_listDummy.setColumn(iIdx, "MAIN_UPJANG", ds_list.getColumn(nfindRow, "MAIN_UPJANG"));
				ds_listDummy.setColumn(iIdx, "MAIN_UPJANGNM", ds_list.getColumn(nfindRow, "MAIN_UPJANGNM"));
				ds_listDummy.setColumn(iIdx, "UPJANG", "");
