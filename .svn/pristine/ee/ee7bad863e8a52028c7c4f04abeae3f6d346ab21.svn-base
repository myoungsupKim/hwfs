<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[--레시피검색및 등록
-식단삭제-자재삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>-1</result-count>
	<statement>
<![CDATA[
DELETE FROM FSM_REAL_RECIPE_ITEM
WHERE 
	 UPJANG = #UPJANG#
	 AND MENU_CD = #MENU_CD#	]]></statement>
	<input default-name="ds_in">
		<col name="UPJANG" size="22" type="NUMERIC" description="" /> 
		<col name="MENU_CD" size="20" type="VARCHAR" description="" /> 
		<col name="RECIPE_CD" size="20" type="VARCHAR" description="" /> 
		<col name="ITEM_CODE" size="12" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_out">
	</output>
</query>
