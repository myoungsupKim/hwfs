      , ATTR04                  	= #ATTR04#
      , ATTR05                  	= #ATTR05#
      , ATTR06                  	= #ATTR06#
      , ATTR07                  	= #ATTR07#
      , ATTR08                  	= #ATTR08#
      , ATTR09                  	= #ATTR09#
      , ATTR10                  	= #ATTR10#
