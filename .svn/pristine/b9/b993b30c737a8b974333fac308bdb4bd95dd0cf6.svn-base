    //외식사업부 필드 20200608 요청자:김유나 : 환불금, 잔액(부가세포함)
	var idx01   = grd_sub.getBindCellIndex("body","balAmt2");	//90
	var idx02   = grd_sub.getBindCellIndex("body","exchAmt");	//90
	var mucd = MuSearchComponent01.getMuCode();
	if(mucd == "2003"){
		grd_sub.setFormatColProperty(idx01, "size", 108);
		grd_sub.setFormatColProperty(idx02, "size", 70);
	} else {
		grd_sub.setFormatColProperty(idx01, "size", 0);
		grd_sub.setFormatColProperty(idx02, "size", 0);
	}
}

