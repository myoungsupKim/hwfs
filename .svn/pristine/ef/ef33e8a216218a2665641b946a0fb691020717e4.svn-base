		//그룹웨어 결재상세 팝업을 호출한다.
		SCGw.sendDetailView (this, ds_accOrderOpProposal.getColumn(0, "elctnsignKey"));
		}
	}else if(e.cell == 9){//운영품의
		if(ds_objectInfoRept.getColumn(e.row, "signProgressStatus1") == "DRAFT"){
			ds_search.setColumn(0, "proposalKind", "O");
			ds_search.setColumn(0, "objectCd", ds_objectInfoRept.getColumn(e.row, "objectCd"));
			fn_searchAccOrderOpProposal();
			
			//그룹웨어 결재상세 팝업을 호출한다.
			SCGw.sendDetailView (this, ds_accOrderOpProposal.getColumn(0, "elctnsignKey"));
		}
	}
	
}

