<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT 
	 RPT_ID AS RPT_ID
	, LINE_NO AS LINE_NO
	, RPT_NAME AS RPT_NAME
	, BACK_COLOR AS BACK_COLOR
	, FONT_COLOR AS FONT_COLOR
	, FONT_SIZE AS FONT_SIZE
	, FONT_TYPE AS FONT_TYPE
	, ITEM1 AS ITEM1
	, ITEM2 AS ITEM2
	, ITEM3 AS ITEM3
	, ORDER_NUM AS ORDER_NUM
	, EXPR_MARK AS EXPR_MARK
	, MARK_PLACE AS MARK_PLACE
	, EXPR_CLASS AS EXPR_CLASS
	, EXPR AS EXPR
	, ATTR01 AS ATTR01
	, ATTR02 AS ATTR02
	, ATTR03 AS ATTR03
	, ATTR04 AS ATTR04
	, ATTR05 AS ATTR05
	, ATTR06 AS ATTR06
	, ATTR07 AS ATTR07
	, ATTR08 AS ATTR08
	, ATTR09 AS ATTR09
	, ATTR10 AS ATTR10
	, CUSER AS CUSER
	, CDATE AS CDATE
	, UUSER AS UUSER
	, UDATE AS UDATE
 FROM MAC_RPT_SET
 WHERE 
	 RPT_ID = #RPT_ID#
	ORDER BY LINE_NO	]]></statement>
