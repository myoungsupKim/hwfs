function fn_ChkManNull(){
	// 팀정보 필수체크
	if(gfn_isNull(edt_tm.getTmCode())){
		g_Message("EE", st_Sabun.text + "은(는) 필수항목입니다.");
		edt_tm.setFocus();
		return false;
	}
	
	// 마케터 정보 필수체크
	if(gfn_isNull(edt_sabun.getSabun())){
			g_Message("EE", Static8.text + "은(는) 필수항목입니다.");
			edt_sabun.setFocus();
			return false;
	}
	
