	//alert(ds_Cond.getColumn(0, "SABUN"));
	
	fn_initData();
}

// 합계
function fn_sum(arg){
	var temp;
		temp = Math.round(ds_List.getCaseSum("ACCT_CLASS== '매출액'",arg) 
					    - ds_List.getCaseSum("ACCT_CLASS== '재료비'",arg) 
						- ds_List.getCaseSum("ACCT_CLASS== '경비'",arg) 
						- ds_List.getCaseSum("ACCT_CLASS== '노무비'",arg));
	return temp;
}


