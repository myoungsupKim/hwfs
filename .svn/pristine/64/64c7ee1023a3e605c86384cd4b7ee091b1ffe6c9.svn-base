function fn_grdSum()
{

	var strDd = yy.value+"01";
	var lstDt  = gfn_subStr(gfn_getLastDate(strDd), 6, 2);
	var nCurGrp = Math.floor((ds_List.rowposition+1)/4);
	//alert("nCurGrp===>"+nCurGrp);
	var nSum1 = 0;
	var nSum2 = 0;
	var nSum3 = 0;
	var nSum4 = 0;
	for( var j = 1 ; j <= 9 ; j++ )
	{
		var sColName = "DD_0" + j;
		
		var nval1 = ds_List.getColumn( nCurGrp*4, sColName );
		var nval2 = ds_List.getColumn( nCurGrp*4+1, sColName );
		var nval3 = ds_List.getColumn( nCurGrp*4+2, sColName );
		var nval4 = nval1 - ( nval2 + nval3 );
		ds_List.setColumn( nCurGrp*4+3, sColName, nval4 );
		
		nSum1 = nSum1 + nval1;
		nSum2 = nSum2 + nval2;
		nSum3 = nSum3 + nval3;
		nSum4 = nSum4 + nval4;
		
	}
	
	for( var j = 10 ; j <= parseInt(lstDt) ; j++ )
	{
		var sColName = "DD_" + j;
		var nval1 = ds_List.getColumn( nCurGrp*4, sColName );
		var nval2 = ds_List.getColumn( nCurGrp*4+1, sColName );
		var nval3 = ds_List.getColumn( nCurGrp*4+2, sColName );
		var nval4 = nval1 - ( nval2 + nval3 );
		ds_List.setColumn( nCurGrp*4+3, sColName, nval4 );
		
		nSum1 = nSum1 + nval1;
		nSum2 = nSum2 + nval2;
		nSum3 = nSum3 + nval3;
		nSum4 = nSum4 + nval4;
	}
	
	ds_List.setColumn( nCurGrp*4,   "MM_AMT", nSum1);
	ds_List.setColumn( nCurGrp*4+1, "MM_AMT", nSum2);
	ds_List.setColumn( nCurGrp*4+2, "MM_AMT", nSum3);
	ds_List.setColumn( nCurGrp*4+3, "MM_AMT", nSum4);

}

// 합계값을 변경했을때, 해당계정의 일자들에 1/N값을 분배한다.
function fn_totSum(obj:Button,  e:ClickEventInfo)
{
	if(ds_List.rowcount < 1)
	{
		g_Message("EE", "적용 할 대상이 없습니다.");
		return;
	}
	
	// 계산식에 사용할 일자구하기.
//////////////////////////////////////////////////////////////////	
	// 현재년월일
	var currYmd = gfn_today();
	// 현재년월
	var currYm = gfn_left( gfn_today(),6);
	// 현재일
	var currDd = gfn_today().substr(6,2); // 6
//////////////////////////////////////////////////////////////////////	
	// 입력년월
	var intYm = yy.value;
	// 입력년월일
	var strDd = yy.value+"01";
	// 입력년월일이 속한 달의 마지막  일
	var lstDy = gfn_subStr(gfn_getLastDate(strDd), 6, 2);
//////////////////////////////////////////////////////////////////////	
	//분배되어질 값
	var nNbn = parseInt(totSum.value);
	//데이타셋 기준의 마지막 필드의 인덱스값
	var cntDy = parseInt(lstDy)+9; // 0부터 시작 ds_List. DD_AMT 앞에 가져오는 컬럼갯수
//////////////////////////////////////////////////////////////////////	
	// 현재 년월일 이후 체크 된 갯수를 구한다.
	// 현재년월과 조회년월이 같으면 금일 이전일은 반영불가. 
	if (currYm == intYm)
	{	
		var nCheckTot = 0;
		for(var m = parseInt(currDd)+6; m <= parseInt(lstDy)+5; m++)
		{
			var sCheck = gd_list.getCellProperty("Head", m, "text");
			
			if( sCheck == "1" )
			{
				nCheckTot = nCheckTot + 1;
			}
		}
	
		nNbn = Math.round( nNbn / nCheckTot);
	
		// 편집불가 그리드 컬럼
		for(var m=parseInt(currDd)+9; m <= parseInt(lstDy)+10; m++)
		{
			var sCheck = gd_list.getCellProperty("Head", m-4, "text");
			if( sCheck == "1" )
			{
				ds_List.setColumn(ds_List.rowposition, m, nNbn );
			}
		}

	}
	else
	{
		var nCheckTot = 0;
		
		for(var m = 6; m <= parseInt(lstDy)+5; m++)
		{
			var sCheck = gd_list.getCellProperty("Head", m, "text");
			if( sCheck == "1" )
			{
				nCheckTot = nCheckTot + 1;
			}
		}
		
		nNbn = Math.round( nNbn / nCheckTot);
		
		for(var m = 9; m <= parseInt(lstDy)+10; m++)
		{
			var sCheck = gd_list.getCellProperty("Head", m-4, "text");
			if( sCheck == "1" )
			{
				ds_List.setColumn(ds_List.rowposition, m, nNbn );
			}
		}	
	}

}

