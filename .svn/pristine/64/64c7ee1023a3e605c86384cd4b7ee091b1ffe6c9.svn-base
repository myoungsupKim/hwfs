// 조회된 결과로 컬럼과 데이터셋을 생성한다.
function fn_CreateData()
{
	// 조회년월의 마지막 일수만큼 컬럼 생성.
	//var dd = ds_List.getColumn(0, "LAST_DD_CNT");
	var strDd = yy.value+"01";
	var dd  = gfn_subStr(gfn_getLastDate(strDd), 6, 2);
	var ym = yy.value;
	
	//재 조회시 공백으로 생성되는 컬럼삭제.
	for (var i=gd_list.getFormatColCount()-1; i>0; i--) {
		gd_list.deleteContentsCol(i);
	}
	//alert("==================>"+gfn_subStr(ds_List.getColumn(18, "ACCT_CD"), 0, 2));
	//그리드 컬럼 생성
	var nStartCell = 6;
	var nEndCell = parseInt(dd) + 5;
	var j = 3;
	
	for (var i=1; i<=dd; i++) {
		gd_list.appendContentsCol();
		gd_list.setCellProperty("Head", nStartCell, "displaytype", "checkbox"); // checkBox
		gd_list.setCellProperty("Head", nStartCell, "edittype", "checkbox"); // checkBox
		gd_list.setCellProperty("Head", nStartCell, "text", "1"); // checkBox
		
		if(i <= 9){
			//gd_list.setCellProperty("head", nStartCell, "text", gfn_subStr(ym, 0, 4)+"-"+gfn_subStr(ym, 4, 2)+"-0"+i); 			//헤드 text
			gd_list.setCellProperty("body", nStartCell, "expr", "Math.round(DD_0"+i+")"); 		//값 바인딩
			gd_list.setCellProperty("body", nStartCell, "text", "bind:DD_0"+i); 		//값 바인딩
			gd_list.setCellProperty("Summ", j, "text", "expr:Math.round((fn_sum('DD_0"+i+"')))"  );
		}else if(i > 9 && i <= dd){
			//gd_list.setCellProperty("head", nStartCell, "text", gfn_subStr(ym, 0, 4)+"-"+gfn_subStr(ym, 4, 2)+"-"+i); 				//헤드 text
			gd_list.setCellProperty("body", nStartCell, "expr", "Math.round(DD_"+i+")"); 		//값 바인딩
			gd_list.setCellProperty("body", nStartCell, "text", "bind:DD_"+i); 		//값 바인딩
			gd_list.setCellProperty("Summ", j, "text", "expr:Math.round((fn_sum('DD_"+i+"')))"  );
		}
		
		gd_list.setCellProperty("body", nStartCell, "align", "right");
		gd_list.setCellProperty("body", nStartCell, "displaytype", "number"); // number
		
		if(i <= 9){
			gd_list.setCellProperty("body", nStartCell, "edittype", "expr:fn_checkCell(currow, DD_0"+i+")");
		}else if(i > 9 && i <= dd){
			gd_list.setCellProperty("body", nStartCell, "edittype", "expr:fn_checkCell(currow, DD_"+i+")");
		}
		
		gd_list.setCellProperty("body", nStartCell, "mask", "#,##0");
		gd_list.setCellProperty("body", nStartCell, "editlimit", "7");
	    gd_list.setCellProperty("body", nStartCell, "editautoselect", "true");
	    
	    gd_list.setCellProperty("Summ", j, "align", "right");
		gd_list.setCellProperty("Summ", j, "displaytype", "number"); // number
	    gd_list.setCellProperty("Summ", j, "edittype", "none");
	    gd_list.setCellProperty("Summ", j, "mask", "#,##0");
	    
	    gd_list.setFormatColProperty(nStartCell, "size", 80);
		
		nStartCell += 1;
		j++;

	}
	
	// 일자가 나오는 헤더를 생성한다.
	///*
	var dd2  = gfn_subStr(gfn_getLastDate(strDd), 6, 2);
	var nStartCell2 = 6 + parseInt(dd2);
	var nEndCell2 = nStartCell2 + parseInt(dd2);
	//alert("nStartCell2==>"+nStartCell2+"<=nEndCell2=>"+nEndCell2);
	for (var z=1; z<=dd2; z++) {
		
		if(z <= 9){
			gd_list.setCellProperty("head", nStartCell2, "text", gfn_subStr(ym, 0, 4)+"-"+gfn_subStr(ym, 4, 2)+"-0"+z);
		}else if(z > 9 && z <= nEndCell2){
			gd_list.setCellProperty("head", nStartCell2, "text", gfn_subStr(ym, 0, 4)+"-"+gfn_subStr(ym, 4, 2)+"-"+z);
		}
	
		nStartCell2 += 1;
	}
	
	//trace(ds_List.saveXML());
	ds_List.rowposition = 0;
}

