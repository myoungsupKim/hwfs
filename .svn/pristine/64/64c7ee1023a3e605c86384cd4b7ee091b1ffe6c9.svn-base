function fn_RealDataCheck(intYy)
{
	fsp_clear(this);
	
	// 현재년월일
	var currYmd = gfn_today();
	// 현재년월
	var currYm = gfn_left(gfn_today(),6);
	// 현재일
	var currDd = gfn_today().substr(6,2); // 6
	// 입력년월
	var intYm = yy.value;
	// 해당월의 마지막일.
	var strDd = yy.value+"01";
	var lstDy =  gfn_subStr(gfn_getLastDate(strDd), 6, 2);
	
	// 확정버튼 보여주기.
	var chkYn = ds_List.getColumn(0, "CONFIRM_YN");
	//alert("chkYn==>"+chkYn);
	
	// 사업계획, 수정계획 등록여부 확인.
	var strIstChk = ds_confirm_YN.getColumn(0, "CONFIRM_YN");
	//alert("strIstChk===>"+strIstChk);
	
	// 등록이 되어있을때만 번튼 보이게.
	if(strIstChk == "0"){
		btn_Confirm.visible = false;
		btn_Apply.visible = true;
	}else{
		if(chkYn == "N"){
			//btn_Confirm.visible = true; // 일추정에서는 '확정'버튼 사용안함.
			btn_Apply.visible = true;
		}else{
			btn_Confirm.visible = false;
			btn_Apply.visible = false;
		}	
	}
	
	// 년월 : 현재년월과 입력년월이 같고, 오늘 이전 실적데이터 수정불가.
	if (currYm == intYm){
	
		// 편집불가 그리드 컬럼
		var nStartCell = 6;
		var nEndCell = parseInt(currDd) + 5;
		
		for(var i=0; i<ds_List.getRowCount(); i++){
			// 현재일 이전 일까지는 수정불가.
			for(var j=nStartCell; j<nEndCell; j++){
				gd_list.setCellProperty("body", j, "edittype", "none");
			}//end for
		}//end for
		
		// 확정시 편집불가 그리드 컬럼
		var nEndCell2 = parseInt(lstDy) + 5;
		if(chkYn == "Y"){
			//gd_list.enable = true;
			for(var k=0; k<ds_List.getRowCount(); k++){
			// 현재일 이전 일까지는 수정불가.
				for(var m=nStartCell; m<=nEndCell2; m++){
					gd_list.setCellProperty("body", m, "edittype", "none");
				}//end for
			}
		}
		
	}else if (currYm < intYm){
	
		// 확정시 편집불가 그리드 컬럼
		var nStartCell = 6;
		var nEndCell2 = parseInt(lstDy) + 5;
		if(chkYn == "Y"){
			//gd_list.enable = true;
			for(var i=0; i<ds_List.getRowCount(); i++){
			// 현재일 이전 일까지는 수정불가.
				for(var j=nStartCell; j<=nEndCell2; j++){
					gd_list.setCellProperty("body", j, "edittype", "none");
				}//end for
			}
		}
	}else{
		// 편집불가 그리드 컬럼
		//gd_list.enable = false;
		
		btn_Apply.visible = false;
		var nStartCell = 6;
		var nEndCell2 = parseInt(lstDy) + 5;
		//if(chkYn == "Y"){
			//gd_list.enable = true;
			for(var i=0; i<ds_List.getRowCount(); i++){
			// 현재일 이전 일까지는 수정불가.
				for(var j=nStartCell; j<=nEndCell2; j++){
					gd_list.setCellProperty("body", j, "edittype", "none");
				}//end for
			}
		//}
	}
}

