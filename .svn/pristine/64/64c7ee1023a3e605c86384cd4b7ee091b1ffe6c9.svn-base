	ds_Cond.addRow();
	ds_Cond.setColumn(0, "CC_CD", ccCd);
}

// 그리드 체크로직
function fn_checkCell( currow, colID )
{  
	var sRtn = "none";
	 
	// 현재년
	var currYy = gfn_today().substr(0,4);
	
	// 현재월
	var currMm = gfn_today().substr(4,2);
	
	// 입력년
	var intYy = yy.value;
	//trace("currYy==>"+currYy); trace("intYy==>"+intYy);
	
	// 구분 : '사업계획', '최종PL'이 아니고
	// 년도 : 현재년도와 입력년도가 같을때 실적데이터 수정불가.
	// 년월 : 현재년월과 입력년월이 같고, 오늘 이전 실적데이터 수정불가.
	
	if (currYy == intYy){
	
		// 편집불가 그리드 컬럼
		// 초기화.
		if( ds_List.getColumn( currow, "ACCT_CD" ) == "44000000" ){
			sRtn = "none";
		}else{
			if( parseInt(colID) >= parseInt(currMm) ){
			sRtn = "masknumber";
			}else{
				sRtn = "none";
			}
		}
	}else if (currYy > intYy){
		sRtn = "none";
	}else{
		if( ds_List.getColumn( currow, "ACCT_CD" ) == "44000000" ){
			sRtn = "none";
		}else{
			sRtn = "masknumber";
		}
	}
	
	return sRtn;
	
}

// 데이터 초기화
function fn_initData(){

	fn_ClearData();
	ds_List.clearData();

