		// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		var retValue = gfn_confirm("confirm.before.search");
 		if (retValue == false) return;
 	}
	
	btn_custNm.enable=true;
 
    ds_scheduleMgmt.clearData();

    var strSvcID    = "searchData";
    var strURL      = "rc/bms/proMgmt/searchData.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_scheduleMgmt=ds_scheduleMgmt";
    var strArg      = "amCd=" + ds_scheduleList.getColumn(ds_scheduleList.rowposition , "amCd")
