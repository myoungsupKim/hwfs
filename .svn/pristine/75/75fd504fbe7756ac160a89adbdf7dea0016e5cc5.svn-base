function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
	//trace("ds_scheduleList=" + ds_scheduleList.rowcount );
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            //trace("===" + ds_amList.getRowCount());
            break;
        case "searchCustMgmt" :
       
			ds_scheduleMgmt.setColumn(ds_scheduleMgmt.rowposition, "custCd", ds_custMst.getColumn(ds_custMst.rowposition,"custCd") );
			break;
		case "searchData" :
			//스케즐 쿼리가 완료 되면 해당 거래처를 읽어 온다.
			fn_serchCustMst();
			break;
        // 저장
        case "save" :
			//trace("RetVal:" + RetVal);
			
			//ds_amList.setColumn(ds_amList.rowposition, "amcd", 0);
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_search();
            break;    
    }
}
