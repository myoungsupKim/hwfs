function ds_scheduleMgmt_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	//예약시간
	if ((e.columnid == "strTime") || (e.columnid == "endTime"))
	{
		 
		var strTime = obj.getColumn(nRow, "strTime");
		var endTime = obj.getColumn(nRow, "endTime");
		 
		if (strTime > endTime)
		{
			
			gfn_alert("msg.err.validator.comparemin", "시작시간^종료시간");	// $1 이(가) $2 보다 큽니다.
			obj.setColumn(nRow, "endTime",strTime);
			cbo_endTime.value=strTime;
			 
			return;
		}
		 
	}
}

