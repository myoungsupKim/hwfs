    var strSvcID    = "searchList";
    var strURL      = "rc/bms/proMgmt/selectList.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_scheduleList=ds_scheduleList";
    var strArg      = "dateFr=" + cal_serchDateFr.text + " dateTo=" + cal_serchDateTo.text
                    + " amCd=" + edt_serchAmNm.amCd  
                    + " maskAuth=" + SCAuth.getMaskAuth(this);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);

}



/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	
	if (gfn_validCheck(ds_scheduleMgmt, ds_scheduleMgmtValid) == false)	return;

	ds_scheduleMgmt.clearData();
	var nRowIdx = ds_scheduleMgmt.addRow();	//행추가
	ds_scheduleMgmt.setColumn(nRowIdx, "useYn", 1);
	cbo_scheduleKid.index=0;
	rdo_statusClass.index=0;
	
	ds_custMst.clearData();
	var nRowIdx = ds_custMst.addRow();	//행추가
	ds_custMst.setColumn(nRowIdx, "useYn", 1);
	btn_custNm.enable=true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_scheduleMgmt.rowposition;
	
	if(ds_scheduleMgmt.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		var bIsProcRow = false;
		if (ds_scheduleMgmt.getRowType(nRow) == Dataset.ROWTYPE_INSERT)
		{
			ds_scheduleMgmt.deleteRow(nRow);
		} else
		{
			ds_scheduleMgmt.setColumn(nRow, "useYn", 0);
			bIsProcRow = true;
		}
		
		if (!bIsProcRow) return;
		
		fn_save();
		
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
