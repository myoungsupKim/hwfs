function fn_save()
{
	if ((gfn_dsIsUpdated(ds_scheduleMgmt)==false) && (gfn_dsIsUpdated(ds_custMst)==false) ) 
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	// 저장시 Validation Check	
	if (gfn_validCheck(ds_scheduleMgmt, ds_scheduleMgmtValid) == false)	return;
	
    var strSvcID    = "save";
    var strURL      = "rc/bms/proMgmt/saveData.xdo";
    var strInDs     = "ds_scheduleMgmt=ds_scheduleMgmt:U";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

