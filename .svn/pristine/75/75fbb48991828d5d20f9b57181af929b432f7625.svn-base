	<statement>
<![CDATA[
UPDATE 
 	SCC_CUST_LOG
SET 
	 CUST_CD = #CUST_CD#
	, CONTRACT_METHOD = #CONTRACT_METHOD#
	, RE_CONTRACT_POSS = #RE_CONTRACT_POSS#
	, CUST_STS = #CUST_STS#
	, OUR_COM_GOOD = #OUR_COM_GOOD#
	, LABOR_GOOD = #LABOR_GOOD#
	, ATTR01 = #ATTR01#
	, ATTR02 = #ATTR02#
	, ATTR03 = #ATTR03#
	, ATTR04 = #ATTR04#
	, ATTR05 = #ATTR05#
	, ATTR06 = #ATTR06#
	, ATTR07 = #ATTR07#
	, ATTR08 = #ATTR08#
	, ATTR09 = #ATTR09#
	, ATTR10 = #ATTR10#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 CUST_MST_ID = #CUST_MST_ID#	]]></statement>
