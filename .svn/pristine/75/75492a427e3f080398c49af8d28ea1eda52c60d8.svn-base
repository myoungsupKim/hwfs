/**
 * @class 거래처 선택 팝업을 호출한 이후에 발생하는 이벤트(거래처 기본정보를 설정한다.)
 * @param p - 발생 Event에 대한정보
 * @param data - 선택된 결과에 대한 정보
 * @return 
 */
function fn_custAfterEvent(o, data)
{
	var nRow = ds_list.rowposition;
	ds_list.setColumn(nRow, "custCustType", data.custtypenm);
	ds_list.setColumn(nRow, "custCustGubun", data.custgubunnm);
	ds_list.setColumn(nRow, "upJong", data.upJong);
	ds_list.setColumn(nRow, "upTae", data.upTae);
	ds_list.setColumn(nRow, "drname", data.drname1);
	ds_list.setColumn(nRow, "bsnNo", data.saupno);
	ds_list.setColumn(nRow, "subBsnNo", data.subUpjangNum);
	ds_list.setColumn(nRow, "corpRegNum", data.bubinno);
	ds_list.setColumn(nRow, "upjangnm", data.custnm);
	ds_list.setColumn(nRow, "sapCustCd", data.sapCustCd);
	
	var addrJson = gfn_jsonStringToObject(data.addrInfo);
	
	if (addrJson.addrKey != "") {
		ds_list.setColumn(nRow, "addr", data.addrInfo);
		div.AddrSearchComponent00.bind();
		var newKey = div.AddrSearchComponent00.newKey();
		ds_list.setColumn(nRow, "addrKey", newKey);
	}
	else {
		ds_list.setColumn(nRow, "addr", data.addrInfo);
		div.AddrSearchComponent00.bind();
		ds_list.setColumn(nRow, "addrKey", "");
	}
	
	div.edt_upjangnm.setFocus();
	div.edt_drname.readonly = true;
	div.edt_upJong.readonly = true;
	div.edt_upTae.readonly = true;
	
}

