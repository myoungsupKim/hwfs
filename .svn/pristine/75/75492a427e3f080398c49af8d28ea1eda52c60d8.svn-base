function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	var nRow = ds_search.addRow();
	
	TmSearchComponent00.applyAuthLevel(MuSearchComponent00);
	UpjangSearchComponent00.setForcedRoleSet("CostCenter");
	UpjangSearchComponent00.applyAuthLevel(TmSearchComponent00);
		
	SCCombo.bind(this, "ComCode", "cbo_s_shopTypCd", null, {groupCode:"SHOP_TYP_CD", useYn:"Y"}, SCCombo.ALL, "fn_includeCS");
	SCCombo.bind(this, "ComCode", "div.cbo_shopTypCd", null, {groupCode:"SHOP_TYP_CD", useYn:"Y"});
	SCCombo.bind(this, "ComCode", "div.cbo_vatGubun", null, {groupCode:"VAT_GUBUN", useYn:"Y"});
	SCCombo.bind(this, "ComCode", "div.cbo_classCd", null, {groupCode:"UPJANG_KIND", useYn:"Y"}, SCCombo.CHOOSE);
	SCCombo.bind(this, "ComCode", "cbo_s_approvalStatus", null, {groupCode:"APPROVAL_STATUS_CD", useYn:"Y"}, SCCombo.ALL);
	
	//ds_search.setColumn(nRow, "startCdate", gfn_getYear() + "0101");
	//ds_search.setColumn(nRow, "endCdate", gfn_today());
	
	ObjCalendar00.fn_initMonthCal(this, "", "", ds_search, "startCdate");
	ObjCalendar01.fn_initMonthCal(this, "", "", ds_search, "endCdate");
	
	div.edt_vatUpjang.setUpjangCode("");
	//div.edt_vatUpjang.setUpjangName("");
	//SCPopup.bind(this, "CcCode", "edt_s_cc", "btn_s_cc", null, {}, {after:"fn_CcAfterEvent"});
	SCPopup.bind(this, "CcCode", "div.edt_cc", "div.btn_cc", null, {}, {after:"fn_CcAfterEvent"});

	div.cal_accOpendate.fn_initMonthCal(this, "", "", ds_list, "accOpendate");	
	div.cal_accClosedate.fn_initMonthCal(this, "", "", ds_list, "accClosedate");
	
	//fn_includeCS();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{	
	// v_ds_check 미사용시 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
	if (gfn_dsIsUpdated(ds_list) == true) 
	{
		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
        if (!gfn_confirm("confirm.before.movepage")) return false;
    }
    
	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}


/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
 	// v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_list) == true) 
 	{
 		var retValue = gfn_confirm("confirm.before.search");	// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
 		if (retValue == false) return;
 	}
 	
 	ds_search.setColumn(ds_search.rowposition, "tmCd", TmSearchComponent00.getTmCode());
 	ds_search.setColumn(ds_search.rowposition, "ccCd", UpjangSearchComponent00.getUpjangCode());
 	
	if (gfn_isNull(ds_search.getColumn(ds_search.rowposition, "upjang")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "upjangnm")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "saupno")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "shopTypCd")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "muCd")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "tmCd")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "drname")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "cuser")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "approvalStatus")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "startCdate")) && 
		gfn_isNull(ds_search.getColumn(ds_search.rowposition, "endCdate")))
	{
		gfn_alert("최소 하나이상의 조건을 입력하셔야합니다.");
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);

    var strSvcID    = "search";
    var strURL      = "sc/scz/upjangMgmt/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "maskAuth="+SCAuth.getMaskAuth(this);
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	
	var nRow = ds_list.addRow();
	
	ds_list.setColumn(nRow, "accOpendate", gfn_today());
	ds_list.setColumn(nRow, "accClosedate", "21001231");
	
	if (Number(ds_includeCS.getColumn(0, "cnt")) > 0) {
		ds_list.setColumn(nRow, "shopTypCd", "02");
		div.cbo_shopTypCd.readonly = true;
	}
	else {
		ds_list.setColumn(nRow, "shopTypCd", "01");
	}
	
	ds_list.setColumn(nRow, "vatGubun", "01");
	ds_list.setColumn(nRow, "addr", div.AddrSearchComponent00.newData());
	ds_list.setColumn(nRow, "vatUpjang", "109770");
	ds_list.setColumn(nRow, "vatUpjangnm", "FC FS(공통)");
	
	
	div.chk_bankAcctNoRequestYn.readonly = true;
	div.edt_upjangnm.setFocus();
	
	div.edt_drname.readonly = false;
	div.edt_upJong.readonly = false;
	div.edt_upTae.readonly = false;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	var nRow = ds_list.rowposition;
	
	if(ds_list.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}

	var rtVal = gfn_confirm("confirm.before.delete");	// 선택된 자료를 삭제 하시겠습니까?
	if (rtVal == true) 
	{	
		ds_list.deleteRow(nRow);
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
 	// v_ds_check 미사용시 변경사항 체크
 	if (gfn_dsIsUpdated(ds_list) == false) 
 	{
 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
 		return;
 	}
	
	var row = ds_list.rowposition;
	
	if (gfn_isNull(ds_list.getColumn(row, "shopTypCd")))
	{
		gfn_alert("업장유형은 필수 입력항목입니다.");
		div.cbo_shopTypCd.setFocus();
		return;
	}

	if (ds_list.getColumn(row, "shopTypCd") == "02")
	{
		if (gfn_isNull(ds_list.getColumn(row, "custcd")))
		{
			gfn_alert("업장유형이 영업인 경우, 거래처는 필수 입력항목입니다.");
			return;
		}
	}
	
	if (gfn_isNull(ds_list.getColumn(row, "upjangnm")))
	{
		gfn_alert("업장명은 필수 입력항목입니다.");
		div.edt_upjangnm.setFocus();
		return;
	}

	if (gfn_isNull(ds_list.getColumn(row, "ccCd")))
	{
		gfn_alert("CC는 필수 입력항목입니다.");
//		div.edt_upjangnm.setFocus();
		return;
	}

	if (gfn_isNull(ds_list.getColumn(row, "vatGubun")))
	{
		gfn_alert("과/면세구분은 필수 입력항목입니다.");
		div.cbo_vatGubun.setFocus();
		return;
	}

	if (gfn_isNull(ds_list.getColumn(row, "vatUpjang")))
	{
		gfn_alert("부가세 신고업장은 필수 입력항목입니다.");
		//div.cbo_vatGubun.setFocus();
		return;
	}

	if (gfn_isNull(ds_list.getColumn(row, "schoolYn")))
	{
		if (ds_list.getColumn(row, "schoolYn") == "Y")
		{
			if (gfn_isNull(ds_list.getColumn(row, "schoolYnCustcd")))
			{
				gfn_alert("중.고거래처는 필수 입력항목입니다.");
				//div.cbo_vatGubun.setFocus();
				return;
			}
		}
	}
	
	if (gfn_isNull(ds_list.getColumn(row, "addrKey"))) {	
		gfn_alert("주소는 필수 입력항목입니다.");
		return;
	}
	
	ds_procList.clearData();

    var strSvcID    = "save";
    var strURL      = "sc/scz/upjangMgmt/saveList.xdo";
    var strInDs     = "ds_list=ds_list:U";
    var strOutDs    = "ds_procList=ds_procList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

