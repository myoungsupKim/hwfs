function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (ds_list.getColumn(ds_list.rowposition, "shopTypCd") == "02")
	{
		if (ds_list.getRowType(ds_list.rowposition) == Dataset.ROWTYPE_NORMAL && ds_list.getColumn(ds_list.rowposition, "bankAcctNoRequestYn") == "Y") {
			div.chk_bankAcctNoRequestYn.readonly = true;
		}
		else {
			div.chk_bankAcctNoRequestYn.readonly = false;
		}
		
		div.edt_drname.readonly = true;
		div.edt_upJong.readonly = true;
		div.edt_upTae.readonly = true;
	}
	else
	{
		div.chk_bankAcctNoRequestYn.value = "N";
		div.chk_bankAcctNoRequestYn.readonly = true;
		div.edt_drname.readonly = false;
		div.edt_upJong.readonly = false;
		div.edt_upTae.readonly = false;
	}
}

function div_cbo_shopTypCd_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	if (obj.value == "02")
	{
		div.chk_bankAcctNoRequestYn.readonly = false;
	}
	else
	{
		div.chk_bankAcctNoRequestYn.value = "N";
		div.chk_bankAcctNoRequestYn.readonly = true;
	}
}
