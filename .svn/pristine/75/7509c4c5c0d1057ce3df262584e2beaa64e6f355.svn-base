<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[업장에 딸린 식당 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT HALL_CD AS CODE, HALL_NAME AS CODE_NAME
  FROM FSA_HALL_MST
 WHERE UPJANG = #UPJANG#
 ORDER BY CODE	]]></statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_Hall">
	</output>
</query>
