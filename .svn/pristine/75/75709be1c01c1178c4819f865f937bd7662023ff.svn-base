function fn_dup_color(fa_com, fa_date, fa_row, fa_tit)
{
	st_pyoTitle.text = "식단평가 : " + fa_tit;
	// alert("fn_dup_color 실행 :fa_com:"+fa_com+"/fa_date:"+fa_date+"/fa_row:"+fa_row+"/");
	if (ds_list.getRowCount() == 0) {
		g_Message("EE", "평가 내용이 없습니다.");
		return;
	}
	// return;
