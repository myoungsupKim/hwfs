************************************************************************************************/
include "lib::lib_com.xjs";
include "lib::SMCommon.xjs";


/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_sToday = gfn_today();


/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	if(!gfn_isNull(pArNum)){
		edt_arNum.value = pArNum;
		ds_search.addRow();
		ds_search.setColumn(0, "arNum", pArNum);
		fn_search();
	}
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	//v_ds_check = "ds_list";
	if(gfn_isNull(pArNum)){
		ds_arData.addRow();
	}
	
	gfn_setSalesTypeCd(this, "arType", "ds_arType", "001");	//채권유형코드
	Tab00.tabpage1.cal_fiscalDate.fn_initMonthCal(this, fv_sToday, null, ds_arData, "fiscalDate"); //달력초기화
	Tab00.tabpage1.cal_withdrawSchdDate.fn_initMonthCal(this, fv_sToday, null, ds_arData, "withdrawSchdDate"); //달력초기화
	SCDataset.bind(this, "ComCode", "ds_dataSet", {groupCode:"DATA_SET", code:"DEF_MU_CODE", useYn:"Y"}, "fn_triggerDatasetEvent");	//사업부조회 
	SCPopup.bind(this, "CcCode", 	"Tab00.tabpage1.edt_ccNm", 	"Tab00.tabpage1.btn_ccCd", 		null, {}, {after:"fn_popCcCdAfter"}); 	//CC코드팝업(입력)	
	SCCombo.bind(this, "ComCode", "Tab00.tabpage1.cbo_payClassCd", null, {groupCode:"SLE_0017", uesYn:"Y"}); //지불조건 Combo처리

}
/*----------------------------------------------------------------------------------------------
 * 설명      : 사업부Combo 조회 후 처리
 *---------------------------------------------------------------------------------------------*/
function fn_triggerDatasetEvent(p, ds)
{
	if (p.oid == "ds_dataSet")
	{
		SCCombo.bind(this, "MU", "Tab00.tabpage1.cbo_mu", null, {defVal:ds.getColumn(0, "set1")}); //사업부 구분 Combo처리
	}
}
function fn_popCcCdAfter(p, data){
	//if(data.code != null && data.code != undefined){
	alert("D");
	if(gfn_transNullToEmpty(data.code)){
		edt_ccCd.value = data.code;
		edt_ccNm.value = data.codeName;
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{	
//	// v_ds_check 미사용시 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
// 	if (gfn_dsIsUpdated(ds_list) == true) 
