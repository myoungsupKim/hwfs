/*----------------------------------------------------------------------------------------------
 * 설명      : 레시피 분류 코드 중복체크(동기로 실행)
 *---------------------------------------------------------------------------------------------*/
function fn_existsSearch(){

	var strSvcID    = "search";
    var strURL      = "/rc/rrm/recipeClassCodeMgnt/selectExists.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "fv_existsCnt=fv_existsCnt";
    var strArg      = "";
    var strCallback = "";
    var bAsync      = false;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
    if(fv_existsCnt > 0){
		gfn_alert("msg.err.duplicatedata", "코드");	// $1 은(는) 이미 등록된 자료입니다.
		if(ds_search.getColumn(0, "codeGb") == "G"){
			edt_mgrpCd.value = "";
			edt_mgrpCd.setFocus();
		}else if(ds_search.getColumn(0, "codeGb") == "M"){
			edt_mcalssCd.value = "";
			edt_mcalssCd.setFocus();
		}else{
			edt_sclassCd.value = "";
			edt_sclassCd.setFocus();
		}
		fv_existsCnt = 0;
    }
}]]></Script>
