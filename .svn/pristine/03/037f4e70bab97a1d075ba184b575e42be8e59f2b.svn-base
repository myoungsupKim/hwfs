 *---------------------------------------------------------------------------------------------*/
function edt_mgrpCd_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	if(obj.value == null) return;
	
	ds_search.setColumn(0, "codeGb", "G");
	ds_search.setColumn(0, "mgrpCd", obj.value);
	
	fn_existsSearch();
}

