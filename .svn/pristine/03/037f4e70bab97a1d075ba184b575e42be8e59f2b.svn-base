/*----------------------------------------------------------------------------------------------
 * 설명      : 대분류 DataSet RowPos 이벤트
 *---------------------------------------------------------------------------------------------*/
function ds_mgrp_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	var nRow = obj.rowposition;
	ds_search.setColumn(0, "mgrpCd", obj.getColumn(nRow, "mgrpCd"));
	ds_search.setColumn(0, "codeGb", "M");
	
	fn_search("M");
	
	if(obj.getRowType(nRow) == 1 || obj.getRowType(nRow) == 4){
		edt_mgrpCd.enable = false;
	}else{
		edt_mgrpCd.enable = true;
	}
}

