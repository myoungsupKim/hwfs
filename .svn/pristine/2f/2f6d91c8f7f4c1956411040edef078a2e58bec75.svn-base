/*----------------------------------------------------------------------------------------------
 * 설명      : 조건 DataSet 변경 이벤트
 * 
 *---------------------------------------------------------------------------------------------*/
function ds_search_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	// 조건 변경 시 DataSet Clear	
	if (e.columnid == "schGubun" || e.columnid == "schDate1" || e.columnid == "schDate2" || e.columnid == "schType" || e.columnid == "schCsSabun") {
		ds_list.clearData();
		ds_detail.clearData();
	}

	return;
	// 달력 컴포넌트의 Default 일자 자동입력 완성 처리보다 실제 이벤트가 더 빨리 처리되고 있음
	
	if(e.columnid == "schPrDate" || e.columnid == "schNeedDate"){
		if(ds_search.getColumn(obj.rowposition,"schType") == "07"){
			if(!gfn_isNull(obj.getColumn(obj.rowposition,"schPrDate")) && !gfn_isNull(obj.getColumn(obj.rowposition,"schNeedDate"))){
				if(obj.getColumn(obj.rowposition,"schPrDate") > obj.getColumn(obj.rowposition,"schNeedDate")){
					gfn_alert("신청일자 이(가) 입고일자 보다 큽니다.");
					obj.setColumn(e.row,e.columnid, "");
					return;
				}
			}
		}else if(ds_search.getColumn(obj.rowposition,"schType") == "29" || ds_search.getColumn(obj.rowposition,"schType") == "97" ){
			/*	2015-11-02	요청:PM
								긴급(29)	입고일이 투데이 월과 같으면 오늘 이전일자 입고일 불가.
											입고일이 전월 일경우 전월+말일로 셋팅
											ex)today = 2015-11-02  이면 2015-11-01 선택불가.
											   2015-10-11 이면 10월 31일로 셋팅. 2015-09-20 이면 09월 30일로 셋팅.
			*/
			if(gfn_subStr(fv_sToday,0,6) == gfn_subStr(obj.getColumn(obj.rowposition,"schNeedDate"),0,6)){
				if(!gfn_isNull(obj.getColumn(obj.rowposition,"schPrDate")) && !gfn_isNull(obj.getColumn(obj.rowposition,"schNeedDate"))){
					if(obj.getColumn(obj.rowposition,"schPrDate") > obj.getColumn(obj.rowposition,"schNeedDate")){
						gfn_alert("신청일자 이(가) 입고일자 보다 큽니다.");
						obj.setColumn(e.row,e.columnid, "");
						return;
					}
				}
			}else if(gfn_subStr(obj.getColumn(obj.rowposition,"schNeedDate"),0,6)  < gfn_subStr(fv_sToday,0,6)){
				var needDate = gfn_subStr(obj.getColumn(obj.rowposition,"schNeedDate"),0,6) + gfn_getLastDateNum(obj.getColumn(obj.rowposition,"schNeedDate"));
				obj.setColumn(e.row,e.columnid, needDate);
				return;
			}
		}	
	}
	
}



