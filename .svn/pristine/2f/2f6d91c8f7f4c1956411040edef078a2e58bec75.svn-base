function btn_upload_onclick(obj:Button,  e:ClickEventInfo)
{
	var sMsg = "";
	
	// 기간 CHECK : 구매유형, 신청일자, 입고일자
	if (gfn_validCheck(ds_search, ds_searchInput, true) == false) return;
		
	// 구매유형이 "07"인 경우에만 업로드가 되게 CHECK
	if (ds_search.getColumn(ds_search.rowposition, "schType")!="07") {
		sMsg = "구매유형이 [식자재_식재영업] 만 가능합니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}

