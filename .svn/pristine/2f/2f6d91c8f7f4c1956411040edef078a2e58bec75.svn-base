/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";

	//조회조건을 초기화한다.
	var nRow= ds_search.addRow();
	ds_search.setColumn(nRow, "schGubun", "1");							// 구분 : 신청일자
	ds_search.setColumn(nRow, "schDate1", gfn_today());					// 현재일자
	ds_search.setColumn(nRow, "schDate2", gfn_today());					// 현재일자
	//ds_search.setColumn(nRow, "schChk1", "");							// 본인자료만조회(CHECK)
	ds_search.setColumn(nRow, "schPrDate", gfn_today());				// 신규 : 신청일자
	ds_search.setColumn(nRow, "schNeedDate", gfn_addDate(gfn_today(),1));				// 신규 : 입고일자
	
	// 등록일자 SET
	cal_fromDate.fn_initMonthCal(this, "", "", ds_search, "schDate1", "Y");	
	cal_toDate.fn_initMonthCal(this, "", "", ds_search, "schDate2", "Y");	
	// 입고일선택
	cal_prDate.fn_initMonthCal(this, "", "", ds_search, "schPrDate", "Y");	
	cal_needDate.fn_initMonthCal(this, "", "", ds_search, "schNeedDate", "Y");	
	
	
	// Session변수 TEST
	edt_prSabun.value = g_EmpNo;
	edt_prUpjang.value = g_Upjang;
	//edt_prSabun.value = "201403853";
	//edt_prUpjang.value = "114998";
}

