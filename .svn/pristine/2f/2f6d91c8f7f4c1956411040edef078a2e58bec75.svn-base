		if (gfn_isDataChange() == true) 
		{		
			// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
			if (!gfn_confirm("confirm.before.movepage")) return false;
		}
	}
    
	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 구매유형조회
        case "searchType" :
            //trace(ds_type.saveXML());
			// 초기화
			fn_type_init();
            break;

		
		// 조회
        case "search" :
            //trace(ds_list.saveXML());
            if (ds_list.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_list, fv_objColInfo);
            }
            // 기본값 SET
            fn_search_init();
			// 상세정보 관련 초기화
			fn_initDetail();
            break;
            
		// 상세정보 조회
        case "searchDetail" :
            //trace(ds_detail.saveXML());
            if (ds_detail.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_detail, fv_objColInfo);
            } else {
				// 진행상태 값 읽어오기(취소용)
				fn_detailStatus();
            }
            break;
            
		// 선택한 신청건에 대한 주문상태 조회
        case "searchMaxStatus" :
			// 진행상태에 따른 버튼 처리
            fn_statusCompSet();
            break;
            
		// 주문서 취소
        case "deletePr" :
			//trace("삭제건수 : " + fv_processCnt);
			if(fv_errorFlag == "Y"){
				gfn_alert(fv_errorMsg);
				fv_errorMsg = "";//초기화
				fv_errorFlag = "";//초기화
			}else if (fv_processCnt > 0) {
				gfn_alert("주문취소 되었습니다.");	// 삭제 되었습니다.
			}
			// 목록 재조회
			fn_search();
            break;    
            
    }
}

