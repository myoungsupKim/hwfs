/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{
	// 조건 SET
	var nRow = ds_search.rowposition;
	
	ds_search.setColumn(nRow, "schGubun", rdo_gubun.value);			// 신청일자(1), 입고일자(2)
	//ds_search.setColumn(nRow, "schSabun", edt_prSabun.value);		// 신청자사번(임시)
	//ds_search.setColumn(nRow, "schUpjang", edt_prUpjang.value);		// 신청자업장(임시)

	// 로그인정보 활용
	ds_search.setColumn(nRow, "schSabun", g_EmpNo);			// 신청자사번
	ds_search.setColumn(nRow, "schUpjang", g_Upjang);		// 신청자업장
	
	//if(chk1.value) ds_search.setColumn(nRow, "schChk1", "1");		// CHECK_1
	//else ds_search.setColumn(nRow, "schChk1", "");		

	// 조회시 Validation Check
	if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;
	
	// 기간 CHECK
	if (cal_fromDate.text > cal_toDate.text) {
 		gfn_alert("msg.err.validator.date.great");	// 종료일이 시작일보다 빠릅니다.
 		cal_fromDate.setFocus();
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_list);

   	//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장 
	fv_objColInfo = gfn_getDatasetInfo (ds_list);
    
    var strSvcID    = "search";
	var strURL      = "fm/fmo/orderRequest/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_list=ds_list";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 * 신규구매신청 팝업 호출
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{
	/*시스템 통제 시간 체크 - 특정일자에 사용을 할 수 없게 한다.*/
	mfn_GetSystemWorkCheck("PO");
	if (ds_common.getColumn(0, 0) == "Y")
	{
		g_Message("EE", "시스템 변경 작업중이라 현 화면을 사용할 수 없습니다. \n사용불가시간(" + ds_common.getColumn(0, 1) + " ~ " + ds_common.getColumn(0, 2) + ") ");
		return false; 
	}

	var sMsg = "";
	
	// 기간 CHECK : 구매유형, 신청일자, 입고일자
	if (gfn_validCheck(ds_search, ds_searchInput, true) == false) return;
		

	// 입고예정일
	var sNeedDate = ds_search.getColumn(ds_search.rowposition, "schNeedDate");		// 화면에서 입력한 입고일


	//2015-09-22  긴급 - 입고일이 오늘이전 발주 금지.
	if(gfn_subStr(fv_sToday,0,6) == gfn_subStr(ds_search.getColumn(ds_search.rowposition,"schNeedDate"),0,6)){
		/*	2015-11-02	요청:PM
							긴급(29)	입고일이 투데이 월과 같으면 오늘 이전일자 입고일 불가.
										입고일이 전월 일경우 전월+말일로 셋팅
										ex)today = 2015-11-02  이면 2015-11-01 선택불가.
										   2015-10-11 이면 10월 31일로 셋팅. 2015-09-20 이면 09월 30일로 셋팅.
		*/	
		if( (ds_type.getColumn(ds_type.rowposition,"poType") == 29 || ds_type.getColumn(ds_type.rowposition,"poType") == 97 )&&  sNeedDate < gfn_today() ){
			//sMsg = "구매 유형이 [" + ds_type.getColumn(ds_type.rowposition,"typeName") + "] 일 경우\n\n오늘 이전 날짜로는 신청할 수 없습니다.";
			sMsg = "입고일이 오늘 이전 날짜로는 신청할 수 없습니다.";
			gfn_alert(sMsg,"","A");
			return;
		}
	}
	
	// 입고일자 기준 CHECK : 구매유형별
	if (fn_getDayStatus(ds_type.getColumn(ds_type.rowposition,"dDays"), ds_type.getColumn(ds_type.rowposition,"dTimes"), sNeedDate) == "0")
	{
		var sDate = gds_currdate.getColumn(0, "YMD_TM");
		sDate = gfn_getFormat(gfn_subStr(sDate, 0, 8), "@@@@-@@-@@") + " " + gfn_getFormat(gfn_subStr(sDate, 8, 6), "@@:@@:@@");
	
		sMsg = "▶현재시간 : " + sDate + "\n\n";
		sMsg = sMsg + cbo_type.text + " 구매유형 입고일 기준\n";
		sMsg = sMsg + " => (D_DAY = " + ds_type.getColumn(ds_type.rowposition,"dDays") + "일" + ds_type.getColumn(ds_type.rowposition,"dTimes") + "시)\n"
		sMsg = sMsg + "※ 입고일이 " + gfn_getFormat(sNeedDate,"@@@@-@@-@@") + " 이전 날짜로는\n";
		sMsg = sMsg + "  구매신청을 할 수 없습니다.";
		gfn_alert(sMsg, "", "A");
		return;
	}

	// 구매유형 SET
	ds_search.setColumn(ds_search.rowposition, "schTypeNm", cbo_type.text);	
	
	// 신청번호 초기화
	fv_prNum = "";

	// 신규 Flag Set
	ds_search.setColumn(ds_search.rowposition, "schFlag", "I");
		
	// 신규구매신청 팝업 호출		
	fn_newPrPop("I");
}

