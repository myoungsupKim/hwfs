// 신청일 및 입고일관련 Focus 이벤트 처리
function fn_calDateCheck(gubun)
{
	if(ds_search.getColumn(0,"schType") == "07"){
		if(!gfn_isNull(ds_search.getColumn(0,"schPrDate")) && !gfn_isNull(ds_search.getColumn(0,"schNeedDate"))){
			if(ds_search.getColumn(0,"schPrDate") > ds_search.getColumn(0,"schNeedDate")){
				gfn_alert("신청일자 이(가) 입고일자 보다 큽니다.");
				if (gubun == "1")
					ds_search.setColumn(0,"schPrDate", "");
				else
					ds_search.setColumn(0,"schNeedDate", "");
				return;
			}
		}
	}else if(ds_search.getColumn(0,"schType") == "29" || ds_search.getColumn(0,"schType") == "97" ){
		/*	2015-11-02	요청:PM
							긴급(29)	입고일이 투데이 월과 같으면 오늘 이전일자 입고일 불가.
										입고일이 전월 일경우 전월+말일로 셋팅
										ex)today = 2015-11-02  이면 2015-11-01 선택불가.
										   2015-10-11 이면 10월 31일로 셋팅. 2015-09-20 이면 09월 30일로 셋팅.
		*/
		
		if(gfn_subStr(fv_sToday,0,6) == gfn_subStr(ds_search.getColumn(0,"schNeedDate"),0,6)){
			if(!gfn_isNull(ds_search.getColumn(0,"schPrDate")) && !gfn_isNull(ds_search.getColumn(0,"schNeedDate"))){
				if(ds_search.getColumn(0,"schPrDate") > ds_search.getColumn(0,"schNeedDate")){
					gfn_alert("신청일자 이(가) 입고일자 보다 큽니다.");
					if (gubun == "1")
						ds_search.setColumn(0,"schPrDate", "");
					else
						ds_search.setColumn(0,"schNeedDate", "");
					return;
				}
			}
		}else if(gfn_subStr(ds_search.getColumn(0,"schNeedDate"),0,6)  < gfn_subStr(fv_sToday,0,6)){
			//  입고일자가 없으면 pass
			if(gfn_isNull(ds_search.getColumn(0,"schNeedDate"))) return;
		
			var needDate = gfn_subStr(ds_search.getColumn(0,"schNeedDate"),0,6) + gfn_getLastDateNum(ds_search.getColumn(0,"schNeedDate"));
			if (gubun == "1")
				ds_search.setColumn(0,"schPrDate", needDate);
			else
				ds_search.setColumn(0,"schNeedDate", needDate);
			return;
		}
	}	

}

