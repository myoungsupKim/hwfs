function Tab00_tabpage2_btn_vistAdd_onclick(obj:Button,  e:ClickEventInfo)
{	
	if(ds_pool_req.rowcount == 0)
	{
		gfn_alert("등록할 업장이 있어야 합니다. 조회 후 업장을 선택하고 등록을 진행하시기 바랍니다.");
		return;
	}else{
		
		//컴포넌트 활성화
		Tab00.tabpage2.PersonalSearchComponent_vstSabun.enable = true;
		Tab00.tabpage2.PersonalSearchComponent_cnslSabun.enable = true;
		Tab00.tabpage2.ObjCalendar_vstDate.enable = true;
	
		var cnt = 0;
		for(var i=0; i<ds_visit.rowcount; i++)
		{
			if(ds_visit.getRowType(i) == 2)
			{
				cnt++
			}
		}
		
		if(cnt>0)
		{
			gfn_alert("입력중인 데이터가 있습니다. 등록후 추가 하실 수 있습니다.");
			return;
		}else{
			var nRow = ds_visit.addRow();
			Tab00.tabpage2.cbo_vstDate.enable = false;
			ds_visit.setColumn(nRow, "vstDate", fv_sToday);
		}
	}
}

function Tab00_tabpage2_btn_visitDel_onclick(obj:Button,  e:ClickEventInfo)
{	
	var nRow = ds_visit.rowposition;
	
	if(ds_visit.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	}
	
	if(ds_visit.getRowType(nRow) == 2){
		ds_visit.deleteRow(nRow);
	}else{
		var rtVal = gfn_confirm("해당 방문일지는 등록 되어 있는 데이터 입니다. \n삭제 하시겠습니까?");	// 선택된 자료를 삭제 하시겠습니까?
		if (rtVal == true) 
		{	
			ds_visit.deleteRow(ds_visit.rowposition);
		}else{
			return;
		}
	}	
}

/*----------------------------------------------------------------------------------------------
 * 설명      : [+] 클릭시 인력조회 팝업
 *---------------------------------------------------------------------------------------------*/
function Tab00_a1_btn_poolAdd_onclick(obj:Button,  e:ClickEventInfo)
{
	// 마감이나 자체마감일경우 배정 안되게 처리
	if(ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "F" || ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus") == "E"){
		gfn_alert("마감이 완료되어 인력배정을 할 수 없습니다.");
		return;
	}
	
	if(ds_pool_req.rowcount == 0)
	{
		gfn_alert("등록할 업장이 있어야 합니다. 조회 후 업장을 선택하고 등록을 진행하시기 바랍니다.");
		return;
	}else{
		var arrParam = new Array();
			arrParam[0] = ds_pool_req.getColumn(ds_pool_req.rowposition, "requestUpjangCd");
					
		var dsObj = gfn_dialog("AdoptionPoolSearchPop"					// Dialog ID
								, "X_FSA::AdoptionPoolSearchPop.xfdl"		// Form URL
								, {fv_Contents:arrParam}			// 배열
								, null, null, null, null, null, false, true, -1);
		if(dsObj != null)
		{
			var iFindRowIdx1 = 0;
			
			for(var i=0; i<dsObj.rowcount; i++)
			{	//기존 ds_pool에 중복데이터가 있는지 체크
				//검색을 당할 ds .findRow(검색하고 싶은 컬럼, 검색하고 싶은 행(검색 키워드의 열과,행), 검색하려는 키워드의 시작행, 마지막행)
				
				iFindRowIdx1 = ds_pool.findRow("adoptionNum", dsObj.getColumn(i, "adoptionNum"), 0, -1);
				
				//중복되지 않은 데이터를 행추가하여 받아온 데이터를 한행씩 붙여넣는다.
				if(iFindRowIdx1 == -1)
				{
					var nRow = ds_pool.addRow();
					ds_pool.copyRow(nRow,dsObj,i);
				}
			}
			//trace(ds_pool.saveXML());
			fn_paramFileSet();
		}
	}	

// 	리턴값을  Dataset으로 받을 경우
// 	var dsRtn = new Dataset();
// 	var dsObj = gfn_dialog(strId, strURL, objArgument, nWidth, nHeight, strStyle, nLeft, nTop, isModeless, bShowTitle, strAlign);
// 
// 	dsRtn.copyData(dsObj);
// 	
// 	if(dsRtn.rowcount > 0) {
// 		trace(dsRtn.saveXML());
//	}	
}

