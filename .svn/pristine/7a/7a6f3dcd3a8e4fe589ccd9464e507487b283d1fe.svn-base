/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			if(ds_pool_req.rowcount > 0)
			{
				fn_searchVisit();
				fn_searchUpjangPool();
            }else{
				Tab00.tabpage.AddrSearchComponent00.clearAll();
				Tab00.a1.AddrSearchComponent00.clearAll();
				ds_pool.clearData();
				ds_visit.clearData();
            }
            break;
        case "searchVisit" :
			Tab00.tabpage2.cbo_vstDate.enable = true;
			
			if(ds_visit.rowcount > 0)
			{
				Tab00.tabpage2.PersonalSearchComponent_vstSabun.enable = true;
				Tab00.tabpage2.PersonalSearchComponent_cnslSabun.enable = true;
				Tab00.tabpage2.ObjCalendar_vstDate.enable = true;
			}else{
				Tab00.tabpage2.PersonalSearchComponent_vstSabun.enable = false;
				Tab00.tabpage2.PersonalSearchComponent_cnslSabun.enable = false;
				Tab00.tabpage2.ObjCalendar_vstDate.enable = false;
			}
			Tab00.a1.AddrSearchComponent00.clearAll();
            break;
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			fn_searchVisit();
			fn_search();
			//fn_udtPoolMst();
			break;    
    }
}

