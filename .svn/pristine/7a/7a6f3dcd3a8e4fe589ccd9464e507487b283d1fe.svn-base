/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_save()
{
	//변경사항 체크
	if (gfn_dsIsUpdated(ds_pool_req) == false && gfn_dsIsUpdated(ds_visit) == false && gfn_dsIsUpdated(ds_pool) == false) {
        gfn_alert("msg.save.nochange"); // 변경된 내역이 없습니다.
        return;
    }
	
	// 저장시 Validation Check	
	//if (gfn_validCheck(grd_list, ds_listValid) == false)	return;
	
	//방문일지 INSERT 시 필요한 업장코드와 채용번호를 넣어준다.
	if(ds_visit.getRowType(ds_visit.rowposition) == 2)
	{
		ds_visit.setColumn(ds_visit.rowposition, "vstUpjangCd", ds_pool_req.getColumn(ds_pool_req.rowposition, "requestUpjangCd"));
		ds_visit.setColumn(ds_visit.rowposition, "vstNum", ds_pool_req.getColumn(ds_pool_req.rowposition, "requestNum"));
	}
	
	
	//취소상태가 아닌 상태에서 배정로우가 없으면 접수로 돌리고 있으면 배정으로 돌린다.(확정과 자체마감은 배정인원를 제거하지 못하게 로직)
	var progressStatus = ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus");
	if(progressStatus == "W" || progressStatus == "R" || progressStatus == "A")
	{
		if(ds_pool.rowcount > 0){
			if(progressStatus != "A")
			{
				if(ds_pool_req.getRowType(ds_pool_req.rowcount-1) != "1")
				{
					gfn_alert("배정 된 인원이 존재합니다. 배정인원을 삭제 후 진행하여 주십시오.");
					return;
				}	
				ds_pool_req.setColumn(ds_pool_req.rowposition, "progressStatus", "A");
			}
		}
	}else if(progressStatus == "F"){
		var iFindRowIdx = ds_pool.findRow("goodYn", "", 0, -1);
		if(iFindRowIdx > -1)
		{
			gfn_alert("합격여부 설정이 끝나지 않았습니다.");
			return;
		}
		ds_pool_req.setColumn(ds_pool_req.rowposition, "lastCloseDate", fv_sToday);
	}else{
		ds_pool_req.setColumn(ds_pool_req.rowposition, "lastCloseDate", fv_sToday);
	}
	
	//배정인력 INSERT 시 필요한 업장코드와 채용번호를 넣어준다.
	if(ds_pool.rowcount != 0)
	{
		for(var i=0; i < ds_pool.getRowCount(); i++)
		{
			if(ds_pool.getRowType(i) == 2)
			{
				ds_pool.setColumn(i, "requestUpjangCd", ds_pool_req.getColumn(ds_pool_req.rowposition, "requestUpjangCd"));
				//alert(ds_pool.getColumn(i, "requestUpjangCd"));
				ds_pool.setColumn(i, "requestNum", ds_pool_req.getColumn(ds_pool_req.rowposition, "requestNum"));
				//alert(ds_pool.getColumn(i, "requestNum"));
			}
			
			
			if(ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus")=="R")//접수상태
			{
				ds_pool.setColumn(i, "adoptionStatus", "R");
				
			}else{ 
				if(ds_pool_req.getColumn(ds_pool_req.rowposition, "progressStatus")=="A"){//배정상태
					
					ds_pool.setColumn(i, "adoptionStatus", "A");
					
				}else{//확정 혹은 자체마감
				
					if(ds_pool.getColumn(i, "goodYn")=="G"){
						ds_pool.setColumn(i, "adoptionStatus", "F");
					}else{
						ds_pool.setColumn(i, "adoptionStatus", "R");
					}
				}
			}
			
		}//for문

	}
	
	
		
    var strSvcID    = "save";
    var strURL      = "fs/fsa/adoptionAssign/saveList.xdo";
    var strInDs     = "ds_pool_req=ds_pool_req:A ds_visit=ds_visit:U ds_pool=ds_pool:A";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

