/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            //trace(ds_list.saveXML());
            if ( tab_claimCnt.tabindex == 5 ) {
				if ( ds_list6.rowcount == 0 ) {
					gfn_alert("조회된 데이터가 없습니다.");
					return;
				}
				tab_claimCnt.tpg_report.grd_list6.enableredraw = false;
				tab_claimCnt.tpg_report.grd_list6.createFormat();
				tab_claimCnt.tpg_report.grd_list6.setBandProperty("Head", "size", 30);
				tab_claimCnt.tpg_report.grd_list6.appendContentsRow("summ");
				
				for ( var i=ds_list6.getColCount()-1; i>=0; i-- ) {
					var  strText = tab_claimCnt.tpg_report.grd_list6.getCellProperty("head", i, "text");
					if ( isNumeric(strText) ) {
						var headTxt = "";
						var strLength = strText.toString().length;
						if ( strLength == 8 ) {
							var week = gfn_getDayName(strText);
							headTxt = strText.substr(4,2)+"."+strText.substr(6,2)+" ("+week+")";
							
						} else if ( strLength == 6 ) {
							headTxt = "'"+strText.substr(2,2)+"."+strText.substr(4,2);
						}
						tab_claimCnt.tpg_report.grd_list6.setCellProperty("head", i, "text", headTxt);
						
						switch ( week ) {
							case "토" :
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("head", i, "color", "dodgerblue");
								break;
								
							case "일" :
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("head", i, "color", "orangered");
								break;
							default :
								
								break;
						} 
						tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "mask", 0);
						
						tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "displaytype", "number");
						tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "padding", "0 6 0 0");
						tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(i, "size", 85);
						tab_claimCnt.tpg_report.grd_list6.setCellProperty("summ", i, "expr", "getSum('"+ds_list6.getColumnInfo(i).name+"')");
						tab_claimCnt.tpg_report.grd_list6.setCellProperty("summ", i, "displaytype", "number");
						tab_claimCnt.tpg_report.grd_list6.setCellProperty("summ", i, "padding", "0 6 0 0");
					} else {
						switch ( strText ) {
							case "total" :
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("head", i, "text", "합계");
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "displaytype", "number");
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "padding", "0 6 0 0");
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("summ", i, "expr", "getSum('total')");
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("summ", i, "displaytype", "number");
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("summ", i, "padding", "0 6 0 0");
								break;
							case "claimType" :
								tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(i, "size", 0);
								break;
								
							case "cls2Nm2" :
							case "cls2Nm" :
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "align", "left");
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "padding", "0 0 0 6");
								
								if ( strText == "cls2Nm2" ) {
									tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(i, "size", 130);
									
								} else if ( strText == "cls2Nm" ) {
									tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(i, "size", 100);
									tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "suppress", 2);
									//tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "expr", "getRowLevel(currow) == 1 ? '소계' : (getRowLevel(currow) == 2 ? '' : cls2Nm)");
								}
																
								break;
							case "cls1Nm" :
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "suppress", 1);
								//tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "expr", "getRowLevel(currow) == 2 ? '합계' : (getRowLevel(currow) == 1 ? getColumn(currow-1, 'cls1Nm') : cls1Nm)");
								tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "expr", "getRowLevel(currow) == 1 ? '합계' : cls1Nm");
								break;
							case "cls2Cd" :
							case "cls1Cd" :
								tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(i, "size", 0);
								break;
						}
						
						if ( i == 0 ) {
							tab_claimCnt.tpg_report.grd_list6.setCellProperty("head", i, "text", "유형");
							tab_claimCnt.tpg_report.grd_list6.mergeContentsCell("head", 0, 0, 0, 5, 0, false);
							tab_claimCnt.tpg_report.grd_list6.mergeContentsCell("summ", 0, 0, 0, 5, 0, false);
							tab_claimCnt.tpg_report.grd_list6.setCellProperty("summ", i, "text", "총계");
							tab_claimCnt.tpg_report.grd_list6.setCellProperty("summ", i, "align", "center middle");
							
							// 합계 부분까지 틀고정
							tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(0, "band", "left");	
							tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(1, "band", "left");	
							tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(2, "band", "left");	
							tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(3, "band", "left");	
							tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(4, "band", "left");	
							tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(5, "band", "left");	
							tab_claimCnt.tpg_report.grd_list6.setFormatColProperty(6, "band", "left");	
							
							
						}
					}
					tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "background", "EXPR(getRowLevel(currow)==1?'#fddcc9ff':(getRowLevel(currow) == 2 ? '#fffbdeff' :  ''))");
					tab_claimCnt.tpg_report.grd_list6.setCellProperty("body", i, "background2", "EXPR(getRowLevel(currow)==1?'#fddcc9ff':(getRowLevel(currow) == 2 ? '#fffbdeff' :  ''))");
					
				}
				ds_list6.keystring.current  = "G:cls1Cd,S:cls2Cd";
				tab_claimCnt.tpg_report.grd_list6.enableredraw = true;
            }
            ds_list6.rowposition = -1;
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;    
            
        case "searchCenter" :
			cbo_center.index = 0;
            break;    
    }
}


function fn_checkTerm() {
	var rtn = false;
	switch ( tab_claimCnt.tabindex ) {
		case 2 :
		case 3 :
		case 4 :
			if ( ds_search.getColumn(0, "schFromDate") < "20180201" 
				&& ds_search.getColumn(0, "schToDate") >= "20180201" ) {
				gfn_alert("2018년 2월 1일 기준으로 클레임 유형이 변경되어 기준일을 포함하여 조회할 수 없습니다.");
				rtn = false;
			} else {
				rtn = true;
			}
			
			break;
		case 5 :
		case 6 :
			if ( ds_search.getColumn(0, "schDtGnb") == "A" ) { // A:입고일 B:접수일
				rtn = true;
			} else {
				rtn = true;
			}
			break;
			
		default :
			rtn = true;
			break;
	}
	
	if ( rtn ) {
		if ( ds_search.getColumn(0, "schFromDate") >= "20180201" ) {
			ds_search.setColumn(0, "schType", "NEW");
		} else {
			ds_search.setColumn(0, "schType", "OLD");
		}
	}
	return rtn;
}
