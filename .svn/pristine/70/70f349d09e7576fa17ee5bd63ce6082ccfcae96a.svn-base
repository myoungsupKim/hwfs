    <Script type="xscript4.0"><![CDATA[include "U_lib::lib_conversionCom.xjs";
var GL_gubun = "PERSON";

function btn_Person_OnClick(obj:Button, e:ClickEventInfo)
{
	if(GL_gubun != "PERSON")
	{
		All_Clear();
		GL_gubun	 			= "PERSON";
		cbo_ManWoman.index 		= '0';
		cbo_Sickness.index 		= '0';
		cbo_PregnancyTerm.index = '0';
	}
	ed_Name.setFocus();
	ed_Type.value					= "N";
	hide_f_Gubun.value			= "PERSON";
	Static19.style.background.color 		= "#D1E7EF";
	Static21.style.background.color 		= "#FFFFFF";
	Static04.style.background.color 		= "#D1E7EF";
	//btn_Person.style.background.color  		= "#8CB0D9";
	//btn_PregnantWomen.style.background.color  	= "#E5F7F6";
	st_BeforeWeight.enable  = false;
	st_WaistSize.enable   	= true;
	st_HipsSize.enable  	= true;
	st_PregnancyTerm.enable = false;
	Static15.enable	 		= false;
	Static16.enable	 		= true;
	Static17.enable	 		= true;
	ed_BeforeWeight.enable 	= false;
	cbo_PregnancyTerm.enable= false;
	ed_WaistSize.enable 	= true;
	ed_HipsSize.enable 	 	= true;	
	cbo_PregnancyTerm.enable= false;
	cbo_ManWoman.enable     = true;
	
	ds_code.filter("CODE<'005'");
	tab_Item.tab1.cbo_Sickness.index = 0;
}

function btn_PregnantWomen_OnClick(obj:Button, e:ClickEventInfo)
{
	if(GL_gubun !="GG")
	{
		All_Clear();
		GL_gubun 	 		="GG";
		cbo_ManWoman.index 	= '0';
		cbo_Sickness.index 	= '0';
	}
	ed_Name.setFocus();
	ed_Type.value					= "P";
	hide_f_Gubun.value			= "GG";
	//btn_Person.style.background.color  		= "#E5F7F6";
	//btn_PregnantWomen.style.background.color  	= "#8CB0D9";
	st_BeforeWeight.enable  = true;
	st_WaistSize.enable   	= false;
	st_HipsSize.enable  	= false;
	st_PregnancyTerm.enable = true;
	Static15.enable	 		= true;
	Static16.enable	 		= false;
	Static17.enable	 		= false;
	Static19.style.background.color 		= "#FFFFFF";
	Static21.style.background.color 		= "#D1E7EF";
	Static04.style.background.color 		= "#D1E7EF";
	ed_BeforeWeight.enable 	= true;
	cbo_PregnancyTerm.enable= true;
	ed_WaistSize.enable 	= false;
	ed_HipsSize.enable 	 	= false;
	cbo_PregnancyTerm.enable= true;
	cbo_ManWoman.value      = "F";
	cbo_ManWoman.enable     = false;
	
	ds_code.filter("CODE>='005'");
	tab_Item.tab1.cbo_Sickness.index = 0;
}

function All_Clear()
{
	ed_Name.value		= "";
	ed_Age.value			= "";
	ed_Height.value		= "";
	ed_Weight.value		= "";
	ed_BeforeWeight.value= "";
	ed_WaistSize.value	= "";
	ed_HipsSize.value	= "";
}

function Form_OnLoad(obj:Form, e:ActivateEventInfo)
{
	ed_Name.setFocus();
	hide_f_Gubun.value = "PERSON";
	ed_Type.value 	 = "N";
}

 
