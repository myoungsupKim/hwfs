function fn_SaveTrxData(real_RMT_ID)
{

    if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"))== "0"){    

		g_Message("EE", "업장을 선택하여 주십시요.\n" +
						"확인하여 주십시요.");        
        edt_UPJANGQuery.setFocus();
        return false;
    }
    
    if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_DATE"))== 0){    

		g_Message("EE", "이체일자가 선택되어 있지 않습니다.\n" +
						"확인하여 주십시요."); 						       
        me_RMT_DATE.setFocus();
        return false;
    }    

    f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID");
    
    if (numFormat(real_RMT_ID)== "0"){
    
		ds_cond.clear();
		ds_cond.addColumn("MAS_RMT_S","INT",256);
					
		// action 정보 초기화
		fsp_clear(this);

		// 서버에서 조회할 정보 추가
		fsp_addSearch(this, "ma/mas:MAS10090E_S002");

		// 서버 호출
		http.Sync = true;
		fsp_callService(this, "", "", "", ds_cond.name + "=ds_list","","");
		http.Sync = false;
		
		real_RMT_ID = ds_cond.getColumn(ds_cond.rowposition, "MAS_RMT_S");
		ds_listInput.setColumn(ds_listInput.rowposition, "RMT_ID", real_RMT_ID);
		
		f_Saved_ID  = 0;
		f_NewDataCheck = true;
    }
    
		
	//이체 대체###########################
	for (i=0;i<ds_MAS_RMT_LINE.rowcount;i++){
		
			ds_MAS_RMT_LINE.setColumn(i, "RMT_ID", real_RMT_ID);
			ds_MAS_RMT_LINE.setColumn(i, "LINE_DATE", ds_listInput.getColumn(ds_listInput.rowposition, "RMT_DATE"));						
			
	}
	//이체 대체###########################
	   
    return true;
}
