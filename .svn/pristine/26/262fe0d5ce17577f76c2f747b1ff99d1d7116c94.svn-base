		}
		
	}


	
	if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID"))!= "0"){
	
		edt_RMT_GUBUN.enable       = false;
		edtBtn_RMT_GUBUN.enable    = false;			
		
