		edt_RMT_FEE.enable         = false;		
		//수정불가위해세팅###################################	
	}	

}



//각 구분별 대체마감체크############################################################
function fn_GetALT_CLOSE(objOutDs, real_RMT_ID)
{

    //대체마감체크#####################################################################
    var str_CloseErr = "";
    
    objOutDs.clear();       
    objOutDs.addColumn("CMS_YN","STRING",256);	     
    objOutDs.addColumn("CLS_YN","STRING",256);	
      
    
	// action 정보 초기화 
    fsp_clear(this);  
	
    // 서버에서 조회할 정보 추가 
	fsp_addSearch(this, "ma/mas:MAS10090E_S003");
	
	//alert(str_ACCTCD);
	//alert(str_DATE_STD);
	// 서버 호출 
	http.Sync = true;
	fsp_callService(this, "", "", "", objOutDs.name + "=ds_list","RMT_ID=" + real_RMT_ID
	                                                           ,"");	
	http.Sync = false;
	

	//if (objOutDs.GetColumn(objOutDs.row, "CMS_YN") == "Y")   str_CloseErr = "";			    
	
	if (objOutDs.getColumn(objOutDs.rowposition, "CLS_YN") == "Y")   str_CloseErr = "마감된 데이터는\n수정/삭제할 수 없습니다.";				
	
	if (gfn_length(str_CloseErr )!= 0){
	
		g_Message("EE", str_CloseErr + "\n" +
						"확인하여 주십시요.");			 
		return false;        
	}

	return true;
	//대체마감체크#####################################################################
 
}			 
