function before_SaveCheck(str_Gubun)
{
        
		//권한에 따른 저장/삭제 가능체크#########################################################################
		if (
				(g_AuthLevel != "99") &&
				(!fn_GetSaveAuthBy_AuthLevel(ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG")))
		    )  return false;		    
		//권한에 따른 저장/삭제 가능체크#########################################################################
	
	          
        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
        
        f_Saved_ID = 0;
        f_NewDataCheck = false;
        
        //가수금 대체 비교 팝업창을 띄운다.
		ds_MAS_RMT_LINE.clearData();				       
        /*
        업무구분별로 addAction 지정세팅##########################################        
        */
		
		
 		if (ds_listInput.rowcount == 0){

			return;
		}

	    if (str_Gubun == "Master"){

			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "UPJANG"))== "0")
			{
			
				g_Message("EE", "업장을 지정하여 주십시요.");
				edt_UPJANGQuery.setFocus();
				return false;
			}
		
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_DATE"))== 0)
			{
			
				g_Message("EE", "이체일자가 누락되었습니다.");
				me_RMT_DATE.setFocus();
				return false;
			}
	        
	        //입력일때 Confirm체크##################################
	        if ( 
	             (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID"))== "0") &&
	             (chk_Auto.value == "0") 
	           )
	        {
				if (!g_Confirm("02")) return false;	             
	        }     
	        //입력일때 Confirm체크##################################
	             
	             
	        //수정일때 Confirm체크##################################
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID"))!= "0")
               )
            {	        
				g_Message("EE", "기존 이체데이터를 수정하려면\n삭제 후 신규 등록해야만 합니다.\n" +
				                "확인하여 주십시요.");                
				return false;   
			}
	        //수정일때 Confirm체크##################################
	        
			//각 구분별 대체 마감체크##############################
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID"))!= "0")
               )
            {			
				if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID"))) return false;
			}
			//각 구분별 대체 마감체크##############################	        
					        
					        
			//마감체크###############################################
			if (!fn_CheckMAClosing(ds_cond, edt_UPJANGQuery.userdata, me_RMT_DATE.value, "002"))
				return false;                
			//마감체크###############################################
			
								        
			//마감체크###############################################
			if (!fn_CheckGAClosing(ds_cond, edt_UPJANGQuery.userdata, me_RMT_DATE.value, "100"))
				return false;                
			//마감체크###############################################
			
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_GUBUN"))== 0){
				
				g_Message("EE", "이체유형이 누락되었습니다.\n" +
				                "확인하여 주십시요.");
                edt_RMT_GUBUN.setFocus();
				return false;                
			}
			
			
			if (gfn_length(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ACNTCD"))== 0){			
									
				g_Message("EE", "계좌번호가 누락되었습니다.\n" +
								"확인하여 주십시요.");
				edt_RMT_ACNTCD.setFocus();
				return false;     	
			}
			
			
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_AMT"))== "0"){			
									
				g_Message("EE", "금액이 누락되었습니다.\n" +
								"확인하여 주십시요.");
				edt_RMT_AMT.setFocus();
				return false;     	
			}		

		    // 이체할 입금액 비교 작업 시작 ###################################################
		    var real_RmtAmt = toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_AMT")) + toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_FEE"));
						
			if(!fn_OpenLineWindow(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID"), real_RmtAmt)) return false;

			// 이체할 입금액 비교 작업 시작 ###################################################
			
			//RMT_ID 지정#################################################################
            if (!fn_SaveTrxData(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID"))) return false;
		}			

        
		//저장#############################################################
	    if (str_Gubun == "Master"){

		    var real_RmtAmt = toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_AMT")) + toNumber(ds_listInput.getColumn(ds_listInput.rowposition, "RMT_FEE"));
			//각 구분별 대체 마감체크##############################저장전 중복확인
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(f_Saved_ID)!= "0")
               )
            {			
				if (!fn_GetALT_CLOSE(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID"))) return false;
			}
			//각 구분별 대체 마감체크##############################저장전 중복확인
			
			
			//가수금 잔액체크#######################################저장전 중복확인
			if (numFormat(ds_listInput.getColumn(ds_listInput.rowposition, "SUS_SEQ"))!= "0"){
			
				if(!fn_GetIsSupenseJanAmtOK(ds_cond, ds_listInput.getColumn(ds_listInput.rowposition, "RMT_SUS_SEQ"), ds_listInput.getColumn(ds_listInput.rowposition, "RMT_AMT"))) return false;
			}
			//가수금 잔액체크#######################################저장전 중복확인
						
			// action 정보 초기화
			fsp_clear(this);	  		
										    		        
	    
            //기존 라인 및 이체 데이터 삭제.          
            if (
                 (ds_listInput.rowcount > 0) &&
                 (numFormat(f_Saved_ID)!= "0")
               )
            {
            
				fsp_addSingle(this, "ma/mas:MAS10090E_D002");
				fsp_addSingle(this, "ma/mas:MAS10090E_D001");
            } 
       
            
            
            //이체  저장
            if (real_RmtAmt > 0){
            				
				fsp_addSingle(this, "ma/mas:MAS10090E_I001");
            }                  		
			
            //가수금 대체 비교 팝업창을 띄운다.
            if (ds_MAS_RMT_LINE.rowcount > 0){
            
				fsp_addMulti(this, "ma/mas:MAS10090E_I002");
            }
                         		
								
			f_Saved_ID = ds_listInput.getColumn(ds_listInput.rowposition, "RMT_ID");
			fsp_callService(this, "", "", "ds_cond=ds_listInput:A ds_MAS_RMT_LINE=ds_MAS_RMT_LINE:A", "",  "", "fn_AfterSaveSearch()");
	    }
	   
	    
		return true;
}


