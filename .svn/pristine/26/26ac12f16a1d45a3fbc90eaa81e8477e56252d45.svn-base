/*----------------------------------------------------------------------------------------------
 * 라인버튼1 클릭 이벤트
 *---------------------------------------------------------------------------------------------*/
function btn_line1Item_onclick(obj:Button,  e:ClickEventInfo)
{
	//채권, 선수금, 매출 팝업호출
	var dsObj = fn_callOccurObjPop(ds_lineData1, "1");
	
	/* 팝업에서 발행대상 선택 후 확인버튼 클릭   : dsObj != null && dsObj.rowcount > 0
	 * 팝업에서 발행대항 미선택 후 확인버튼 클릭 : dsObj != null && dsObj.rowcount == 0
	 * 팝업에서 취소버튼 클릭 : dsObj == null
	 */
	if(dsObj != null){
		if(dsObj.rowcount > 0){
			if(rdo_publishClass.value == "1"){
				//라인의 발행대상 Dataset에 복사
				ds_lineData1.clearData();
				ds_lineData1.copyData(dsObj);
				//리턴받은 데이터 Dataset에 매핑
				//fn_settingLineData(ds_taxShetDtl1, dsObj, "1");
			}else{
				//금액비교
				ds_lineData1.clearData();
				ds_lineData1.copyData(dsObj);				
			//	fn_compareAmt(ds_taxShetDtl1, dsObj, ds_lineData1);
			}
		}else{
			ds_lineData1.clearData();
			ds_taxShetDtl1.clearData();
		}
		
		//공급가액,세액 세팅
		fn_calTotalAmtAndTax();
	}
}

