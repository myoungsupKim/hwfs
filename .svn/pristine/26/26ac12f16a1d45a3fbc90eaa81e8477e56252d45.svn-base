/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
            break;
        
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			this.close();
            break;    
        case "searchHanwha" :
			if(ds_hanwha.rowcount > 0){
				ds_taxShetMst.setColumn(0, "supplierBizNo", ds_hanwha.getColumn(0, "saupno"));
				ds_taxShetMst.setColumn(0, "supplierBizNm",ds_hanwha.getColumn(0, "custnm"));
				ds_taxShetMst.setColumn(0, "supplierCeonm", ds_hanwha.getColumn(0, "drname1"));
				ds_taxShetMst.setColumn(0, "supplierAddr", ds_hanwha.getColumn(0, "addr") +' '+ ds_hanwha.getColumn(0, "detailAddrEnc"));
				ds_taxShetMst.setColumn(0, "supplierBizType", ds_hanwha.getColumn(0, "upTae"));
				ds_taxShetMst.setColumn(0, "supplierBizCat", ds_hanwha.getColumn(0, "upJong"));
			}
			break;
		case "searchCust" :
			if(rdo_publishClass.value == "1"){
				ds_taxShetMst.setColumn(0, "custNm", ds_hanwha.getColumn(0, "custnm"));
				//개인 거래처인 경우 구분값 02로 설정 2017.12.20 김호석(주민등록번호로 세금계산서 발행) 01: 개인, 02:일반 , 03:외국인(현재 사용안함)
				//alert(ds_hanwha.getColumn(0, "custtype"));
				if (ds_hanwha.getColumn(0, "custtype") == "010") {
					ds_taxShetMst.setColumn(0, "rcvrBizNoClass","02");
				}
				if(!gfn_isNull(fv_jongSaupNo) && ds_taxShetMst.getColumn(0, "muCd") == "2004"){
					ds_taxShetMst.setColumn(0, "rcvrSubUpjangNum", fv_jongSaupNo);
				}else{
					ds_taxShetMst.setColumn(0, "rcvrSubUpjangNum", ds_hanwha.getColumn(0, "subUpjangNum"));				
				}
				ds_taxShetMst.setColumn(0, "rcvrBizNo", ds_hanwha.getColumn(0, "saupno"));
				ds_taxShetMst.setColumn(0, "rcvrBizNm", ds_hanwha.getColumn(0, "custnm"));
				ds_taxShetMst.setColumn(0, "rcvrCeonm",ds_hanwha.getColumn(0, "drname1"));
				
				ds_taxShetMst.setColumn(0, "rcvrAddr", ds_hanwha.getColumn(0, "addr") +' '+ ds_hanwha.getColumn(0, "detailAddrEnc") +' '+ ds_hanwha.getColumn(0, "etcAddr"));
				ds_taxShetMst.setColumn(0, "rcvrBizType", ds_hanwha.getColumn(0, "upTae"));
				ds_taxShetMst.setColumn(0, "rcvrBizCat", ds_hanwha.getColumn(0, "upJong"));			
			}
			
			//최신 고객 공급받는 담당자 세팅
			if(ds_taxShetMst.getColumn(0, "muCd") != "2004"){
				fn_selectInchargeInfo("", ds_hanwha.getColumn(0, "custcd"));
			}
			break;
		case "searchCcSubUpjang"	: //종사업장 번호 조회
			ds_taxShetMst.setColumn(0, "supplierSubUpjangNum", ds_ccSubUpjang.getColumn(0,"subUpjangNum"));
		//	if(rdo_publishClass.value == "1"){	//순발행일경우만 다시 셋팅
		//		ds_taxShetMst.setColumn(0, "supplierSubUpjangNum", ds_ccSubUpjang.getColumn(0,"subUpjangNum"));
		//	}
			//if(!gfn_isNull(ds_taxShetMst.getColumn(0, "newhghtUpjang"))){
			if(gfn_isNull(ds_taxShetMst.getColumn(0, "custCd"))){
				upSchCustCd = "";
			}else{
				upSchCustCd = ds_taxShetMst.getColumn(0, "custCd");
			}
			if(!gfn_isNull(ds_taxShetMst.getColumn(0, "newhghtUpjang"))){
				if(ds_taxShetMst.getColumn(0, "muCd") != "2004"){
					fn_selectInchargeInfo(ds_taxShetMst.getColumn(0, "newhghtUpjang"), upSchCustCd);
				}else{
					fn_selectInchargeInfo(ds_taxShetMst.getColumn(0, "upjangCd"), upSchCustCd);
				}
			}


			//}
			break;
    }
}


