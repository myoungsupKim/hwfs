/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	//팀장권한 이상이면 본사발행 체크 가능 ( 홈플러스의 경우 본사로 발행해야 한다고 함)
	//FS사업부인 경우만 보이도록 수정 2018.03.06 김호석	
	fv_muCd = arrArg[0];		
	if(fv_muCd == "2002"){
		if (g_AuthLevel >= "16")   {
			chk_Homeplus.visible = true;
		}
	}
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "";
	
	//Dataset 초기화
	var nRow = ds_taxShetMst.addRow();
	ds_amt.addRow();			//공급가액
	ds_tax.addRow();			//세액
	
	//부가세코드 검색
	fn_selectVatCdList();
	
	//Combo
	SCCombo.bind(this, "ComCode", "rdo_publishClass", ds_publishClass, {groupCode:"PUBLISH_CLASS", useYn:"Y", defVal:"1"}); 				//발행구분
	SCCombo.bind(this, "ComCode", "cbo_rcptBillingClass", ds_rcptBillingClass, {groupCode:"RCPT_BILLING_CLASS", useYn:"Y", defVal:"02"}); 	//영수/청구구분
	SCCombo.bind(this, "ComCode", "cbo_taxShetKind", ds_taxShetKind, {groupCode:"TAX_SHET_KIND", useYn:"Y", set1:"1", defVal:"01"}); 				//세금계산서종류
	SCCombo.bind(this, "ComCode", "cbo_taxShetClsfi", ds_taxShetClsfi, {groupCode:"TAX_SHET_CLSFI", useYn:"Y", defVal:"01"}); 				//세금계산서분류
	SCCombo.bind(this, "ComCode", "cbo_vatZeroUpjang", ds_vatZeroUpjang, {groupCode:"VAT_ZERO_UPJANG", useYn:"Y"}); 	//영수/청구구분

	//Popup
	//SCPopup.bind(this, "CcCode", "edt_ccNm", "btn_cc",	null, {}, {after:"fn_popCcCdAfter"}); 					//업장(CC) 팝업
	
	//첨부파일
	//ObjAtchFile.setSingleFile(true);
	ObjAtchFile.setDataset("ds_file");	//DataSet연결
	ObjAtchFile.setExtention("xml");
	
	//첨부파일 버튼제어(발행구분 정발행 -> 비활성)
	btn_fileAdd.enable = false;
		
	//발행구분 값 셋팅
	ds_taxShetMst.setColumn(0, "publishClass", rdo_publishClass.value);	//1:순발행 2:역발행
	
	//파라미터 변수
	fv_muCd = arrArg[0];
	fv_tmCd = arrArg[1];
	fv_tmNm = arrArg[2];
	fv_ccCd = arrArg[3];

	
	//사업부, 부서(팀), CC 설정
	TmSearchComponent01.applyAuthLevel(MuSearchComponent01);
	UpjangSearchComponent01.setForcedRoleSet("CostCenter");
	UpjangSearchComponent01.applyAuthLevel(TmSearchComponent01);
	UpjangSearchComponent00.applyAuthLevel(TmSearchComponent01);    //업장	
	//TmSearchComponent01.setApplyAuth(false);
	//UpjangSearchComponent00.setApplyAuth(false);
	//초기값 세팅
	fn_clearSearch();
	
	//달력 초기화
	fn_chgCalDate();
		
	cal_publishDate.fn_initMonthCal(this, fv_taxDay, "fn_calendarFromCallBack", ds_taxShetMst, "publishDate", "Y");						//발행일자
	//10일 이전이면 전달 말일로 셋팅
	cal_fiscalDate.fn_initMonthCal(this, fv_taxDay, "fn_fiscalDateAfter", ds_taxShetMst, "fiscalDate", "Y");		//세금계산서일자
	
	//작성일자 오늘일자로 셋팅
	edt_writeYyyy.value = fv_taxDay.substring(0, 4);
	edt_writeMm.value = fv_taxDay.substring(4, 6);
	edt_writeDd.value = fv_taxDay.substring(6, 8);	
}

/*----------------------------------------------------------------------------------------------
 * 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_clearSearch(){
	MuSearchComponent01.setMuCode(arrArg[0]);
	ds_taxShetMst.setColumn(0, "muCd", arrArg[0]);	//사업부코드
	ds_taxShetMst.setColumn(0, "tmCd", arrArg[1]);	//부서(팀)코드
	ds_taxShetMst.setColumn(0, "tmNm", arrArg[2]);	//부서(팀)명
	ds_taxShetMst.setColumn(0, "ccCd", arrArg[3]);	//업장(CC)코드
	ds_taxShetMst.setColumn(0, "ccNm", arrArg[4]);	//업장(CC)명

	if (ds_taxShetMst.getColumn(0, "muCd") == "2002" && g_Cc_Cd == arrArg[3]){
		ds_taxShetMst.setColumn(0, "upjangCd", g_Upjang);	//업장코드
		ds_taxShetMst.setColumn(0, "upjangNm", g_UpjangNm);	//업장명						
	}else{
		ds_taxShetMst.setColumn(0, "upjangCd", "");	//업장코드
		ds_taxShetMst.setColumn(0, "upjangNm", "");	//업장명		
	}
	
	if(!gfn_isNull(ds_taxShetMst.getColumn(0, "ccCd"))){
		ds_taxShetMst.setColumn(0, "newhghtUpjang", ds_taxShetMst.getColumn(0, "ccCd"));			//신고사업장코드
		ds_taxShetMst.setColumn(0, "newhghtUpjangNm", ds_taxShetMst.getColumn(0, "ccNm"));	//신고사업장명
		//신고사업장 정보 검색
		//fn_selectUpjangAndCustInfo("upjang");
	}
	//CC코드에 종사업장 번호 조회
	fn_searchCcSubUpjang(arrArg[3]);

	if(fv_muCd == "2004"){
		ds_taxShetMst.setColumn(0, "vatCd", "10");						//부가세구분선택(Default:과세매출일반)
		ds_taxShetMst.setColumn(0, "rcptBillingClass", "02");			//영수/청구 구분(Default:청구)
		comp_upjang.visible=true;
		stc_upjangTitle.text="본사";
		stc_subUpjangTitle.text="하위 업장";
		comp_mainUpjang.visible=true;
		UpjangSearchComponent00.visible=false;
		schCust.enable=false;
	}else if(fv_muCd == "2005"){	//구매 물류 추가
		ds_taxShetMst.setColumn(0, "vatCd", "10");						//부가세구분선택(Default:과세매출일반)
		ds_taxShetMst.setColumn(0, "rcptBillingClass", "02");			//영수/청구 구분(Default:청구)	
	}else{
		if(fv_muCd == "2003"){	//외식은 업장 안보이게 처리
			comp_mainUpjang.visible=false;
			UpjangSearchComponent00.visible=false;
			stc_upjangTitle.text="";
		}
		ds_taxShetMst.setColumn(0, "vatCd", "20");						//부가세구분선택(Default:과세매출일반)
		ds_taxShetMst.setColumn(0, "rcptBillingClass", "01");			//영수/청구 구분(Default:영수)
	}
	
	//한화 본사 정보 조회
	fn_searchHanwha();
	ds_taxShetMst.setColumn(0, "smsDispatchYn", "Y");		//SMS전송여부(Default:예)
	ds_taxShetMst.setColumn(0, "taxShetClsfi", "01");		//세금계산서분류(Default:일반)
}
