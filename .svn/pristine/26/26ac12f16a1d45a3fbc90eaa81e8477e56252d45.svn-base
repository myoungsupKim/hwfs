function fn_selectInchargeInfo(newhghtUpjang, custCd){
	var strSvcID    = "incharge";

	if(newhghtUpjang != "" && custCd != ""){
		strSvcID	= "incharge_all";	
	}else if(custCd != ""){
		strSvcID	= "incharge_cust";
	}
	if(gfn_isNull(ds_taxShetMst.getColumn(0, "subUpjang"))){
		subUpjangCd = "";
	}else{
		subUpjangCd = ds_taxShetMst.getColumn(0, "subUpjang");
	}
	
    var strURL      = "sm/stm/taxInvoiceMgntHq/selectInchargeInfo.xdo";
    var strInDs     = "";
    var strOutDs    = "ds_incharge=ds_incharge";
    var strArg      = "newhghtUpjang="+wrapQuote(newhghtUpjang)+" custCd="+ wrapQuote(custCd) +" subUpjang="+ wrapQuote(subUpjangCd);
    var strCallback = "fn_callBackIncharge";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

