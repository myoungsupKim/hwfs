/*----------------------------------------------------------------------------------------------
 * 라인의 발행대상 Dataset을 하나의 Dataset으로 합치기
 *---------------------------------------------------------------------------------------------*/
 function fn_mergeLineDataset(){
	ds_lineData.clearData();

	if(ds_lineData1 != null && ds_lineData1.rowcount > 0){
		ds_lineData.appendData(ds_lineData1);
	}
	if(ds_lineData2 != null && ds_lineData2.rowcount > 0){
