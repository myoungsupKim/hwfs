		//부가세 기준으로 셋팅되게 수정:2015-12-09 김명호
		if (ds_taxShetMst.getColumn(0, "vatCd") == "10" || ds_taxShetMst.getColumn(0, "vatCd") == "13" || ds_taxShetMst.getColumn(0, "vatCd") == "40") {
			ds_taxShetMst.setColumn(0, "sysClass", "2100");
		}else{
			ds_taxShetMst.setColumn(0, "sysClass", "2200");	
		}	
	}else{
		ds_taxShetMst.setColumn(0, "sysClass", "2200");
	}
	//ds_taxShetMst.setColumn(0, "sysClass", "5000");
}

