		ds_lineData2.clearData();
		ds_lineData3.clearData();
		ds_lineData4.clearData();
		ds_taxShetDtl1.clearData();
		ds_taxShetDtl2.clearData();
		ds_taxShetDtl3.clearData();
		ds_taxShetDtl4.clearData();
		
		if((!gfn_isNull(cls.custcd)) && (cls.custcd != "0")){
			ds_taxShetMst.setColumn(0,"custCd", cls.custcd);
			ds_taxShetMst.setColumn(0,"custNm", cls.custnm);
			fn_searchCust(cls.custcd);
		}
		
		if(ds_taxShetMst.getColumn(0, "muCd") == "2004"){
			comp_upjang.setUpjangCode("");
			comp_upjang.setUpjangName("");
		}
	}	
	
	fn_ccCd_changed(obj, cls);	//종사업장 조회
	
	if(!gfn_isNull(cls.tmCd)){
		ds_taxShetMst.setColumn(0,"tmCd", cls.tmCd);
		ds_taxShetMst.setColumn(0,"tmNm", cls.tmNm);
	}
	ds_taxShetMst.setColumn(0,"ccCd", cls.ccCd);
	ds_taxShetMst.setColumn(0,"ccNm", cls.ccNm);
	if(rdo_publishClass.value == "1"){		//정발행일때만 다시 셋팅
		fn_chgCalDate();
	}
	return true;
}
