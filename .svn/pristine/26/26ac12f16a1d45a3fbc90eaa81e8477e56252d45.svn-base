/*----------------------------------------------------------------------------------------------
 * 부가세구분에 따른 세액계산
 * ds			: 적용할 데이터셋
 * colApplyAmt	: 발행금액
 * colSupplyAmt : 공급가액 컬럼명
 * colVatAmt    : 세액 컬럼명
 *---------------------------------------------------------------------------------------------*/
function fn_calVatAmt(ds:Dataset, colApplyAmt, colSupplyAmt, colVatAmt){
	var vatRateYn = ds_taxShetMst.getColumn(0, "vatRateYn");
	var applyAmt = 0;
	var supplyAmt = 0;
	var vatAmt = 0;
	
	for(var i=0; i<ds.rowcount; i++){
		if(vatRateYn == "Y"){
			applyAmt = ds.getColumn(i, colApplyAmt);
			supplyAmt = Math.round(Number(applyAmt) / 1.1);
			vatAmt = Number(applyAmt) - Number(supplyAmt);
		}else{
			applyAmt = ds.getColumn(i, colApplyAmt);
			supplyAmt = applyAmt;
			vatAmt = 0;
		}
		ds.setColumn(i, colSupplyAmt, supplyAmt);
		ds.setColumn(i, colVatAmt, vatAmt);
	}
	
	fn_calTotalAmtAndTax();
}

