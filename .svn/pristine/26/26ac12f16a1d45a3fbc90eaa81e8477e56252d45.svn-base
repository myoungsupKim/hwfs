//달력 셋팅
function fn_calTaxDateChange(sDay){

	//12월 세금계산서 발행 제한
	/*
	if(fv_muCd == "2003"){
		//pass
	} else {
		if ( ((ds_taxShetMst.getColumn(0, "publishDate") > '20201200') || (ds_taxShetMst.getColumn(0, "fiscalDate") > '20201200'))  ) {
			alert("12월 세금계산서 발행 불가(물리적 분리로 SAP처리 불가)");
			ds_taxShetMst.setColumn(0, "publishDate","20201130");
			ds_taxShetMst.setColumn(0, "fiscalDate","20201130");
			//return false;
		}
	}
	*/
	
	//테스르를 위해 임시로 주석처리 20200128

	if(cal_publishDate.mae_cal.value > gfn_today() || cal_fiscalDate.mae_cal.value > gfn_today())
	{
		g_Message("EE", "현재일자보다 클 수 없습니다.");
		cal_publishDate.mae_cal.value = sDayBef;
		cal_fiscalDate.mae_cal.value = sDayBef;
		return;
	}
	
	sDayBef = sDay.substr(0, 8);
	edt_writeYyyy.value = sDay.substr(0, 4);
	edt_writeMm.value = sDay.substr(4, 2);
	edt_writeDd.value = sDay.substr(6, 2);
	if(!gfn_isNull(msk_itemSupplyDateMM1.value)){		
		msk_itemSupplyDateMM1.value = sDay.substr(4, 2);
		msk_itemSupplyDateDD1.value = sDay.substr(6, 2);
	}
	if(!gfn_isNull(msk_itemSupplyDateMM2.value)){
		msk_itemSupplyDateMM2.value = sDay.substr(4, 2);
		msk_itemSupplyDateDD2.value = sDay.substr(6, 2);
	}
	if(!gfn_isNull(msk_itemSupplyDateMM3.value)){
		msk_itemSupplyDateMM3.value = sDay.substr(4, 2);
		msk_itemSupplyDateDD3.value = sDay.substr(6, 2);
	}
	if(!gfn_isNull(msk_itemSupplyDateMM4.value)){
		msk_itemSupplyDateMM4.value = sDay.substr(4, 2);
		msk_itemSupplyDateDD4.value = sDay.substr(6, 2);
	}
	
	//한화 본사 정보 조회
	//fn_searchHanwha();
	
}
/*달력 초기화*/
function fn_chgCalDate(){
	if (gfn_isNull(ds_taxShetMst.getColumn(0,"ccCd"))){
		ds_taxShetMst.setColumn(0, "publishDate", fv_sToday);
		ds_taxShetMst.setColumn(0, "fiscalDate", fv_sToday);
	}else{
		mfn_GetCcMonthClsYn("C", ds_taxShetMst.getColumn(0,"ccCd"), gfn_subStr(gfn_addMonth(fv_sToday, -1), 0, 6), "01");
		if (ds_common.getColumn(0, 0) == "N")
		{
			fv_taxDay = gfn_getLastDate(gfn_addMonth(fv_sToday, -1));
			ds_taxShetMst.setColumn(0, "publishDate", fv_taxDay);
			ds_taxShetMst.setColumn(0, "fiscalDate", fv_taxDay);
		}else{
			ds_taxShetMst.setColumn(0, "publishDate", fv_sToday);
			ds_taxShetMst.setColumn(0, "fiscalDate", fv_sToday);
		}
	}
	fn_calTaxDateChange(ds_taxShetMst.getColumn(0, "fiscalDate"));
}
