/*----------------------------------------------------------------------------------------------
 * 설명      : 고객(CUST) 팝업 after 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_custSearchAfter(p, data){
	//정발행일경우에만 표시
	if(rdo_publishClass.value == "1"){
		objArr = data.drname1.split(",");
		//alert(data.drname1.length); //11 
		//alert(objArr.length); //3
		//alert(data.saupno);
		
		if (data.saupno == '3148500376' || data.saupno == '2028116577' || data.saupno == '4178501562' || data.saupno == '3628501073') {
			//한화솔루션 중앙연구소, 한화솔루션  대표자 전부 표시되도록 수정
			if (data.drname1.length > 40) {
				ds_taxShetMst.setColumn(0, "rcvrCeonm", objArr[0] + " 외 " + (objArr.length -1) + "명");	
			} else {
				ds_taxShetMst.setColumn(0, "rcvrCeonm", data.drname1);		
			}
		} else {
			if (objArr.length > 3){
			//alert(objArr[0]);
				ds_taxShetMst.setColumn(0, "rcvrCeonm", objArr[0] + " 외 " + (objArr.length -1) + "명");	
			}else{	
				//대표자명 기존대로 외 1명으로 표시 
				if (data.drname1.length > 40) {
					if (objArr[0].length > 40) {
						//ds_taxShetMst.setColumn(0, "rcvrCeonm", objArr[0] + " 외 1명");		
						alert("첫번째 대표자명은 40자를 넘을 수 없습니다.\n 정보기획팀에 문의 바랍니다.");
					} else {
						//ds_taxShetMst.setColumn(0, "rcvrCeonm", objArr[0] + " 외 1명");		
						ds_taxShetMst.setColumn(0, "rcvrCeonm", objArr[0] + " 외 " + (objArr.length -1) + "명");	
					}
				} else {
					ds_taxShetMst.setColumn(0, "rcvrCeonm", data.drname1);		
				}
				
			}
		}
		ds_taxShetMst.setColumn(0, "rcvrSubUpjangNum", data.subUpjangNum);
		ds_taxShetMst.setColumn(0, "rcvrBizNo", data.saupno);
		ds_taxShetMst.setColumn(0, "rcvrBizNm", data.custnm);
		ds_taxShetMst.setColumn(0, "rcvrAddr", data.addr +' '+ data.detailAddrEnc +' '+ data.etcAddr);
		ds_taxShetMst.setColumn(0, "rcvrBizType", data.upTae);
		ds_taxShetMst.setColumn(0, "rcvrBizCat", data.upJong);
		//alert(data.custtype);
		//개인 거래처인 경우 구분값 02로 설정 2017.12.20 김호석(주민등록번호로 세금계산서 발행) 01: 개인, 02:일반 , 03:외국인(현재 사용안함)
		if (data.custtype == "010") {
			ds_taxShetMst.setColumn(0, "rcvrBizNoClass","02");
		}
		//품목 초기화
		ds_lineData1.clearData();
		ds_lineData2.clearData();
		ds_lineData3.clearData();
		ds_lineData4.clearData();
		ds_taxShetDtl1.clearData();
		ds_taxShetDtl2.clearData();
		ds_taxShetDtl3.clearData();
		ds_taxShetDtl4.clearData();		
	}
	
	//최신 고객 공급받는 담당자 세팅
	fn_selectInchargeInfo(ds_taxShetMst.getColumn(0, "upjangCd"), data.custcd);
}

