************************************************************************************************/
var fv_sToday = gfn_today();
var fv_muCd = "";
var fv_tmCd = "";
var fv_tmNm = "";
var fv_ccCd = "";
var fv_jongSaupNo = "";
var invoiceItemCnt = 1;		//품목정보에 사용
var fv_taxDay = gfn_today();
var sDayBef = "";
var PurchaseExpiryDateTime = "";
var t_net_amt = 0;
var t_vat_amt = 0;
