	}
	if(ds_lineData4 != null && ds_lineData4.rowcount > 0){
		ds_lineData.appendData(ds_lineData4);
	}
 }

/*----------------------------------------------------------------------------------------------
 * 팝업에서 받은 데이터 화면 Dataset에 매핑
 * dsDtl : 화면의 라인아이템 Dataset
 * dsObj : 팝업에서 선택한 Dataset
 * itemSeq : 화면의 라인순번(DB저장시필요)
 *---------------------------------------------------------------------------------------------*/
function fn_settingLineData(dsDtl:Dataset, dsObj:Dataset, itemSeq){
	var vatRate = ds_vatCd.getColumn(ds_vatCd.rowposition, "vatRate");		//세액율
	var totApplyAmt = 0;
	var totSupplyAmt = 0;
	var totVatAmt = 0;
	
	var applyAmt = 0;	//발행금액
	var supplyAmt = 0;	//공급가액
	var vatAmt = 0;		//세액
	for(var i=0; i<dsObj.rowcount; i++){
		applyAmt = dsObj.getColumn(i, "applyAmt");
		supplyAmt = dsObj.getColumn(i, "supplyAmt");
		vatAmt = dsObj.getColumn(i, "vatAmt");
		
		totApplyAmt += Number(applyAmt);
		totSupplyAmt += Number(supplyAmt);
		totVatAmt += Number(vatAmt);
	}
	
	dsDtl.clearData();
	var nrow = dsDtl.addRow();
	if(fv_muCd == "2004"){
		dsDtl.setColumn(nrow, "itemNm", "식자재");
	}
	dsDtl.setColumn(nrow, "itemSupplyDateMM", ds_taxShetMst.getColumn(0, "fiscalDate").substr(4, 2));
	dsDtl.setColumn(nrow, "itemSupplyDateDD", ds_taxShetMst.getColumn(0, "fiscalDate").substr(6, 2));
	dsDtl.setColumn(nrow, "itemSeq", itemSeq);
	dsDtl.setColumn(nrow, "itemApplyAmt", totApplyAmt);
	dsDtl.setColumn(nrow, "itemSupplyAmt", totSupplyAmt);
	dsDtl.setColumn(nrow, "itemVatAmt", totVatAmt);
}

function fn_settingLineDataPop(dsDtl:Dataset, itemSeq, totApplyAmt, totSupplyAmt, totVatAmt){
	dsDtl.clearData();
	var nrow = dsDtl.addRow();
	if(fv_muCd == "2004"){
		dsDtl.setColumn(nrow, "itemNm", "식자재");
	}

	dsDtl.setColumn(nrow, "itemSupplyDateMM", ds_taxShetMst.getColumn(0, "fiscalDate").substr(4, 2));
	dsDtl.setColumn(nrow, "itemSupplyDateDD", ds_taxShetMst.getColumn(0, "fiscalDate").substr(6, 2));
	dsDtl.setColumn(nrow, "itemSeq", itemSeq);
	dsDtl.setColumn(nrow, "itemApplyAmt", totApplyAmt);
	dsDtl.setColumn(nrow, "itemSupplyAmt", totSupplyAmt);
	dsDtl.setColumn(nrow, "itemVatAmt", totVatAmt);
}

