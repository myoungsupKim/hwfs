function fn_upjangAfterEvent(obj, cls)
{
	fv_jongSaupNo = "";		//하위업장에 종사업장 초기화(2015-11-03 추가)
	
	if(rdo_publishClass.value == "1"){		//정발행일때만 다시 셋팅
