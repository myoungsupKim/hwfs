/*----------------------------------------------------------------------------------------------
 * 설명      : 대체처리
 *---------------------------------------------------------------------------------------------*/
function fn_saveProcess(){
		
	//12월 처리 제한 
	/*
	if(MuSearchComponent01.getMuCode() == "2003"){
		//pass
	} else {
		if ((ds_search.getColumn(0,"receiveDate") > '20201200') ) {
			ds_search.setColumn(0,"receiveDate","20201130");
			alert("12월 처리 불가");
			//ds_taxShetMst.setColumn(0, "publishDate","20201130");
			//ds_taxShetMst.setColumn(0, "fiscalDate","20201130");
			return;
		}
	}
	*/

	//조회내역 셋팅 입금일자, 영업유형, 비고
	ds_search.setColumn(0,"receiveDate", cal_receiveDate.text);
	ds_search.setColumn(0,"altTypeCd", cbo_altType.value);
	ds_search.setColumn(0,"remark", edt_remark.value);
	
	ds_search.setColumn(0,"inqueryStartDate", cal_occurDateFr.text);
	ds_search.setColumn(0,"inqueryEndDate", cal_occurDateTo.text);
	ds_search.setColumn(0,"inqueryMuCd", MuSearchComponent01.getMuCode());
	ds_search.setColumn(0,"inqueryTmCd",TmSearchComponent01.getTmCode());
	ds_search.setColumn(0,"inqueryCcCd", UpjangSearchComponent01.getUpjangCode());
	
	for (var i=0; i<ds_listSuspense.rowcount; i ++){
		var sAltNum   = ds_listSuspense.getColumn(i, "altNum");     //대체번호
		var iApplyAmt = ds_listSuspense.getColumn(i, "applyAmt");   //대체금액
		
		if (iApplyAmt > 0){
			if (gfn_isNull(sAltNum)){
				ds_listSuspense.setColumn(i, "rowStatus", "INSERT");   //가수금대체 INSERT
			} else {
				ds_listSuspense.setColumn(i, "rowStatus", "UPDATE");   //가수금대체 UPDATE
			}
		} else {
			if(!gfn_isNull(sAltNum)){
				ds_listSuspense.setColumn(i, "rowStatus",	"DELETE");	//수정모드에서 적용금액이 0 이면 단건 대체취소
			}else{
				if(gfn_isNull(iApplyAmt) || iApplyAmt == 0){
					alert("선택한 가수금에 대체 금액이 없습니다.");
					return;
				}				
			}
		}
		
		//입금일자, 비고셋팅
		ds_listSuspense.setColumn(i,"receiveDate", ds_search.getColumn(0,"receiveDate"));        //입금일자
		ds_listSuspense.setColumn(i,"remark", ds_search.getColumn(0,"remark"));                  //비고
		ds_listSuspense.setColumn(i,"suspenseAltType", ds_search.getColumn(0,"altTypeCd"));      //대체유형
	}
	
	//대체유형에 따른 계정코드 셋팅
 	for (var i = 0; i<ds_list.rowcount;i++){
		ds_list.setColumn(i, "acctCd", fv_receiveAcctCd);
		
		if (ds_list.getColumn(i,"receiveAmt") == ""){
		    ds_list.setColumn(i,"receiveAmt",0);
		}
	}
	if(!confirm("저장 하시겠습니까")){
		return;
	}	
	
    var strSvcID    = "save";
    var strURL      = "sm/sar/saleCashAlter/saveList.xdo";
    var strInDs     = "ds_search=ds_search:A ds_list=ds_list:U ds_listSuspense=ds_listSuspense";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

