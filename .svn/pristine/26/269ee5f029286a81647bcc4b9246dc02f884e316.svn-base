/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
    
		case "altType" :
		
            if(ds_altType.rowcount < 2){
				if(gfn_isNull(ds_altType.getColumn(0, "typeCd"))){
					
					//데이터가 없을 경우 전체조회
					ds_altType.clearData();
					gfn_setSalesTypeCd(this, "altType2", "ds_altType", "013");
				}
            }else{
				ds_altType.filter("typeCd != 'B07' && typeCd != 'B16'");
				cbo_altType.index = 0;
            }
            break;
            
     	// 입금유형 조회
        case "altType2" :
			ds_altType.filter("typeCd != 'B07' && typeCd != 'B16'");
			ds_altType.insertRow(0);
			//ds_altType.setColumn(0, "typeCd", "");
			ds_altType.setColumn(0, "typeNm", "전체");
			cbo_altType.index = 0;
            break;

		// 대체마스터 정보 조회
        case "search" :
        
			var orgAltTypeCd = ds_cashMstData.getColumn(0, "altTypeCd");
			
            if (ds_cashMstData.rowcount > 0){
                //ds_search 셋팅
                if (ds_search.rowcount > 0){
                    ds_search.setColumn(0, "salesCashReceiveNum", ds_cashMstData.getColumn(0, "salesCashReceiveNum"));   //영업현금입금번호
                    ds_search.setColumn(0, "altNum", ds_cashMstData.getColumn(0, "suspenseAltNum"));                     //가수금대체번호
                    ds_search.setColumn(0, "inqueryMuCd", ds_cashMstData.getColumn(0, "inqueryMuCd"));                   //조회사업부
                    ds_search.setColumn(0, "inqueryCcCd", ds_cashMstData.getColumn(0, "inqueryCcCd"));                   //조회업장CC
                    ds_search.setColumn(0, "inqueryTmCd", ds_cashMstData.getColumn(0, "inqueryTmCd"));                   //조회팀
                    ds_search.setColumn(0, "muCd", ds_cashMstData.getColumn(0, "muCd"));                                 //사업부
                    ds_search.setColumn(0, "tmCd", ds_cashMstData.getColumn(0, "inqueryTmCd"));                          //팀
                    ds_search.setColumn(0, "tmNm", ds_cashMstData.getColumn(0, "inqueryTmNm"));                                 //팀
                    ds_search.setColumn(0, "ccCd", ds_cashMstData.getColumn(0, "ccCd"));                                 //업장CC
                    ds_search.setColumn(0, "altTypeCd", ds_cashMstData.getColumn(0, "altTypeCd"));                       //대체유형
                    ds_search.setColumn(0, "remark", ds_cashMstData.getColumn(0, "remark"));                             //적요
                    ds_search.setColumn(0, "upjangCd", ds_cashMstData.getColumn(0, "upjangCd"));                         //업장
                    ds_search.setColumn(0, "upjangNm", ds_cashMstData.getColumn(0, "upjangNm"));                         //업장
                    ds_search.setColumn(0, "receiveDate", ds_cashMstData.getColumn(0, "altDate"));                         //업장
                    ds_search.setColumn(0, "occurDateFr", ds_cashMstData.getColumn(0, "inqueryStartDate"));                         //업장
                    ds_search.setColumn(0, "occurDateTo", ds_cashMstData.getColumn(0, "inqueryEndDate"));                         //업장
                    
                    MuSearchComponent01.setMuCode(ds_cashMstData.getColumn(0, "inqueryMuCd"));
                    UpjangSearchComponent01.setUpjangCode(ds_cashMstData.getColumn(0, "inqueryCcCd"));
                    UpjangSearchComponent01.setUpjangName(ds_cashMstData.getColumn(0, "inqueryCcNm"));
                    UpjangSearchComponent00.setUpjangCode(ds_cashMstData.getColumn(0, "upjangCd"));
                    UpjangSearchComponent00.setUpjangName(ds_cashMstData.getColumn(0, "upjangNm"));
                    
                } else {
                    ds_search.clearData();
                    var nRow = ds_search.addRow();

                    ds_search.setColumn(nRow, "salesCashReceiveNum", ds_cashMstData.getColumn(0, "salesCashReceiveNum"));   //영업현금입금번호
                    ds_search.setColumn(nRow, "altNum", ds_cashMstData.getColumn(0, "suspenseAltNum"));                     //가수금대체번호
                    ds_search.setColumn(nRow, "inqueryMuCd", ds_cashMstData.getColumn(0, "inqueryMuCd"));                   //조회사업부
                    ds_search.setColumn(nRow, "inqueryCcCd", ds_cashMstData.getColumn(0, "inqueryCcCd"));                   //조회업장CC
                    ds_search.setColumn(nRow, "inqueryTmCd", ds_cashMstData.getColumn(0, "inqueryTmCd"));                   //조회팀
                    ds_search.setColumn(nRow, "muCd", ds_cashMstData.getColumn(0, "muCd"));                                 //사업부
                    ds_search.setColumn(nRow, "ccCd", ds_cashMstData.getColumn(0, "ccCd"));                                 //업장CC
                    ds_search.setColumn(nRow, "altTypeCd", ds_cashMstData.getColumn(0, "altTypeCd"));                       //대체유형
                    ds_search.setColumn(nRow, "remark", ds_cashMstData.getColumn(0, "remark"));                             //적요
                    ds_search.setColumn(nRow, "upjangCd", ds_cashMstData.getColumn(0, "upjangCd"));                         //업장
                    ds_search.setColumn(nRow, "upjangNm", ds_cashMstData.getColumn(0, "upjangNm"));                         //업장
                    
                    MuSearchComponent01.setMuCode(ds_cashMstData.getColumn(0, "inqueryMuCd"));
                    UpjangSearchComponent01.setUpjangCode(ds_cashMstData.getColumn(0, "inqueryCcCd"));
                    UpjangSearchComponent01.setUpjangName(ds_cashMstData.getColumn(0, "inqueryCcNm"));
                    UpjangSearchComponent00.setUpjangCode(ds_cashMstData.getColumn(0, "upjangCd"));
                    UpjangSearchComponent00.setUpjangName(ds_cashMstData.getColumn(0, "upjangNm"));
                    
                }
            }
			//콤보조회
			fn_setCombo();
   
            fn_ar_search();
            
			//조회 후 대체유형 세팅
			ds_search.setColumn(0, "altTypeCd", orgAltTypeCd);            
            break;
            
        // 마스터 정보 조회
        case "searchDetail" :
            if (!gfn_isNull(pReceiveNum)){
                var sCcCd = ds_search.getColumn(0,"ccCd");
                var sumReceiveAmt=0;
				if (ds_list.rowcount > 0){
					for (var i = 0;i<ds_list.rowcount;i++){
						var receiveAmt = ds_list.getColumn(i, "receiveAmt");
						sumReceiveAmt += receiveAmt;
					}
					msk_receiveAmt.value = sumReceiveAmt;
				}
				
				ds_search.setColumn(0, "receiveDateFrom", ds_search.getColumn(0, "occurDateFr"));
				ds_search.setColumn(0, "receiveDateTo", ds_search.getColumn(0, "occurDateTo"));
				ds_search.setColumn(0, "ccCd", "'" + sCcCd + "'");

				//가수금대체상세조회
				fn_searchSuspenseAltDetail();
            }
            break;
                        
        //가수금상세조회후 후단처리
        case "searchSuspenseAltDetail" :
            var sumSusApplyAmt=0;
            if (ds_listSuspense.rowcount > 0){
                for (var i = 0;i<ds_listSuspense.rowcount;i++){
                    var susApplyAmt = ds_listSuspense.getColumn(i, "applyAmt");
                    sumSusApplyAmt += susApplyAmt;
                }
                msk_suspenseAmt.value = sumSusApplyAmt;
            }
            break;
            
        // 영업현금 잔액 체크
        case "getCashAltYn" :
			//trace("fv_cashAltYn:"+fv_cashAltYn);
			if(fv_cashAltYn == "N"){
				gfn_alert("영업현금 잔액이 부족합니다.");
				return;
			}else{
				//alert(ds_listSuspense.getRowType(0));
				gfn_getSuspenseAltYn(this, "getSuspenseAltYn", "ds_listSuspense");
			}
            break;
            
        // 가수금 잔액 체크
        case "getSuspenseAltYn" :
			//trace("fv_suspenseAltYn:"+fv_suspenseAltYn);
			if(fv_suspenseAltYn == "N"){
				gfn_alert("가수금 잔액이 부족합니다.");
				return;
			}else{
				//alert(ds_list.rowcount);
				//ds_listSuspense.updatecontrol = false;
				//for(var i=0; i<ds_listSuspense.rowcount; i++){
				//	ds_listSuspense.setRowType(i, Dataset.ROWTYPE_INSERT);
				//}
				fn_saveProcess();
			}
            break;
            
        // 저장
        case "save" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			//ds_listSuspense.updatecontrol = true;
			opener.fn_search();
			close();
            break;
              
        // 취소
        case "cancle" :
			gfn_alert("msg.save.success");	// 저장 되었습니다.
			opener.fn_search();
			close();
            break;          
    }
}


