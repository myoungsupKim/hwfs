************************************************************************************************/
var fv_sToday = gfn_today();
var bCreateMode	= true;		// 신규모드(단, 부모창에서 대체번호를 가져올 경우 대체번호에 해당되는 대체정보만 조회한다. bCreateMode = false : 수정모드)
var fv_receiveAcctCd=null;
var fv_cashAltYn = "";
var fv_suspenseAltYn = "";
var fv_taxDay = fv_sToday;

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	fn_clearSearch();
		
	if(!gfn_isNull(pReceiveNum)){

		fn_search();
	}
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	v_ds_check = "ds_list";
	
//	ds_search.clearData();
	//fn_clearSearch();
	/*영업현금정보 초기화*/
	if(gfn_isNull(pReceiveNum)){
		ds_search.addRow();
		if(arrSchVal.length > 0){
			ds_search.setColumn(0, "muCd",    			arrSchVal[0]);
			ds_search.setColumn(0, "muNm",    			arrSchVal[1]);
			ds_search.setColumn(0, "tmCd",    			arrSchVal[2]);
			ds_search.setColumn(0, "tmNm",    			arrSchVal[3]);
			ds_search.setColumn(0, "ccCd",    			arrSchVal[4]);
			ds_search.setColumn(0, "ccNm",    			arrSchVal[5]);
			ds_search.setColumn(0, "occurDateFr",		arrSchVal[8]);
			ds_search.setColumn(0, "occurDateTo",		arrSchVal[9]);
			if (g_Cc_Cd == arrSchVal[4]) {
				ds_search.setColumn(0, "upjangCd",   g_Upjang);
				ds_search.setColumn(0, "upjangNm",   g_UpjangNm);
			}
						
			MuSearchComponent01.setMuCode(arrSchVal[0]);
		}
		
		fn_chgCalDate();
		
		cal_occurDateFr.fn_initMonthCal(this, null, null, ds_search, "occurDateFr");
		cal_occurDateTo.fn_initMonthCal(this, null, "fn_calendarToCallBack", ds_search, "occurDateTo");
		cal_receiveDate.fn_initMonthCal(this, fv_taxDay, "fn_calendarFromCallBack", ds_search, "receiveDate","Y");
	} else {
		MuSearchComponent01.enable=false;
		TmSearchComponent01.enable=false;
		UpjangSearchComponent01.enable=false;
		UpjangSearchComponent00.enable  = false;
	    cal_occurDateFr.enable=false;
		cal_occurDateTo.enable=false;
		cal_receiveDate.enable=false;
		cbo_altType.enable=false;
		btn_suspense.enable=false;
		edt_custCd.enable=false;
		edt_custNm.enable=false;
		btn_receiveCancle.visible=true;
		btn_receive.visible=false;

		cal_occurDateFr.fn_initMonthCal(this, null, null, ds_search, "occurDateFr");
		cal_occurDateTo.fn_initMonthCal(this, null, "fn_calendarToCallBack", ds_search, "occurDateTo");
		cal_receiveDate.fn_initMonthCal(this, fv_sToday, "fn_calendarFromCallBack", ds_search, "receiveDate","Y");
	}					//사용여부

    //gfn_setSalesTypeCd(this, "altType", "ds_altType", "013");	//영업유형코드
    

    if(gfn_isNull(pReceiveNum)){
        ds_search.setColumn(0,"altTypeCd","K01");    //DEFAULT  가수금(사업장)
		ds_search.setColumn(0, "occurDateTo",		fv_taxDay);
    }
    
	
	ds_listSuspense.clearData();
	
	//콤보조회
	fn_setCombo();
}
/*
//달력 초기화
*/
function fn_calendarFromCallBack()
{
	if(gfn_isNull(ds_search.getColumn(0,"ccCd"))){
		alert("업장 또는 CC를 선택해 주세요.");
		ds_search.setColumn(0, "receiveDate", fv_sToday);
		return false;
	}
	//입금일자가 2015-10-01 이전 입력 안되게 체크
	mfn_GetCcMonthClsYn("C", ds_search.getColumn(0,"ccCd"), gfn_subStr(ds_search.getColumn(0,"receiveDate"), 0, 6), "01");
	if (ds_common.getColumn(0, 0) == "Y")
	{
		g_Message("EE", "해당월은 월마감이 완료되어 작업할 수 없습니다.");
		ds_search.setColumn(0, "receiveDate", fv_taxDay);
//		return false; 
	}	
	ds_search.setColumn(0, "occurDateTo", ds_search.getColumn(0, "receiveDate"));	
}
/*달력 초기화*/
function fn_chgCalDate(){
	if (gfn_isNull(ds_search.getColumn(0,"ccCd"))){
		ds_search.setColumn(0, "receiveDate", fv_sToday);
		ds_search.setColumn(0, "occurDateTo", fv_sToday);
	}else{
		mfn_GetCcMonthClsYn("C", ds_search.getColumn(0,"ccCd"), gfn_subStr(gfn_addMonth(fv_sToday, -1), 0, 6), "01");

		if (ds_common.getColumn(0, 0) == "N")
		{
			fv_taxDay = gfn_getLastDate(gfn_addMonth(fv_sToday, -1));
			ds_search.setColumn(0, "receiveDate", fv_taxDay);
			ds_search.setColumn(0, "occurDateTo", fv_taxDay);
		}else{
			ds_search.setColumn(0, "receiveDate", fv_sToday);
			ds_search.setColumn(0, "occurDateTo", fv_sToday);
		}
	}
}

function fn_calendarToCallBack()
{
	//채권 조회는 입금 일자 이전으로 할것 : 2015-10-11 김동표 차장 추가 요청(김명호)
	if(ds_search.getColumn(0,"receiveDate") < ds_search.getColumn(0,"occurDateTo")){
		alert("영업 현금은 입금 일자 이전 데이터만 조회가 가능합니다.");
		ds_search.setColumn(0, "occurDateTo", ds_search.getColumn(0, "receiveDate"));	
		return;
	}
}
/*----------------------------------------------------------------------------------------------
 * 설명      : 팀(부서)의 팝업을 호출하기 전에 발생되는 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_tmBeforeEvent(obj)
{
	obj.qoMuCd = ds_search.getColumn(0,"muCd");
	obj.qoUseYn = "Y";
	
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 업장의 팝업을 호출하기 전에 발생되는 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_upjangBeforeEvent(obj, cls)
{
	//cls.setTmCd(ds_search.getColumn(0, "tmCd"));
	cls.setTmCd(TmSearchComponent01.getTmCode());
	
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 업장의 팝업을 호출한 후에 발생되는 이벤트
 *---------------------------------------------------------------------------------------------*/
function fn_upjangAfterEvent(obj, cls)
{
    ds_search.setColumn(0,"muCd", cls.muCd);
    ds_search.setColumn(0,"tmCd", cls.tmCd);
	ds_search.setColumn(0,"ccCd", cls.ccCd);
	ds_search.setColumn(0,"ccNm", cls.ccNm);
	fn_setCombo();
	fn_chgCalDate();
	return true;
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{	
//	// v_ds_check 미사용시 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
// 	if (gfn_dsIsUpdated(ds_listAr) == true) 
// 	{
// 		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
//         if (!gfn_confirm("confirm.before.movepage")) return false;
//     }
//     
//	// 조회프로그램에도 Return true; 기술해 주세요!
// 	return true;

	// v_ds_check 사용시 변경사항 체크
	if(gfn_length(v_ds_check) != 0)
	{	
		// v_ds_check 설정기준 변경사항 체크
		if (gfn_isDataChange() == true) 
		{		
			// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
			if (!gfn_confirm("confirm.before.movepage")) return false;
		}
	}
    
	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}


/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 조회 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_search()
{	
	// 조회시 Validation Check
	//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;
	if (!gfn_isNull(pReceiveNum)){
	    ds_search.clearData();
	    ds_search.addRow();
	    ds_search.setColumn(0, "salesCashReceiveNum", pReceiveNum);
	    ds_search.setColumn(0, "altNum", gv_altNum);
	}
	
