/*----------------------------------------------------------------------------------------------
 * 설명      : 일괄적용금액 및 일괄적용구분 변경에 따른 ds_list 적용금액 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_change_receiveAmt_listAr(allEditYn, allAmt)
{
    if (allAmt == "" || allAmt == "0" || allAmt == null && allEditYn == "N"){
        msk_allEditAmt.enable = true;
        //적용금액 일괄 잔액금액으로 셋팅
		for (var i = 0 ; i < ds_list.rowcount ; i ++){
			ds_list.setColumn(i, "receiveAmt", 0);
		}
    } else {
		if (allEditYn == "Y"){
			msk_allEditAmt.enable = false;
			
			//적용금액 일괄 잔액금액으로 셋팅
			for (var i = 0 ; i < ds_list.rowcount ; i ++){
				ds_list.setColumn(i, "receiveAmt", ds_list.getColumn(i, "occurAmt"));
			}
		} else if (allEditYn == "N"){
			msk_allEditAmt.enable = true;
			
			//적용금액을 순차적으로 차감 일괄적용금액만큼
			for (var i = 0 ; i < ds_list.rowcount ; i ++){
				var arBalAmt = ds_list.getColumn(i, "occurAmt");
				
				if (allAmt > arBalAmt){
					allAmt -= arBalAmt;
					ds_list.setColumn(i, "receiveAmt", ds_list.getColumn(i, "occurAmt"));
				} else {
					ds_list.setColumn(i, "receiveAmt", allAmt);
					allAmt = 0;
				}
			}
		}
	}
}


/*----------------------------------------------------------------------------------------------
 * 설명      : 대체처리시 체크
 *---------------------------------------------------------------------------------------------*/
function fn_validCheck(){

// 	// v_ds_check 미사용시 변경사항 체크
	if (gfn_dsIsUpdated(ds_list) == false) 
	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return false;
	}

// 	// v_ds_check 사용시 변경사항 체크
// 	if (gfn_isDataChange() == true) 
// 	{		
// 		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
// 		return;
// 	}
	
	//금액 Over check
	for (i=0; i < ds_list.getRowCount(); i++){
		if(ds_list.getColumn(i, "receiveAmt") != 0){
			if((ds_list.getColumn(i, "occurAmt") - ds_list.getColumn(i, "receiveAmt")) < 0){
				alert("적용후 잔액이 0보다 작습니다.");
				ds_list.setColumn(i, "receiveAmt", "0");
				return false;
			}
		}
	}
	
	//입금유형이 가수금(사업장):B02  시 rowStatus 셋팅해줌
	//if(msk_receiveAmt.value==0 && ds_search.getColumn(ds_search.rowposition,"altTypeCd") != "K03"){
	//가수금(사업장) 이면 체크하도록 변경 20160128 맹수영
	if(msk_receiveAmt.value==0 && ds_search.getColumn(ds_search.rowposition,"altTypeCd") == "K01"){
		alert("처리 입금액을 입력해 주시기 바랍니다.1");
		return false;
	}

	if (ds_listSuspense.rowcount == 0 && ds_search.getColumn(ds_search.rowposition,"altTypeCd") == "K01"){
		alert("가수금(사업장) 내역이 없습니다");
		return false;
	}
	
	//입금액과 가수금잔액 비교
	if (msk_receiveAmt.value != msk_suspenseAmt.value && ds_search.getColumn(ds_search.rowposition,"altTypeCd") == "K01"){
		alert("처리입금액과 가수금잔액이 틀립니다");
		return false;
	}
	
	if (gfn_isNull(cal_receiveDate.text)){
	    alert("입금일자는 필수 입력사항입니다");
	    return false;
	}
	
	if (gfn_isNull(cbo_altType.value)){
	    alert("대체유형은 필수 입력사항입니다");
	    return false;
	}
	
//	if (gfn_isNull(edt_remark.value)){
//	    alert("적요는 필수 입력사항입니다");
//	    return false;
//	}
	return true;
}


