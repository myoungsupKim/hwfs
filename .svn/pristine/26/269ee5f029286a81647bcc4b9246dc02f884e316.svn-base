function grd_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	var iOccurAmt	= 0;	//발생금액
	var iReceiveAmt	= 0;	//입금금액

	if((e.cell == 0) || (e.cell == 2)){	//입금금액 Setting
		iOccurAmt   = ds_list.getColumn(ds_list.rowposition, "occurAmt");	//obj.getCellValue(e.row, e.cell);
		iReceiveAmt = ds_list.getColumn(ds_list.rowposition, "receiveAmt");	//obj.getCellValue(e.row, e.cell+1);
		if(iOccurAmt == iReceiveAmt){
			ds_list.setColumn(ds_list.rowposition, "receiveAmt", 0);
		}else{
			ds_list.setColumn(ds_list.rowposition, "receiveAmt", iOccurAmt);
		}
	}
	fn_changeAmtRe();
}

