 *---------------------------------------------------------------------------------------------*/
function fn_setDataList(dsObj){
 		
	if(ds_listSuspense.rowcount > 0) ds_listSuspense.clearData();
	
	var nRow = 0;
	var sumApplyAmt = 0;   //가수금잔액

	//ds_listSuspense.assign(dsObj);
	//ds_listSuspense.copyData(dsObj,true);
	
	if(dsObj.rowcount > 0){
	
		gv_altNum	= "";

		for(var i=0; i<dsObj.rowcount; i++){
			nRow = ds_listSuspense.addRow();
