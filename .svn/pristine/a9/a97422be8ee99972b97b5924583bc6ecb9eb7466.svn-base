	if(!gfn_isNull(ds_List.getColumn(0, "CONFIRM_YN"))){
		ds_Cond.setColumn(0, "CONFIRM_YN", ds_List.getColumn(0, "CONFIRM_YN"));
	}else{
		ds_Cond.setColumn(0, "CONFIRM_YN", "N");
	}
	
	//
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "MAP30220E_T002";
	strURL   = "U_svc::" + "ma/map/MAP30220E_T002.jsp";

