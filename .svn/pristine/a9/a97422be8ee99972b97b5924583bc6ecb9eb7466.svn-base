			btn_Apply.visible = false;
			Cbo_gbn.visible = false;
		}else{
			btn_Confirm.visible = true; // 확정 후에도 계속 '확정'가능하게 수정.
			btn_Apply.visible = false;
			Cbo_gbn.visible = false;
		}	
	}
}

