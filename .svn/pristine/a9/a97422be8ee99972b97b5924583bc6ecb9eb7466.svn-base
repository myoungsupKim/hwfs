// 그리드 체크로직
function fn_checkCell( currow, colID )
{  
	var sRtn = "none";
	 
	// 현재년
	var currYy = gfn_today().substr(0,4);
	
	// 현재월
	var currMm = gfn_today().substr(4,2);
	
	// 입력년
	var intYy = yy.value;
	//trace("currYy==>"+currYy); trace("intYy==>"+intYy);
	// 사업계획, 수정계획 개선과제 적용여부 확인.
	var strCmfYn = ds_cbo_GBN.getColumn(0, "CONFIRM_YN");
	var strIpvYn = ds_cbo_GBN.getColumn(0, "IMPROV_YN");
	var strBpcYn = ds_cbo_GBN.getColumn(0, "BPC_SEND_YN");
	var strOrgYn = ds_cbo_GBN.getColumn(0, "ORGIN_YN");
	
	// 구분 : '사업계획', '최종PL'이 아니고
	// 년도 : 현재년도와 입력년도가 같을때 실적데이터 수정불가.
	// 년월 : 현재년월과 입력년월이 같고, 오늘 이전 실적데이터 수정불가.
	// 아래로직은 주석처리.
	
	if (currYy == intYy){
	
		// 편집불가 그리드 컬럼
		// 초기화.
		if( ds_List.getColumn( currow, "ACCT_CD" ) == "44000000" ){
			sRtn = "none";
		}else{
			//sRtn = "masknumber";
			if( parseInt(colID) >= parseInt(currMm) ){
			sRtn = "masknumber";
			}else{
				sRtn = "none";
			}
		}
	}else if (currYy > intYy){
		sRtn = "none";
	}else{
		if( ds_List.getColumn( currow, "ACCT_CD" ) == "44000000" ){
			sRtn = "none";
		}else{
			sRtn = "masknumber";
		}
	}
	
	// 확정시 편집불가 그리드 컬럼
	if(strCmfYn == "Y" && strBpcYn == "Y"){
		sRtn = "none";
	}
	
	return sRtn;
	
}

function fn_grdSum()
{

	var nCurGrp = Math.floor((ds_List.rowposition+1)/4);
	//alert("nCurGrp===>"+nCurGrp);
	var nSum1 = 0;
	var nSum2 = 0;
	var nSum3 = 0;
	var nSum4 = 0;
	for( var j = 1 ; j <= 9 ; j++ )
	{
		var sColName = "MM_AMT_0" + j;
		var nval1 = ds_List.getColumn( nCurGrp*4, sColName );
		var nval2 = ds_List.getColumn( nCurGrp*4+1, sColName );
		var nval3 = ds_List.getColumn( nCurGrp*4+2, sColName );
		var nval4 = nval1 - ( nval2 + nval3 );
		ds_List.setColumn( nCurGrp*4+3, sColName, nval4 );
		
		nSum1 = nSum1 + nval1;
		nSum2 = nSum2 + nval2;
		nSum3 = nSum3 + nval3;
		nSum4 = nSum4 + nval4;
	}
	
	for( var j = 10 ; j <= 12 ; j++ )
	{
		var sColName = "MM_AMT_" + j;
		var nval1 = ds_List.getColumn( nCurGrp*4, sColName );
		var nval2 = ds_List.getColumn( nCurGrp*4+1, sColName );
		var nval3 = ds_List.getColumn( nCurGrp*4+2, sColName );
		var nval4 = nval1 - ( nval2 + nval3 );
		ds_List.setColumn( nCurGrp*4+3, sColName, nval4 );
		
		nSum1 = nSum1 + nval1;
		nSum2 = nSum2 + nval2;
		nSum3 = nSum3 + nval3;
		nSum4 = nSum4 + nval4;
	}
	
	ds_List.setColumn( nCurGrp*4, "SUM_AMT", nSum1);
	ds_List.setColumn( nCurGrp*4+1, "SUM_AMT", nSum2);
	ds_List.setColumn( nCurGrp*4+2, "SUM_AMT", nSum3);
	ds_List.setColumn( nCurGrp*4+3, "SUM_AMT", nSum4);

}

