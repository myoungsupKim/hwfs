		if (ds_List.rowcount < 1)
			g_Message("EE", "조회된 내역이 없습니다.");
	}else{
		g_Message("EE", "조회중 오류가 발생했습니다. 관리자에게 문의하세요." + '\n' + '(' + errMsg + ')' );
	}
	
	// 수정계획시 해당년도의 실적은 수정불가.
	fn_RealDataCheck(yy.value);
}

