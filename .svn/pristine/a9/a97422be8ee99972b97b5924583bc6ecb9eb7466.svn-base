/*------------------------------------------------------------(60)
	1. Function ID : fn_Search()
	2. 개       요 : 업무프로세스에 따른 조회작업
	3. 기       능 : 검색조건에 해당하는 정보를 조회
	4. 인 수 설 명 : 
------------------------------------------------------------*/
function fn_search()
{	
	//필수항목 
	if (!fn_ChkManNull()) return;
	
	// 초기화
	fn_ClearData();
	ds_List.clearData();
	//ds_Cond.clearData();
	ds_Cond.addRow();
	
	var intYy;
	var preYy;
	// 입력년도의 1~12월
	if(!gfn_isNull(yy.value)){
		intYy = yy.value;
		preYy = yy.value-1;
	}
	
	// 현재월일
	var currYmd = intYy+gfn_today().substr(4,4);
	ds_Cond.setColumn(0, "INPUT_YY",  intYy);
	ds_Cond.setColumn(0, "INPUT_YMD",  currYmd);
	ds_Cond.setColumn(0, "BIZ_CLASS", Cbo_gbn.value);
	ds_Cond.setColumn(0, "BIZ_CLASS2", Cbo_gbn2.value);
	
	// 월추정이 등록되어있는지 확인.
	fn_ConfirmYn();
	
	// 조회
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "MAP30220E_T001";
	strURL   = "U_svc::" + "ma/map/MAP30220E_T001.jsp";
	
	// 데이터셋 설정
	strInDatasets  = "ds_Cond=ds_Cond ds_CcCd=ds_CcCd";
	strOutDatasets = "ds_List=ds_List";
	
	// 파라메터 설정
	strArgument  = "";
	strArgument = " g_Yy=" + wrapQuote(preYy);
	
	// 콜백함수 지정
	strCallbackFunc = "fn_CallBack";
	//strCallbackFunc = "";
	
	// 서버 호출 
	jsp_transaction(this, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, false, false, true);
}

// 필수항목 검사
function fn_ChkManNull(){
	// 팀정보 필수체크
	if(gfn_isNull(edt_tm.getTmCode())){
		g_Message("EE", st_Sabun.text + "은(는) 필수항목입니다.");
		edt_tm.setFocus();
		return false;
	}
	
	// 마케터 정보 필수체크
	if(gfn_isNull(edt_sabun.getSabun())){
			g_Message("EE", Static8.text + "은(는) 필수항목입니다.");
			edt_sabun.setFocus();
			return false;
	}
	
	// 계획년월 필수체크
	if(gfn_isNull(yy.value)){
		g_Message("EE", Static6.text + "은(는) 필수항목입니다.");
		yy.setFocus();
		return false;
	}
	
	// 구분
	if(gfn_isNull(Cbo_gbn2.value)){
		g_Message("EE", Static20.text + "은(는) 필수항목입니다.");
		Cbo_gbn2.setFocus();
		return false;
	}
	
	return true;
}
