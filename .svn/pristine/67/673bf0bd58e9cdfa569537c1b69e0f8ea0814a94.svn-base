<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT CODE AS LOC_CD, 
       CODE_NAME AS LOC_NAME  
  FROM SCC_COMMON_CODE 
 WHERE (GROUP_CODE ='MA0040')
 ORDER BY CODE_NAME
 	]]></statement>
 	<input default-name="ds_cbo_loc_in">
	</input>
	<output default-name="ds_cbo_loc">
		<col name="LOC_CD" size="20" type="VARCHAR" description="" /> 
		<col name="LOC_NAME" size="100" type="VARCHAR" description="" /> 
	</output>
</query>
