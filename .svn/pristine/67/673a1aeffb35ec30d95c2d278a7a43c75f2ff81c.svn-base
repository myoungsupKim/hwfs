/************************************************************************************************
 * 공통 라이브러리 INCLUDE 영역 (필수)
************************************************************************************************/
include "lib::lib_com.xjs";


/************************************************************************************************
 * FORM 변수 선언 영역 (필수)
************************************************************************************************/
var fv_sToday = gfn_today();

var fv_funeralAdjustDateSumColInfo;		// ds_funeralAdjustDateSum 데이터셋 컬럼정보
var fv_funeralAdjustMonthSumColInfo;		// ds_funeralAdjustMonthSum 데이터셋 컬럼정보
var fv_funeralAdjustYearSumColInfo;	// ds_funeralAdjustYearSum 데이터셋 컬럼정보

/************************************************************************************************
 * FORM EVENT 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{
	//SCCombo.bind(this, "ComCode", "grd_deadGuest.funeralParlor", ds_parlorType, {groupCode:"FUNERAL_PARLOR_TYPE", useYn:"Y", defVal:"004"});	// 빈소,호실
	
	ds_search.clearData();
	var nRow= ds_search.addRow();
	ds_search.setColumn(nRow, "muCd", g_Mu_Cd);
	ds_search.setColumn(nRow, "muNm", g_Mu_Nm);
	ds_search.setColumn(nRow, "tmCd", g_Tm_Cd);
	ds_search.setColumn(nRow, "tmNm", g_Tm_Nm);
	ds_search.setColumn(nRow, "upjangCd", g_Upjang);
	ds_search.setColumn(nRow, "upjangNm", g_UpjangNm);
	ds_search.setColumn(nRow, "funeralHallCd", g_Upjang);
	
	// 입관일 오늘 날짜로 설정
	cal_adjsutFrom.fn_initMonthCal(this, fv_sToday, "", ds_search, "adjsutFrom");
	cal_adjustTo.fn_initMonthCal(this, fv_sToday, "", ds_search, "adjsutTo");
	
	// 급식, 식재, 외식, 구매 사업부는 팀, 업장 변경 불가.
	if((gfn_indexOf(ds_search.getColumn(0,"muNm"), "FS", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "식재", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "외식", 0) >= 0)
			||(gfn_indexOf(ds_search.getColumn(0,"muNm"), "구매", 0) >= 0)) {
		
		edt_upjangNm.enable = false;
		
	// FC사업지원본부는 사업부/팀/업장 변경 가능	
	}else if(gfn_indexOf(ds_search.getColumn(0,"muNm"), "FC", 0) >= 0) {
		edt_upjangNm.enable = true;
	}
}



/************************************************************************************************
 * 공통기능 FUNCTION 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 부서 세팅 
 *---------------------------------------------------------------------------------------------*/
//MU가 변경될 때에 대한 이벤트
function fn_muItemChangedEvent(obj, evt)
{
    edt_tmNm.setTmCode("");
    edt_upjangNm.setUpjangCode("");
        
    ds_search.setColumn(0,"muCd", edt_muNm.getMuCode());
    ds_search.setColumn(0,"muNm", edt_muNm.getMuName());
}

//TM이 변경될 때에 대한 이벤트
function fn_tmChangedEvent(obj, data)
{
    edt_upjangNm.setUpjangCode("");   
    ds_search.setColumn(0,"tmCd", edt_tmNm.getTmCode());
    ds_search.setColumn(0,"tmNm", edt_tmNm.getTmName());
}

//upjang이 변경될 때에 대한 이벤트
function fn_upjangChangedEvent(obj, data) {
    ds_search.setColumn(0,"upjangCd", edt_upjangNm.getUpjangCode());
    ds_search.setColumn(0,"upjangNm", edt_upjangNm.getUpjangName());
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_search()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 고인 목록 조회
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_search()
{
	// 조회시 Validation Check
	//if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;
	
	// 정산일자 DATETO가 작을경우 리턴
	if(cal_adjsutFrom.fn_getValue() > cal_adjustTo.fn_getValue()){
		gfn_alert("정산일자 TO날짜가 작습니다.");
		cal_adjustTo.setFocus();
		cal_adjustTo.fn_setValue(fv_sToday);
		return;
	}
	
    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(tab_funeralAdjustSum.tabpage1.grd_adjustDateSum);
    gfn_clearSortMark(tab_funeralAdjustSum.tabpage2.grd_adjustMonthSum);
    gfn_clearSortMark(tab_funeralAdjustSum.tabpage3.grd_adjustYearSum);
    
    //서비스 쿼리결과로 데이터가 없는 경우 데이터셋 컬럼정보가 사라지는 현상을 복원하기 위해 현재 컬럼정보 저장
    fv_funeralAdjustDateSumColInfo  = gfn_getDatasetInfo(ds_funeralAdjustDateSum);
	fv_funeralAdjustMonthSumColInfo = gfn_getDatasetInfo(ds_funeralAdjustMonthSum);
	fv_funeralAdjustYearSumColInfo  = gfn_getDatasetInfo(ds_funeralAdjustYearSum);
    
    // 조회조건 Param
    ds_search.setColumn(0, "funeralHallCd", ds_search.getColumn(0, "upjangCd"));
	ds_search.setColumn(0, "adjsutFrom", cal_adjsutFrom.mae_cal.value);
	ds_search.setColumn(0, "adjsutTo", cal_adjustTo.mae_cal.value);
//	ds_search.setColumn(0, "maskAuth", SCAuth.getMaskAuth(this));
	
	var strSvcID    = "searchFuneralAdjustSumList";
    var strURL      = "fs/fsf/funeralAdjustSum/selectFuneralAdjustSumList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_funeralAdjustDateSum=ds_funeralAdjustDateSum ds_funeralAdjustMonthSum=ds_funeralAdjustMonthSum ds_funeralAdjustYearSum=ds_funeralAdjustYearSum";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_Close()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 단기 버튼 이벤트
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_Close()
{
	this.close();
}

/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID) {
		case "searchFuneralAdjustSumList" :
            if(ds_funeralAdjustDateSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralAdjustDateSum, fv_funeralAdjustDateSumColInfo);
			}
			if(ds_funeralAdjustMonthSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralAdjustMonthSum, fv_funeralAdjustMonthSumColInfo);
			}
			if(ds_funeralAdjustYearSum.getRowCount() == 0){ //서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo(ds_funeralAdjustYearSum, fv_funeralAdjustYearSumColInfo);
			}
			break;
    }
}


/************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역 (필수)
************************************************************************************************/
/*
 ******************************************************************************************
 * 함  수  명   : fn_searchEnter()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 조회조건 enter 입력 이벤트
 * 작  성  자   : 백정훈
 * 작성  일자   : 2015-04-03
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
function fn_searchEnter(obj, e:KeyEventInfo)
{	
	if(e.keycode==13){
		fn_search();
	}
}
