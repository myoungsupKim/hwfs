<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[사업장 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT  A.UPJANG
       ,F.GRP_SEQ
       ,A.MU_NM        
       ,A.TM_NM
       ,A.UPJANGNM
       ,(SELECT NM_KOR FROM HLDC_HR_PERSONAL WHERE SABUN = A.CHARGE_SABUN) CHARGE_NM
       ,G.GRP_NAME
       ,'0' CHK
  FROM  SCO_UPJANG_MST_V A
       ,FSA_UPJANG_GRP F
       ,FSA_UPJANG_GRP_MST G
 WHERE A.UPJANG = F.UPJANG(+)
   AND F.GRP_SEQ = G.GRP_SEQ(+)
   AND A.CLOSE_DATE IS NULL   
<isNotNull col="MU_CD">
   AND A.MU_CD = #MU_CD#
</isNotNull>
<isNotNull col="TM_CD">
   AND A.TM_CD = #TM_CD#
</isNotNull>
<isNotNull col="UPJANG">
   AND A.UPJANG = #UPJANG#
</isNotNull>
 ORDER BY A.TM_NM, A.UPJANGNM	</statement>
	<input default-name="ds_cond">
	</input>
	<output default-name="ds_list">
	</output>
</query>
