{
	if (e.row == -1) return;
	
	//if( e.cell == 13 ){
	if( gfn_getCellColId(obj, e.cell, null ) == 'zipCode2' ){
		fn_zipCodePopup();
	//}else if( e.cell == 6 ){
	}else if( gfn_getCellColId(obj, e.cell, null ) == 'machineNm' ){
