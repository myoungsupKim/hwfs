/*----------------------------------------------------------------------------------------------
 * 설명      : 지원종류중(기물) 잔여수량 가져오기
 *---------------------------------------------------------------------------------------------*/
function fn_rmaQuantity()
{
	if(ds_grdUtensilList.getColumn(ds_grdUtensilList.rowposition,'machineClassCd') == null)return;
	
	var nRow = ds_grdUtensilList.rowposition;

	ds_search.setColumn(0,'machineClassCd',ds_grdUtensilList.getColumn(nRow,'machineClassCd'));
	ds_search.setColumn(0,'machineKindCd',ds_grdUtensilList.getColumn(nRow,'machineKindCd'));
	ds_search.setColumn(0,'machineCd',ds_grdUtensilList.getColumn(nRow,'machineCd'));
		
	var strSvcID    = "searchRmaQuantity";
    var strURL      = "fs/fsc/cateringSupportReg/CateringSupportRegQuantityList.xdo";    
    var strInDs     = "ds_search=ds_search";
    var strOutDs    = "ds_rmaQuantity=dsQuantityOutput";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
	
}


