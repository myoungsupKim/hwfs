		SCPopup.Address.newData(ds_list, nRow);
		ds_list.setColumn(nRow, "lendTermSdate",gfn_today());       // 지원기간
		ds_list.setColumn(nRow, "lendTermEdate", gfn_today());      // 지원기간 
		ds_list.setColumn(nRow, "apprvYn", "");       				// 승인여부
		ds_list.setColumn(nRow, "rmaDate", gfn_today());      		// 반납일자
		ds_list.setColumn(nRow, "requestUpjangCd", edt_upjangNm.getUpjangCode() );       // 업장코드
		ds_list.setColumn(nRow, "cntcNum", ds_upjangInfo.getColumn(ds_upjangInfo.rowposition, "cntcNum") );       // 연락처
		ds_list.setColumn(nRow, "zipCode1", ds_upjangInfo.getColumn(ds_upjangInfo.rowposition, "zipCode") );       // // 우편번호1
		// 결재자 이두순으로 고정
		ds_list.setColumn(nRow, "firstSignPrcgEmpNm", "이두순" );       // 	결재자
		ds_list.setColumn(nRow, "firstSignPrcgEmp", 200110240 );       // 	결재자
		
		//ds_list.setColumn(nRow, "addrInfo", AddrSearchComponent01.newData() );       // 주소
		
		//ds_list.setColumn(nRow, "zipCode2", ds_upjangInfo.getColumn(ds_upjangInfo.rowposition, "zipCode2") );       // // 우편번호2
		//ds_list.setColumn(nRow, "addr", ds_upjangInfo.getColumn(ds_upjangInfo.rowposition, "baseAddr") );       // 주소
		//ds_list.setColumn(nRow, "addrDetail", ds_upjangInfo.getColumn(ds_upjangInfo.rowposition, "addrDetail") );       // 상세주소
		ds_list.setColumn(nRow, "addrKey", ds_upjangInfo.getColumn(ds_upjangInfo.rowposition, "addrKey") );       // addrKey
		ds_list.setColumn(nRow, "addrInfo", ds_upjangInfo.getColumn(ds_upjangInfo.rowposition, "addrInfo") );       // addrKey
	
		SCPopup.bind(this, SCPopup.AddrSearch, "grd_utensilList.zipCode", "GRID:", null, {addrKey:"addrKey", addrInfo:"addrInfo", addrClass:"addrClass", baseAddr:"baseAddr", detailAddr:"detailAddr", etcAddr:"etcAddr"});

		//임시
		ds_list.setColumn(nRow, "machineCd", 100);       				
//		ds_list.setColumn(nRow, "rmaQuantity", 100);
       	
	} else if( cbo_catering.value == '004'){
		ds_list = ds_grdCookGuidList;								// 조리지도
		grd_list = grd_cookGuidList;
		nRow = ds_list.addRow();	
		
		ds_list.setColumn(nRow, "guidTermSdate",gfn_today());       // 지도기간
		ds_list.setColumn(nRow, "guidTermEdate", gfn_today());      // 지도기간		
		ds_list.setColumn(nRow, "allcYn", "");       				// 배정여부
		ds_list.setColumn(nRow, "requestUpjangCd", edt_upjangNm.getUpjangCode() );       // 업장코드 	
		
	}
	
		
	// Grid 입력 cell 지정
	grd_list.setFocus();
	grd_list.setCellPos(1);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{

	var ds_list = null;
	if(  cbo_catering.value == '001'){	
		ds_list = ds_grdCeleList;				//행사 요청	
	} else if( cbo_catering.value == '002'){
		ds_list = ds_grdCookHumanList;			//조리인력		
	} else if( cbo_catering.value == '003' ){
		ds_list = ds_grdUtensilList;			// 기기/기물 요청
	} else if( cbo_catering.value == '004'){
		ds_list = ds_grdCookGuidList;			// 조리지도
	}

	var nRow = ds_list.rowposition;	

	if(ds_list.rowposition == -1) 
	{
		gfn_alert("msg.err.grid.noselect", "삭제할 row");	// 선택된 항목이 없습니다.
		return;
	} 
	 
