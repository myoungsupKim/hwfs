function ds_grdUtensilList_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{

	if( e.columnid == 'requestQuantity') {
		var nRow = obj.rowposition;

		if (parseFloat(  obj.getColumn(nRow, 'rmaQuantity') ) < parseFloat( obj.getColumn(nRow, 'requestQuantity') )) 
		{
			gfn_alert("msg.err.validator.comparemin", "신청수량^잔여수량");	// $1 이(가) $2 보다 큽니다.
//			 obj.setColumn(nRow, 'requestQuantity' , obj.getOrgColumn(nRow,"requestQuantity"));
			 obj.setColumn(nRow, 'requestQuantity',""); 
		}
		// 잔여수량 체크
		fn_calQuantity();
	}
}

function edt_upjangNm_onkeydown(obj:Edit, e:KeyEventInfo)
{
	// Enter 키
	if(e.keycode == 13) 
	{		
        if (gfn_isNull(obj.tmCd))
        {
			edtBtn_upjangNm.click();
		}
	}
	// Del 키
	else if(e.keycode == 46) 
	{		
		ds_search.setColumn(0,'upjangCd','');
		ds_search.setColumn(0,'upjangNm','');
		
	}
}


function ds_grdCeleList_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{
	var nRow = obj.rowposition;
	if( e.columnid == 'dutyTime' && gfn_length(obj.getColumn(nRow, 'dutyTime')) > 0  ) {					
		obj.setColumn(nRow,'dutyTime' ,gfn_rPad( obj.getColumn(nRow, 'dutyTime') ,'0', 4 ))
	}
	
	if(e.columnid == "prcgTelno1"){
		if(obj.getColumn(nRow, "prcgTelno1") == "010"
		|| obj.getColumn(nRow, "prcgTelno1") == "016" 
		|| obj.getColumn(nRow, "prcgTelno1") == "017"
		|| obj.getColumn(nRow, "prcgTelno1") == "011"
		|| obj.getColumn(nRow, "prcgTelno1") == "018"
		|| obj.getColumn(nRow, "prcgTelno1") == "019"
		)
		{
			gfn_alert("휴대폰 번호는 입력 하실 수 없습니다.");
			obj.setColumn(nRow, "prcgTelno1", "");
			return false;
		}
	}
}

