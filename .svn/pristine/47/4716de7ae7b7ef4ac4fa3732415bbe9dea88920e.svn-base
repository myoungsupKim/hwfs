/*----------------------------------------------------------------------------------------------
 * 설명      : 신규 버튼 클릭 시 (필수-공통버튼에서 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_insert()
{	

	var ds_list = null;
	var grd_list = null;
	var nRow = 0;
	if(  cbo_catering.value == '001'){	
	
		ds_list = ds_grdCeleList;		//행사 요청	
		grd_list = grd_celeList;
		nRow = ds_list.addRow();
		
		ds_list.setColumn(nRow, "celeTermSdate",gfn_today());       // 행사기간 
		ds_list.setColumn(nRow, "celeTermEdate", gfn_today());      // 행사기간 
		ds_list.setColumn(nRow, "supHumanStrengthCd", '001');       // 지원인력 		
		ds_list.setColumn(nRow, "allcYn", "");       				// 배정여부 	
		ds_list.setColumn(nRow, "requestUpjangCd", edt_upjangNm.getUpjangCode() );       // 업장코드 	

		
	} else if( cbo_catering.value == '002'){
		ds_list = ds_grdCookHumanList;	//조리인력
		grd_list = grd_cookHumanList;
		nRow = ds_list.addRow();	
		
		ds_list.setColumn(nRow, "supTermSdate", gfn_today());       // 지원기간
		ds_list.setColumn(nRow, "supTermEdate", gfn_today());       // 지원기간 
		ds_list.setColumn(nRow, "supHumanStrengthCd", '001');       // 지원인력 		
		ds_list.setColumn(nRow, "allcYn", "");       				// 배정여부 
		ds_list.setColumn(nRow, "requestUpjangCd", edt_upjangNm.getUpjangCode() );       // 업장코드 	
		
	} else if( cbo_catering.value == '003' ){
		ds_list = ds_grdUtensilList;								// 기기/기물 요청
