//잔여수량 체크
function fn_calQuantity(){

	insQty = 0;		//등록수량
	var modQty = 0;		//수정수량
	var delQty = 0;		//삭제수량
	
	var qty = 0;
	var orgQty = 0;
	
	//A/S
	for(var i=0; i<ds_grdUtensilList.rowcount; i++){
		ds_grdUtensilList.filter("machineCd == '"+ ds_grdUtensilList.getColumn(ds_grdUtensilList.rowposition, "machineCd")+"'");
		
		//등록수량
		if(ds_grdUtensilList.getRowType(i) == 2){
			qty = gfn_isNull(ds_grdUtensilList.getColumn(i, "requestQuantity")) ? 0 : ds_grdUtensilList.getColumn(i, "requestQuantity");
			insQty += Number(qty);
			
		//수정수량
		}else if(ds_grdUtensilList.getRowType(i) == 4){
			qty = gfn_isNull(ds_grdUtensilList.getColumn(i, "requestQuantity")) ? 0 : ds_grdUtensilList.getColumn(i, "requestQuantity");
			orgQty = gfn_isNull(ds_grdUtensilList.getColumn(i, "orgQuantity")) ? 0 : ds_grdUtensilList.getColumn(i, "orgQuantity");
			modQty += (Number(orgQty) - Number(qty));

		//삭제수량
		}else if(ds_grdUtensilList.getRowType(i) == 8){
			qty = gfn_isNull(ds_grdUtensilList.getColumn(i, "orgQuantity")) ? 0 : ds_grdUtensilList.getColumn(i, "orgQuantity");
			delQty += qty;
		}
		ds_grdUtensilList.filter("");
	}
	
	
 	//trace("최초잔여수량 >> " + fv_orgResidualQty);
 	//trace("등록수량 >> " + insQty);
 	//trace("수정수량 >> " + modQty);
 	//trace("삭제수량 >> " + delQty);
// 	trace("============================");
	//최초잔여수량은 전역변수로 등록되어 있음
	//최초잔여수량 - 등록수량 + 수정수량 - 삭제수량
	var qty = Number(fv_orgResidualQty) - Number(insQty) + Number(modQty);
	//var qty = Number(fv_orgResidualQty) - Number(insQty) + Number(modQty) + Number(delQty);
	if(qty <0 ){
		gfn_alert("msg.err.validator.comparemin", "신청수량^잔여수량");	// $1 이(가) $2 보다 큽니다.
		ds_grdUtensilList.setColumn(ds_grdUtensilList.rowposition, 'requestQuantity',"");
	}else{
		ds_grdUtensilList.setColumn(ds_grdUtensilList.rowposition, "rmaQuantity", qty);
	}
	
}

