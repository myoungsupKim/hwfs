/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{	
	// Form Load 시 공통 기능 처리(obj:Form, bCreateEvent:이벤트 생성 여부)
	gfn_formOnLoad(this);
	
	// 초기값 셋팅
	fn_init();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{

	// 업데이트여부를 체크할 DataSet, 여러개 있을 경우 ;로 구분
	//v_ds_check = "ds_list";
	v_ds_check = "ds_grdCeleList;ds_grdCookHumanList;ds_grdUtensilList;ds_grdCookGuidList";
	
	
	SCCombo.bind(this, "ComCode", "cbo_catering", ds_cboDmnd, {groupCode:"FS0205", useYn:"Y", defVal:"001"}); 			//지원 종류
	SCCombo.bind(this, "ComCode", "grd_celeList.supHumanStrengthCd", ds_choSupHuman, {groupCode:"FS0206", useYn:"Y"}); 	//행사
	SCCombo.bind(this, "ComCode", "grd_cookHumanList.supHumanStrengthCd", ds_choSupHuman02, {groupCode:"FS0207", useYn:"Y"}); 		//조리인력 
	SCCombo.bind(this, "ComCode", "grd_cookHumanList.eduTrglst", ds_choEduTrglst, {groupCode:"FS0208", useYn:"Y"}); 	//조리지도
	

	//조회조건을 초기화한다.
	var nRow= ds_search.addRow();
	
	ds_search.setColumn(nRow, "muCd", g_Mu_Cd);
	ds_search.setColumn(nRow, "muNm", g_Mu_Nm);
	ds_search.setColumn(nRow, "tmCd", g_Tm_Cd);
	ds_search.setColumn(nRow, "tmNm", g_Tm_Nm);
	ds_search.setColumn(nRow, "upjangCd", g_Upjang);
	ds_search.setColumn(nRow, "upjangNm", g_UpjangNm);
	ds_search.setColumn(nRow, "loginPersMask", g_PersInfoMask);
	ds_search.setColumn(nRow, "maskAuth", SCAuth.getMaskAuth(this));
	
	
	ds_search.setColumn(nRow, "toDay", "");
	ds_search.setColumn(nRow, "fromDay", "");
	ds_search.setColumn(nRow, "catering", "001");	
	
	//조회 조건 날짜 초기값
	cal_fromDay.fn_initMonthCal(this,gfn_getFirstDate(),"", ds_search, "fromDay" );	
	cal_toDay.fn_initMonthCal(this,gfn_getLastDate(),"", ds_search, "toDay" );	
	
	//그리드 위치 조정
	grd_cookHumanList.position.top = grd_celeList.position.top;
	grd_utensilList.position.top = grd_celeList.position.top;
	grd_cookGuidList.position.top = grd_celeList.position.top;
	
	edt_tmNm.applyAuthLevel(edt_muNm);
    edt_upjangNm.applyAuthLevel(edt_tmNm);
    edt_upjangNm.applyMainUpjang(null);

	fn_viewControl('001');
	
	
	
	// 현재 업장의 기준 정보 받아오기
	fn_searchUpjangInfo();
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 화면 닫기 전 변경 유무 체크 (필수-Frame에서 창 닫힐때 호출됨)
 *---------------------------------------------------------------------------------------------*/
function fn_close()
{	
//	// v_ds_check 미사용시 변경사항 체크(입력 프로그램일때 변경되는 Dataset 전체 기술)
// 	if (gfn_dsIsUpdated(ds_list) == true) 
// 	{
// 		// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
//         if (!gfn_confirm("confirm.before.movepage")) return false;
//     }
//     
//	// 조회프로그램에도 Return true; 기술해 주세요!
// 	return true;

	// v_ds_check 사용시 변경사항 체크
	if(gfn_length(v_ds_check) == 0) 
	{	
		// v_ds_check 설정기준 변경사항 체크
		if (gfn_isDataChange() == true) 
		{		
			// 변경된 데이터가 있습니다. 현재 화면을 닫겠습니까?
			if (!gfn_confirm("confirm.before.movepage")) return false;
		}
	}
    
	// 조회프로그램에도 Return true; 기술해 주세요!
	return true;
}


