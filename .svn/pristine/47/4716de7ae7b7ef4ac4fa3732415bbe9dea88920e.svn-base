	// 이메일 여부 체크(함수 체크시 예제)  - isEmail:true(체크여부 true/false)
	
	// 오류시 focus 지정                      - focus:cal_toDate(Componet ID로 form에서부터의 경로)
	
	
	// 조회시 Validation Check
	if (gfn_validCheck(ds_search, ds_searchValid, true) == false) return;
	
	if(ds_search.getColumn(ds_search.rowposition, "tmNm") == null || ds_search.getColumn(ds_search.rowposition, "tmNm") == ""){
		gfn_alert("msg.err.validator.required","팀");
		return;
	}
	
	if(ds_search.getColumn(ds_search.rowposition, "upjangNm") == null || ds_search.getColumn(ds_search.rowposition, "upjangNm") == "")
	{
		gfn_alert("msg.err.validator.required","사업장");
		return;
	}

    // Grid sort 표시 초기화(필수)
    //gfn_clearSortMark(grd_list);
    gfn_clearSortMark(grd_celeList);		//행사 요청
    gfn_clearSortMark(grd_cookHumanList);	//조리인력
    gfn_clearSortMark(grd_utensilList);		//기기/기물 요청
    gfn_clearSortMark(grd_cookGuidList);	//조리지도
    
    var strSvcID    = "CateringSupportRegList";
    var strURL      = "fs/fsc/cateringSupportReg/CateringSupportRegList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_grdCeleList=dsCeleOutput ds_grdCookHumanList=dsCookHumanOutput ds_grdUtensilList=dsUtensilOutput ds_grdCookGuidList=dsCookGuidOutput";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    
    SCPopup.bind(this, SCPopup.AddrSearch, "grd_utensilList.zipCode", "GRID:", null, {addrKey:"addrKey", addrInfo:"addrInfo", addrClass:"addrClass", baseAddr:"baseAddr", detailAddr:"detailAddr", etcAddr:"etcAddr"});
}


