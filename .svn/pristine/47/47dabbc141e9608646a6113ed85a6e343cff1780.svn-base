		div_detail.msk_investAmt.value = fn_calInvestAmt(ds_dtlList.getColumn(rowIdx, "needQty"), ds_dtlList.getColumn(rowIdx, "lossRate"), ds_dtlList.getColumn(rowIdx, "gramPrice"));
