// 그리디 입력 체크.
function fn_RealDataCheck(intYy)
{
	fsp_clear(this);
	
	// 현재년
	var currYy = gfn_left(gfn_today(),4);
	
	// 현재월
	var currMm = gfn_today().substr(5,1); // 6
	
	// 입력년
	var intYy = yy.value;

		
	// 확정버튼 보여주기.
	var chkYn = ds_List.getColumn(1, "CONFIRM_YN");
	//alert("chkYn===>"+chkYn);
	// 사업계획, 수정계획 등록여부 확인.
	var strIstChk = ds_confirm_YN.getColumn(0, "CONFIRM_YN");
	//alert("strIstChk===>"+strIstChk);
	
	// 등록이 되어있을때만 버튼 보이게.
	if(strIstChk == "0"){
	    //변경 20151215
		btn_Confirm.visible = false;
	//	btn_Confirm.visible = true;
		for(var k=0; k<ds_ListDtl2.getRowCount(); k++){
		// 현재일 이전 일까지는 수정가.
			for(var m=5; m<=16; m++){
				gd_list.setCellProperty("body", m, "edittype", "masknumber");
				gd_listDtl2.setCellProperty("body", m, "edittype", "masknumber");
			}//end for
		}
		
	}else{
		if(chkYn == "N"){
			btn_Confirm.visible = true;
			for(var k=0; k<ds_ListDtl2.getRowCount(); k++){
			// 현재일 이전 일까지는 수정가.
				for(var m=5; m<=16; m++){
					gd_list.setCellProperty("body", m, "edittype", "masknumber");
					gd_listDtl2.setCellProperty("body", m, "edittype", "masknumber");
				}//end for
			}
		}else{
			btn_Confirm.visible = false;
			
			// 확정 후 조리직 데이터 입력불가.
			for(var k=0; k<ds_ListDtl2.getRowCount(); k++){
			// 현재일 이전 일까지는 수정불가.
				for(var m=5; m<=16; m++){
					gd_list.setCellProperty("body", m, "edittype", "none");
					gd_listDtl2.setCellProperty("body", m, "edittype", "none");
				}//end for
			}
		}
	}
	
	// 사업계획, 수정계획 개선과제 적용여부 확인.
	var strOrgYn = gfn_nvl(ds_orgin_YN.getColumn(0, "ORGIN_YN"), "");
	var strSndYn = gfn_nvl(ds_orgin_YN.getColumn(0, "BPC_SEND_YN"), "");
	//alert("strOrgYn==>"+strOrgYn + " , " + strSndYn);
	
	// 등록이 되어있을때만 버튼 보이게.
	if(strOrgYn == "Y"){
	    if(strSndYn == "N"){
			btn_LastConfirm.visible = true;
		} else {
			btn_LastConfirm.visible = false;
		}
	}else{
		if(strSndYn == "N"){
		
			btn_Confirm.visible = false;

			//추가 맹수영 20151221
			if (Cbo_gbn.value == "[FC] 2016년 사업계획 1차"){		
			
				btn_LastConfirm.visible = false;
			}
			else
			{
				btn_LastConfirm.visible = true;
			}
			
		}else{
			btn_Confirm.visible = false;
			btn_LastConfirm.visible = false;
		}	
	}
	
	if (strOrgYn == "" && strSndYn == "" && ds_List.rowcount > 1) {
	    btn_Confirm.visible = true;
	}
	
	// 재무일때는 수정불가, 버튼 숨김
	var strCt = rd_CostType.value;
	if(strCt == "U"){
		btn_Confirm.visible = false;
		btn_LastConfirm.visible = false;
	}
}

// 그리드 체크로직
function fn_checkCell( currow, colID )
{  
	var sRtn = "none";
	 
	// 현재년
	var currYy = gfn_today().substr(0,4);
	
	// 현재월
	var currMm = gfn_today().substr(4,2);
	
	// 입력년
	var intYy = yy.value;
	//trace("currYy==>"+currYy); trace("intYy==>"+intYy);
	// 사업계획, 수정계획 개선과제 적용여부 확인.
	//var strCmfYn = ds_cbo_GBN.getColumn(0, "CONFIRM_YN");
	var strCmfYn = ds_confirm_YN.getColumn(0, "CONFIRM_YN");
	//var strBpcYn = ds_cbo_GBN.getColumn(0, "BPC_SEND_YN");
	var strBpcYn = ds_orgin_YN.getColumn(0, "BPC_SEND_YN");
	
	// 구분 : '사업계획', '최종PL'이 아니고
	// 년도 : 현재년도와 입력년도가 같을때 실적데이터 수정불가.
	// 년월 : 현재년월과 입력년월이 같고, 오늘 이전 실적데이터 수정불가.
	// 아래로직은 주석처리.
	if (currYy == intYy){
		// 편집불가 그리드 컬럼 43201120
		if( ds_List.getColumn( currow, "ACCT_CD" ) == "43102010" || ds_List.getColumn( currow, "ACCT_CD" ) == "43102020" || ds_List.getColumn( currow, "ACCT_CD" ) == "43102030" || ds_List.getColumn( currow, "ACCT_CD" ) == "43201120" || ds_List.getColumn( currow, "ACCT_CD" ) == "43201130" || ds_List.getColumn( currow, "ACCT_CD" ) == "43201140" || ds_List.getColumn( currow, "ACCT_CD" ) == "43201150"){
			sRtn = "none";
		}else{
			//sRtn = "masknumber";
			if( parseInt(colID) >= parseInt(currMm) ){
				sRtn = "masknumber";
			}else{
				sRtn = "none";
			}
		}
	}else if (currYy > intYy){
		sRtn = "none";
	}else{
		if( ds_List.getColumn( currow, "ACCT_CD" ) == "43102010" || ds_List.getColumn( currow, "ACCT_CD" ) == "43102020" || ds_List.getColumn( currow, "ACCT_CD" ) == "43102030" || ds_List.getColumn( currow, "ACCT_CD" ) == "43201120" || ds_List.getColumn( currow, "ACCT_CD" ) == "43201130" || ds_List.getColumn( currow, "ACCT_CD" ) == "43201140" || ds_List.getColumn( currow, "ACCT_CD" ) == "43201150"){
			sRtn = "none";
		}else{
			sRtn = "masknumber";
		}
	}
	
	// 재무일때는 수정불가
	var strCt = rd_CostType.value;
	
	if(strCt == "U"){
		sRtn = "none";
	}
	//추가 20151215 맹수영
	else{
		sRtn = "masknumber";
	}
	 
	// 확정시 편집불가 그리드 컬럼
	//alert("strCmfYn==>"+strCmfYn); alert("strBpcYn==>"+strBpcYn);
	if(strCmfYn == "1" && strBpcYn == "Y"){
		sRtn = "none";
	}
	//추가 20151215 맹수영
	else{
		sRtn = "masknumber";
	}
	
	return sRtn;	
}

