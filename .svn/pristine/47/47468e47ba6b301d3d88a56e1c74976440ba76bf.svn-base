// 조리직 변경시 처리.
function ds_ListDtl2_oncolumnchanged(obj:Dataset, e:DSColChangeEventInfo)
{	
	var getSum = 0;

	if( e.columnid == "MM_AMT_01" || 
	    e.columnid == "MM_AMT_02" || 
	    e.columnid == "MM_AMT_03" || 
	    e.columnid == "MM_AMT_04" || 
	    e.columnid == "MM_AMT_05" || 
	    e.columnid == "MM_AMT_06" || 
	    e.columnid == "MM_AMT_07" || 
	    e.columnid == "MM_AMT_08" || 
	    e.columnid == "MM_AMT_09" || 
	    e.columnid == "MM_AMT_10" || 
	    e.columnid == "MM_AMT_11" || 
	    e.columnid == "MM_AMT_12"  )
	{
		getSum = ds_ListDtl2.getColumn(0, e.columnid) + ds_ListDtl2.getColumn(1, e.columnid) + ds_ListDtl2.getColumn(2, e.columnid);
		//alert("getSum==>"+getSum);
		
		if( obj.getColumn(obj.rowposition, "ACCT_CD") == "43102010" )
		{
			fn_subSum( e.columnid, e.newvalue, "43102010", getSum );
		}
