// 공통경비 + 조리직 직원급여 합계.
function fn_subSum( colID, colValue, acctCd, inSum )
{
	var nRow = ds_List.findRow("ACCT_CD", acctCd );
	var nRow1 = ds_ListDtlTmp.findRow("ACCT_CD", acctCd );
	var nSum = 0;
	
	// 4대보험 적용
	var nRowA = ds_List.findRow("ACCT_CD", "43201120" );// 국민연금
	var nRowB = ds_List.findRow("ACCT_CD", "43201130" );// 건강보험
	var nRowC = ds_List.findRow("ACCT_CD", "43201140" );// 산재보험
	var nRowD = ds_List.findRow("ACCT_CD", "43201150" );// 고용보험
	var nSumA = 0;
	var nSumB = 0;
	var nSumC = 0;
	var nSumD = 0;
	
	//
	//alert("colID==>"+colID); // MM_AMT_01
	//alert("colValue==>"+colValue); // 100
	//alert("acctCd==>"+acctCd); // 43102010
	//alert("xx=>"+ds_ListDtlTmp.getColumn( nRow1, colID ));
	
	//alert("111=>"+ds_ListDtlTmp.getColumn(1, colID));
	//alert("222=>"+ds_ListDtlTmp.getColumn(2, colID));
	//alert("333=>"+ds_ListDtlTmp.getColumn(3, colID));
	
	
	//nSumA = ds_ListDtlTmp.getColumn(1, colID) + ds_ListDtlTmp.getColumn(2, colID) + ds_ListDtlTmp.getColumn(3, colID);
	//alert("nSumA===>"+nSumA);
	
	//alert("1111=>"+ds_ListDtlTmp2.getColumn(0, colID));
	
	if( nRow1 != -1 )
	{
		nSum = ds_ListDtlTmp.getColumn( nRow1, colID ) + colValue;
		nSum = Math.round(nSum);
		ds_List.setColumn( nRow, colID, nSum );
		
		// 2, 9, 11, 12월일때는 전월 값을 복사하여 사용.
		if(colID == "MM_AMT_02"){
			//alert("MM_AMT_02MM_AMT_02=>"+ds_List.getColumn(nRowA, "MM_AMT_01") );
			ds_List.setColumn(nRowA, "MM_AMT_02", ds_List.getColumn(nRowA, "MM_AMT_01"));
			ds_List.setColumn(nRowB, "MM_AMT_02", ds_List.getColumn(nRowB, "MM_AMT_01"));
			ds_List.setColumn(nRowC, "MM_AMT_02", ds_List.getColumn(nRowC, "MM_AMT_01"));
			ds_List.setColumn(nRowD, "MM_AMT_02", ds_List.getColumn(nRowD, "MM_AMT_01"));
		}else if(colID == "MM_AMT_09"){
			ds_List.setColumn(nRowA, "MM_AMT_09", ds_List.getColumn(nRowA, "MM_AMT_08"));
			ds_List.setColumn(nRowB, "MM_AMT_09", ds_List.getColumn(nRowB, "MM_AMT_08"));
			ds_List.setColumn(nRowC, "MM_AMT_09", ds_List.getColumn(nRowC, "MM_AMT_08"));
			ds_List.setColumn(nRowD, "MM_AMT_09", ds_List.getColumn(nRowD, "MM_AMT_08"));
		}else if(colID == "MM_AMT_11"){
			ds_List.setColumn(nRowA, "MM_AMT_11", ds_List.getColumn(nRowA, "MM_AMT_10"));
			ds_List.setColumn(nRowB, "MM_AMT_11", ds_List.getColumn(nRowB, "MM_AMT_10"));
			ds_List.setColumn(nRowC, "MM_AMT_11", ds_List.getColumn(nRowC, "MM_AMT_10"));
			ds_List.setColumn(nRowD, "MM_AMT_11", ds_List.getColumn(nRowD, "MM_AMT_10"));
		}else if(colID == "MM_AMT_12"){
			ds_List.setColumn(nRowA, "MM_AMT_12", ds_List.getColumn(nRowA, "MM_AMT_10"));
			ds_List.setColumn(nRowB, "MM_AMT_12", ds_List.getColumn(nRowB, "MM_AMT_10"));
			ds_List.setColumn(nRowC, "MM_AMT_12", ds_List.getColumn(nRowC, "MM_AMT_10"));
			ds_List.setColumn(nRowD, "MM_AMT_12", ds_List.getColumn(nRowD, "MM_AMT_10"));
		}else{
			// 국민연금(43201120)
			nSumA = (ds_ListDtlTmp.getColumn(1, colID) + ds_ListDtlTmp.getColumn(2, colID) + ds_ListDtlTmp.getColumn(3, colID) + inSum) * (ds_ListDtlTmp2.getColumn(0, colID) / 100);
			nSumA = Math.round(nSumA);
			ds_List.setColumn( nRowA, colID, nSumA );
			
			// 건강보험(43201130)
			nSumB = (ds_ListDtlTmp.getColumn(1, colID) + ds_ListDtlTmp.getColumn(2, colID) + ds_ListDtlTmp.getColumn(3, colID) + inSum) * (ds_ListDtlTmp2.getColumn(1, colID) / 100);
			nSumB = Math.round(nSumB);
			ds_List.setColumn( nRowB, colID, nSumB );
			
			// 산재보험(43201140)
			nSumC = (ds_ListDtlTmp.getColumn(1, colID) + ds_ListDtlTmp.getColumn(2, colID) + ds_ListDtlTmp.getColumn(3, colID) + inSum) * (ds_ListDtlTmp2.getColumn(2, colID) / 100);
			nSumC = Math.round(nSumC);
			ds_List.setColumn( nRowC, colID, nSumC );
			
			// 고용보험(43201150)
			nSumD = (ds_ListDtlTmp.getColumn(1, colID) + ds_ListDtlTmp.getColumn(2, colID) + ds_ListDtlTmp.getColumn(3, colID) + inSum) * (ds_ListDtlTmp2.getColumn(3, colID) / 100);
			ds_List.setColumn( nRowD, colID, nSumD );	
		}
		
	}else{
		nSum = colValue;
		nSum = Math.round(nSum);
		ds_List.setColumn( nRow, colID, nSum );
		
		// 2, 9, 11, 12월일때는 전월 값을 복사하여 사용.
		if(colID == "MM_AMT_02"){
			//alert("MM_AMT_02MM_AMT_02=>"+ds_List.getColumn(nRowA, "MM_AMT_01") );
			ds_List.setColumn(nRowA, "MM_AMT_02", ds_List.getColumn(nRowA, "MM_AMT_01"));
		}else if(colID == "MM_AMT_09"){
			ds_List.setColumn(nRowA, "MM_AMT_09", ds_List.getColumn(nRowA, "MM_AMT_08"));
		}else if(colID == "MM_AMT_11" || colID == "MM_AMT_12"){
			ds_List.setColumn(nRowA, "MM_AMT_11", ds_List.getColumn(nRowA, "MM_AMT_10"));
		}else if(colID == "MM_AMT_11" || colID == "MM_AMT_12"){
			ds_List.setColumn(nRowA, "MM_AMT_12", ds_List.getColumn(nRowA, "MM_AMT_10"));
		}else{
			// 국민연금(43201120)
			nSumA = inSum * (ds_ListDtlTmp2.getColumn(0, colID) / 100);
			nSumA = Math.round(nSumA);
			ds_List.setColumn( nRowA, colID, nSumA );
			
			// 건강보험(43201130)
			nSumB = inSum * (ds_ListDtlTmp2.getColumn(1, colID) / 100);
			nSumB = Math.round(nSumB);
			ds_List.setColumn( nRowB, colID, nSumB );
			
			// 산재보험(43201140)
			nSumC = inSum * (ds_ListDtlTmp2.getColumn(2, colID) / 100);
			nSumC = Math.round(nSumC);
			ds_List.setColumn( nRowC, colID, nSumC );
			
			// 고용보험(43201150)
			nSumD = inSum * (ds_ListDtlTmp2.getColumn(3, colID) / 100);
			nSumD = Math.round(nSumD);
			ds_List.setColumn( nRowD, colID, nSumD );
		}
		
	}
}

