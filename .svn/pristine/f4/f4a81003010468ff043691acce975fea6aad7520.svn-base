<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>delete</type>
	<description><![CDATA[레시피 전체 삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>
DELETE FROM FSM_REAL_RECIPE_MST
WHERE 
	 UPJANG = #UPJANG#
	 AND MENU_CD = #MENU_CD#
<isNotNull col="RECIPE_CD">
              AND RECIPE_CD = #RECIPE_CD#
</isNotNull>	</statement>
	<input default-name="ds_list">
		<col name="UPJANG" size="22" type="NUMERIC" description="" /> 
		<col name="MENU_CD" size="20" type="VARCHAR" description="" /> 
		<col name="RECIPE_CD" size="50" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_list">
	</output>
</query>
