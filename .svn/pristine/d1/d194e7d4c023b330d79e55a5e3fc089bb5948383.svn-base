<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>update</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
UPDATE 
 	MAC_RPT_SET
SET 
	 RPT_NAME = #RPT_NAME#
	, BACK_COLOR = #BACK_COLOR#
	, FONT_COLOR = #FONT_COLOR#
	, FONT_SIZE = #FONT_SIZE#
	, FONT_TYPE = #FONT_TYPE#
	, ITEM1 = #ITEM1#
	, ITEM2 = #ITEM2#
	, ITEM3 = #ITEM3#
	, ORDER_NUM = #ORDER_NUM#
	, EXPR_MARK = #EXPR_MARK#
	, MARK_PLACE = #MARK_PLACE#
	, EXPR_CLASS = #EXPR_CLASS#
	, EXPR = #EXPR#
	, ATTR01 = #ATTR01#
	, ATTR02 = #ATTR02#
	, ATTR03 = #ATTR03#
	, ATTR04 = #ATTR04#
	, ATTR05 = #ATTR05#
	, ATTR06 = #ATTR06#
	, ATTR07 = #ATTR07#
	, ATTR08 = #ATTR08#
	, ATTR09 = #ATTR09#
	, ATTR10 = #ATTR10#
	, CUSER = #CUSER#
	, CDATE = #CDATE#
	, UUSER = #g_EmpNo#
	, UDATE = SYSDATE
WHERE 
	 RPT_ID = #RPT_ID#
	 AND LINE_NO = #LINE_NO#	]]></statement>
	<input default-name="ds_uin">
		<col name="RPT_NAME" size="100" type="VARCHAR" description="" /> 
		<col name="BACK_COLOR" size="20" type="VARCHAR" description="" /> 
		<col name="FONT_COLOR" size="20" type="VARCHAR" description="" /> 
		<col name="FONT_SIZE" size="5" type="VARCHAR" description="" /> 
		<col name="FONT_TYPE" size="1" type="VARCHAR" description="" /> 
		<col name="ITEM1" size="100" type="VARCHAR" description="" /> 
		<col name="ITEM2" size="100" type="VARCHAR" description="" /> 
		<col name="ITEM3" size="100" type="VARCHAR" description="" /> 
		<col name="ORDER_NUM" size="22" type="NUMERIC" description="" /> 
		<col name="EXPR_MARK" size="10" type="VARCHAR" description="" /> 
		<col name="MARK_PLACE" size="1" type="VARCHAR" description="" /> 
		<col name="EXPR_CLASS" size="1" type="VARCHAR" description="" /> 
		<col name="EXPR" size="200" type="VARCHAR" description="" /> 
		<col name="ATTR01" size="200" type="VARCHAR" description="" /> 
		<col name="ATTR02" size="200" type="VARCHAR" description="" /> 
		<col name="ATTR03" size="200" type="VARCHAR" description="" /> 
		<col name="ATTR04" size="200" type="VARCHAR" description="" /> 
		<col name="ATTR05" size="200" type="VARCHAR" description="" /> 
		<col name="ATTR06" size="22" type="NUMERIC" description="" /> 
		<col name="ATTR07" size="22" type="NUMERIC" description="" /> 
		<col name="ATTR08" size="22" type="NUMERIC" description="" /> 
		<col name="ATTR09" size="22" type="NUMERIC" description="" /> 
		<col name="ATTR10" size="22" type="NUMERIC" description="" /> 
		<col name="CUSER" size="20" type="VARCHAR" description="" /> 
		<col name="CDATE" size="7" type="DATE" description="" /> 
		<col name="g_EmpNo" size="20" type="VARCHAR" description="" /> 
		<col name="RPT_ID" size="20" type="VARCHAR" description="" /> 
		<col name="LINE_NO" size="22" type="NUMERIC" description="" /> 
	</input>
	<output default-name="ds_uout">
