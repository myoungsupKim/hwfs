function btn_DeleteSp_OnClick(obj:Button, e:ClickEventInfo)
{
	if (ds_List_Detail.rowcount < 1)
	{
		g_Message("EE", fn_AlertMsg("006"));
		return;
	}
	if (ds_List_Detail.rowposition < 0)
	{
		g_Message("EE", "삭제할 주문내역을 선택하세요.");
		return;
	}
	if ((ds_List_Detail.getColumn(ds_List_Detail.rowposition,"LINE_STATUS") <> "정산") &&
		(ds_List_Detail.getColumn(ds_List_Detail.rowposition,"LINE_STATUS") <> "배송"))
	{
		g_Message("EE", "정산 및 배송상태만 삭제가능합니다!");
		return;
	}
	if (! g_Confirm("01", '삭제 후엔 복구되지 않습니다!\n\n선택하신 주문내역을 삭제하시겠습니까?!')) return;
	
	//삭제처리
	var strArgument = "strSpID=" + ds_List_Detail.getColumn(ds_List_Detail.rowposition,"SP_ID");
	// 서버 호출 
	jsp_transaction(this, "FSP00016E_T002", "U_svc::" + "fs/fsp/FSP00016E_T002.jsp", "", "", strArgument, "", false, false, true);
	//
	ds_List_Detail.deleteRow(ds_List_Detail.rowposition);
	g_Message("01","삭제되었습니다.");
}


