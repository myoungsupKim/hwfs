	ds_Cond.setColumn(0, "SEASON_NUM",  cb_Season.value);
	ds_Cond.setColumn(0, "SP_DATET",  me_PrDateT.value);
	ds_Cond.setColumn(0, "STATUS",    cbo_Status.value);
	ds_Cond.setColumn(0, "BILL_NO",   ed_BillNo.value);

	ds_Cond.setColumn(0, "PR_SABUN_MU",   cbo_PR_SABUN_MU.value);
	ds_Cond.setColumn(0, "ITEM_MU",       cbo_ITEM_MU.value);
	ds_Cond.setColumn(0, "CUSTCD",        ed_CustCd.userdata);
	ds_Cond.setColumn(0, "ITEM_CODE",     ed_ItemCd.userdata);

	ds_Cond.setColumn(0, "PR_SABUN_CC_CD", ed_PR_SABUN_CC_CD.userdata);
	ds_Cond.setColumn(0, "SALE_TYPE",        cbo_SALE_TYPE.value);	

	ds_Cond.setColumn(0, "PR_SABUN", ed_PR_SABUN.userdata);
	ds_Cond.setColumn(0, "RQ_SABUN", ed_RQ_SABUN.userdata);

	ds_Cond.setColumn(0, "SP_ID", ed_SpID.value);

	ds_Cond.setColumn(0, "SORT",     rd_Sort.value);

	// 조회
	var strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc;
	// 경로 설정
	strSvcID = "FSP00016E_T001";
	strURL   = "U_svc::" + "fs/fsp/FSP00016E_T001.jsp";
	// 데이터셋 설정
	strInDatasets  = "ds_Cond=ds_Cond";
	strOutDatasets = "ds_List_Detail=ds_List";

	// 파라메터 설정
	if (fa_Special_Auth == "Y")
		strArgument = "sType='QryItmAuth'";
	else
		strArgument = "sType='QryItm'";
		
	// 콜백함수 지정
	strCallbackFunc = "fn_CallBack_Search";
	// 서버 호출 
	jsp_transaction(this, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, false, false, true);
}
