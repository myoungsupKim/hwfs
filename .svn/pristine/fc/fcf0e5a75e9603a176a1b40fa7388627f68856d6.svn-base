		<col name="V_GUBUN" size="255" type="VARCHAR" description="" /> 
		<col name="V_TYPE_CD" size="255" type="VARCHAR" description="" /> 		
		<col name="V_ORGID" size="" type="NUMBER" description="" />
		<col name="V_ID" size="" type="NUMBER" description="" />
		<col name="V_AMT" size="" type="NUMBER" description="" />
	</input>
	<output default-name="ds_list">
<col name="JANAMT" size="" type="NUMBER" description="" />
	</output>
</query>