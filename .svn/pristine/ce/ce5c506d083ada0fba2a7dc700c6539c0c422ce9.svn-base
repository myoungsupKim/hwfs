<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
    SELECT (SELECT SAUPNO FROM HLDC_ST_CUST WHERE CUSTCD = #CUSTCD#) EQUAL_SAUPNO
    FROM DUAL
</statement>
	<input default-name="ds_cond">
		<col name="CUSTCD" size="" type="NUMERIC" description="" /> 
	</input>
	<output default-name="ds_list">
		<col name="EQUAL_SAUPNO" size="50" type="VARCHAR" description="" /> 
		
		
	</output>
</query>