<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>


 SELECT RTNDEPT_ID,
        RTNDEPT_IDNM,
        BUDDEPT_ID,
        BUDDEPT_IDNM,
        DR_ACCTCD,
        DR_ACCTCDNM,
        CR_ACCTCD,
        CR_ACCTCDNM,
        FEE_ACCTCD,
        FEE_ACCTCDNM,
        ACNTCD,
        BANK_BRANCHCD,
        CUSTCD,
        MANAGE_DEPT_ID,
        FEE_CUSTCD,
        AMT,
        FEE,
        DESCRIPTION_NM,
        DR_PARACD1,
        DR_PARACD2,
        DR_PARACD3,
        DR_PARACD4,
        DR_PARACD5,
        DR_PARACD6,
        CR_PARACD1,
        CR_PARACD2,
        CR_PARACD3,
        CR_PARACD4,
        CR_PARACD5,
        CR_PARACD6,
        (SELECT NVL(EXP_CD, 'N') FROM HLDC_SC_DEPT WHERE DEPT_ID = RTNDEPT_ID) EXP_CD,
        SUS_SEQ,
        PARTIAL_SEQ,
        #TRX_DATE#||'01'||'-'||TO_CHAR(LAST_DAY(TO_DATE(#TRX_DATE#||'01', 'YYYYMMDD')), 'YYYYMMDD') DATE_BETWEEN,
        SORT_GUBUN
 FROM (
    SELECT B.DEPT_ID       RTNDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) RTNDEPT_IDNM,
           B.DEPT_ID       BUDDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) BUDDEPT_IDNM,
           DECODE(A.DR_ACCTCD, '2110451', '', '2110452', '', A.DR_ACCTCD)   DR_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = DECODE(A.DR_ACCTCD, '2110451', '', '2110452', '', A.DR_ACCTCD)) DR_ACCTCDNM,
           A.CR_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_ACCTCDNM,
           A.FEE_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) FEE_ACCTCDNM,
           SC_CRYPTO_FUN('DEC', A.ACNTCD_ENC , '3') AS ACNTCD,
           --(SELECT BANK_BRANCHCD FROM HLDC_ST_ACNT WHERE SC_CRYPTO_FUN('DEC', ACNTCD_ENC , '3') = SC_CRYPTO_FUN('DEC', A.ACNTCD_ENC , '3')) BANK_BRANCHCD,
           (SELECT BANK_BRANCHCD FROM HLDC_ST_ACNT WHERE ACNTCD_ENC = A.ACNTCD_ENC) BANK_BRANCHCD,
           A.CUSTCD,
           B.DEPT_ID MANAGE_DEPT_ID,
           --(SELECT CUSTCD FROM HLDC_ST_ACNT V1, HLDC_ST_BANK_BRANCH V2 WHERE V1.BANK_BRANCHCD = V2.BANK_BRANCHCD AND SC_CRYPTO_FUN('DEC', V1.ACNTCD_ENC , '3') = SC_CRYPTO_FUN('DEC', A.ACNTCD_ENC , '3')) FEE_CUSTCD,
           (SELECT CUSTCD FROM HLDC_ST_ACNT V1, HLDC_ST_BANK_BRANCH V2 WHERE V1.BANK_BRANCHCD = V2.BANK_BRANCHCD AND V1.ACNTCD_ENC = A.ACNTCD_ENC) FEE_CUSTCD,
           NVL(SUM(A.AMT), 0)           AMT,
           NVL(SUM(A.FEE), 0)           FEE,
           MAX(C.UPJANGNM)||' 입금자동전표'      DESCRIPTION_NM,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD6,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD6,
           ''     SUS_SEQ,
           ''     PARTIAL_SEQ,
           DECODE(A.DR_ACCTCD, '2110451', '2', '2110452', '2', '1') SORT_GUBUN
    FROM MAS_RCP A,
         HLDC_SC_DEPT B,
         HLDC_ST_UPJANG C
    WHERE (A.UPJANG    = C.UPJANG)
    AND   (C.DEPT_ID   = B.DEPT_ID)
    AND   (B.PT_CD     = #PT_CD#)
    AND   (A.RCP_DATE  BETWEEN #TRX_DATE#||'01' AND #TRX_DATE#||'31')
    AND   (TRIM(A.GAMU) IS NULL)
    --GROUP BY B.DEPT_ID, A.DR_ACCTCD, A.CR_ACCTCD, A.FEE_ACCTCD, SC_CRYPTO_FUN('DEC', A.ACNTCD_ENC , '3'),
    GROUP BY B.DEPT_ID, A.DR_ACCTCD, A.CR_ACCTCD, A.FEE_ACCTCD, A.ACNTCD_ENC,
             A.CUSTCD
    UNION ALL
    SELECT B.DEPT_ID       RTNDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) RTNDEPT_IDNM,
           B.DEPT_ID       BUDDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) BUDDEPT_IDNM,
           A.DR_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_ACCTCDNM,
           ''            CR_ACCTCD,
           ''            CR_ACCTCDNM,
           ''            FEE_ACCTCD,
           ''            FEE_ACCTCDNM,
           SC_CRYPTO_FUN('DEC', A.ACNTCD_ENC , '3') AS ACNTCD,
           D.BANK_BRANCHCD,
           A.CUSTCD,
           B.DEPT_ID       MANAGE_DEPT_ID,
           (SELECT CUSTCD FROM HLDC_ST_BANK_BRANCH WHERE BANK_BRANCHCD = D.BANK_BRANCHCD) FEE_CUSTCD,
           NVL(A.AMT, 0)           AMT,
           0                       FEE,
           C.UPJANGNM||' 입금자동전표'      DESCRIPTION_NM,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD6,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD6,
           TO_CHAR(A.SUS_SEQ)     SUS_SEQ,
           TO_CHAR(A.PARTIAL_SEQ) PARTIAL_SEQ,
           '3' SORT_GUBUN
    FROM MAS_RCP A,
         HLDC_SC_DEPT B,
         HLDC_ST_UPJANG C,
         HLDC_ST_ACNT D
    WHERE (A.UPJANG    = C.UPJANG)
    AND   (C.DEPT_ID   = B.DEPT_ID)
    --AND   (SC_CRYPTO_FUN('DEC', A.ACNTCD_ENC , '3')    = SC_CRYPTO_FUN('DEC', D.ACNTCD_ENC , '3'))
    AND   A.ACNTCD_ENC = D.ACNTCD_ENC    
    AND   (B.PT_CD  = #PT_CD#)
    AND   (A.RCP_DATE  BETWEEN #TRX_DATE#||'01' AND #TRX_DATE#||'31')
    AND   (A.DR_ACCTCD = '2110452')
    AND   (TRIM(A.GAMU) IS NULL)
    UNION ALL
    SELECT B.DEPT_ID       RTNDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) RTNDEPT_IDNM,
           B.DEPT_ID       BUDDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) BUDDEPT_IDNM,
           DECODE(A.RMT_DR_ACCTCD, '2110451', '', '2110452', '', A.RMT_DR_ACCTCD)   DR_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = DECODE(A.RMT_DR_ACCTCD, '2110451', '', '2110452', '', A.RMT_DR_ACCTCD)) DR_ACCTCDNM,
           A.RMT_CR_ACCTCD CR_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_ACCTCDNM,
           '' FEE_ACCTCD,
           '' FEE_ACCTCDNM,
           SC_CRYPTO_FUN('DEC', A.RMT_ACNTCD_ENC , '3') AS ACNTCD,
           --(SELECT BANK_BRANCHCD FROM HLDC_ST_ACNT WHERE SC_CRYPTO_FUN('DEC', ACNTCD_ENC , '3') = SC_CRYPTO_FUN('DEC', A.RMT_ACNTCD_ENC , '3')) BANK_BRANCHCD,
           (SELECT BANK_BRANCHCD FROM HLDC_ST_ACNT WHERE ACNTCD_ENC = A.RMT_ACNTCD_ENC) BANK_BRANCHCD,
           NULL CUSTCD,
           B.DEPT_ID MANAGE_DEPT_ID,
           --(SELECT CUSTCD FROM HLDC_ST_ACNT V1, HLDC_ST_BANK_BRANCH V2 WHERE V1.BANK_BRANCHCD = V2.BANK_BRANCHCD AND SC_CRYPTO_FUN('DEC', V1.ACNTCD_ENC , '3') = SC_CRYPTO_FUN('DEC', A.RMT_ACNTCD_ENC , '3')) FEE_CUSTCD,
           (SELECT CUSTCD FROM HLDC_ST_ACNT V1, HLDC_ST_BANK_BRANCH V2 WHERE V1.BANK_BRANCHCD = V2.BANK_BRANCHCD AND V1.ACNTCD_ENC = A.RMT_ACNTCD_ENC) FEE_CUSTCD,
           NVL(SUM(A.RMT_AMT), 0)       AMT,
           NVL(SUM(A.RMT_FEE), 0)       FEE,
           MAX(C.UPJANGNM)||' 입금자동전표 계좌이체'      DESCRIPTION_NM,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD6,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD6,
           ''     SUS_SEQ,
           ''     PARTIAL_SEQ,
           DECODE(A.RMT_CR_ACCTCD, '2110451', '5', '2110452', '5', '4') SORT_GUBUN
    FROM MAS_RMT A,
         HLDC_SC_DEPT B,
         HLDC_ST_UPJANG C
    WHERE (A.UPJANG    = C.UPJANG)
    AND   (C.DEPT_ID   = B.DEPT_ID)
    AND   (B.PT_CD     = #PT_CD#)
    AND   (A.RMT_DATE  BETWEEN #TRX_DATE#||'01' AND #TRX_DATE#||'31')
    AND   (TRIM(A.GAMU) IS NULL)
    --GROUP BY B.DEPT_ID, A.RMT_DR_ACCTCD, A.RMT_CR_ACCTCD, SC_CRYPTO_FUN('DEC', A.RMT_ACNTCD_ENC , '3')
    GROUP BY B.DEPT_ID, A.RMT_DR_ACCTCD, A.RMT_CR_ACCTCD, A.RMT_ACNTCD_ENC
    UNION ALL
    SELECT /*+RULE*/B.DEPT_ID       RTNDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) RTNDEPT_IDNM,
           B.DEPT_ID       BUDDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) BUDDEPT_IDNM,
           A.RMT_DR_ACCTCD   DR_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_ACCTCDNM,
           '' CR_ACCTCD,
           '' CR_ACCTCDNM,
           '' FEE_ACCTCD,
           '' FEE_ACCTCDNM,
           SC_CRYPTO_FUN('DEC', A.RMT_ACNTCD_ENC , '3') ACNTCD,
           '' BANK_BRANCHCD,
           NULL CUSTCD,
           B.DEPT_ID MANAGE_DEPT_ID,
           NULL FEE_CUSTCD,
           NVL(A.RMT_AMT, 0)       AMT,
           0                            FEE,
           C.UPJANGNM||' 입금자동전표 계좌이체'      DESCRIPTION_NM,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_DR_ACCTCD) DR_PARACD6,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.RMT_CR_ACCTCD) CR_PARACD6,
           TO_CHAR(A.RMT_SUS_SEQ)     SUS_SEQ,
           TO_CHAR(A.RMT_PARTIAL_SEQ) PARTIAL_SEQ,
           '6' SORT_GUBUN
    FROM MAS_RMT A,
         HLDC_SC_DEPT B,
         HLDC_ST_UPJANG C,
         HLDC_ST_ACNT D
    WHERE (A.UPJANG    = C.UPJANG)
    AND   (C.DEPT_ID   = B.DEPT_ID)
    --AND   (SC_CRYPTO_FUN('DEC', A.RMT_ACNTCD_ENC , '3') = SC_CRYPTO_FUN('DEC', D.ACNTCD_ENC , '3'))
    AND   (A.RMT_ACNTCD_ENC = D.ACNTCD_ENC)
    AND   (B.PT_CD     = #PT_CD#)
    AND   (A.RMT_DATE  BETWEEN #TRX_DATE#||'01' AND #TRX_DATE#||'31')
    AND   (A.RMT_DR_ACCTCD = '2110452')
    AND   (TRIM(A.GAMU) IS NULL)
    UNION ALL
    SELECT B.DEPT_ID RTNDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) RTNDEPT_IDNM,
           B.DEPT_ID BUDDEPT_ID,
           (SELECT MU_NM||' '||CC_NM FROM HLDC_SC_DEPT_V WHERE DEPT_ID = B.DEPT_ID) BUDDEPT_IDNM,
           A.DR_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_ACCTCDNM,
           A.CR_ACCTCD,
           (SELECT ACCTNM6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_ACCTCDNM,
           '' FEE_ACCTCD,
           '' FEE_ACCTCDNM,
           '' ACNTCD,
           '' BANK_BRANCHCD,
           A.CUSTCD,
           B.DEPT_ID MANAGE_DEPT_ID,
           NULL FEE_CUSTCD,
           NVL(SUM(A.AMT), 0)       AMT,
           0                        FEE,
           MAX(C.UPJANGNM)||' 입금자동전표 영업대체'      DESCRIPTION_NM,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.DR_ACCTCD) DR_PARACD6,
           (SELECT PARACD1 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD1,
           (SELECT PARACD2 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD2,
           (SELECT PARACD3 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD3,
           (SELECT PARACD4 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD4,
           (SELECT PARACD5 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD5,
           (SELECT PARACD6 FROM HLDC_ST_ACCOUNT WHERE ACCTCD = A.CR_ACCTCD) CR_PARACD6,
           ''     SUS_SEQ,
           ''     PARTIAL_SEQ,
           '7' SORT_GUBUN
    FROM MAS_TRX_SUBST A,
         HLDC_SC_DEPT B,
         HLDC_ST_UPJANG C
    WHERE (A.UPJANG    = C.UPJANG)
    AND   (C.DEPT_ID   = B.DEPT_ID)
    AND   (B.PT_CD      = #PT_CD#)
    AND   (A.ADJ_DATE   BETWEEN #TRX_DATE#||'01' AND #TRX_DATE#||'31')
    AND   (TRIM(A.GAMU) IS NULL)
    AND   (A.POSTING_YN = 'Y')
    AND   (A.ADJ_KIND   = 'AR')
    GROUP BY B.DEPT_ID, A.CUSTCD, A.DR_ACCTCD, A.CR_ACCTCD, A.TAX_KIND
   )
   ORDER BY RTNDEPT_ID, SORT_GUBUN
   
   
   
   
   
   
   
   
   
   
   
   
  
</statement>
	<input default-name="ds_cond">
	<col name="PT_CD" size="255" type="VARCHAR" description="" /> 		
	<col name="TRX_DATE" size="255" type="VARCHAR" description="" /> 		
	</input>
	<output default-name="ds_listSource">
		<col name="RTNDEPT_ID" size="" type="NUMERIC" description="" /> 
    <col name="RTNDEPT_IDNM" size="255" type="VARCHAR" description="" /> 		
    <col name="BUDDEPT_ID" size="" type="NUMERIC" description="" /> 
    <col name="BUDDEPT_IDNM" size="255" type="VARCHAR" description="" /> 		
    <col name="DR_ACCTCD" size="255" type="VARCHAR" description="" /> 		
    <col name="DR_ACCTCDNM" size="255" type="VARCHAR" description="" /> 		
    <col name="CR_ACCTCD" size="255" type="VARCHAR" description="" /> 		
    <col name="CR_ACCTCDNM" size="255" type="VARCHAR" description="" /> 		
    <col name="FEE_ACCTCD" size="255" type="VARCHAR" description="" /> 		
    <col name="FEE_ACCTCDNM" size="255" type="VARCHAR" description="" /> 		
    <col name="ACNTCD" size="255" type="VARCHAR" description="" /> 		
    <col name="BANK_BRANCHCD" size="255" type="VARCHAR" description="" /> 		                
    <col name="CUSTCD" size="" type="NUMERIC" description="" /> 
    <col name="MANAGE_DEPT_ID" size="" type="NUMERIC" description="" /> 
    <col name="FEE_CUSTCD" size="" type="NUMERIC" description="" />     
    <col name="AMT" size="" type="NUMERIC" description="" /> 	
    <col name="FEE" size="" type="NUMERIC" description="" /> 	
    <col name="DESCRIPTION_NM" size="255" type="VARCHAR" description="" /> 	
    <col name="DR_PARACD1" size="255" type="VARCHAR" description="" /> 	
    <col name="DR_PARACD2" size="255" type="VARCHAR" description="" /> 	
    <col name="DR_PARACD3" size="255" type="VARCHAR" description="" /> 	
    <col name="DR_PARACD4" size="255" type="VARCHAR" description="" /> 	
    <col name="DR_PARACD5" size="255" type="VARCHAR" description="" /> 	
    <col name="DR_PARACD6" size="255" type="VARCHAR" description="" /> 	
    <col name="CR_PARACD1" size="255" type="VARCHAR" description="" /> 	
    <col name="CR_PARACD2" size="255" type="VARCHAR" description="" /> 	
    <col name="CR_PARACD3" size="255" type="VARCHAR" description="" /> 	
    <col name="CR_PARACD4" size="255" type="VARCHAR" description="" /> 	
    <col name="CR_PARACD5" size="255" type="VARCHAR" description="" /> 	
    <col name="CR_PARACD6" size="255" type="VARCHAR" description="" /> 	
    <col name="EXP_CD" size="255" type="VARCHAR" description="" /> 	    
    <col name="SUS_SEQ" size="" type="NUMERIC" description="" /> 	
    <col name="PARTIAL_SEQ" size="" type="NUMERIC" description="" /> 	
    <col name="DATE_BETWEEN" size="255" type="VARCHAR" description="" /> 	    
    
	</output>
</query>