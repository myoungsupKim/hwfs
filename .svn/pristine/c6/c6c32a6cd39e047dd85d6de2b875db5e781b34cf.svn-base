		tab_mas10020.tabpage3.grd_ctrt.setFormatColProperty(39, "size", 90);	// 매점임대료
		tab_mas10020.tabpage3.grd_ctrt.setFormatColProperty(40, "size", 97);	// 카페운영여부
		tab_mas10020.tabpage3.grd_ctrt.setFormatColProperty(41, "size", 102);	// 카페운영임대료
		tab_mas10020.tabpage3.grd_ctrt.setFormatColProperty(42, "size", 93);	// 자판기보유 여부
		tab_mas10020.tabpage3.grd_ctrt.setFormatColProperty(43, "size", 111);	// 위탁여부
