	var arrItm     = "";
	for(var i=0; i<=ds_list.rowcount; i++) 
	{
		if(!gfn_isNull(ds_list.getColumn(i, "SAL_ITEM"))) 
		{
			salItemChk = ds_list.getColumn(i, "SAL_ITEM");
			
			arrItm = salItemChk.split(",");
			
			var itemNm1 = "";
			var itemNm2 = "";
			var itemNm3 = "";
			var itemNm4 = "";
			
			if(arrItm[0] == "1")
			{
				itemNm1 = "음료";
			}
			
			if(arrItm[1] == "2")
			{
				itemNm2 = "커피";
			}
			
			if(arrItm[2] == "3")
			{
				itemNm3 = "유제품";
			}
			
			if(arrItm[3] == "4")
			{
				itemNm4 = "기타";
			}
			
			if(!gfn_isNull(itemNm1) || !gfn_isNull(itemNm2) || !gfn_isNull(itemNm3) || !gfn_isNull(itemNm4))
			{
				ds_list.setColumn(i, "SAL_ITEM_NM", itemNm1 + "," + itemNm2 + "," + itemNm3 + "," + itemNm4);	
			}
			
		}
	}
	
			
	fsp_clear(this);
	
	if (chk_History.value){
		fsp_addSearch(this, "ma/mas:MAS10020S_S005");
    }
    else {
		fsp_addSearch(this, "ma/mas:MAS10020S_S004");
	}
	
	fsp_callService(this, "", "", "ds_cond=ds_cond","ds_cust=ds_cust","","fn_searchPurifier");
	//trace(ds_cust.saveXML());
}

function fn_searchPurifier(){
	
	//Transaction 설정
	var formObj			= this;
	var strSvcID		= "MAS10021P_T001";
	var strURL			= "U_svc::" + "ma/mas/MAS10021P_T001.jsp";
	var strInDatasets	= "inDs_Cond=ds_cond";
	var strOutDatasets	= "ds_List=ds_List";
	var strArgument     = "";
	var strCallbackFunc	= "";
	var isErrorNotCall	= false;
	var isNotShow		= false;	//false 메세지 보여줌 --> sync일때 표현 안됨.
	var isSync			= true;		//true 다실행하고 실행됨.(async)

	//서버호출
	jsp_transaction(formObj, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, isErrorNotCall, isNotShow, isSync);
}

