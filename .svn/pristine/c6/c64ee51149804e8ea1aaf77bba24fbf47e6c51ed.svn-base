************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : Form Load (필수)
 *---------------------------------------------------------------------------------------------*/
function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(obj, false);
	
	// 초기값 셋팅
	fn_init();
}
/*----------------------------------------------------------------------------------------------
 * 설명      : 초기값 셋팅
 *---------------------------------------------------------------------------------------------*/
function fn_init()
{		
	SCCombo.bind(this, "ComCode", "cbo_gender", "", {groupCode:"GENDER_TYPE", useYn:"Y", set1:"S", defVal:""}, SCCombo.ALL);
	SCCombo.bind(this, "ComCode", "grd_pool_list.supDutyGroup", ds_adoptionGb, {groupCode:"ADOPTION_GB", useYn:"Y", defVal:"001"});
	SCCombo.bind(this, "ComCode", "grd_pool_list.supDuty", ds_dutyType, {groupCode:"DUTY_TYPE", useYn:"Y", defVal:""}, SCCombo.ALL);
	
	SCRadio.bind(this, "ComCode", "rdo_gender", ds_gender, {groupCode:"GENDER_TYPE", useYn:"Y", set1:"S"});
	SCRadio.bind(this, "ComCode", "rdo_dutyCook", ds_dutyCook, {groupCode:"DUTY_TYPE", useYn:"Y", set1:"1"});
	SCRadio.bind(this, "ComCode", "rdo_dutySubCook", ds_dutySubCook, {groupCode:"DUTY_TYPE", useYn:"Y", set1:"2"});
	
	SCCheckbox.bind(this, "ComCode", "div_duty1", {groupCode:"DUTY_TYPE", useYn:"Y", set1:"1"}, {appearance:{usingDb:true}}, {onclick:"fn_dutyOnClickEvent"}); //1. 기본
	SCCheckbox.bind(this, "ComCode", "div_duty2", {groupCode:"DUTY_TYPE", useYn:"Y", set1:"2"}, {appearance:{usingDb:true}}, {onclick:"fn_dutyOnClickEvent"}); //1. 기본
	
	SCPopup.bind(this, SCPopup.AddrSearch, "grd_pool_list.", "GRID:", null, {addrKey:"addrKey", addrInfo:"addrInfo", baseAddr:"baseAddr"});
		
	ds_search.addRow();
	
	fn_getSido();
	//ds_search.setColumn(0, 'maskAuth', SCAuth.getMaskAuth(this));
	ds_search.setColumn(0, 'maskAuth', "0");
	ds_search.setColumn(0, "loginPersMask", g_PersInfoMask);
	
	AddrSearchComponent00.enable = false;
	
	// 업장코드 파라미터 셋팅
	ds_search.setColumn(0, "upjangCd",fv_Contents);
	
	
}

/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/

function ds_poolList_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{	

	var nRow = ds_pool.rowposition;
	if( nRow < 0 ){
		return ;
	}	
	
	var sPossDuty = ds_pool.getColumn(nRow, 'possDuty' );
	
	SCCheckbox.setValue("div_duty1", sPossDuty , SCCheckbox.ONLY_VALUE);
	SCCheckbox.setValue("div_duty2", sPossDuty , SCCheckbox.ONLY_VALUE);
	
	
	
	if( obj.getRowType(nRow) == '2' ){
		 fv_nRowNum = 0;		 
		return ;
	}

	fv_nRowNum = nRow;
	
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_Search()
 * 기      능   : [조회버튼] 검색조건에 해당하는 정보를 조회
 ******************************************************************************************
 */
function fn_search(obj:Button, e:ClickEventInfo)
{
	fn_searchList();
}

function fn_searchList()
{
	 // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_pool_list);

    var strSvcID    = "search";
    var strURL      = "fs/fsa/adoptionPoolSearchPop/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_pool=ds_pool";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

function fn_getSido(){
	var strSvcID    = "sidoCombo";
    var strURL      = "fs/fsa/adoptionPoolList/selectSidoCombo.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_sido=dsSido";
    var strArg      = "";
    var strCallback = "";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_Choice()
 * 기      능   : [선택버튼] 선택버튼을 클릭했을때 처리되는 작업
 ******************************************************************************************
 */
function fn_Choice(obj:Button, e:ClickEventInfo)
{
	if(ds_pool.rowcount > 0)
	{
		fn_return();
	}else{
		gfn_alert("조회 후 배정인원을 체크하고 선택버튼을 눌러 주십시오.");
	}	
}

function fn_return()
{
	var nRow = null;
    
    for(var i=0; i< ds_pool.rowcount; i++)
    {
		if(ds_pool.getColumn(i, "chk") == 1)
		{
			nRow = ds_copyPool.addRow();
			ds_copyPool.copyRow(nRow, ds_pool, i);
		}
    }

	var tempDs = new Dataset();
	tempDs.assign(ds_copyPool);
    this.close(tempDs);	
}



/*----------------------------------------------------------------------------------------------
 * 설명      : 바디영역 체크박스 클릭시
 *---------------------------------------------------------------------------------------------*/
function grd_pool_list_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.cell == 0) 
	{
		//체크박스가 모두 체크되면 헤더의 체크도 표시되도록
		if(ds_pool.getCaseCount("chk=='1'")==ds_pool.getRowCount()){ //체크값없음
			grd_pool_list.setCellProperty("Head", 0, "text", 1);
		}else{
			grd_pool_list.setCellProperty("Head", 0, "text", 0);
		}
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 헤더영역 체크박스 클릭시
 *---------------------------------------------------------------------------------------------*/
function grd_pool_list_onheadclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.cell == 0) 
	{
		if(obj.getCellValue(-1,0) == 0)
		{
			grd_pool_list.setCellProperty("Head", 0, "text", 1);
			for(var i=0; i<ds_pool.rowcount; i++)
			{
				ds_pool.setColumn(i, "chk", 1);
			}
		}else{
			grd_pool_list.setCellProperty("Head", 0, "text", 0);
			for(var i=0; i<ds_pool.rowcount; i++)
			{
				ds_pool.setColumn(i, "chk", 0);
			}
		}
	}
}

function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 코드분류 조회
        case "search" :
			if(ds_pool.rowcount == 0)
			{
				AddrSearchComponent00.clearAll();
            }
            break;
          
    }
}]]></Script>
