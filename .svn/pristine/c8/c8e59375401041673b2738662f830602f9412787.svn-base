    BORN_CITY = #BORN_CITY#,
    HIGH_SCHOOL = #HIGH_SCHOOL#,
    UNIV = #UNIV#,
    GRAD_SCHOOL = #GRAD_SCHOOL#,
    P_CHARACTER = #P_CHARACTER#,
    HOBBY = #HOBBY#,
    PREF_FOOD = #PREF_FOOD#,
    SMOKING_YN = #SMOKING_YN#,
    DRINK_QTY = #DRINK_QTY#,
    WED_GUBUN = #WED_GUBUN#,
    WED_YMD = #WED_YMD#,
    SPOUSE_YMD = #SPOUSE_YMD#,
    ETC_SPECIALFACT = #ETC_SPECIALFACT#,
     USE_YN	            = #USE_YN#,
 	   UUSER              = #UUSER#,
 	   UDATE              = SYSDATE
WHERE (UPJANG        = #UPJANG#)
AND 	(CUSTCD        = #CUSTCD#)
AND 	(USER_GUBUN    = #USER_GUBUN#)
 ]]></statement>
