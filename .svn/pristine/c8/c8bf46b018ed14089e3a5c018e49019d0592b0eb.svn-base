<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[자재대분류 가져오기]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
		<isEqual col="BLANK" value="Y">
			SELECT '' AS CLASS_CODE, '' AS CLASS_NAME FROM DUAL
			UNION ALL
		</isEqual>
			SELECT CLASS_CODE, CLASS_NAME
			FROM HLDC_PO_ITEM_CLASS_HLDC_V
			WHERE CLASS_TYPE = 'M'
		<isEqual col="MU_CD" value="2004">
			AND CLASS_CODE BETWEEN 'F10' AND 'F27'
		</isEqual>
		<isNotEqual col="MU_CD" value="2004">
			AND CLASS_CODE BETWEEN 'F10' AND 'F26'
		</isNotEqual>
	</statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_cbo_item_classM">
		<col name="CLASS_CODE" size="255" type="VARCHAR" description="" /> 
		<col name="CLASS_NAME" size="255" type="VARCHAR" description="" /> 
	</output>
</query>
