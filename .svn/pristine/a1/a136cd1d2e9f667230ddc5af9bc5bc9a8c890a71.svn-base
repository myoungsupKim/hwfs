	}

	//ds임시생성 해제
	Destroy("ds_Dummyot");

	return  bYn;
}

function fn_ChkQtySta(obj,nRow,varNewValue)
{
    //수량 없으면 패스...
    if (toNumber(varNewValue) == 0) return true;

//============================================================================================
//클레임 요청수량은 제한 무시 2010.12.20 오성주,임태수
//============================================================================================
return true;
//============================================================================================
//클레임 요청수량은 제한 무시 2010.12.20 오성주,임태수
//============================================================================================

	//최소발주량 체크
	if ( (toNumber(obj.getColumn(nRow,"MIN_ORD_QTY")) > 0) && (toNumber(obj.getColumn(nRow,"MIN_ORD_QTY")) > toNumber(varNewValue)) )
	{
		g_Message("EE","해당 상품은 최소발주량이 " + obj.getColumn(nRow,"MIN_ORD_QTY") + "입니다.");
		return false;
	}
	//소숫점발주여부
	if ( (obj.getColumn(nRow,"POINT_FLAG") != "Y") && (Math.ceil(toNumber(varNewValue)) != toNumber(varNewValue)) )
	{
		g_Message("EE","해당 상품은 소숫점발주가 불가합니다.");
		return false;
	}
	//발주승수량
	if ( (toNumber(obj.getColumn(nRow,"MULTIPLIER_QTY")) > 0) && ((Math.round(toNumber(varNewValue) * 1000) % Math.round(toNumber(obj.getColumn(nRow,"MULTIPLIER_QTY")) * 1000)) <> 0) )
	{
		g_Message("EE","해당 상품은 발주승수량이 " + obj.getColumn(nRow,"MULTIPLIER_QTY") + "입니다.");
		return false;
	}
	//
	return true;
}
