function fn_ChkManNull()
{
	if ( ffn_ChkNull(ed_Upjang.userdata) )
	{
		g_Message("EE", "사업장을 선택하세요.");
		ed_Upjang.setFocus();
		return true;
	}
	if (ffn_ChkNull(cbo_Subinv.value))
	{
		if (cbo_GB.value == "S")
			g_Message("EE", "검수창고를 선택하세요.");
		else
			g_Message("EE", "검수부서를 선택하세요.");
		if (cbo_Subinv.enable) cbo_Subinv.setFocus();
		return true;
	}
	if ( ffn_ChkNull(me_NeedDate.value) || (gfn_length(me_NeedDate.value)< 8) )
	{
		g_Message("EE", "입고예정일을 정확하게 입력하세요.");
		me_NeedDate.setFocus();
		return true;
	}
	if ( ffn_ChkNull(me_TranDate.value) || (gfn_length(me_TranDate.value)< 8) )
	{
		g_Message("EE", "검수일자를 정확하게 입력하세요.");
		me_TranDate.setFocus();
		return true;
	}
	if( me_TranDate.value < "20080701" )
	{
		g_Message("EE","2008년7월1일 이전 데이터는 IFIS에서 입고작업할 수 없습니다.");
		me_TranDate.setFocus();
		return true;
	}	
	//과거소급가능여부 체크
	if ( (me_TranDate.enable) && (me_TranDate.value < gds_currdate.getColumn(0,"YMD")) )
	{
		if (! g_Confirm("04", '현재일자 이전으로 입고작업을 할 경우 과거소급 입고작업이 불가능한 구매유형은 제외됩니다!\n                작업을 계속하시겠습니까?!'))
		{
			me_TranDate.value = gds_currdate.getColumn(0,"YMD");
			me_TranDate.setFocus();
			return true;
		}
	}
	//센터재고 검수일=입고예정일 확인
	if ( (me_TranDate.enable) && (me_TranDate.value <> me_NeedDate.value) )
	{
		if (! g_Confirm("04", '입고예정일과 검수일자가 상이한 경우 센터재고 품목은 검수가 불가합니다!\n                작업을 계속하시겠습니까?!'))
		{
			me_TranDate.setFocus();
			return true;
		}
	}
	//
	return false;
}

function fn_ChkData()
{
	if (ds_List.rowcount <= 0)
	{
		return "저장할 자료가 없습니다.";
	}
	//본사담당자가 아닌 경우
	//식자재의 경우 전일 및 정오 이후에는 감량/부분입고/클레임 등록불가.(가상센터에 물린 업장은 제외)
	if (gfn_length(Gfa_call_param)== 0)
	{
		if ( (cbo_GB.value == "S") && (ds_Subinv.getColumn(ds_Subinv.rowposition,"CENTER_TYPE") == "01") &&
		     (gfn_subStr(ds_Subinv.getColumn(ds_Subinv.rowposition,"SUBINV_CODE"),0,1)== "C") )
		{
			fn_CurrDateWeek();
			if ( (me_NeedDate.value < gds_currdate.getColumn(0, "YMD")) || (gfn_subStr(gds_currdate.getColumn(0, "YMD_TM"),8,4)>= "1200") )
			{
				var nRow;
				nRow = ds_List.findRowExpr("UNDELIVERED_QTY>0||gfn_length(CLAIM_KIND)>0");
				if (nRow >= 0)
				{
					ds_List.rowposition = nRow; gd_list.setFocus();
					return "전일입고 및 금일 12:00 이후에는 감량/부분입고/클레임의 등록이 불가합니다.";
				}
			}
		}
	}
	//
	for (i=0; i<ds_List.rowcount; i++)
	{
		//클레임 이중등록 체크
		if ((! ffn_ChkNull(ds_List.getColumn(i,"CLAIM_KIND"))) && (toNumber(ds_List.getColumn(i,"C_PO_ID")) <> -1))
		{
			gd_list.rowposition = i; gd_list.setFocus(); gd_list.setCellPos(11);
			return "[" + gfn_toString(i+1)+ "]번 항목은 이미 클레임이 등록되어있습니다.\n클레임유형:" + ds_List.getColumn(i,"C_CLAIM_KIND") + "\n요청일시:" + ds_List.getColumn(i,"C_REQ_DATE") + " " + ds_List.getColumn(i,"C_REQ_TIME");
		}
		//클레임 요청량 체크
		if (numFormat(ds_List.getColumn(i,"REQ_QTY"))== "0") continue;
		if (ffn_ChkNull(ds_List.getColumn(i,"CLAIM_KIND")))
		{
			ds_List.rowposition = i; gd_list.setFocus(); gd_list.setCellPos(11);
			return "[" + gfn_toString(i+1)+ "]번 항목이 요청량은 있으나 클레임유형이 누락되었습니다.";
		}
		if (ffn_ChkNull(ds_List.getColumn(i,"REQ_KIND")))
		{
			ds_List.rowposition = i; gd_list.setFocus(); gd_list.setCellPos(12);
			return "[" + gfn_toString(i+1)+ "]번 항목이 요청량은 있으나 요청사항이 누락되었습니다.";
		}
		if ((ds_List.getColumn(i,"REQ_KIND") == "입고요청") || (ds_List.getColumn(i,"REQ_KIND") == "교환"))
		{
			if (ffn_ChkNull(ds_List.getColumn(i,"REQ_DATE")))
			{
				ds_List.rowposition = i; gd_list.setFocus(); gd_list.setCellPos(13);
				return "[" + gfn_toString(i+1)+ "]번 항목이 요청사항이 [입고요청] 혹은 [교환]임에도 입고요청일자가 누락되었습니다.";
			}
			if (ffn_ChkNull(ds_List.getColumn(i,"REQ_TIME")))
			{
				ds_List.rowposition = i; gd_list.setFocus(); gd_list.setCellPos(14);
				return "[" + gfn_toString(i+1)+ "]번 항목이 요청사항이 [입고요청] 혹은 [교환]임에도 입고요청시간이 누락되었습니다.";
			}
		}
	}
}

