function ds_List_CanColumnChange(obj:Dataset, e:DSColChangeEventInfo)
{
	//수량체크...
	if ((e.columnid == "TRANS_QTY") || (e.columnid == "UNDELIVERED_QTY"))
	{
		//센터재고 품목은 작업불가...
		if (! ffn_ChkNull(obj.getColumn(e.row,"CT_SUBINV_CODE")))
		{
			g_Message("EE","센터재고 품목은 변경불가하며 무조건 정상입고처리 됩니다!\n이상품목에 대해선 정상입고 후 [센터물품 반송등록]을 하시기 바랍니다.");
			return false;
		}
		//(+)구매유형 체크
		if ((obj.getColumn(e.row,"MINUS_FLAG") <> "Y") && (toNumber(e.newvalue) < 0))
		{
			g_Message("EE","본 검수자재는 " + obj.getColumn(e.row,"TYPE_NAME") + "구매유형으로 \n(-)입고가 불가합니다!");
			return false;
		}
		//(-)구매유형 체크
		if ((obj.getColumn(e.row,"MINUS_FLAG") == "Y") && (toNumber(e.newvalue) > 0))
		{
			g_Message("EE","본 검수자재는 " + obj.getColumn(e.row,"TYPE_NAME") + "구매유형으로 \n(+)입고가 불가합니다!");
			return false;
		}
		//발주량 초과 입고불가.
		if (Math.abs(toNumber(e.newvalue)) > Math.abs(toNumber(obj.getColumn(e.row,"PO_QTY"))))
		{
			g_Message("EE","발주량을 초과할 수 없습니다!");
			return false;
		}
		//소수점,최소발주량,발주승수량 체크
		//if (! fn_ChkQtySta(obj,nRow,varNewValue)) return false;
	}
	//진행상태 변경체크...
	else if (e.columnid == "LINE_STATUS")
	{
		//센터재고 품목은 작업불가...
		if (! ffn_ChkNull(obj.getColumn(e.row,"CT_SUBINV_CODE")))
		{
			g_Message("EE","센터재고 품목은 변경불가하며 무조건 정상입고처리 됩니다!\n이상품목에 대해선 정상입고 후 [센터물품 반송등록]을 하시기 바랍니다.");
			return false;
		}
		//배송중은 전량미입고인 경우만 가능
		if (e.newvalue == "009")
		{
			if (toNumber(obj.getColumn(e.row,"TRANS_QTY")) != 0)
			{
				g_Message("EE","입고량이 존재합니다!");
				return false;
			}
		}
		//부분입고인 경우 입고량도 미입고량도 있어야 한다.
		else if (e.newvalue == "010")
		{
			if ((toNumber(obj.getColumn(e.row,"UNDELIVERED_QTY")) == 0) || (toNumber(obj.getColumn(e.row,"TRANS_QTY")) == 0))
			{
				g_Message("EE","부분입고 상태가 아닙니다!");
				return false;
			}
		}
		//정상입고는 전량 입고일 경우만 가능
		else if (e.newvalue == "011")
		{
			if (toNumber(obj.getColumn(e.row,"UNDELIVERED_QTY")) != 0)
			{
				g_Message("EE","미입고량이 존재합니다!");
				return false;
			}
		}
		//감량입고는 전량 입고만 아니면 됨.
		else if (e.newvalue == "012")
		{
			if (toNumber(obj.getColumn(e.row,"UNDELIVERED_QTY")) == 0)
			{
				g_Message("EE","정상입고 상태입니다!");
				return false;
			}
		}
/*		
		//클레임 등록은 감량만 가능.
		if ((! ffn_ChkNull(obj.GetColumn(nRow,"CLAIM_KIND"))) && (varNewValue != "012"))
		{
			g_Message("EE","클레임이 등록되어 감량만 가능합니다!");
			return false;
		}
*/
	}
	//클레임 등록유형
	else if (e.columnid == "CLAIM_KIND")
	{
		//if ((! ffn_ChkNull(varNewValue)) && (obj.GetColumn(nRow,"LINE_STATUS") <> "012"))
		if ((! ffn_ChkNull(e.newvalue)) && (toNumber(obj.getColumn(e.row,"C_PO_ID")) <> -1))
		{
			//g_Message("EE","클레임 등록은 감량만 가능합니다!");
			g_Message("EE","이미 클레임이 등록되어있습니다.\n클레임유형:" + obj.getColumn(e.row,"C_CLAIM_KIND") + "\n요청일시:" + obj.getColumn(e.row,"C_REQ_DATE") + " " + obj.getColumn(e.row,"C_REQ_TIME"));
			return false;
		}
	}
	//클레임 요청량
	else if (e.columnid == "REQ_QTY")
	{
		//센터재고 품목은 작업불가...
		if (! ffn_ChkNull(obj.getColumn(e.row,"CT_SUBINV_CODE")))
		{
			g_Message("EE","센터재고 품목은 변경불가하며 무조건 정상입고처리 됩니다!\n이상품목에 대해선 정상입고 후 [센터물품 반송등록]을 하시기 바랍니다.");
			return false;
		}
		//
		if ((toNumber(e.newvalue) != 0) && (toNumber(obj.getColumn(e.row,"C_PO_ID")) != -1))
		{
			g_Message("EE","이미 클레임이 등록되어있습니다.\n클레임유형:" + obj.getColumn(e.row,"C_CLAIM_KIND") + "\n요청일시:" + obj.getColumn(e.row,"C_REQ_DATE") + " " + obj.getColumn(e.row,"C_REQ_TIME"));
			return false;
		}
		//발주량을 초과할 수 없다.
		if (Math.abs(toNumber(e.newvalue)) > Math.abs(toNumber(obj.getColumn(e.row,"PO_QTY"))))
		{
			g_Message("EE","요청량은 발주량보다 클 수 없습니다!");
			return false;
		}
		//소수점,최소발주량,발주승수량 체크
		if (! fn_ChkQtySta(obj,e.row,e.newvalue)) return false;
	}
	//
	return true;
}

var curRw = 0;
function ds_List_OnColumnChanged(obj:Dataset, e:DSColChangeEventInfo)
{
	//입고량 변경에 따른 변경처리
	//trace("e.columnid ==>> "+e.columnid);
	if (e.columnid == "TRANS_QTY")
	{
		//입고량 변경에 따른 미입고량 변경
		var nQty = toNumber(obj.getColumn(e.row,"PO_QTY")) - toNumber(e.newvalue);
		//trace(" UNDELIVERED_QTY( nQty ) ==>> "+nQty);
		obj.setColumn(e.row,"UNDELIVERED_QTY",nQty);

		//전량 미입고시 기본은 배송중으로
		//trace("  toNumber(e.newvalue)  ===>>> "+toNumber(e.newvalue));
		if (toNumber(e.newvalue) == 0)
			obj.setColumn(e.row,"LINE_STATUS","009");
		//부분입고 시 식자재창고는 감량으로 그외는 부분입고로...
		else if (Math.abs(toNumber(e.newvalue)) < Math.abs(toNumber(obj.getColumn(e.row,"PO_QTY"))))
		{
			if (gfn_subStr(ds_Subinv.getColumn(ds_Subinv.rowposition,"SUBINV_CODE"),0,1)== "C")
				obj.setColumn(e.row,"LINE_STATUS","012");
			else
				obj.setColumn(e.row,"LINE_STATUS","010");
		//전량입고시 정상입고
		}else {
			obj.setColumn(e.row,"LINE_STATUS","011");
		}	
	}
	//미입고량 변경 시 입고량 수정
	else if (e.columnid == "UNDELIVERED_QTY")
	{
		var nQty = toNumber(obj.getColumn(e.row,"PO_QTY")) - toNumber(e.newvalue);
		//alert(nQty);
		obj.setColumn(e.row,"TRANS_QTY",nQty);
		//obj.setColumn(e.row,"REQ_QTY",obj.getColumn(e.row, e.newvalue));
	}
	//감량입고처리 시 식재창고의 경우 미입고량을 클레임 수량으로 기본설정
	else if (e.columnid == "LINE_STATUS")
	{
		//trace("ds_Subinv.getColumn(ds_Subinv.rowposition,SUBINV_CODE) ===>>> "+ds_Subinv.getColumn(ds_Subinv.rowposition,"SUBINV_CODE"));
		if ((e.newvalue == "012") && (gfn_subStr(ds_Subinv.getColumn(ds_Subinv.rowposition,"SUBINV_CODE"),0,1)== "C") && (toNumber(obj.getColumn(e.row,"C_PO_ID")) == -1))
			obj.setColumn(e.row,"REQ_QTY",obj.getColumn(e.row,"UNDELIVERED_QTY"));
	}
	else if(e.columnid == "REQ_DATE") {
		if(!gfn_isDate8(obj.getColumn(e.row,"REQ_DATE"))) {
			
			//obj.setColumn(e.row,"REQ_DATE", e.oldvalue);
			curRw = e.row;
			this.setTimer(1,10);
			return;
		}
	}
}

