			, ITEM_CLASS2:""
			, ITEM_CLASS3:""
			, ITEM_CLASS4:""
			, NEED_DATE:ds_menuItemList.getColumn(e.row, "NEED_DATE")
	};	
	
	var rtn = gfn_dialog("", "U_FSM::FSM90054P.xfdl",arg);
	
	if(gfn_length(gfn_trim(rtn)) == 0 || gfn_trim(rtn) == null)
