	fsp_addSingle(this, "fs/fsm:FSM00060H_D009");
	fsp_addSingle(this, "fs/fsm:FSM00060H_D010");
	fsp_addSingle(this, "fs/fsm:FSM00060H_I002");
	fsp_addSingle(this, "fs/fsm:FSM00060H_I003");
	fsp_addSingle(this, "fs/fsm:FSM00060H_I004");
	fsp_callService(this, "", "", "", "", arg, "fn_callbackCreateUpjangRecipe");
	
}

function fn_callbackCreateUpjangRecipe(errCode, errMsg) 
{
	if(errCode!=0)
