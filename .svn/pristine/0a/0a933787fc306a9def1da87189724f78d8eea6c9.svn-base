 ******************************************************************************************
 */
function fn_search()
{
	// 조회조건체크
	if (gfn_length(ds_input.getColumn(0,"MENU_CLASS"))==0)
	{
		g_Message("EE", "개별식단분류를 선택하여 주십시오.");
		cbo_S_MENU_CLASS.setFocus();
		return;
	}
	
	// 초기화
	ds_list.clearData();
	ds_Master.clearData();
	ds_Detail1.clearData();
	ds_Detail2.clearData();
	
	
	if(ds_input.getColumn(0,'RECIPE_NAME') == '전체') {
	
	   ds_input.setColumn(0,'RECIPE_NAME', ''); 
	}
	
	
	// 조회
	var actionName, cmdName, inData, outData, otherArg, callBackFnc;
	
	inData			= "ds_input=ds_input";
	outData			= "ds_Master=ds_list";
	callBackFnc		= "fn_afterSearchList";
	
	 //trace(ds_input.saveXML());
	
	fsp_clear(this); 
    fsp_addSearch(this, "fs/fsm:FSM00010E_S001");									// 조회 
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출 
}

/*
 ******************************************************************************************
 * 함  수  명   : fn_Insert()
 * 기      능   : [입력버튼] 입력 시 처리되는 작업
 * 비      고	: 팝업사용
 ******************************************************************************************
 */
function fn_insert()
{
	if (gfn_dsIsUpdated(ds_Master)==true || gfn_dsIsUpdated(ds_Detail1)==true || gfn_dsIsUpdated(ds_Detail2)==true)
	{
		g_Message("EE", "변경된 데이터가 있습니다.\n 저장 후 입력해주십시오.");
		return;
	}
	
	var strMsg,nRow;
	
	if (ds_list.rowcount >0 && ds_Detail2.rowcount > 0)
	{
		nRow = ds_list.rowposition;
		
		strMsg 	= 	"[" + ds_list.getColumn(nRow,"SINGLE_MENU_CD") + "] " + ds_list.getColumn(nRow,"MENU_NAME");
		strMsg	+=	"\n식단을 복사하시겠습니까?";
		strMsg	+=	"\n\n복사하시려면 확인을";
		strMsg	+=	"\n초기화하시려면 취소를 눌러주세요.";
		
		if (g_Confirm("04",strMsg))
		{
			fn_MenuCopy();
			return;
		}
	}
	
	nRow = ds_Master.addRow();
	ds_Master.setColumn(nRow,"MENU_CLASS",cbo_S_MENU_CLASS.value);

	ds_Detail1.clearData();
	ds_Detail2.clearData();
	
	fn_formSetting();
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_Save()
 * 기      능   : [저장버튼] 저장 시 처리되는 작업
 ******************************************************************************************
 */
function fn_save()
{
	// 변경여부 확인
	if(gfn_dsIsUpdated(ds_Master)==false && gfn_dsIsUpdated(ds_Detail1)==false && gfn_dsIsUpdated(ds_Detail2)==false)
	{
		g_Message("EE", "변경된 데이터가 없습니다.");
		return;
	}
	
	if(!fn_SaveCheck())
	{
		return;
	}
	
    // 서비스셋팅
    var actionName, cmdName, inData, outData, otherArg, callBackFnc;
    var insertSql, updateSql, deleteSql,saveFlagColumn, keySqlName, keyIncrement, callbackSql; 
    
   // trace(GetRowType(ds_Master,ds_Master.rowposition));
    
    if (GetRowType(ds_Master,ds_Master.rowposition)== "insert")
	{    
	
	    ds_in0.clearData();
	    
	    ds_in0.copyData(ds_in);
	
	    inData		= "ds_in0=ds_in0";
	    outData		= "ds_in=ds_in"; 
	    callBackFnc	= "fn_afterSearchKey";
	    keyIncrement= -1; 
    
        fsp_clear(this); 

		fsp_addSearch(this, "fs/fsm:FSM00010E_S004",true);	// 레시피코드조회 
		http.Sync = true;
	    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출  
	    http.Sync = false;
   
    }
    else
    {
		ds_in.clearData();
		ds_in.addRow();
		ds_in.setColumn(0,"SINGLE_MENU_CD",ds_Master.getColumn(ds_Master.rowposition,"SINGLE_MENU_CD"));
    }
    
   // trace('save');
    
    inData		= "ds_Master=ds_Master:U ds_Detail1=ds_Detail1:U ds_Detail2=ds_Detail2:U ds_in=ds_in ds_input=ds_input";
    outData		= "ds_Master=ds_list ds_Detail1=ds_Detail1 ds_in=ds_in"; 
    callBackFnc	= "fn_afterSave";
    keyIncrement= -1;
    
    fsp_clear(this);
    
    fsp_addSave(this, "fs/fsm:FSM00010E_I001", "fs/fsm:FSM00010E_U001", "fs/fsm:FSM00010E_D001",saveFlagColumn, keySqlName, keyIncrement, "");
    fsp_addSave(this, "fs/fsm:FSM00010E_I002", "fs/fsm:FSM00010E_U002", "fs/fsm:FSM00010E_D002",saveFlagColumn, keySqlName, keyIncrement, "");
    //tit_addSaveActionInfo(this, "fs/fsm:FSM00010E_I003", "fs/fsm:FSM00010E_U003", "fs/fsm:FSM00010E_D003",saveFlagColumn, keySqlName, keyIncrement, "");
    fsp_callService(this, actionName, cmdName, inData, outData, otherArg, callBackFnc);		// 서비스 호출 
   
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_Delete()
 * 기      능   : [삭제버튼] 삭제 시 처리되는 작업
 ******************************************************************************************
 */
function fn_delete()
{
	if(ds_list.getRowCount()==0)
	{
		g_Message("EE", "삭제할 데이터가 없습니다.");
		return;
	}
	
	if (gfn_dsIsUpdated(ds_Master)==true || gfn_dsIsUpdated(ds_Detail1)==true || gfn_dsIsUpdated(ds_Detail2)==true)
	{
		g_Message("EE", "변경된 데이터가 있습니다.\n 저장 후 삭제해십시오.");
		return;
	}
	
	var strMsg,btp,nRow,nFRow;
	
	nRow 	= ds_list.rowposition;
	nFRow 	= ds_Master.findRow("SINGLE_MENU_CD",ds_list.getColumn(nRow,"SINGLE_MENU_CD"));
	
	strMsg = "[" + ds_list.getColumn(nRow,"SINGLE_MENU_CD") + "] " + ds_list.getColumn(nRow,"MENU_NAME") + "를 삭제하시겠습니까?";
	
	// 삭제확인
	if(!g_Confirm("01",strMsg))
	{
		return false;
	}
	
	// 사진1 삭제
	btp = fsp_fileDelete(serverSaveDir, ds_list.getColumn(nRow,"SPHOTO_PATH_1"),true);
	if (!btp)
	{
		g_Message("EE", "식단사진 삭제를 실패하였습니다.");
		return;
	}
	
	// 사진2 삭제
	btp = fsp_fileDelete(serverSaveDir, ds_list.getColumn(nRow,"SPHOTO_PATH_2"),true);
	if (!btp)
	{
		g_Message("EE", "식단사진 삭제를 실패하였습니다.");
		return;
	}
	
	// Data삭제
	ds_list.deleteRow(nRow);
	ds_Master.deleteRow(nFRow);
	ds_Master.applyChange();
	ds_Master.rowposition = -1;
	ds_Detail1.clearData();
	ds_Detail2.clearData();
	
	ds_Master.UnFilter();
	
	// 삭제
	fsp_addSave(this, "", "", "fs/fsm:FSM00010E_D001");
	fsp_addSave(this, "", "", "fs/fsm:FSM00010E_D004");
	//tit_addSaveActionInfo(this, "", "", "fs/fsm:FSM00010E_D005");
	fsp_callService(this, "", "", "ds_list=ds_list:A",""); 
}

