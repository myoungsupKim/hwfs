	}
	
	if (gfn_length(ds_Master.getColumn(ds_Master.rowposition,"SINGLE_MENU_CD"))==0)
	{
		ds_Master.setColumn(ds_Master.rowposition,"SINGLE_MENU_CD",ds_in.getColumn(0,"SINGLE_MENU_CD"));
		ds_Master.applyChange();
		fn_SearchSub(ds_Master.rowposition);
	}
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_MenuCopy()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 현재리스트에있는 메뉴를 복사하여 새로운 메뉴(식단)생성
 * 작  성  자   : 박지영
 * 작성  일자   : 2007-12-10  
 ******************************************************************************************
 */
function fn_MenuCopy()
{
	var nARow;
	
	ds_Master.addRow();
	
	ds_Copy1.clearData();
	ds_Copy2.clearData();
	
	Copy(ds_Copy1,ds_Detail1);
	Copy(ds_Copy2,ds_Detail2);
	
	grd_Detail.enableredraw = false;
	
	ds_Detail1.clearData();
	ds_Detail2.clearData();
	
	for(i=0;i<ds_Copy1.rowcount;i++)
	{
		nARow = ds_Detail1.addRow();
		ds_Detail1.copyRow(nARow,ds_Copy1,i);
		ds_Detail1.setColumn(nARow,"SINGLE_MENU_CD","");
	}
	
	for(i=0;i<ds_Copy2.rowcount;i++)
	{
		nARow = ds_Detail2.addRow();
		ds_Detail2.copyRow(nARow,ds_Copy2,i);
		ds_Detail2.setColumn(nARow,"SINGLE_MENU_CD","");
	}
	
	grd_Detail.enableredraw = true;
