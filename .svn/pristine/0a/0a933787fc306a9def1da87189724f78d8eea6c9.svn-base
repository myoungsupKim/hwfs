/*
 ******************************************************************************************
 * 함  수  명   : fn_RecipeAdd()
 * 입      력   : 없음
 * 반      환   : 없음
 * 기      능   : 레시피의 중복여부를 체크하며,레시피와 식자재를 복사한다.
 * 작  성  자   : 박지영
 * 작성  일자   : 2007-12-17  
 ******************************************************************************************
 */
function fn_RecipeAdd()
{
	var nCnt, strRECIPE_CD, nFRow, strSINGLE_MENU_CD, strRECIPE_NAME, strNoDetail, strMsg;
	
	nCnt = 0;
	strSINGLE_MENU_CD 	= ds_Master.getColumn(ds_Master.rowposition,"SINGLE_MENU_CD");
	
	grd_Detail.enableredraw = false; 
 
	
	for(var i=0;i<ds_Copy1.rowcount;i++)
	{
		// 중복된 레시피 체크
		strRECIPE_CD = ds_Copy1.getColumn(i,"RECIPE_CD");
		nFRow = ds_Detail1.findRow("RECIPE_CD", strRECIPE_CD); 
		
		if (nFRow == -1) { 
		
			ds_Copy4.filter("");
			
			// 자제정보있는지 체크
			ds_Copy2.filter("RECIPE_CD==" + wrapQuote(strRECIPE_CD)); 
			
			if (ds_Copy2.rowcount == 0) {
				strNoDetail += " " + ds_Copy1.getColumn(i,"RECIPE_NAME") + " ";
			} else {
				// 레시피정보추가
				nARow = ds_Detail1.addRow();
				ds_Detail1.copyRow(nARow,ds_Copy1,i);
				ds_Detail1.setColumn(nARow,"SINGLE_MENU_CD",strSINGLE_MENU_CD);
				ds_Detail1.setColumn(nARow,"RECIPE_SEQ",0);
				
				// 자재정보추가
				for(var j=0;j<ds_Copy2.rowcount;j++)
				{
					nARow = ds_Detail2.addRow();
					
					ds_Detail2.copyRow(nARow,ds_Copy2,j);
					ds_Detail2.setColumn(nARow,"SINGLE_MENU_CD",strSINGLE_MENU_CD);
					ds_Detail2.setColumn(nARow,"RECIPE_SEQ",0);
					ds_Detail2.setColumn(nARow,"ITEM_RATE",100);
				}
			}
		} else {
			strRECIPE_NAME = gfn_nvl(ds_Copy3.getColumn(i,"RECIPE_NAME"));
			nCnt = nCnt + 1;
		} 
	}
	
	grd_Detail.enableredraw = true;
	
	strNoDetail = gfn_replace(strNoDetail,"  "," , ");
	
	if (gfn_length(gfn_trim(strRECIPE_NAME))!= 0 || gfn_length(gfn_trim(strNoDetail))!= 0)
	{
		strMsg = 	"레시피 복사가 실패하였습니다.\n";
		if (gfn_length(strRECIPE_NAME)!= 0)
		{
		
		    if(gfn_nvl(nCnt,0) > 0) {
		
			    strMsg +=	"  - 레시피 중복 : " + strRECIPE_NAME + iif(nCnt>1," 외 " + (nCnt-1) + " 건")+ "\n";
			} else {
			   strMsg +=	"  - 레시피 중복 : " + strRECIPE_NAME + "\n";
			}
		}
		if (gfn_length(strNoDetail)!= 0)
		{
			strMsg +=	"  - 자제없음 : " + strNoDetail;
		}
		
		g_Message("EE", strMsg);
	}
}
/*
 ******************************************************************************************
 * 함  수  명   : fn_RecipeSetting(strColumn)
 * 입      력   : strColumn = 셋팅컬럼명
 * 반      환   : 없음
 * 기      능   : 컬럼에 따라 레시피,자재 값 셋팅
 * 작  성  자   : 박지영
 * 작성  일자   : 2007-12-10  
 ******************************************************************************************
 */
function fn_RecipeSetting(strColumn)
{
	var nRow,strRecipe,vValue;
	
	nRow 		= ds_Detail2.rowposition;
	strRecipe	= ds_Detail2.getColumn(nRow,"RECIPE_CD");
	vValue 		= ds_Detail2.getColumn(nRow,strColumn);
	
	for(i=0;i<ds_Detail2.rowcount;i++)
	{
		if (ds_Detail2.getColumn(i,"RECIPE_CD") == strRecipe)
		{
			ds_Detail2.setColumn(i,strColumn,vValue);
		}
	}
}
