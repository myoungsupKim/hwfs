<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[끼니-코너 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT 0 CHK
     , A.MEAL
     , C.CODE_NAME AS MEAL_NAME
     , A.CORNER
     , D.CODE_NAME AS CORNER_NAME
     , B.EXPECT_MEAL_QTY
  FROM (
        SELECT DISTINCT
               B.MEAL
             , B.CORNER
          FROM FSA_HALL_MST A
             , FSA_HALL_DTL B
         WHERE A.UPJANG  = #UPJANG#
           AND A.HALL_CD = #HALL_CD#
           AND A.USE_YN  = 'Y'
           AND B.USE_YN  = 'Y'
           AND A.UPJANG  = B.UPJANG
           AND A.HALL_CD = B.HALL_CD
       ) A
     , (
        SELECT MEAL
             , CORNER
             , SUM(EXPECT_MEAL_QTY) AS EXPECT_MEAL_QTY
          FROM FSM_REAL_MENU_MST M_MST
         WHERE UPJANG = #UPJANG#
           AND HALL_CD = #HALL_CD#
           AND MENU_DATE BETWEEN #START_DATE#  AND #END_DATE#
         GROUP BY MEAL, CORNER
       ) B
     , SCC_COMMON_CODE C
     , SCC_COMMON_CODE D
 WHERE A.MEAL = B.MEAL(+)
   AND A.CORNER = B.CORNER(+)
   AND A.MEAL = C.CODE
   AND A.CORNER = D.CODE
   AND C.GROUP_CODE = 'FS0023'
   AND D.GROUP_CODE = 'FS0022'
 ORDER BY A.MEAL, A.CORNER
    </statement>
	<input default-name="ds_cond">
	</input>
	<output default-name="ds_mealCorner">
	</output>
</query>
