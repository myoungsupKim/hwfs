	//날자  
 	cal_serchDateFr.fn_initMonthCal(this, fv_sToday, "fn_calendarCallBackFr", null, null, "Y");
 	cal_serchDateTo.fn_initMonthCal(this, fv_sToday, "fn_calendarCallBackTo", null, null, "Y");
