function fn_init()
{
	//[[기본예제
	TmSearchComponent00.applyAuthLevel(MuSearchComponent00);
	UpjangSearchComponent00.applyAuthLevel(TmSearchComponent00);
	UpjangSearchComponent00.setForcedRoleSet("OutletUpjang");
	//UpjangSearchComponent00.applyMainUpjang(null);
	//기본예제]]

	//[[업장권한 예제 - 업장에 리더, 소팀장, 팀장이 매핑된 경우에 대한 처리
	TmSearchComponent01.applyAuthLevel(MuSearchComponent01);
	UpjangSearchComponent01.applyAuthLevel(TmSearchComponent01, false);
	//업장권한 예제 - 업장에 리더, 소팀장, 팀장이 매핑된 경우에 대한 처리//

	//[[예외권한 예제 - 쿼리문일 직접 넘긴다.
	TmSearchComponent02.applyAuthLevel(MuSearchComponent02);
	UpjangSearchComponent02.applyAuthLevel("AND A.UPJANG = '116258' "); //==>버튼의 기능이 할성화 된다.
	//UpjangSearchComponent02.applyAuthLevel("AND A.UPJANG = '116258'", false); //==>버튼의 기능이 비할성화 된다.
	//예외권한 예제 - 쿼리문일 직접 넘긴다.]]

	//[[식재에 대한 업장 조회
	TmSearchComponent03.applyAuthLevel(MuSearchComponent03);
	UpjangSearchComponent03.applyAuthLevel(TmSearchComponent03);
	UpjangSearchComponent03.setMainUpjang(true);
	UpjangSearchComponent04.applyAuthLevel(TmSearchComponent03);
	UpjangSearchComponent04.applyMainUpjang(UpjangSearchComponent03);
	//식재에 대한 업장 조회]]

}

