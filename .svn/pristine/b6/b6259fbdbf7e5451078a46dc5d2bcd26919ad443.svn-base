function Button00_onclick(obj:Button,  e:ClickEventInfo)
{
	var strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc;
	var sFirst = "N";
	// 초기화
	//ds_potype.clearData();
	// 경로 설정
	strSvcID = "SCCALRAM001_T001";
	strURL   = "U_svc::" + "sc/scc/SCCALRAM001_T001.jsp";
	
	// 데이터셋 설정
//	trace(dsPoAuthType.saveXML());
	
	strInDatasets  = "";
	strOutDatasets = "ds_PopLst=ds_List";
	// 파라메터 설정
	strArgument  = "g_Upjang=" + wrapQuote(g_Upjang);
	strArgument += " g_EmpNo=" + wrapQuote(g_EmpNo);
	strArgument += " g_SystemId=" + wrapQuote(g_SystemId);
	strArgument += " g_MainUpjang=" + wrapQuote(g_Main_Upjang);
	strArgument += " g_CustCd=" + wrapQuote(g_CustCd);
	strArgument += " sFirst=" + wrapQuote(sFirst);
	// 콜백함수 지정
	strCallbackFunc = "fn_CallBack_Popup";

	//http.Sync = true;
	//Transaction(strSvcID,strURL,strInDatasets,strOutDatasets,strArgument,strCallbackFunc, false);
	jsp_transaction(this, strSvcID, strURL, strInDatasets, strOutDatasets, strArgument, strCallbackFunc, false, false, true);
	//http.Sync = false;
}

function fn_CallBack_Popup(SvcID, errCode, errMsg)
{
	if (errCode == 0)
	{
		trace(errCode + "/" + errMsg);
		//g_Message("EE","팝업정보 조회오류(" + errMsg + ")");
		return;
	}
	//팝업처리
	//var nFrom, nTop, nLeft;
	
	if (ds_PopLst.getRowCount() < 1) return;
	
	var strId       = "";
	var strURL      = "";
	var objArgument = {};
	var nWidth      = 0;
	var nHeight     = 0;
