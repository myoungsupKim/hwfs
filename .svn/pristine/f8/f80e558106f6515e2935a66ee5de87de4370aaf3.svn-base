	fv_objColInfo = gfn_getDatasetInfo (ds_file);
	fv_objColInfo1 = gfn_getDatasetInfo (ds_list);

	var strSvcID    = "search";
	if(tab_photo.tabindex == 0)
		var strURL      = "fm/fmc/claimReg/selectPC.xdo";
	if(tab_photo.tabindex == 1)
		var strURL      = "fm/fmc/claimReg/selectMobile.xdo";
	var strInDs     = "";
	var strOutDs    = "ds_list=ds_list ds_file=ds_file";
	var strArg      = "poId=" + wrapQuote(pv_poId) ;
	    strArg     += " poNum=" + wrapQuote(pv_poNum);
	    strArg     += " claimNum=" + wrapQuote(pv_claimNum);
		strArg     += " schDate=" + wrapQuote(ds_search.getColumn(0, "schDate"));
		strArg     += " schUpjang=" + wrapQuote(pv_upjang);
		if(ds_list.getColumn(0, "fileId") == undefined)
				strArg     += " refSeq=''";
		else
				strArg     += " refSeq=" + wrapQuote(ds_list.getColumn(0, "fileId"));
	var strCallback = "fn_callBack";
	var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}


/*----------------------------------------------------------------------------------------------
 * 설명      : 저장 버튼 클릭 시 (저장 버튼 클릭 시)
 *---------------------------------------------------------------------------------------------*/
function fn_save(str)
{
	// v_ds_check 미사용시 변경사항 체크
	if (gfn_dsIsUpdated(ds_file) == false) 
	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return;
	}
	
	if(str == 'delete')
	{
		var strSvcID    = "delete";
	}else{
		var strSvcID    = "save";
	}
	
    if(tab_photo.tabindex == 0)
    {
		// 첨부 파일 추가가 있을때만
		if(ObjAtchFile.fn_countDs("insert") > 0)
		{	
			var rtn = ObjAtchFile.fn_uploadFile();		
			if (rtn < 0)	return;
		}
		
    	var strURL      = "fm/fmc/claimReg/saveAttachFile.xdo";
    	var strInDs     = "ds_list=ds_list:A ds_file=ds_file:A";	//첨부파일 참조일련번호로 인해 ds_file:A로 설정해야 함
    }
	else //if(tab_photo.tabindex == 1)
	{
		if(fn_fileCopy()) return;
		
		var strURL      = "fm/fmc/claimReg/saveAttachFileCopy.xdo";
		var strInDs     = "ds_list=ds_list:A ds_file=ds_fileCopy:A";	//첨부파일 참조일련번호로 인해 ds_file:A로 설정해야 함	
	}    
    var strOutDs    = "ds_file=ds_file";
    var strArg      = "seq=" + wrapQuote(ds_list.getColumn(0,"fileId"));
        strArg     += " poID=" + wrapQuote(ds_list.getColumn(0,"poId"));
        strArg     += " poNum=" + wrapQuote(pv_poNum);
        strArg     += " claimNum=" + wrapQuote(pv_claimNum);
        
    var strCallback = "fn_callBack";
    var bAsync      = true;
    
    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}


/*----------------------------------------------------------------------------------------------
 * 설명      : 삭제 버튼 클릭 시 
 *---------------------------------------------------------------------------------------------*/
function fn_delete()
{
	if(tab_photo.tabindex == 1) return;
	if(ds_file.getRowCount() == 0){
		gfn_alert("데이터가 없습니다.");
		return;
	}
	
	var rtVal = gfn_confirm("자료를 삭제 하시겠습니까?");		// 선택된 자료를 삭제 하시겠습니까?
	
	if (rtVal == true) 
	{	
		ds_file.deleteAll();
		fn_save("delete");
		opener.fn_photoYn(0);
		tab_photo.tpg_PC.img_improvViewer.image = "";
	}
}

/*----------------------------------------------------------------------------------------------
 * 설명 : 닫기 / 취소 버튼 클릭
 *----------------------------------------------------------------------------------------------*/
function fn_close(retResult)
{
    var retVal = new Array();
    retVal[0] = retResult;
    close(retVal);
}

/*----------------------------------------------------------------------------------------------
 * 설명 : check된 파일목록을 insert타입으로 변경 및 중복체크
 *----------------------------------------------------------------------------------------------*/
function fn_fileCopy()
{
	ds_fileCopy.updatecontrol = false;
    ds_file.filter("chk==1");
    ds_fileCopy.copyData(ds_file, true);
    ds_file.filter("");
    
    if(ds_fileCopy.rowcount == 0)
	{	
		gfn_message("A", "체크된 항목이 없습니다.");
		return true;
	}
    
    for(var i=0; i<ds_fileCopy.rowcount; i++)
    {
		for(var j=0; j<ds_fileOverlep.rowcount; j++)
		{	
			if(ds_fileCopy.getColumn(i, 'orgnlFileNm') == ds_fileOverlep.getColumn(j, 'orgnlFileNm'))
			{
				gfn_message("A", "중복되는 파일명이 존재 합니다.\n(" +ds_fileCopy.getColumn(i, 'orgnlFileNm')+ ")");
				return true;
			}
		}
		ds_fileCopy.setRowType(i, 'i');
    }
}


/************************************************************************************************
 * 사용자 FUNCTION 영역 (필수)
************************************************************************************************/


/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
	// 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
	if (nErrorCode < 0) 
	{
		return;
	}
	
    switch(strSvcID)
    {
		// 조회
        case "search" :
            //trace(ds_list.saveXML());
            //trace(ds_file.saveXML());
            if (ds_list.getRowCount() == 0) {
				//서비스 쿼리 결과로 데이터가 없는 경우 데이터셋 컬럼정보 복원
				gfn_loadDatasetInfo (ds_list, fv_objColInfo1);
            }
            
            if(ds_list.getRowCount() == 0)
            {
				var nRow = ds_list.addRow();
				ds_list.setColumn(nRow, 'poId',pv_poId);
				ds_list.setColumn(nRow, 'poNum',pv_poNum);
				ds_list.setColumn(nRow, 'claimNum',pv_claimNum);
				ds_list.setColumn(nRow, 'upjang',pv_upjang);
				ds_list.setColumn(nRow, 'centerCode',pv_centerCode);
				ds_list.setColumn(nRow, 'subinvCode',pv_subinvCode);
				ds_list.setColumn(nRow, 'itemCode',pv_itemCode);
				ds_list.setColumn(nRow, 'itemName',pv_itemName);
				ds_list.setColumn(nRow, 'itemSize',pv_itemSize);
				ds_list.setColumn(nRow, 'cname',g_Name);
				ds_list.setColumn(nRow, 'cuser',g_EmpNo);
				ds_list.setColumn(nRow, 'cdate',fv_sToday);
            }
            //trace(ds_list.saveXML());
            break;
        
        // 저장
        case "save" :
			// 첨부파일 저장시 첨부파일 참조일련번호가 전달됨
			if(!gfn_isNull(fv_RefSeq)) ds_file.setColumn(ds_file.rowposition,"refSeq",fv_RefSeq);
			
			if(tab_photo.tabindex == 0)
			{
				ds_fileOverlep.clearData();
				fn_search();
			}
			else
			{
				var nRow;
				for(var i=0; i<ds_fileCopy.rowcount; i++)
				{
					nRow = ds_fileOverlep.addRow();
					ds_fileOverlep.setColumn(nRow,'orgnlFileNm', ds_fileCopy.getColumn(i,'orgnlFileNm'));
				}
				ds_fileCopy.clearData();
			}
			opener.fn_photoYn(1);
			gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;    

        // 삭제
        case "delete" :
			gfn_message("A", "PC 사진이 삭제 되었습니다.\n\n모바일 사진 삭제는 모바일에서 삭제가 가능합니다");	// 삭제 되었습니다.
			fv_closeFlag = "OK";
            break;    
    
    }
}

