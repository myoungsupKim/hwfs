/*----------------------------------------------------------------------------------------------
 * 설명      : 이미지 열기
 *---------------------------------------------------------------------------------------------*/
function attachImage(nRow)
{
	var fd  = new FileDialog;
	
	var vFile = fd.open("FileOpen", FileDialog.LOAD);
	
	var rtn = vFile.open(VirtualFile.openRead | VirtualFile.openBinary);
	vFile.seek(0,VirtualFile.seekBegin);
	var bData = new Buffer(vFile.read(vFile.getFileSize()));
	
	ds_list.setColumn(nRow,"blob",bData.data);

	vFile.close(); 	

}

/*----------------------------------------------------------------------------------------------
 * 설명      : 파일 미리보기 이미지 변경
 *---------------------------------------------------------------------------------------------*/
function ds_file_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{	
	var nRow = ds_file.rowposition;
	if (ds_file.getColumn(nRow, "relPath") != null)
	{
		
		if(tab_photo.tabindex == 0)
		{
			var nPath = gv_webRoot +"upload"+ ds_file.getColumn(nRow, "relPath") + "/" + ds_file.getColumn(nRow, "saveFileNm");
			tab_photo.tpg_PC.img_improvViewer.image = nPath;
		}
		else // tab_photo.tabindex == 1
		{
			var nPath = gv_webRoot+"upload/ms"+ ds_file.getColumn(nRow, "relPath") + "/" + ds_file.getColumn(nRow, "saveFileNm");
			tab_photo.tpg_mobile.img_improvViewer.image = nPath;
		}
	}
}

