 *---------------------------------------------------------------------------------------------*/
function fn_popCcCdAfter(p, data){
	if(p.oid.indexOf("cost") > 0){
		ds_search.setColumn(0, "costattachCcCd", data.code);
		ds_search.setColumn(0, "costattachCcNm", data.codeName);
	}else{
		ds_search.setColumn(0, "ccCd", data.code);
		ds_search.setColumn(0, "ccNm", data.codeName);
	}
}

