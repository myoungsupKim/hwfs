<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[레시피명중복]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
SELECT RECIPE_NAME AS CD
       , RECIPE_NAME AS NM
       , COUNT(*) CNT
<isNotEqual col="DT_GB" value="ALL">
       , FMM_MENU_EVAL_NAME_FUN(#UPJANG#,#SUBINV_CODE#,#MENU_DATE#,#MENU_DATE#,'AND B.RECIPE_NAME= ''' || RECIPE_NAME || '''') INFO
</isNotEqual>
<isEqual col="DT_GB" value="ALL">
       , FMM_MENU_EVAL_NAME_FUN(#UPJANG#,#SUBINV_CODE#,#MENU_FDATE#,#MENU_TDATE#,'AND B.RECIPE_NAME= ''' || RECIPE_NAME || '''') INFO
</isEqual>
  FROM FMM_RECIPE
 WHERE UPJANG = #UPJANG#
   AND MENU_CD IN (
                   SELECT MENU_CD
                     FROM FMM_MENU
                    WHERE UPJANG = #UPJANG#
                      AND SUBINV_CODE = #SUBINV_CODE#
<isNotEqual col="DT_GB" value="ALL">
                      AND MENU_DATE = #MENU_DATE#
</isNotEqual>
<isEqual col="DT_GB" value="ALL">
                      AND MENU_DATE BETWEEN #MENU_FDATE# AND #MENU_TDATE#
</isEqual>
                  )
   AND RECIPE_NAME NOT IN (
                            SELECT CODE_NAME
                              FROM SCC_COMMON_CODE
                             WHERE GROUP_CODE = 'FS0035'
                          )
GROUP BY RECIPE_NAME
HAVING COUNT(*)>1
ORDER BY COUNT(*) DESC,RECIPE_NAME	</statement>
	<input default-name="ds_in">
	</input>
	<output default-name="ds_list">
	</output>
</query>
