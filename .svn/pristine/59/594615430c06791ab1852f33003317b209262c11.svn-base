				ed_Tm.value="";
				ed_Tm.userdata="";
				ed_Upjang.value="";
				ed_Upjang.userdata="";
			}
			break;
		case "ed_Upjang":
			if( gfn_length(gfn_trim(e.posttext)) == 0 )
			{
				ed_Upjang.value="";
				ed_Upjang.userdata="";
			}
			break;
		case "me_From":
			if( gfn_length(gfn_trim(e.posttext)) == 0 )
			{
				me_From.value="";
			}
			break;
		case "me_To":
			if( gfn_length(gfn_trim(e.posttext)) == 0 )
			{
				me_To.value="";
			}
			break;
	}
}

