	{
		g_Message("EE","개선사진이 등록되어있지 않습니다.");
		return;
	}

	var arr = fsp_fileRead(httpFile
						 , serverSaveDir
						 , ds_imgeList.getColumn(nRow, "IMPROV_SVR_IMGE")
						 , ""
						 , ds_imgeList.getColumn(nRow, "IMPROV_ORGNL_IMGE"));
