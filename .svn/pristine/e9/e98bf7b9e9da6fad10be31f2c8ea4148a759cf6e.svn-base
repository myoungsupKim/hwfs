/******************************************************************************************
 * 함ㅡ수ㅡ명 : btn_uploadImge_onclick(obj:Button,  e:ClickEventInfo)
 * 기ㅡㅡㅡ능 : 점검사진등록
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-03-23
 ******************************************************************************************/
function btn_uploadImge_onclick(obj:Button,  e:ClickEventInfo)
{
	v_objFileDialog.filter = "image files (*.bmp;*.jpg;*.jpeg;*.gif;*.png)|*.bmp;*.jpg;*.jpeg;*.gif;*.png";
	
	var v_arrFileList;
	if (strLoadPath == "") v_arrFileList = v_objFileDialog.open("파일 추가", FileDialog.MULTILOAD, "%MYDOCUMENT%");
	else v_arrFileList = v_objFileDialog.open("파일 추가", FileDialog.MULTILOAD, strLoadPath);
	
	if (v_arrFileList)
	{
		for (var i = 0; i < v_arrFileList.length; i++) 
		{
			fn_addFile(v_objFileDialog, v_arrFileList[i]);
			strLoadPath = v_arrFileList[i].fullpath;
		}
	}
}

/******************************************************************************************
 * 함ㅡ수ㅡ명 : btn_uploadImprovImge_onclick(obj:Button,  e:ClickEventInfo)
 * 기ㅡㅡㅡ능 : 개선사진등록
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-03-23
 ******************************************************************************************/
function btn_uploadImprovImge_onclick(obj:Button,  e:ClickEventInfo)
{
	var v_objFileDialog = new FileDialog();
	v_objFileDialog.defaultextention = true;
	v_objFileDialog.filter = "image files (*.bmp;*.jpg;*.jpeg;*.gif;*.png)|*.bmp;*.jpg;*.jpeg;*.gif;*.png";
	
	var v_file;
	if (strLoadPath == "") v_file = v_objFileDialog.open("파일 추가", FileDialog.LOAD, "%MYDOCUMENT%");
	else v_file = v_objFileDialog.open("파일 추가", FileDialog.LOAD, strLoadPath);
	
	if (v_file)
	{
		fn_addImprovFile(v_objFileDialog, v_file);
		strLoadPath = v_file.fullpath;
	}
}

/******************************************************************************************
 * 함ㅡ수ㅡ명 : ds_imgeList_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
 * 기ㅡㅡㅡ능 : 사진목록 선택시 이미지 뷰어 미리보기 처리
