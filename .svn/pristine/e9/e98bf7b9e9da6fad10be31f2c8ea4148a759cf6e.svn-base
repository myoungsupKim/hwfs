/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_delete(obj:Button, e:ClickEventInfo) 
 * 기ㅡㅡㅡ능 : 삭제
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-03-23
 ******************************************************************************************/
function fn_delete(obj:Button, e:ClickEventInfo) 
{
	if(!gfn_dsIsUpdated(ds_imgeList))
	{
		g_Message("EE","변경된 데이터가 없습니다.");
		return false;
	}

    // 삭제확인
	if(!g_Confirm("01","사진을 삭제하시겠습니까?")){
		return;
	}
	
	if(CALL_TYPE == "CHECK")
	{	
		var nRowCnt = ds_imgeList.getRowCount();	
		var btp1 = true;
		var btp2 = true;
		for (var i=nRowCnt; i>=0; i--) 
		{
			if (ds_imgeList.getColumn(i, "CHK") == 1)
			{
				if (ds_imgeList.getRowType(i) == 4)
				{
					// No Image일 경우는 스킵
					if(gfn_indexOf(ds_imgeList.getColumn(i,"SVR_IMGE"), "theme://", 0) > -1) 
					{
						fn_deleteFile(ds_imgeList.getColumn(i, "SRLNUM"));
						continue;
					}
				
					btp1 = fsp_fileDelete(serverSaveDir, ds_imgeList.getColumn(i, "SVR_IMGE"), true);
					if (!gfn_isNull(ds_imgeList.getColumn(i, "IMPROV_SVR_IMGE")))
					{
						btp2 = fsp_fileDelete(serverSaveDir, ds_imgeList.getColumn(i, "IMPROV_SVR_IMGE"), true);
					}
					if (btp1 && btp2){
						fn_deleteFile(ds_imgeList.getColumn(i, "SRLNUM"));
					}else{
						g_Message("EE", "사진 삭제를 실패하였습니다.");
						return;
					}
				}
				else
				{
					ds_imgeList.deleteRow(i);
				}
			}
		}
	}
	else 
	{
		var nRowCnt = ds_imgeList.getRowCount();	
		var btp1 = true;
		var btp2 = true;
		for (var i=nRowCnt; i>=0; i--) 
		{
			if (ds_imgeList.getColumn(i, "CHK") == 1)
			{
				if(gfn_isNull(ds_imgeList.getColumn(i, "IMPROV_SVR_IMGE")))
				{
					continue;
				}
			
				if (ds_imgeList.getRowType(i) == 4)
				{
					btp1 = fsp_fileDelete(serverSaveDir, ds_imgeList.getColumn(i, "IMPROV_SVR_IMGE"), true);
					if (btp1) {
						fn_deleteImprovFile(ds_imgeList.getColumn(i, "SRLNUM"));
					}else{
						g_Message("EE", "사진 삭제를 실패하였습니다.");
						return;
					}
				}
			}
		}	
	}		
}

