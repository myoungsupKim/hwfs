var serverSaveDir = "FSC_RESULT_PHOTO";						// 파일패스
var serverSaveUrl = fsp_getServerDir(serverSaveDir) + "/";	// 파일패스URL
var fileLimitSize = 2097152;
var strLoadPath = "";

//=========================================================================================
// [ PART 3 ]
// Form Loading 시 작업 정의
//
//=========================================================================================
function form_onload(obj:Form, e:LoadEventInfo)
{
	gfn_formOnLoad(obj, true);
	edt_hlClassNm.value   = HL_CLASS_NM;
	edt_hmClassNm.value   = HM_CLASS_NM;
	edt_classNm.value     = CLASS_NM;
	edt_chkContents.value = CHK_CONTENTS;
	
	strLoadPath = FDIALOG_PATH;
	
	ds_input.addRow();
	ds_input.setColumn(0,"TABLE_TYPE", this.opener.ds_resultMst.getColumn(0,"TABLE_TYPE"));
	ds_input.setColumn(0,"FORM_CD", this.opener.ds_resultMst.getColumn(0,"FORM_CD"));
	ds_input.setColumn(0,"CUSTCD", this.opener.ds_resultMst.getColumn(0,"CUSTCD"));
	ds_input.setColumn(0,"CHK_DD", this.opener.ds_resultMst.getColumn(0,"CHK_DD"));
	ds_input.setColumn(0,"CLASS_CD", CLASS_CD);
	ds_input.setColumn(0,"SUB_COMP_NM", this.opener.ds_resultMst.getColumn(0,"SUB_COMP_NM"));

 	ds_cond.addRow();
 	ds_cond.setColumn(0, "GUBUN", "05");
 	Tab00.tabpage2.cal_rgstDate.value = gfn_today();

	if(CALL_TYPE == "CHECK")
	{
		Tab00.tabpage1.btn_uploadImge.visible = true;
		Tab00.tabpage1.btn_uploadReq.visible = true;
		Tab00.tabpage1.btn_uploadImprovImge.visible = false;
		Tab00.tabpage2.enable = true;
		Tab00.tabpage2.enableevent = true;
	}
	else
	{
		Tab00.tabpage1.btn_uploadImge.visible = false;
		Tab00.tabpage1.btn_uploadReq.visible = false;
		Tab00.tabpage1.btn_uploadImprovImge.visible = true;
		Tab00.tabpage2.enable = false;
		Tab00.tabpage2.enableevent = false;
		Tab00.removeTabpage(1);
	}

	fn_search();
}

