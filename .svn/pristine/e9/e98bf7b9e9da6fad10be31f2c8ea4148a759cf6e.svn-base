/******************************************************************************************
 * 함ㅡ수ㅡ명 : fn_fileUpload()
 * 기ㅡㅡㅡ능 : 파일업로드
 * 작ㅡ성ㅡ자 : 김민수
 * 작성ㅡ일자 : 2015-03-23
 ******************************************************************************************/
function fn_fileUpload()
{
	nRowCnt = ds_imgeList.getRowCount();
	for (i=0; i<nRowCnt; i++) 
	{
		// No Image일 경우는 스킵
		if(gfn_indexOf(ds_imgeList.getColumn(i,"FILE_PATH"), "theme://", 0) > -1) 
		{
			ds_imgeList.setColumn(i,"SVR_IMGE", "theme://convertImages/img_NoImageBig.png");
			continue;
		}
		
		if (ds_imgeList.getRowType(i) == 2)
		{
			var arr = fsp_fileWrite(httpFile, serverSaveDir, ds_imgeList.getColumn(i,"FILE_PATH"), ds_imgeList.getColumn(i,"ORGNL_IMGE"));
			
			if (arr[0] != 'S')
			{
				return false;
			}
			ds_imgeList.setColumn(i,"SVR_IMGE", arr[2]);
			ds_imgeList.setColumn(i,"FILE_SIZE", arr[3]);
			
			if (ds_imgeList.getColumn(i,"IMPROV_FILE_PATH") != null)
			{
				arr = fsp_fileWrite(httpFile, serverSaveDir, ds_imgeList.getColumn(i,"IMPROV_FILE_PATH"), ds_imgeList.getColumn(i,"IMPROV_ORGNL_IMGE"));			
				
				if (arr[0] != 'S')
				{
					return false;
				}			
				ds_imgeList.setColumn(i,"IMPROV_SVR_IMGE", arr[2]);
				ds_imgeList.setColumn(i,"IMPROV_FILE_SIZE", arr[3]);
			}
		}
		else if (ds_imgeList.getRowType(i) == 4)
		{
			var arr = fsp_fileWrite(httpFile, serverSaveDir, ds_imgeList.getColumn(i,"IMPROV_FILE_PATH"), ds_imgeList.getColumn(i,"IMPROV_ORGNL_IMGE"));			
			
			if (arr[0] != 'S')
			{
				return false;
			}			
			ds_imgeList.setColumn(i,"IMPROV_SVR_IMGE", arr[2]);
			ds_imgeList.setColumn(i,"IMPROV_FILE_SIZE", arr[3]);
		}
	}
	return true;
}

