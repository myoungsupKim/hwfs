	{
		gfn_alert("msg.save.nochange");	// 변경된 내역이 없습니다.
		return;
	}
		
	for(var i=0; i<ds_dtlList.rowcount; i++){
				
		//수정일때만 품목 코드가 변경되었는지 체크
