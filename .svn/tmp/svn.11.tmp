/*----------------------------------------------------------------------------------------------
 * 설명      : 검체 리스트 조회
 *---------------------------------------------------------------------------------------------*/
function fn_doSearch()
{
    if (gfn_isNull(cal_fromDay.fn_getValue()) || gfn_isNull(cal_toDay.fn_getValue()))
    {
        gfn_alert("접수일자를 입력하여 주십시오!");
        return;
    }
    if (cal_fromDay.fn_getValue() > cal_toDay.fn_getValue())
	{
		gfn_alert("시작일이 종료일보다 큽니다.");
		return;
	}
	if (cal_fromDayTest.fn_getValue() > cal_toDayTest.fn_getValue())
	{
		gfn_alert("시험 시작일이 종료일보다 큽니다.");
		return;
	}
	
	//2018.07.04 김명섭 주석처리
	//if (gfn_dsIsUpdated(ds_testRegList))
	//{
		// 검색을 진행하면 변경된 데이터가 사라집니다. 계속 진행 하시겠습니까?
    //    var retValue = gfn_confirm("confirm.before.search");
	//	if (retValue == false) return;
    //}

    // Grid sort 표시 초기화(필수)
    gfn_clearSortMark(grd_testRegList);
    gfn_clearSortMark(grd_specmList);
    gfn_clearSortMark(grd_testArticleList);

    // Dataset을 초기화한다.
    ds_testRegList.clearData();
    ds_specmList.clearData();
    ds_testArticleList.clearData();

    var strSvcID    = "selectList";
    var strURL      = "ls/ltm/testResultInquery/selectList.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "ds_testRegList=ds_testRegList";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
}

/*----------------------------------------------------------------------------------------------
 * 설명      : 접수 리스트 클릭
 *---------------------------------------------------------------------------------------------*/
function grd_testRegList_oncellclick(obj:Grid, e:GridClickEventInfo)
{
	if (e.cell == 10) return;
    if (e.cell != 11)
    {
        // 의뢰정보 클릭 시 전자결재 상신 또는 상신상세 버튼 연동
        fn_gwUserButtonInit();

        //if (e.oldrow == e.row) return;

        // Grid sort 표시 초기화(필수)
        gfn_clearSortMark(grd_specmList);
        gfn_clearSortMark(grd_testArticleList);

        // Dataset을 초기화한다.
        ds_specmList.clearData();
        ds_testArticleList.clearData();

        ds_search.setColumn(0, "reqNum", ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum"));

        var strSvcID    = "selectSpecmList";
        var strURL      = "ls/ltm/testResultInquery/selectSpecmList.xdo";
        var strInDs     = "ds_search=ds_search:A";
        var strOutDs    = "ds_specmList=ds_specmList";
        var strArg      = "";
        var strCallback = "fn_callBack";
        var bAsync      = true;

        gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync);
    }
    else
    {
        var iPublishCnt = ds_testRegList.getColumn(ds_testRegList.rowposition, "publishCnt");
        if (Number(iPublishCnt) == 0) return;

        // 성적서 다운로드
        //var objHttp = new HttpObject();
        var strFileName = gfn_replace(ds_testRegList.getColumn(ds_testRegList.rowposition, "acceptnm"), ".", "") + ".zip";
        var strSrvFileName = "";
        var iPublishCnt = ds_testRegList.getColumn(ds_testRegList.rowposition, "publishCnt");
        // 성적서 재발급 건은 파일명이 틀리다.
        if (Number(iPublishCnt) > 1)
        {
            strSrvFileName = ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum") + "_reissue.zip";
        }
        else
        {
            strSrvFileName = ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum") + ".zip";
        }
        if (LimsUtil.fileDownload(this, objHttp, "/ls/inspectrpt/", gfn_trim(strFileName), strSrvFileName, false))
        {
            gfn_alert("다운로드가 완료 되었습니다!");
        }
    }
}

