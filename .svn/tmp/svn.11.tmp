/**
 * 그룹웨어 상신 버튼 클릭 시 상신 팝업을 호출한다.
 * @param obj Button
 * @param e ClickEventInfo
 * @return none
 */
function fn_gwDraftPopup(rtnVal, draftGubun)
{
    fv_retVal = rtnVal;

    var strFileName = ds_testRegList.getColumn(ds_testRegList.rowposition, "acceptnm") + ".zip";
    var strSrvFileName = "";
    var iPublishCnt = ds_testRegList.getColumn(ds_testRegList.rowposition, "publishCnt");
    // 성적서 재발급 건은 파일명이 틀리다.
    if (Number(iPublishCnt) > 1)
    {
        strSrvFileName = ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum") + "_reissue.zip";
    }
    else
    {
        strSrvFileName = ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum") + ".zip";
    }

    //그룹웨어에 전달할 파라메터 생성
    var arrParams	= SCGw.newParams ();

    //(필수) 결재문서 서비스명 - 문서양식별로 설정된 서비스명 설정
    var gwServiceId = "HWRC_FC_LRM_TEST_RESULT";
    //var gwTitle = ds_testRegList.getColumn(ds_testRegList.rowposition, "upjangNm") + " 시험결과 상신";
    var gwTitle = rtnVal.limsDraftTitle;
    // 부적합 결재 상신인 경우
    if (draftGubun == '2')
    {
        gwServiceId = "HWRC_FC_LRM_TEST_NOTFIT_RESULT";
        //gwTitle = ds_testRegList.getColumn(ds_testRegList.rowposition, "upjangNm") + " 부적합결과 상신";
        gwTitle = rtnVal.limsDraftTitle;
    }
    SCGw.setParams (arrParams, SCGw.GW_SERVICE_NAME, gwServiceId);

    //(필수) 본문 URL : (주의) .do로 호출해야 하며 GET 방식으로 파라미터를 전달 (id라는 key는 사용 불가)
    var sBodyUrl = "/ls/ltm/testResultInquery/makeGwDraftContent.do?searchId=" + ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum")
                                            + "&insptPurp=" + rtnVal.insptPurp
                                            + "&insptMtod=" + rtnVal.insptMtod
                                            + "&attachFileNm=" + gfn_nvl(rtnVal.attachFileNm, "")
                                            + "&limsBigo=" + gfn_nvl(rtnVal.limsBigo, "")
                                            + "&limsAfterProc=" + gfn_nvl(rtnVal.limsAfterProc, "")
                                            + "&fileName=" + strFileName
                                            + "&srvFileName=" + strSrvFileName
                                            + "&draftGubun=" + draftGubun
                                            
                                            											//업장 추가 20160316  맹수영   요청자: 김민채, 최준열
	                                        + "&upjangNm=" + ds_testRegList.getColumn(ds_testRegList.rowposition, "upjangNm")

                                            ;
    SCGw.setParams (arrParams, SCGw.GW_BODY_URL, sBodyUrl);

    //(필수) 본문 제목 – 문서양식 별로 설정된 업무명
    SCGw.setParams (arrParams, SCGw.GW_BODY_TITLE, gwTitle);

    //(필수)문서 방향 : 가로 또는 세로 방향 (GW_VERTICAL, GW_HORIZONTAL)
    SCGw.setParams (arrParams, SCGw.GW_PORTRAIT, SCGw.VERTICAL);

    // 성적서 발행건이 있으면 파일을 첨부한다.
    if (Number(iPublishCnt) > 0)
    {
        //(선택)첨부문서 목록 : json 형태 문자열
        fv_attachList = [{
            fileName : rtnVal.fileName,
            filePath : rtnVal.filePath
        }];
        SCGw.setParams (arrParams, SCGw.GW_ATTACH_LIST, gfn_jsonObjectToString(fv_attachList));
    }

    //(선택)Client Id : 결재 instance id와 연관을 맺기위한 업무별 ID로 unique key를 이용, 생략하면 상신시각이 됨(최대 40 byte)
    //SCGw.setParams (arrParams, SCGw.GW_CLIENT_ID, edt_id.value);

    strDraftGubun = draftGubun;

    //그룹웨어 상신 팝업을 호출한다.
    SCGw.sendDraft (this, arrParams);
}

