/************************************************************************************************
 * TRANSACTION 및 CallBack 영역 (필수)
************************************************************************************************/
/*----------------------------------------------------------------------------------------------
 * 설명      : 트랜잭션 콜백 함수 (필수)
 *---------------------------------------------------------------------------------------------*/
function fn_callBack(strSvcID, nErrorCode, strErrorMsg)
{
    // 에러시 gfn_callBack에서 메시지 제공, 화면별 특정 에러 발생시에만 사용하세요.
    if (nErrorCode < 0)
    {
        return;
    }

    switch(strSvcID)
    {
        // 의뢰목록 조회
        case "selectList" :
            fv_retVal = {};
            strDraftGubun = '1';

            btn_gwDraft.enable = false;
            btn_gwDetail.enable = false;
            btn_gwDraftNotfit.enable = false;
            btn_gwDetailNotfit.enable = false;

            if (ds_testRegList.rowposition != -1) {
                // 강제로 첫 row 클릭
                grd_testRegList_oncellclick(grd_testRegList, evnentInfo);
            }
            break;
        // 검체목록 조회
        case "selectSpecmList" :
            if (ds_specmList.rowposition != -1) {
                fn_searchDetail();
            }
            break;
        case "selectInsptUserList" :	// 담당자
            ds_userList.insertRow(0);
            ds_userList.setColumn(0, "sabun", "");
            ds_userList.setColumn(0, "userNm", "전체");
            cbo_userList.index = 0;
            break;
        // 시험항목조회
        case "selectTestArticleList" :
			if(ds_testRegList.getColumn(ds_testRegList.rowposition, "progressStatus") < '200')
			{
				if(techMasterYn == "Y") btn_saveDiary.enable = true;
				else btn_saveDiary.enable = false;
			}
			var tSts, pSts;
			tSts = ds_testArticleList.getColumn(0, "progressTotsts");
			pSts= ds_testArticleList.getColumn(0, "progressSts");
			if(tSts != null && tSts != "undefined") StaticTsts.text = "Total" + tSts;
			if(pSts != null && pSts != "undefined") StaticSts.text = "검체" + pSts;
			//filter
			if(chk_notyet.value == 1) ds_testArticleList.filter("completeDd == ''");
			else ds_testArticleList.filter("completeDd == '' || completeDd != ''");
            break;
        case "saveInsptPurpMtod" :
            if (fv_iProcessCnt > 0) gfn_alert("결재 상신 되었습니다!");	// 저장 되었습니다.
            fv_retVal = {};
            strDraftGubun = '1';
            // 의뢰정보 클릭 시 전자결재 상신 또는 상신상세 버튼 연동
            fn_gwUserButtonInit();
            break;
        // 검체 폐기 저장
        case "saveDisuseYn" :
			if (fv_iProcessCnt > 0) gfn_alert("msg.save.success");	// 저장 되었습니다.
			if (ds_testRegList.rowposition != -1) {
                // 강제로 첫 row 클릭
                grd_testRegList_oncellclick(grd_testRegList, evnentInfo);
            }
            break;
        // 성적서 발급여부를 변경
        case "saveInspectRptIssueYn" :
			if (fv_iProcessCnt > 0) gfn_alert("msg.save.success");	// 저장 되었습니다.
            break;
        case "selectUserList" :	// 기술책임자조회
			//trace(ds_userList2.saveXML());
			// 기술책임자체크
			for(var i=0;i<ds_userList2.rowcount;i++)
			{
				if(ds_userList2.getColumn(i,"sabun") ==  gfn_getUserId())
				{
					//기술책임자
					techMasterYn = "Y";
					techrespUser = ds_userList2.getColumn(i,"sabun");
					techrespUserNm = ds_userList2.getColumn(i,"userNm");
					break;
				}
			}
            break;
        case "saveResultList" :
            if (fv_iProcessCnt > 0) gfn_alert("msg.save.success");	// 저장 되었습니다.
            //시험완료승인중 진행상태 변경
            if(ds_retval.getColumn(0,"procClass") == "180")
            {
				ds_testRegList.setColumn(ds_testRegList.rowposition,"progressStatus","180");
				ds_testRegList.setRowType(ds_testRegList.rowposition, Dataset.ROWTYPE_NORMAL);
            }
            if (ds_specmList.rowposition != -1) {
                fn_searchDetail();
            }
            break;
    }
}

