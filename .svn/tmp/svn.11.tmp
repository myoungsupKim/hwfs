 ***************************************************************************/
function fn_testDiaryCheck()
{
    // 접수 승인완료 / 시험진행중
    if (fn_testDiaryWriteViewCheck('WRITE'))
    {
        return 'button';
    }
    else
    {
        return 'normal';
    }
}

