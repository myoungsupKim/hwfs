function scgw_ondraftnotify(instanceId)
{
    //alert("instance id=[" + instanceId + "]"+" SCGw.GW_DRAFT_STATUS :"+SCGw.GW_DRAFT_STATUS);
    if (strDraftGubun == '1')
    {
        ds_testRegList.setColumn(ds_testRegList.rowposition, "signInstanceId", instanceId);	 //결재InstanceId 설정
        ds_testRegList.setColumn(ds_testRegList.rowposition, "signProgressStatus", SCGw.GW_DRAFT_STATUS);	 //결재진행상태를 상신으로 설정
    }
    else
    {
        ds_testRegList.setColumn(ds_testRegList.rowposition, "signInstanceIdNotfit", instanceId);	 //결재InstanceId 설정
        ds_testRegList.setColumn(ds_testRegList.rowposition, "signNotfitProgressStatus", SCGw.GW_DRAFT_STATUS);	 //결재진행상태를 상신으로 설정
    }

    ds_testRegList.updatecontrol = false;
    ds_testRegList.setRowType(ds_testRegList.rowposition, Dataset.ROWTYPE_NORMAL);
    ds_testRegList.updatecontrol = true;

    fn_testResultDraft();	//결재정보 저장
}

/**
 * 전자결재 결과 정보를 업무테이블에 저장 한다.
 * @param
 * @return
 */
function fn_testResultDraft()
{
    ds_search.setColumn(0, "reqNum", ds_testRegList.getColumn(ds_testRegList.rowposition, "reqNum"));
    if (strDraftGubun == '1')
    {
        ds_search.setColumn(0, "signInstanceId", ds_testRegList.getColumn(ds_testRegList.rowposition, "signInstanceId"));
    }
    else
    {
        ds_search.setColumn(0, "signInstanceId", ds_testRegList.getColumn(ds_testRegList.rowposition, "signInstanceIdNotfit"));
    }
    ds_search.setColumn(0, "insptPurp", fv_retVal.insptPurp);
    ds_search.setColumn(0, "insptMtod", fv_retVal.insptMtod);
    ds_search.setColumn(0, "draftGubun", strDraftGubun);

    var strSvcID    = "saveInsptPurpMtod";
    var strURL      = "/ls/ltm/testResultInquery/saveInsptPurpMtod.xdo";
    var strInDs     = "ds_search=ds_search:A";
    var strOutDs    = "";
    var strArg      = "";
    var strCallback = "fn_callBack";
    var bAsync      = true;

    gfn_transaction(this, strSvcID, strURL, strInDs, strOutDs, strArg, strCallback, bAsync, false);
}

