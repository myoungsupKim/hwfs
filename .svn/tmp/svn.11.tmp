 ***************************************************************************/
function btn_searchTestDiary_onclick(obj:Button,  e:ClickEventInfo)
{
    // 접수 승인완료 / 시험진행중
    if (fn_testDiaryWriteViewCheck('VIEW'))
    {
        if (ds_testArticleList.rowposition == -1)
        {
            gfn_alert("시험항목을 선택하여 주십시오!");
            return;
        }

        var bDiaryWriteYn = false;
        if (ds_testArticleList.getColumn(ds_testArticleList.rowposition, "diaryWriteYn") == 'N')
        {
            bDiaryWriteYn = true;
        }
        else
        {
            bDiaryWriteYn = false;
        }
        var objParam = {
            "reqNum" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "reqNum")
          , "acceptNum" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "acceptNum")
          , "articleCd" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "articleCd")
          , "sabun" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "sabun")
          , "completeYn" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "completeYn")
          , "diaryWriteYn" : bDiaryWriteYn
        };

        var rtnVal = gfn_dialog("TestDiaryRegPop"				// dialogId
                       , "X_LTM::TestDiaryRegPop.xfdl"			// Url
                       , {fv_Contents:objParam}			// 파라미터
                       , null, null, null, null, null, false, true, -1);
        if (rtnVal != null && rtnVal != "undefined" && rtnVal == "OK")
        {
            fn_searchDetail();
        }
    }
    else
    {
        gfn_alert("시험일지 조회가 불가능 합니다.");
        return;
    }
}

/***************************************************************************
 * 설명   : 시험목록 클릭 시
 ***************************************************************************/
function grd_testArticleList_oncellclick(obj:Grid, e:GridClickEventInfo)
{
    // 일지 작서여부 cell 클릭
    if (e.cell == 16)
    {
        // 접수 승인완료 / 시험진행중
        if (fn_testDiaryWriteViewCheck('VIEW'))
        {
            // 일지를 작성하지 않았으면 시험일지를 띄울때 입력 가능한 상태로 변경
            if (eval(obj.binddataset).getColumn(e.row, "diaryWriteYn") == 'N')
            {
                var bDiaryWriteYn = false;
                if (eval(obj.binddataset).getColumn(eval(obj.binddataset).rowposition, "diaryWriteYn") == 'N')
                {
                    bDiaryWriteYn = true;
                }
                else
                {
                    bDiaryWriteYn = false;
                }
                var objParam = {
                    "reqNum" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "reqNum")
                  , "acceptNum" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "acceptNum")
                  , "articleCd" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "articleCd")
                  , "sabun" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "sabun")
                  , "completeYn" : ds_testArticleList.getColumn(ds_testArticleList.rowposition, "completeYn")
                  , "diaryWriteYn" : bDiaryWriteYn
                };

                var rtnVal = gfn_dialog("TestDiaryRegPop"				// dialogId
                               , "X_LTM::TestDiaryRegPop.xfdl"			// Url
                               , {fv_Contents:objParam}			// 파라미터
                               , null, null, null, null, null, false, true, -1);
                if (rtnVal != null && rtnVal != "undefined" && rtnVal == "OK")
                {
                    fn_searchDetail();
                }
            }
        }
    }
}

