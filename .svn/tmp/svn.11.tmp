function ds_testArticleList_onvaluechanged(obj:Dataset,  e:DSColChangeEventInfo)
{
	//판정결과변경
	if(e.columnid == "decision")
	{
		//판정결과 최종판정 반영
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "lastDecision", e.newvalue);
		//판정일자 공백이면 당일날짜 세팅
		if(ds_testArticleList.getColumn(ds_testArticleList.rowposition, "testdd") == "") ds_testArticleList.setColumn(ds_testArticleList.rowposition, "testdd", curDate);
		//기술책임자
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "techrespUser", techrespUser);
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "techrespUserNm", techrespUserNm);
		//일지작성여부(작성)
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "diaryWriteYn", "I");
	}
	//최종판정결과변경
	if(e.columnid == "lastDecision")
	{
		//판정일자 공백이면 당일날짜 세팅
		if(ds_testArticleList.getColumn(ds_testArticleList.rowposition, "testdd") == "") ds_testArticleList.setColumn(ds_testArticleList.rowposition, "testdd", curDate);
		//기술책임자
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "techrespUser", techrespUser);
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "techrespUserNm", techrespUserNm);
		//일지작성여부(작성)
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "diaryWriteYn", "I");
	}
	//결과변경
	if(e.columnid == "result")
	{
		//판정일자 공백이면 당일날짜 세팅
		if(ds_testArticleList.getColumn(ds_testArticleList.rowposition, "testdd") == "") ds_testArticleList.setColumn(ds_testArticleList.rowposition, "testdd", curDate);
		//기술책임자
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "techrespUser", techrespUser);
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "techrespUserNm", techrespUserNm);
		//일지작성여부(작성)
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "diaryWriteYn", "I");
	}
	//판정일자변경
	if(e.columnid == "testdd" && e.newvalue != "")
	{
		//일자유효성체크
		if(!gfn_isDate(e.newvalue))
		{
			gfn_alert("날짜형식이 유효하지 않습니다.");
			ds_testArticleList.setColumn(ds_testArticleList.rowposition, "testdd", "");
			return;
		}
		//기술책임자
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "techrespUser", techrespUser);
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "techrespUserNm", techrespUserNm);
		//일지작성여부(작성)
		ds_testArticleList.setColumn(ds_testArticleList.rowposition, "diaryWriteYn", "I");
	}
}

