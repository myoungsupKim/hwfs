             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+10,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || '2200', 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)) / 60, 2) ELSE 0 END AS NIGHT_TIME10

