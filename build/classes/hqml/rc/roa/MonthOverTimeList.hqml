             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+7,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || '2200', 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)) / 60, 2) ELSE 0 END AS NIGHT_TIME7
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+8,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_START_TIME) ELSE '' END AS DUTY_START_TIME8
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+8,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_END_TIME) ELSE '' END AS DUTY_END_TIME8
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+8,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || MAX(DUTY_START_TIME), 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)  - MAX(DEDUCTION_TIME)) / 60, 2) ELSE 0 END AS WORK_TIME8
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+8,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || '2200', 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)) / 60, 2) ELSE 0 END AS NIGHT_TIME8
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+9,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_START_TIME) ELSE '' END AS DUTY_START_TIME9
