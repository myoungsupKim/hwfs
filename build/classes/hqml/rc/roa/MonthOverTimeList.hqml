             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+7,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_END_TIME) ELSE '' END AS DUTY_END_TIME7
