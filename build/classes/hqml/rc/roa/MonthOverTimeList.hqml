             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+31,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || '2200', 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)) / 60, 2) ELSE 0 END AS NIGHT_TIME31
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+32,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_START_TIME) ELSE '' END AS DUTY_START_TIME32
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+32,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_END_TIME) ELSE '' END AS DUTY_END_TIME32
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+32,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || MAX(DUTY_START_TIME), 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)  - MAX(DEDUCTION_TIME)) / 60, 2) ELSE 0 END AS WORK_TIME32
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+32,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || '2200', 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)) / 60, 2) ELSE 0 END AS NIGHT_TIME32
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+33,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_START_TIME) ELSE '' END AS DUTY_START_TIME33
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+33,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_END_TIME) ELSE '' END AS DUTY_END_TIME33
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+33,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || MAX(DUTY_START_TIME), 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)  - MAX(DEDUCTION_TIME)) / 60, 2) ELSE 0 END AS WORK_TIME33
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+33,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || '2200', 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)) / 60, 2) ELSE 0 END AS NIGHT_TIME33
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+34,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_START_TIME) ELSE '' END AS DUTY_START_TIME34
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+34,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_END_TIME) ELSE '' END AS DUTY_END_TIME34
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+34,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || MAX(DUTY_START_TIME), 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)  - MAX(DEDUCTION_TIME)) / 60, 2) ELSE 0 END AS WORK_TIME34
             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+34,'YYYYMMDD') = OCCUR_DATE THEN ROUND((((TO_DATE(MAX(DUTY_END_DATE) || MAX(DUTY_END_TIME), 'YYYY-MM-DD HH24:MI:SS') - TO_DATE(MAX(DUTY_START_DATE) || '2200', 'YYYY-MM-DD HH24:MI:SS')) * 86400 / 60)) / 60, 2) ELSE 0 END AS NIGHT_TIME34
          FROM RSS_OVER_TIME_WORK_ATT
         WHERE UPJANG = :upjang
           AND OCCUR_DATE >= (SELECT MIN(OCCUR_DATE) FROM RSS_OVER_TIME_WEEK_DATE_INFO WHERE YEAR||MONTH = :fr_month)
           AND OCCUR_DATE <= (SELECT TO_CHAR(TO_DATE(MAX(OCCUR_DATE), 'YYYYMMDD') + 6, 'YYYYMMDD') FROM RSS_OVER_TIME_WEEK_DATE_INFO WHERE YEAR||MONTH = :fr_month)
         GROUP BY EMP_NM, OCCUR_DATE
       )
 GROUP BY EMP_NM
	]]>
	</statement>

	<!-- 월별 근무일을 조회한다. -->
	<statement name="selectWorkDayList">
	/*+ com.hwfs.rc.roa.dao.MonthOverTimeListDAO.selectWorkDayList */
	<![CDATA[
SELECT CD_SY AS YY
     , CD_SM AS MM
     , CD_SD AS DD
     , CD_SY||CD_SM||CD_SD AS YYYYMMDD
  FROM SCC_CALENDA_DATA
 WHERE CD_SY||CD_SM||CD_SD >= (SELECT MIN(OCCUR_DATE) FROM RSS_OVER_TIME_WEEK_DATE_INFO WHERE YEAR||MONTH = :fr_month)
   AND CD_SY||CD_SM||CD_SD <= (SELECT TO_CHAR(TO_DATE(MAX(OCCUR_DATE),'YYYYMMDD') + 6, 'YYYYMMDD') FROM RSS_OVER_TIME_WEEK_DATE_INFO WHERE YEAR||MONTH = :fr_month)
 ORDER BY CD_NO
	]]>
	</statement>
</hqml>
