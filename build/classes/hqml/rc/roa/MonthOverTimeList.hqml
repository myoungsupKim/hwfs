             , CASE WHEN TO_CHAR(TO_DATE(:min_day,'YYYYMMDD')+10,'YYYYMMDD') = OCCUR_DATE THEN MAX(DUTY_START_TIME) ELSE '' END AS DUTY_START_TIME10
